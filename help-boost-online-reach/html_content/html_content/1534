	<script type="text/javascript">
	function ich_author_ajax(fn, user_id, pagenum) {
		$('#show a').removeClass('active');
		$('#'+fn).addClass('active');
		$.ajax({
			url:'/',
			data: {ich_author_fn: fn, user_id: user_id, pagenum: pagenum},
			type:'GET',
			beforeSend: function() {
				$('#ich-author-content').html('<p></p>');
				document.location.href = "#show";
			},
			success: function(html) {
				$('#ich-author-content').html(html);
			}
		});
		return false;
	}
	</script>
