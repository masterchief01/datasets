	<script type="text/javascript">
	jQuery(document).ready(function() {
		ich_idle_plugin_secs = 0;
		ich_idle_plugin_increment = function() {
			ich_idle_plugin_secs++;
			if(ich_idle_plugin_secs > 60*20) {
				window.location.reload();
			}
		}
		var ich_idle_plugin_interval = setInterval("ich_idle_plugin_increment()", 1000);
		jQuery(this).mousemove(function(e){
			ich_idle_plugin_secs = 0;
		});
		jQuery(this).keyup(function(e){
			ich_idle_plugin_secs = 0;
		});
	})
	</script>
<script type="text/javascript">
function ich_login_submit() {
	var params = $('#ich_login_form').serialize();
	params += "&ich_login_action=" + $('#ich_login_action').val();
	$.post("/", params,
		function(data) {
			if(data.result == 'success') {
				tb_remove();
				$('#signin').html('<a href="/user-settings">Settings</a><span>|</span><a href="#" onclick="ich_login_logout();return false;">Log Out</a>');
				$('#respond-login').html('<p><a href="/user-settings">Settings</a><span>|</span><a href="#" onclick="ich_login_logout();return false;">Log Out</a></p>');
				ich_comments_reload();
			} else {
				$('#ich_login_message').show().html(data.message);
			}
		}, "json");
}
function ich_login_logout() {
	$.post("/", "ich_login_action=logout",
		function(data) {
			$('#signin').html('<a href="#TB_inline?height=224&width=285&inlineId=ich_login_dlg&modal=true" class="thickbox" onclick="ich_login_login();return false;">Sign-In</a><span>|</span><a href="#TB_inline?height=224&width=285&inlineId=ich_login_dlg&modal=true" class="thickbox" onclick="ich_login_register();return false;">Register</a>');
			$('#respond-login').html('<p>You must be logged-in to post a comment.</p><p><a href="#TB_inline?height=224&width=285&inlineId=ich_login_dlg&modal=true" class="thickbox" onclick="ich_login_login();return false;">Login To Your Account</a></p>');
			tb_init('#signin a.thickbox, #respond-login a.thickbox');
			ich_comments_reload();
		});
}
function ich_login_register() {
	$('#ich_login_message').hide();
	$('#ich_login_dlg_wrap h3').text('Register');
	$('#ich_login_form #ich_login_action').val('register');
	$('#ich_login_form #password').css('visibility','hidden').css('display','none');
	$('#ich_login_form #email').css('visibility','visible').css('display','block');
	$('#ich_login_register').hide();
	$('#ich_login_login').show();
}
function ich_login_forgot() {
	$('#ich_login_dlg_wrap h3').text('Forgot Password');
	$('#ich_login_form #ich_login_action').val('forgot');
	$('#ich_login_form #password').css('visibility','hidden').css('display','block');
	$('#ich_login_form #email').css('display','none');
}
function ich_login_login() {
	$('#ich_login_message').hide();
	$('#ich_login_dlg_wrap h3').text('Login');
	$('#ich_login_form #ich_login_action').val('login');
	$('#ich_login_form #password').css('visibility','visible').css('display','block');
	$('#ich_login_form #email').css('visibility','hidden').css('display','none');
	$('#ich_login_login').hide();
	$('#ich_login_register').show();
}
function ich_login_twitter() {
	tb_remove();
	$('#twc_button').click();
}
function ich_login_facebook() {
	tb_remove();
	FB.Connect.requireSession();
}
</script>
