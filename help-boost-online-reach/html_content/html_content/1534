<script type="text/javascript">
jQuery(document).ready(function() {
	jQuery('#search input, #ich_login_widget input').focus(function() {
		jQuery(this).css('background','#fff');
	});
	jQuery('#search input, #ich_login_widget input').blur(function() {
		if(jQuery(this).val() == '') {
			jQuery(this).css('background','none');
		}
	});
	jQuery('#search input, #user_login, #user_password').val('');
});
</script>
