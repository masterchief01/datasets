<script type="text/javascript">
    // Pinit fix for IE 8 (they spell transparency wrong on the iframe)
    (function ($) {
        if ($.browser.msie) {
            var maxRuns = 10;
            var ol = window.onload;

            var fixit = function (max) {
                var iframe = $(".plusonepin iframe");

                if (iframe.length === 0 && max > 0) {
                    setTimeout(function () { fixit(max - 1); }, 250);
                }
                else if (iframe.length > 0) {
                    iframe.each(function () {
                        var i = $(this).get(0);

                        if (i && i.allowTransparency !== undefined) {
                            i.allowTransparency = true;
                        }
                    });
                }
            };

            window.onload = (typeof ol == 'function') ? function () { ol(); fixit(maxRuns); } : function () { fixit(maxRuns); };
        }
    })(jQuery);
</script>

