  );
    function showRaters(entryId) {
        new Ajax.Updater('ratersDiv','/ajax/raters', {
            parameters: {"id":entryId},
            onSuccess: function( t ) {
            	Effect.BlindDown( $("ratersDiv") );
            },
            onFailure: function( t ) {
                feedBack.add('error ' + t.status + ' -- ' + t.statusText);
            }
        });
    }
