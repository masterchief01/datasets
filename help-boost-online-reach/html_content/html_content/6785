	function on_blur()
	{
		if(document.getElementById('nav-search').value == "")
		{
		document.getElementById('nav-search').value = "search here...";
		document.getElementById('nav-search').style.color = "#999";
		}
	}
	function on_focus()
	{
		document.getElementById('nav-search').value = "";
		document.getElementById('nav-search').style.color = "#444";
	}

	function open_reply_comment(comment_id, post_id, back_url, blog_name)
	{
		
		var div = document.getElementById('reply-comment'+comment_id);	
		var postdata ='comment_id='+ comment_id + '&post_id=' + post_id + '&back_url=' + back_url + '&blog_name=' + blog_name;
		div.innerHTML = '<div style="font:bold 1.2em/1.2em Georgia, serif;padding:20px 0 35px;"><center><img src="/css-img/wheel.gif" /> &nbsp; &nbsp;Loading...</center></div>';

		var handleSuccess = function(o){if(o.responseText !== undefined){div.innerHTML = o.responseText;}}
		var handleFailure = function(o){if(o.responseText !== undefined){div.innerHTML=  o.statusText;}}
		var Callback ={success:handleSuccess,failure: handleFailure};

		var request = YAHOO.util.Connect.asyncRequest("POST", '/reply_comment.php', Callback, postdata);
	}
	
	function lightboxImageOn(image_id, Image_title)
	{
		var comment_image = 'comment_image';
		var Image_title = "Image uploaded by - " + Image_title;
		var div = document.getElementById('lb-edit');
		div.innerHTML = '<div style="font:bold 1.2em/1.2em Georgia, serif;padding:80px 0 35px;"><center><img src="/css-img/loading.gif" /> &nbsp; &nbsp;Loading...</center></div>';

		$.ajax({
		type: "POST",
		url: "/show_media.php",
		data: 'image_id='+image_id + '&type=' + comment_image + '&title=' + Image_title,
		success: function(msg)
		{
			var id ='#lb-edit';
			$(id).html(msg);
		}
		});

		var black = document.getElementById('fade');
		black.style.display = 'block';

		div.style.display = 'block';
		window.scrollTo(0,0);
	}


	function lightboxVideoOn(video_id, You_Tube_title)
	{
		var video='video';
		var You_Tube_title = "Video uploaded by - " + You_Tube_title;
		var div = document.getElementById('lb-edit');
		div.innerHTML = '<div style="font:bold 1.2em/1.2em Georgia, serif;padding:80px 0 35px;"><center><img src="/css-img/loading.gif" /> &nbsp; &nbsp;Loading...</center></div>';

		$.ajax({
		type: "POST",
		url: "/show_media.php",
		data: 'video_id=' + video_id + '&type=' + video + '&title=' +  You_Tube_title,
		success: function(msg)
		{
			var id ='#lb-edit';
			$(id).html(msg);
		}
		});

		var black = document.getElementById('fade');
		black.style.display = 'block';

		div.style.display = 'block';
		window.scrollTo(0,0);
	}

	function lightboxoff(id)
	{
		var light = document.getElementById(id);
		light.style.display = 'none';
	}

