<script>
	function show_top_tabs(divid)
		{
			if(divid=='topic-voted')
			{
				$('topic-commented').style.display='none';
				$('topic-voted').style.display='block';
				$('1').addClassName('active');
				$('2').removeClassName('active');

			}
			else
			{
				$('topic-commented').style.display='block';
				$('topic-voted').style.display='none';
				$('2').addClassName('active');
				$('1').removeClassName('active');
			}
		}
	</script>

