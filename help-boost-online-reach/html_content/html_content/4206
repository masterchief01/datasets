<!-- Facebook -->
<div id="fb-root"></div> 
<script type="text/javascript">

  window.fbAsyncInit = function() {
    require(['r29', 'domReady!'], function (R29) {

      FB.init({ appId: '155023867867753', status: true, cookie: true, xfbml: false});
      
      /* Check if the user is NOT logged into R29, but IS logged into Facebook.  Fix it */
      if (R29.url.param.getVar('fb') && !R29.user.token() && FB.getSession()) { //We have some sort of session, we *may* be logged in.
        FB.getLoginStatus(function(response) {
          if(response.status == 'connected') {
            return document.location = 'https://welcome.refinery29.com/connect/facebook?from=http%3A%2F%2Fwww.refinery29.com%2F%3Flogin%3D1&amp;sig=cdd48&amp;app=blog&amp;fb=1';
          }    
        });      
      }
          
      //FACEBOOK LOGIN
      $('a.fb_button').live('click', function() {
        R29.FBLoginPopup();
        return false;
      });
      
      if (typeof window.fbAsyncOnInit == "function") {
        window.fbAsyncOnInit();
      }
      if (typeof window.fangate_close == "function") {
        window.fangate_close();
      }
      
      /* GOOGLE ANALYTICS */
      FB.Event.subscribe('edge.create', function(targetUrl, fb_ref) {
      	if (fb_ref._attr.ref.indexOf('homepage') >=0){
      	  var label = fb_ref._attr.ref.slice(9);
      		_gaq.push(['_trackEvent', 'Homepage', 'Facebook Logged-out Like', label]);
      	}
      	
      	else if (fb_ref._attr.ref && $("'[data-ref='" + fb_ref._attr.ref + "']")) {
          if (fb_ref._attr.ref == $("'[data-ref='" + fb_ref._attr.ref + "']").attr('data-ref')) {
            $("'[data-ref='" + fb_ref._attr.ref + "']").click();
          }
      	}
      	else if ($(fb_ref.dom).prop('tagName') == "FB:LIKE-BOX" && ("'[data-ref='" +  $(fb_ref.dom).attr('ref') + "']")) {
          if ($(fb_ref.dom).attr('ref') == $("'[data-ref='" + $(fb_ref.dom).attr('ref') + "']").attr('data-ref')) {
            $("'[data-ref='" + $(fb_ref.dom).attr('ref') + "']").click();
          }
      	}
        _gaq.push(['_trackSocial', 'facebook', 'like', targetUrl]);
      });
      
      FB.Event.subscribe('edge.remove', function(targetUrl) {
        _gaq.push(['_trackSocial', 'facebook', 'unlike', targetUrl]);
      });
      
      FB.Event.subscribe('message.send', function(targetUrl) {
        _gaq.push(['_trackSocial', 'facebook', 'send', targetUrl]);
      });
    
    });
  };
  require(['r29'], function (R29) {
    R29.FBLoginPopup = function () {
      FB.login(function(response) {
      
        if (response.session) {
          //They *may* have given us perms, but who knows.
          return document.location = 'https://welcome.refinery29.com/connect/facebook?from=http%3A%2F%2Fwww.refinery29.com%2F%3Flogin%3D1&amp;sig=cdd48&amp;app=blog&amp;fb=1';
        } else {
          //Oh well, something is wrong, let's try the normal login
          return document.location = 'https://welcome.refinery29.com/connect/facebook?from=http%3A%2F%2Fwww.refinery29.com%2F%3Flogin%3D1&amp;sig=cdd48&amp;app=blog&amp;fb=1';
        }
        
       },{ scope: 'email' } );
    }

  });
  
</script>

