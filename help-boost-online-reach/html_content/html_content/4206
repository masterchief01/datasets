  });

  var _gaq = _gaq || [];

  // break up host into app, subdomain and tld
  var tmp = /^([^\.]+\.)([\w\.]+)?(refinery29|rf29)\.(\w+)(:\d+)?$/.exec(window.location.host.toLowerCase()),
    
      // application
      app = tmp[1] ? tmp[1] : 'www',
      
      // subdomain
      sub = tmp[2] ? tmp[2] : '',
      
      // domain
      dom = tmp[3] ? tmp[3] : 'refinery29',

      // top level domain
      tld = tmp[4] ? tmp[4] : 'com';
  
  _gaq.push(['_setAccount', 'UA-315022-1']);
  
  _gaq.push(['_setDomainName', '.' + dom + '.' + tld]);
  
  _gaq.push(['_setCustomVar',
    1,                   // This custom var is set to slot #1.  Required parameter.
    'Category',     // The name acts as a kind of category for the user activity.  Required parameter.
    'refinery29', // This value of the custom variable.  Required parameter.
    3                    // Sets the scope to page-level.  Optional parameter.
  ]);
   

  
  _gaq.push(['_setCustomVar', //USE THIS  R29.cookie.get('R29_UID');
    5,                   // This custom var is set to slot #5.  Required parameter.
    'Log-in Status',     // The name acts as a kind of category for the user activity.  Required parameter.
    'Not Logged In', // This value of the custom variable.  Required parameter.
    2                    // Sets the scope to page-level.  Optional parameter.
  ]);





  // Adding back in data-click tracking -JS, 6.26.12
  // Content > Events in Google Analytics
  $('[data-click]').live('click', function() {
    if ($(this).attr('data-click') == "Pinterest Article Images") {
      
    }
    else {
      _gaq.push(['_trackEvent',
        ($(this).attr('data-category') !== undefined) ? $(this).attr('data-category') : 'User Click Tracking', // Category (required)
        $(this).attr('data-click'), // Action (required)
        ($(this).attr('data-label') !== undefined)    ? $(this).attr('data-label')    : 'refinery29', // Label (optional); Default: current category
        null, // Value (optional, integer) - empty
        true // Non-interaction (optional, boolean) - event hit will not be used in bounce-rate calculation
      ]);
    }
    if ($(this).attr('data-track-secondary')){
      tracking_vars = $(this).attr('data-track-secondary').split(',');
      _gaq.push(['_trackEvent', tracking_vars[0], tracking_vars[1], tracking_vars[2]]);
    }
  });

      _gaq.push(['_trackPageview']);
  

