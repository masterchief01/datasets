<script type="text/javascript">
  require(['r29', 'api', 'domReady!'], function(R29) {
    // TWITTER

    // Using our API endpoint with cached tweet
    R29.api._get('/twitter', { account:'refinery29', count: 1, include_rts: 1 }, function(d) {
      if (!d.result) {
        return;
      }

      var tweet = d.result[0];

      var text  = tweet.text;

      
        text = text.replace(
          /((https?\:\/\/)|(www\.))(\S+)(\w{2,4})(:[0-9]+)?(\/|\/([\w#!:.?+=&%@!\-\/]))?/gi,
          function(url){
              var full_url = url;
              if (!full_url.match('^https?:\/\/')) {
                  full_url = 'http://' + full_url;
              }
              return '<a target="_blank" href="' + full_url + '">' + url + '</a>';
          }
        );
      

      $("#latest-tweet").html(text);
      $("#tweet-timestamp").html((new Date(tweet.created_at)).ago());
    });

    // Change height of all social-content divs to match the greatest one.
    $('ul.expansions').css({ 'overflow' : 'hidden', 'height' : '0' });
    $('#twitter-expanded').show();

    var social_height = 10 + $('#twitter-expanded .social-content').height(); // to account for the "follow" button that didn't load yet
    $('.social-content').css('height', social_height);
    $('#twitter-expanded').hide();
    $('ul.expansions').removeAttr('style');

    // INSTAGRAM
    (function () {
      
      var urls = [
        'http://static3.refinery29.com/blog/4aee44/images/social-net-widget/instagram-thumb-8.png',
        'http://static2.refinery29.com/blog/4aee44/images/social-net-widget/instagram-thumb-9.png',
        'http://static1.refinery29.com/blog/4aee44/images/social-net-widget/instagram-thumb-10.png',
        'http://static1.refinery29.com/blog/4aee44/images/social-net-widget/instagram-thumb-11.png'
      ];
      var i = 0;
      $("#instagram-images li").each( function() {
        $(this).html("<a href=\"http://www.instagram.com/refinery29\" target=\"_blank\" onclick=\"_gaq.push(['_trackEvent', 'User Click Tracking', 'Social Networks Widget', 'Instagram - Images', null, true]);\"><img src=\"" + urls[i] + "\"></a>");
        i++;
      });
    })();

    // HOVERS AND EXPANSIONS
    $('.social-plugins').hover (
      function () {
        clearTimeout($(".social-plugins").data('timeoutId'));
      },
      function() {
        var timeoutId = setTimeout(function() {
          // Remove hovers from all icons.
          $('.fb-icon').attr('class','fb-icon');
          $('.in-icon').attr('class','in-icon');
          $('.tw-icon').attr('class','tw-icon');
          $('.pi-icon').attr('class','pi-icon');
          $('ul.expansions').children().hide();
        }, 200);
        $(".social-plugins").data('timeoutId', timeoutId);
      }
    );

    // Icon hovers and expansions.
    $('.fb-icon').hover (
      function () {
        $('ul.expansions').children().hide();
        // Hide the other icon hovers.
        $('.in-icon').attr('class','in-icon');
        $('.tw-icon').attr('class','tw-icon');
        $('.pi-icon').attr('class','pi-icon');
        if (!$('.fb-icon').hasClass('fb-hovered')) $('.fb-icon').toggleClass('fb-hovered');
        $('#facebook-expanded').show();
      },
      function() {
      }
    );

    $('.in-icon').hover (
      function () {
        $('ul.expansions').children().hide();
        $('.fb-icon').attr('class','fb-icon');
        $('.tw-icon').attr('class','tw-icon');
        $('.pi-icon').attr('class','pi-icon');
        if (!$('.in-icon').hasClass('in-hovered')) $('.in-icon').toggleClass('in-hovered');
        $('#instagram-expanded').show();
      },
      function() {
      }
    );

    $('.tw-icon').hover (
      function () {
        $('ul.expansions').children().hide();
        $('.fb-icon').attr('class','fb-icon');
        $('.in-icon').attr('class','in-icon');
        $('.pi-icon').attr('class','pi-icon');
        if (!$('.tw-icon').hasClass('tw-hovered')) $('.tw-icon').toggleClass('tw-hovered');
        $('#twitter-expanded').show();
      },
      function() {
      }
    );

    $('.pi-icon').hover (
      function () {
        $('ul.expansions').children().hide();
        $('.fb-icon').attr('class','fb-icon');
        $('.in-icon').attr('class','in-icon');
        $('.tw-icon').attr('class','tw-icon');
        if (!$('.pi-icon').hasClass('pi-hovered')) $('.pi-icon').toggleClass('pi-hovered');
        $('#pinterest-expanded').show();
      },
      function() {
      }
    );

  });
</script>

      				        				        				    
