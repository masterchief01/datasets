function Redirect()
{
	var txtSearchBottom = document.getElementById('txtbottomSearch');
	if(txtSearchBottom.value != '')
		location.href = 'search.aspx?q=' + txtSearchBottom.value;
	else
		location.href = 'search.aspx';
}
