   <script type="text/javascript" language="JavaScript">
		function swapNavNew(oImg) {
			var reImgOn = /_hov\.png$/i;  // match "nav" + optional "sub" + 2digits + "_hov.png"
			var reImgSel = /_sel\.png$/i;
			var newImgNm;
			if (reImgOn.test(oImg.src)) {
				newImgNm = oImg.src.replace(/_hov.png/i,".png");
				oImg.src = newImgNm;
			} else if(!reImgSel.test(oImg.src)){
				newImgNm = oImg.src.replace(/\.png/i,"_hov.png");
				oImg.src = newImgNm;				
			}
		}
   </script>
