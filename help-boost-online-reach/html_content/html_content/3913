<script type="text/javascript">
	var quick_participate_slideshow_19152 = null;
	function onSlideshowFormSubmitEnd_19152(o)
	{
		quick_participate_slideshow_19152.onFormSubmitEnd(o);
	}
	var callback_19152 = function()
	{
		quick_participate_slideshow_19152 = new QuickSlideshowParticipate(19152);
		quick_participate_slideshow_19152.show();	
	}
	var preloading_callback_19152 = function()
	{	
				   HuffConnect.showLoader();
			}
	Y.util.Event.addListener('sf_user_participation_btn_19152', 'click', function(event)
	{
		E.preventDefault(event);
		
		//starting to load needed js libraries :
		jQuery.getScript("/include/lib/jquery/jquery.form.js", function() {});
		HPEventModule.Load('slideshow_participate');
		
		
		if (!HuffCookies.getUserName())
		{
			QuickLogin.onLoginSuccess = function() {
				HPEventModule.Wait('slideshow_participate', callback_19152, preloading_callback_19152);
			};
			QuickLogin.pop();
			return;
		}
		HPEventModule.Wait('slideshow_participate', callback_19152, preloading_callback_19152, false);
		
	});
		
	/*
	//believe that if in the url we has already #return , it means that user already logged, but to prevent complaints, adding some check for that
	//that condition should work just for case when user want upload to youtube file
	if (/#return$/i.test(document.location.href))
	{
		if (HuffCookies.getUserName())
		{
			HPEventModule.Wait('slideshow_participate', callback_19152, preloading_callback_19152);
		}
	}*/
	//<div class="clear"></div>
</script>

