"hier","eVar");for(var i=0;i<slst.length;i++){if(slst[i]=="hier")
{for(var h=1;h<6;h++){svb(slst[i]+h);}}else if(slst[i]=="eVar")
{for(var e=1;e<21;e++){svb(slst[i]+e);}}else if(slst[i]=="prop")
{for(var p=1;p<26;p++){svb(slst[i]+p);}}else{svb(slst[i]);}}
}function svb(vn){eval('s_'+vn+'=""');}
s_vp_sprcln();

