											<script type="text/javascript">
    jQuery(document).ready(function() {
        jQuery("#pane").next().css({width: "300px", overflow: "hidden"});

        var stories = jQuery("#pane dd").css("display", "none").remove();
        jQuery('<div class="current" id="mostpop_content"></div>')
            .css({paddingBottom: "1px"})
            .insertAfter("#pane")
            .append(stories);

        jQuery("#pane dt").addClass("closed").css("cursor", "pointer").click(function() {
            if (!jQuery(this).hasClass("open")) {
                jQuery("#pane .open").removeClass("open").addClass("closed");
                jQuery(this).removeClass("closed").addClass("open");
                var ID = parseInt(this.id.substr(5)); //remove the 'panel' word
                stories.css("display", "none").get(ID-1).style.display = "block";
            }
        });
        jQuery("#panel1").click();
    });
</script>
