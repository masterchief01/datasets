			OAS_rns = OAS_rn.substring (2, 11);



            // Add segments from rev_sci cookie to OAS_query.
            OAS_query += (function() {
                var hasStorage = (function() {
                    try {
                        localStorage.setItem("xxx_localstorage_test", "xxx_localstorage_test");
                        localStorage.removeItem("xxx_localstorage_test");
                        return true;
                    } catch(e) {
                        return false;
                    }
                }());

                var revSciString = "";
                if (hasStorage && false){
                    revSciString = localStorage.getItem('rsi_segs');
                    if (!revSciString){
                        revSciString = ""
                    }
                } else {
                    var cookieMatch = document.cookie.match(/rsi_segs=([^;]*)/)
                    if(cookieMatch && cookieMatch.length > 1) {
                        revSciString = cookieMatch[1]
                    }
                }

                var retSegs = "a=" + (revSciString.match(/(E\d+)?/g).join('&a=')) + "&";

                if(hasStorage) {
                    localStorage.setItem('aparams', retSegs)
                }

                return retSegs;
            })();
		</script>
            	    





		
										

        		
		
			            						
										
					
                                                                                                                                                                                                                            
