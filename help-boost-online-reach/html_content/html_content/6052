    MVN.queue(function(){
        if(MVN.user){
            $('.newFooter > ul li').first().replaceWith(
                $('<li/>')
                    .append($('<h4/>').text('My Account'))
                    .append($('<a/>').attr('href', 'http://www.myvoicenation.com/'+MVN.user.username+'/').text('Profile')).append($('<br/>'))
                    .append($('<a/>').attr('href', 'http://www.myvoicenation.com/profile/mobile/nyv/').text('Mobile')).append($('<br/>'))
                    .append($('<a/>').attr('href', 'http://www.myvoicenation.com/profile/newsletters/nyv/').text('Newsletters')).append($('<br/>'))
                    .append($('<span data-omni-action="logout"/>').text('Logout'))
            );
        } else {
                            if (/(blog(s|test)\.)|[\?&]customad=/.test(window.location.href) || $("span.affinity").length > 0) {
                    return;
                }
                var count = parseInt(VVM.getCookie('submodal'), 10) - 1;
                if (isNaN(count)){
                    if (VVM.cookiesEnabled()) {
                        count = 0;
                    } else {
                        return;
                    }
                }
                if (count > 0) {
                    VVM.setCookie('submodal', count, null);
                } else if (count === 0) {
                    MVN.omni.buildModal('subscribe');
                    if (MVN.omni.Modals['subscribe']) {
                        MVN.omni.Modals['subscribe'].show();
                    }
                }
                    }
    })
</script>
<!-- End /p/af/v/e/Footer -->        </div>
    </div>

</div><!-- /pagecontainer -->
