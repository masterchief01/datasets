<script type="text/javascript">
jQuery(function($) {
	$('.form-newsletter input').innerLabel('Enter email for special offers');
	
	$('.zoom').jqzoom({ title:false, zoomWidth:276, zoomHeight:276, xOffset:17, yOffset:0 });
	$('.gallery a').click(function() {
		$('.view').html('<a href=""><img src="" width="450" height="450" /></a>').children('a').attr('href', $(this).attr('href')).children('img').attr('src', $(this).attr('href'))
			.parent().jqzoom({ title:false, zoomWidth:276, zoomHeight:276, xOffset:17, yOffset:0 });
		return false;
	});
	
	if ($('.slideshow').is('div')) { var slideshow = $('.slideshow').slideshow({interval:3500, showControls:true}); }
	
	$('.menu-item-etsy a, .menu-item-facebook a, .menu-item-twitter a').attr('target','_blank');
	
	$('.cn_product #more').hide().after(' <a id="more-show">More...</a>').closest('.description').css('cursor', 'pointer').hover(function() {
		$('#more', this).show();
	}, function() {
		$('#more', this).hide();
	});
});

(function($) {
	$.fn.innerLabel = function(text) {
		return this.each(function() {
			$(this).blur(function() {
				if ($(this).val() == '') {
					$(this).val(text);
				}
			});
			
			$(this).focus(function() {
				if ($(this).val() == text) {
					$(this).val('');
				}
			});
		});
	}
})(jQuery);
</script>

