					$('video_buzz_element_2070808_1023551').addClassName('video-embed-noflash');
					$('video_buzz_element_2070808_1023551').style.height = '340px';
				} else {
					if (vd.type == 'youtube') {
						if( isIE8() ) { //IE8 :(
							Event.observe(window, 'beforeunload', function(){ //for ie8 we need to handle page exits in a special manner
								$$("object").each(function(vidPlayer) { //grab the player(s)
									vidPlayer.remove(); //remove it so js calls no longer occur on exit to non-existant objects
								});
							}, false);
							if(isIE8()) { //internet explorer doesn't have access to the src from SWFObject, so must pass in alternative manner
								
