					&lt;/span&gt;
				&lt;/span&gt;
			&lt;/span&gt;
		&lt;/div&gt;

							&lt;a href=&quot;#&quot; class=&quot;bf_dom rdio_react&quot; rel:bf_bucket_data=&quot;{&amp;quot;rdio_react&amp;quot;:{&amp;quot;title&amp;quot;: &amp;quot;#{name_html_encoded}&amp;quot;, &amp;quot;rdiourl&amp;quot;: &amp;quot;#{url}&amp;quot;}}&quot; rel:gt_act=&quot;ignore&quot;&gt;React&lt;/a&gt;
						&lt;/div&gt;
					&lt;/div&gt;
					&lt;div class=&quot;clear&quot;&gt;&amp;nbsp;&lt;/div&gt;
				
			</textarea>
