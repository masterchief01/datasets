						&lt;div class=&quot;track_info_right&quot;&gt;
							
		&lt;div class=&quot;rdio_controls&quot; rel:video_url=&quot;#{url}&quot;&gt;
			&lt;a href=&quot;#{url}&quot; class=&quot;bf_dom rdio_sprite rdio_play&quot; rel:bf_bucket=&quot;rdio_play&quot; rel:gt_act=&quot;ignore&quot;&gt;Play&lt;/a&gt;
			&lt;a href=&quot;#{url}&quot; class=&quot;bf_dom rdio_sprite rdio_pause&quot; rel:bf_bucket=&quot;rdio_pause&quot; rel:gt_act=&quot;ignore&quot;&gt;Pause&lt;/a&gt;
			&lt;span class=&quot;rdio_playing_info&quot;&gt;
				&lt;span class=&quot;rdio_progress_bar&quot;&gt;
					&lt;span class=&quot;rdio_progress_position&quot;&gt;
