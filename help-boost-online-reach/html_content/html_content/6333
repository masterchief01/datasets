  <script type="text/javascript">
    var video_info = {"bread_crumb":["<a href=\"/\">Home</a>","<a href=\"/video/\">Video</a>"],"playlist_id":"740380963001","video_id":"1439278179001"}
    var old_url = '';
    var notFirstVid = false;
	var isashow = false;
    var startID = video_info.video_id;
    function addScriptTag(id, url, callback) {
	  var scriptTag = document.createElement("script");
	  var noCacheIE = '&noCacheIE=' + (new Date()).getTime();

	  // Add script object attributes
	  scriptTag.setAttribute("type", "text/javascript");
	  scriptTag.setAttribute("charset", "utf-8");
	  scriptTag.setAttribute("src", url + "&callback=" + callback + noCacheIE);
	  scriptTag.setAttribute("id", id);

	  var head = document.getElementsByTagName("head").item(0);
	  head.appendChild(scriptTag);
    }

    var space_shows = new Array();
    var space_channels = new Array();
    function getShowsVideos() {
      for (show in show_sequence){
        space_shows[show_sequence[show].sequence] = {"id":show,
                                            "referenceId":show_sequence[show].referenceId,
                                            "name":show_sequence[show].name,
                                            "shortDescription":null,
                                            "thumbnailURL":show_sequence[show].thumbnailURL,
                                            "filterTags":[],
                                            "playlistType":"EXPLICIT"}
      }
      addScriptTag("ttrPopular", "http://api.brightcove.com/services/library?"+
                   "command=find_playlist_by_id&playlist_id=" + video_info.playlist_id +
                   "&token=Jf08V2LSSN1szAQI3pr1hL3tOm0QdluOzvHd8zJTmVM.",
                   "ttrPopularResponse");
    }
    
    function ttrPopularResponse(jsonData) {
      space_video_channel = {'items':jsonData.videos};
      sortedReply(space_video_channel);
    }

    var exp;
    var content;
    var sortedIDS;
    var video, player, search, menu, social;
    function onTemplateLoaded(e){
        player	        = brightcove.getExperience("myExperience");
        content	        = player.getModule(APIModules.CONTENT);
        exp		        = player.getModule(APIModules.EXPERIENCE);
        video 	        = player.getModule(APIModules.VIDEO_PLAYER);
        search 	        = player.getModule(APIModules.SEARCH);
        menu            = player.getModule(APIModules.MENU);
        social          = player.getModule(APIModules.SOCIAL);
        content.addEventListener(BCContentEvent.MEDIA_COLLECTION_LOAD, onMediaCollectionLoad);
        exp.addEventListener(BCExperienceEvent.TEMPLATE_READY,onTemplateReady);
        video.addEventListener(BCMediaEvent.CHANGE,onMediaChanged);
        search.addEventListener(BCSearchEvent.RESULT,onSearch);
    }

    function onTemplateReady(){
        getShowsVideos();
    }
    
    function onShowClick(e){
      if (typeof(show_sequence[e.elementData.id]) != 'undefined'){
        if (typeof(e.elementData.videos) == 'undefined' ){
          getMediashow(e.elementData.id, "addShow");
        }else{
          h={'items':e.elementData.videos};
          sortedReply(h);
        }
        isashow = true;
        $jq(".vidhead_name").html(e.elementData.name);
        $jq(".vidhead_right").html(['<a href="/">Home</a>','<a href="/video/">Video</a>','Video Shows'].join(" > "));
      }
    }
    
    function getMediaVideos(playlist_id){
      if (typeof(space_channels[playlist_id]) == 'undefined'){
        getMediashow(playlist_id, "addChannel");
      }else{
        h={'items':space_channels[playlist_id].videos};
        sortedReply(h);
      }
    }
    
    function getMediashow(id, callback){
        addScriptTag(callback, "http://api.brightcove.com/services/library?"+
                     "command=find_playlist_by_id&playlist_id="+id+
                     "&token=Jf08V2LSSN1szAQI3pr1hL3tOm0QdluOzvHd8zJTmVM.",callback+'Response');
    }
    
    function addShowResponse(jsonData){
      for (show in space_shows){
        if (space_shows[show].id == jsonData.id){
          space_shows[show].videoIds = jsonData.videoIds;
          space_shows[show].videos = jsonData.videos;
        }
      }
      var vs = exp.getElementByID("videoListShows")
      vs.setData(space_shows);
      h={'items':jsonData.videos};
      sortedReply(h);
    }
    
    function addChannelResponse(jsonData){
      space_channels[jsonData.id] = jsonData;
      h={'items':jsonData.videos};
      sortedReply(h);
    }
    
    function sortedReply(data){
        var tmpArray = [];
        var sortedArray = [];
        var viewCountIds = [];
        var pubDateIds = [];
        try{
            for(var i = 0; i < data["items"].length; i++){
                if(data["items"][i]){
                    tmpArray.push({id: data["items"][i]["id"], viewCount: data["items"][i]["playsTotal"], pubDate: data["items"][i]["publishedDate"]});
                }
            }
        }catch(exep){}
        for(var i = 0; i < tmpArray.length; i++ ){
            sortedArray.push(tmpArray[i].id);
        }
        pubDateIds = sortedArray;

        tmpArray.sort(sortViewCountArray);
        for(var i = 0; i < tmpArray.length; i++ ){
            viewCountIds.push(tmpArray[i].id);
        }

        sortedIDS = {viewCount: viewCountIds, pubDates: pubDateIds};
        function sortViewCountArray(a,b){
            var x = parseInt(a.viewCount);
            var y = parseInt(b.viewCount);
            return ((x < y) ? 1 : ((x > y) ? -1 : 0));
        }

        content.getMediaInGroupAsynch(sortedArray.concat(space_shows));
        var t = sortedArray;
        return sortedArray;
    }
    
    function onTabClick(e){
        var playList = exp.getElementByID("videoListShows");
        playList.setSelectedIndex(-1);
    }
    var showData = [];
    function onMediaCollectionLoad(e){
        var tmpArray = [];

        var showsList = exp.getElementByID("videoListShows");
        var tabListShows = exp.getElementByID("videoList");
        var firstLoad = 0;
        if(showsList.getNumItems() < 1){
            firstLoad = 1;
            for(var i=0; i < space_shows.length; i++){
                showData.push((space_shows[i]));
            }
            showsList.addEventListener("elementClick", onShowClick);
           
            showsList.setData(showData);

            tabListShows.addEventListener("elementClick", onTabClick);
        }
		if(isashow){
		  var d_name = "Episodes";
		}else{
		  var d_name = "Latest Videos";
		}
		
        tabList = exp.getElementByID("playlistTabs");
        var topPlaylist = {
            displayName: "Most Popular",
            mediaIds: sortedIDS.viewCount
        };
        var newestPlaylist = {
            displayName: d_name,
            mediaIds: sortedIDS.pubDates
        };
        try{

            var b = content.createRuntimeMediaCollection(newestPlaylist,"playlist");
            tabList.replaceTabAt(b,0);
            var a = content.createRuntimeMediaCollection(topPlaylist,"playlist");
            tabList.replaceTabAt(a,1);

            tabList.setSelectedIndex(0);
        }catch(e){
            alert(e);
        }
        var playList = exp.getElementByID("videoList");
        playList.setSelectedIndex(0);
        if(startID > 0){
            video.loadVideo(startID);
            playList.setSelectedIndex((startID == e["mediaCollection"]["mediaIds"][0])? 0 : -1 ); 
            startID = 0;
        }else{
            video.loadVideo(e["mediaCollection"]["mediaIds"][0]);
        }
    }
    
    function onMediaChanged(e){
      if(old_url == ''){
        setTimeout("notFirstVid=true;", 10000);
      }
      $jq("#video_description").html(e.media.shortDescription);
      if (typeof(player_bottom_info[e.media.id]) == 'object')
      {
        $jq("#video_credit").html('Credit: '+player_bottom_info[e.media.id].credit);
        document.getElementById("video_partner_img").style.display='none';
        document.getElementById("video_partner_img").src=player_bottom_info[e.media.id].partner_image;
        document.getElementById("video_partner_link").href=player_bottom_info[e.media.id].partner_link;
		
		var current_url = 'http://www.space.com'+player_bottom_info[e.media.id].link;
		social.setLink(current_url);
        
        video_page_update(current_url);
      }else{
        $jq("#video_credit").html('');
        document.getElementById("video_partner_img").src='';
		social.setLink('http://www.space.com/video')
        
        toggle_hidden('comments_cont', false);
        old_url = 'changed';
      }
    }

    function playVideo(vidID){
        video.loadVideo(vidID);
    }

    sfHover = function() {
      var sfEls = document.getElementById("cat_link").getElementsByTagName("LI");
      for (var i=0; i<sfEls.length; i++) {
        sfEls[i].onmouseover=function() {
          this.className+=" sfhover";
        }
        sfEls[i].onmouseout=function() {
          this.className=this.className.replace(new RegExp(" sfhover\\b"), "");
        }
      }
    }
    $jq(document).ready(function(){
        sfHover();
        $jq(".cat_final").click(function(){
			isashow = false;
            var breadCrumb = [];
            breadCrumb.push('<a href="/">Home</a>');
            breadCrumb.push('<a href="/video/">Video</a>');
            breadCrumb.push($jq(this).parent().parent().parent().children("span").find("input[type=hidden]")[0].name);
            breadCrumb.push($jq(this).find("input[type=hidden]")[0].name);
            $jq(".vidhead_name").html('Video');
            var ids = $jq(this).find("input[type=hidden]").val();
            if (ids != ''){
              $jq(".vidhead_right").html(breadCrumb.join(" > "));
            }
            var playList = exp.getElementByID("videoListShows");
            playList.setSelectedIndex(-1);
            getMediaVideos(ids);
        });
        $jq(".cat_parent_name").click(function(){
			isashow = false;
            var hide_info = $jq(this).find("input[type=hidden]")[0];
            var breadCrumb = [];
            breadCrumb.push('<a href="/">Home</a>');
            breadCrumb.push('<a href="/video/">Video</a>');
            breadCrumb.push(hide_info.name);
            $jq(".vidhead_name").html('Video');
            var ids = hide_info.value;
            if (ids != ''){
              $jq(".vidhead_right").html(breadCrumb.join(" > "));
            }
            var playList = exp.getElementByID("videoListShows");
            playList.setSelectedIndex(-1);
            getMediaVideos(ids);
        });
    });
    
    function video_page_update(current_url){
        if (old_url != current_url){
            old_url = current_url;
            update_comments(current_url);
            if(notFirstVid){
                reloadScripts();
            }
        }
    }
    function update_comments(url){
	  var fbCom = document.getElementById('fb_comment_cont');
	  fbCom.innerHTML = '';
	  fbCom.innerHTML = '<fb:comments href="'+ url +'" num_posts="10" width="615" ></fb:comments>';
	  FB.XFBML.parse(fbCom);
	  toggle_hidden('comments_cont', true);
    }
</script>


