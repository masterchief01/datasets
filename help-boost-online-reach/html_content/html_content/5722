<script type="text/javascript">jQuery(document).ready(function() {
jQuery('ul').removeClass('hidden');
jQuery('div.jcarousel_waiting').hide();
  jQuery('ul#nc_related_photo_galleries').jcarousel({
    visible: null,
    scroll: 3,
    vertical: false,
    wrap: 'null',
    auto: 0,
    animation: 'fast',
    start: 1,
    buttonNextHTML: '<div>></div>',
    buttonPrevHTML: '<div><</div>',
    itemFirstInCallback: trigger_js_scroll_4,
    itemLastInCallback: fetchMoreCarousel_4,
    initCallback: initialize_4,
    itemVisibleInCallback: item_visible_4,
    itemVisibleOutCallback: item_invisible_4
  });
});
function fetchMoreCarousel_4(carousel, lastLI, index, carouselState) {
  //alert('Scroll.');
  target = '/nodecarousel/fetch';
  oldLast = carousel.last;
  nid_id = $('.node').get(0).id;
  var nidmatch = nid_id.match(/node-(\d*)/);
  if (nidmatch.length) {
    nid = nidmatch[1];
  }
  //alert('node: ' + nid);
  //alert('node id?' + nid);
  if ($("#nc_related_photo_galleries").children("li").length == oldLast) {

    $.getJSON(target, {start: oldLast, name: 'related_photo_galleries', ajax: 'true', nid: nid}, function(j) {
      var items = '';
      if (j.length > 0) {
        for (var i = 0; i < j.length; i++) {
          carousel.add(oldLast + i + 1, j[i]);
          carousel.size(oldLast + i + 1);
        }
        // If auto-scrolling, this starts it up again after fetching the next set.
        carousel.startAuto();
      }
    });
  }
}

function item_visible_4(carousel, listItem, index, state) {}
function item_invisible_4(carousel, listItem, index, state) {}
    function trigger_js_scroll_4(carousel, listItem, index, state) {
      related_gallery_scroll('related_photo_galleries', index, state);
    }

function initialize_4(carousel, state) {
  if (state == 'init') {
    var carouselName = 'related_photo_galleries';
    var outermostDiv = $("#nc_" + carouselName).parent().parent();
    var first = '';
    var last  = '';
    outermostDiv.addClass("nc_" + carouselName);
    outermostDiv.children(".jcarousel-clip").addClass("nc_clip_" + carouselName);
    $("#nc_" + carouselName).children("li").addClass("nc_item_" + carouselName);
    if (first != '') {
      outermostDiv.children(".jcarousel-prev").before("<div class='jcarousel-first' id='nc_first_4'>" + first + "</div>");
    }
    if (last != '') {
      outermostDiv.children(".jcarousel-next").after("<div class='jcarousel-last' id='nc_last_4'>" + last + "</div>");
    }

    jQuery('div#nc_first_4').click(function(e) {
      carousel.scroll(1, true);
    });

    jQuery('div#nc_last_4').click(function(e) {
      carousel.scroll($("#nc_" + carouselName + " li").length, true);
    });
  }
}
