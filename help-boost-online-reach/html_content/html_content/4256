    <script language="javascript" type="text/javascript">
        function tabcontent(num) {
            var listItems = document.getElementById("tabmenu").children;
            var len = listItems.length;
            for (var i = 0; i < len; i++) {
                var anchorId = listItems[i].childNodes[0].id;
                if (anchorId != num) {
                    document.getElementById(anchorId).style.borderTop = "1px solid #666666";
                    document.getElementById(anchorId).style.borderLeft = "1px solid #666666";
                    document.getElementById(anchorId).style.borderRight = "1px solid #666666";
                    document.getElementById(anchorId).style.borderBottom = "0px";
                    document.getElementById(anchorId).style.height = "27px";
                }
                else {
                    document.getElementById(anchorId).style.borderTop = "1px solid #000000";
                    document.getElementById(anchorId).style.borderLeft = "1px solid #000000";
                    document.getElementById(anchorId).style.borderRight = "1px solid #000000";
                    document.getElementById(anchorId).style.borderBottom = "2px solid #ffffff";
                    document.getElementById(anchorId).style.height = "25px";
                }
            }
            var xmlhttp;
            if (num.length == 0) {
                document.getElementById("sponsored-links").innerHTML = "";
                return;
            }
            if (window.XMLHttpRequest) {
                xmlhttp = new XMLHttpRequest();
            }
            else {
                xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
            }
            xmlhttp.onreadystatechange = function () {
                if (xmlhttp.readyState == 4 && xmlhttp.status == 200) {
                    document.getElementById("sponsored-links").innerHTML = xmlhttp.responseText;
                }
            }
            xmlhttp.open("GET", "/Widget.aspx/htmlmodulebyid/?htmlModuleId=" + num, true);
            xmlhttp.send();
        }

    </script>

    </body>
</html>