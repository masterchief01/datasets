            $(document).ready(function() {
                $.ajax({ type: 'GET',
                url: '/renderinsidepcmag/260269/5',
                    dataType: 'html',
                    timeout: 5000,
                    success: function(result) {
                        if (result != null && result.length > 0)
                            $("#pcmag-inside").html($(result));
                        else
                            $("#pcmag-inside").empty();
                    },
                    error: function(xhr, textStatus, errorThrown) {
                        $("#pcmag-inside").empty();
