            var ZIFF_BuyItNowCallback = (function () {
                var fns = [];

                var subscribe = function (fn) {
                    fns.push(fn);
                };

                var exec = function (results) {
                    for (var i = 0; i < fns.length; i++) {
                        fns[i](results);
                    }
                };

                return {
                    subscribe: subscribe,
                    exec: exec
                };
            } ());
