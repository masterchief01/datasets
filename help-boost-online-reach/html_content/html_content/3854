    <script type="text/javascript">
               window._ghearst_vars = (function() {
        var
            metaKeyWords = (function(){
                var
                    i                  = 0,
                    metaKeywordContent = '',
                    nameAttribute      = null,
                    metaCollection     = document.getElementsByTagName('meta');

                for (i = 0; i < metaCollection.length; i++) {
                    nameAttribute = metaCollection[i].name.search(/keywords/);
                    if (nameAttribute!= -1) {
                        metaKeywordContent = metaCollection[i].content;
                        metaKeywordContent = metaKeywordContent.replace(/ /g,''); // to replace space
                    }
                }
                return encodeURIComponent(metaKeywordContent);
            })();
        return {
            'ams_ads_script_src': 'http://subscribe.wmagazine.com/ams/page-ads.js?ad_category_prefix=fashion&browser_path=%2Ffashion%2F2012%2F09%2Fmfw-spring-2013-street-day1-ss&cat_prefixes=%2Cfashion%2C2012%2C09%2Cmfw-spring-2013-street-day1-ss&keywords='+metaKeyWords+'&position_list=AMS_WMG_GLOBAL_SLIDESHOW%2CAMS_WMG_POPUP_FLOATINGAD%2CAMS_WMG_GLOBAL_NAVBAR_ROLLOVER%2CAMS_WMG_GLOBAL_RIGHTRAIL_A%2CAMS_WMG_GLOBAL_RIGHTRAIL_B%2CAMS_WMG_GLOBAL_PROMOTOP%2CAMS_WMG_CONTENTPAGE_ZRAIL%2CAMS_WMG_GLOBAL_NAVBAR&site_prefix=wmagazine&subdomain=www&url_name=mfw-spring-2013-street-day1-ss'
        };
    })();

        (function($) {
            var show = function() {
                $('.ecom-placement').each(function() {
                    $(this).css('visibility', 'visible');
                });
            };
            
            $LAB.script(_ghearst_vars.ams_ads_script_src)
                .wait(function() {
                    
                    CN.hearstQue.run(window.pageAds);
                });

            
            setTimeout(show, 10 * 1000);
        }(jQuery));
    </script>











    
    


































    
