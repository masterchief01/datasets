<script type="text/javascript">
CN.hearstQue.register(function(pageAds){
    var div = null;

    if ((typeof pageAds !== 'undefined') && (typeof pageAds.AMS_WMG_POPUP_FLOATINGAD !== 'undefined') && (CN.url.params('nojoy') != 1) ) {

        div = document.getElementById("AMS_WMG_POPUP_FLOATINGAD");
        div.innerHTML = "";

        
        jQuery("#AMS_WMG_POPUP_FLOATINGAD").html(pageAds.AMS_WMG_POPUP_FLOATINGAD.replace(/document.write\(.*\)/gi,"/*filtered by amg-magnet:document.write(...)*/"));
        jQuery(div).css({
            visibility: 'visible'
        });
    }

    
});

</script>



        </div>
    


        
            
            
            
    

    </div>
</div>

    





