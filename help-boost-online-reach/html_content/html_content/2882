window.addEvent(itEventType, function() {
  if (! $('rtimes') ) { return };

  // If there is only one item don't use the carousel
  if ( $('rt_content').getChildren('li').length == 1 ) {
    return;
  }

  var itemwidth = 298;

  var scrollCarousel =  new iCarousel("rt_content", {
    item: {
      klass: "rt_item",
      size: itemwidth
    },
    animation: {
      duration: 500,
      amount: 1,
      rotate: {
        type: "auto",
        interval: 7000,
        onMouseOver: "stop"
      }
    },
    shuffle: false, //turned off for now while developing number list
  });

