function recipeSearchSubmit() { 
				recipe_val = document.getElementById("edit-search-block-form-1").value;
				if (recipe_val != "Search Recipes..." && recipe_val.replace(/^\s*(\S*(?:\s+\S+)*)\s*$/, "$1") != "") {
					location = "/recipe_search?recipe=" + encodeURIComponent(recipe_val); 
				}
			}
