<script type="text/javascript">

function checkMagentoRegistration(uid){
    jQuery.ajax({
        type: "POST",
        url: "http://www.recyclebank.com/customer/account/loginpostfb",
        data: ({fb_id:uid,referer:''}),
        success: function(response){

            console.log(response);
            switch(response)
            {
                case '0':
                    redirectToCreateWithFb(); break;
                case 'inactive':
                    redirectToLogin(); break;
                case '':
                case undefined:
                    var href=window.location.href;
                    console.log(href);
                    var replace = href.match(/(___store=\w+&___from_store=\w+)/);
                    console.log(replace);
                    if(replace != null && replace!=''){
                        href = href.replace(replace[0], "");
                    }
                    window.location.href = href;
                    break;
                default:
                    window.location.href = response; break;
            }
        },
        error: function(response){
            console.log(response);

        }
    })
}

function redirectToCreateWithFb(){
    window.location.href='https://www.recyclebank.com/customer/account/createwithfb/';
}

function redirectToLogin(){
    window.location.href='https://www.recyclebank.com/customer/account/login/';
}


function login_user(){
    FB.getLoginStatus(function(response) {
        if (response.authResponse)
            checkMagentoRegistration(response.authResponse.userID);
    })
}

</script>


