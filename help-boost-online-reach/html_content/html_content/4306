    var query = window.location.search.substring(1);
    var url = document.URL;
    url = url.replace( /#/, "" );
    if ( permalink == url ) {
        var e=document.createElement('script');e.setAttribute('type','text/javascript');e.setAttribute('charset','UTF-8');e.setAttribute('src','http://static.typepad.com/.shared:v20120705.02-0-g56d1ec6:typepad:en_us//js/pinmarklet.js?r='+Math.random()*99999999);document.body.appendChild(e);
    } else if ( query != "pintix=1" ) {
        window.location=permalink+"?pintix=1";
    } else {
        var e=document.createElement('script');e.setAttribute('type','text/javascript');e.setAttribute('charset','UTF-8');e.setAttribute('src','http://static.typepad.com/.shared:v20120705.02-0-g56d1ec6:typepad:en_us//js/pinmarklet.js?r='+Math.random()*99999999);document.body.appendChild(e);
    }
}
</script>

