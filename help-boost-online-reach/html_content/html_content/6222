			/* <![CDATA[ */ 
				BuzzLoader.register(function() {
					try {
						var agent=navigator.userAgent.toLowerCase();
						var is_mobile = agent.match(/iphone|ipod|android|ipad/);
						var so = BF_initSwfObject("http://www.youtube.com/v/133t-GKy_ZI",{id:"video_embed_i-am-un-chien-hologram-feat-jrf-from-stuck-79xy-ntk"});
