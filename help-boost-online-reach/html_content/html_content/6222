<li id="mupton:sup-g-79xy-16f1" rel:buzz_id="349547" class="contrib bf_dom" rel:bf_bucket="track" track="{&quot;c&quot;:&quot;71GG0U7&quot;,&quot;u&quot;:&quot;7899N7&quot;,&quot;buzz&quot;:&quot;sup-g-79xy-16f1&quot;,&quot;user&quot;:&quot;mupton&quot;,&quot;types&quot;:[100],&quot;queries&quot;:[]}"   rel:data='{"buzz_id":"349547","type":"contribution","uri":"sup-g-79xy-16f1","form_id":"5"}' rel:ptool="true">
		
  <div class="what notranslate" id="contribwhat_349547" >  
		<div class="video">
		
    
			<p class="video-embed-big has-embed" id="video_buzz_element_sup-g-79xy-16f1"></p>
			
			<script type="text/javascript" charset="utf-8">
			/* <![CDATA[ */ 
				BuzzLoader.register(function() {
					try {
						var agent=navigator.userAgent.toLowerCase();
						var is_mobile = agent.match(/iphone|ipod|android|ipad/);
						var so = BF_initSwfObject("http://www.youtube.com/v/XY-HH9dueZY",{id:"video_embed_sup-g-79xy-16f1"});
						var vid_url = "http://www.youtube.com/v/XY-HH9dueZY";
						var vid_id = "video_buzz_element_sup-g-79xy-16f1";						
						if( ( is_mobile && ! vid_url.match(/youtube\.com\/v\//) ) || (! is_mobile && so.installedVer && so.installedVer.major < 8 ) ) {
							$(vid_id).addClassName('video-embed-noflash');
							$(vid_id).style.height = '340px';
						} else {
							if(vid_url.match(/youtube\.com\/v\//) ) {
								var you_tube_tmpl = new Template('<embed src="#{src}" type="application/x-shockwave-flash" allowscriptaccess="always" allowfullscreen="true" width="425" height="355" bgcolor="#000" wmode="transparent"></embed>');
								var you_tube_markup = you_tube_tmpl.evaluate({ src: vid_url+'?version=3&hl=en&fs=1' });
								var vspan = document.createElement('span');
								vspan.innerHTML = you_tube_markup;
								$(vid_id).appendChild(vspan);
							} else { 
								try{so.addParam("wmode", "transparent");}catch(e) {}
								so.write(vid_id);
							}
						}
					} catch(e) {
						console.error(e);
					}
				
				}, 6);
			/* ]]> */
			</script>
			
			<p>Sup G</p>
		</div>
		  	
