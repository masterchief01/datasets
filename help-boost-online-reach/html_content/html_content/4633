					<script language="javascript">
						$('.postType a').click(function(){
							$('.postType a.on').each(function(){
								$(this).removeClass('on');
							});
							$(this).addClass('on');
							if(this.id == 'recent'){
								$('#postsModule .postsWrap .popularPosts').hide();
								$('#postsModule .postsWrap .recentPosts').show();
							} else {
								$('#postsModule .postsWrap .popularPosts').show();
								$('#postsModule .postsWrap .recentPosts').hide();
							}
							return false;
						});
					</script>
