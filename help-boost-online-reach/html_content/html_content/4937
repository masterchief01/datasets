		if(-1 !== document.cookie.indexOf("MMC_FAST_MODE")) {
			MMC_FAST_MODE = true;
		}

		if(-1 !== document.cookie.indexOf("MMC_NOAD_MODE")) {
			MMC_NOAD_MODE = true;
		}

		if(-1 !== document.cookie.indexOf("MMC_NOSOCIAL_MODE")) {
			MMC_NOSOCIAL_MODE = true;
		}

		if(-1 !== document.cookie.indexOf("MMC_NOTAG_MODE")) {
			MMC_NOTAG_MODE = true;
		}
