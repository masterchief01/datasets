<div class="widget HTML" id="html_yui-gen78"><script type="text/javascript">
if ( typeof addthis_config === 'undefined' ) {
addthis_config = {};
}

if (typeof console === "undefined") {
  console = {
      log : function(msg) {
          return;
      }
   };
}

// Enable the newer OAuth style ShareThis widget.

if ( typeof switchTo5x === 'undefined' ) {
switchTo5x = false;
}

(function() {

/**
* Handles setting up sharing widget on a page. Usage info below.
*
* Modify WIDGET_PROVIDER to be either AddThis or ShareThis
* Change USE_APPENDER to be SOCIAL_BOOKMARKS_APPENDER or HEADER_BODY_APPENDER
* Change publisher/username/template/etc as desired in PROVIDER_DEFAULTS
*/

// Either ShareThis or AddThis
var WIDGET_PROVIDER = "ShareThis";

// Appends to post-social-bookmarking div
var SOCIAL_BOOKMARKS_APPENDER = function(e, content) {
var child = e.find('.post-social-bookmarking:first');
child.append(content);
};

// Inserts after the post-header div
var HEADER_BODY_APPENDER = function(e, content) {
var header = e.find('.post-header:first');
header.after(content);
};
// Change this to whichever appender you want
var USE_APPENDER = SOCIAL_BOOKMARKS_APPENDER;

/**
* Finds posts and post content.
*
* Find Posts
* -> pull out url and title
* -> append template using appropriate appender
* -> apply share method to template content
*/
var DEFAULT_FIND_FUNCTION = function(provider) {
var template = provider.compendium.template;
template = provider.compendium.templates[template];

if ( typeof template === 'undefined' ) {
throw new Error('Template ' + provider.compendium.template + ' is undefined');
}

$('div.post').each(function() {
var e = $(this);
var header = e.find('.post-header');
var link = header.find('h2 a');
var url = link.attr('href');
var title = link.text();
var content = $(template).clone();
content.each(function() {
provider.share($(this), url, title);
});
//provider.append(e, content);
});
};
var USE_FINDER = DEFAULT_FIND_FUNCTION;

var PROVIDER_DEFAULTS = {
ShareThis: {
publisher: '48879049-a78f-497b-a417-e590abf72df0',
/*
* http://help.sharethis.com/customization/callback
*
* (the value of clickCallBack) will take 1 argument, we
* will pass the type of service that the user clicked to
* open the Sharethis widget (i.e. facebook). Please note
* that this call back method will only work for chicklets
* outside the ShareThis button, and not for clicks that
* happen from inside the ShareThis widget. If the user
* clicks on the ShareThis button to open the widget we
* will pass 'sharethis'
*/
clickCallBack: function (svc) {
if (typeof allTrackersEvent === 'function') {
allTrackersEvent(['_trackSocial', svc, 'share']);
}
},
compendium: {
template: 'chicklets', // Set this to 'chicklets' to enable Google Analytics social tracking
finder: USE_FINDER,
appender: USE_APPENDER,
templates: {
basic: '<span class="st_sharethis"></span>',
chicklets: '<a target="_blank" href="http://www.exacttarget.com" name="cta=ctawebsite-SocialAboutET052611"><img src="http://cdn.content.compendiumblog.com/uploads/user/1a556805-affb-4faf-9619-39be7a281e1a/d8067321-f7d4-432d-8497-8aa8acf35b29/blog_socialButtonsAboutET.jpg"/></a><span class="st_twitter_custom"><img src="http://cdn.content.compendiumblog.com/uploads/user/1a556805-affb-4faf-9619-39be7a281e1a/d8067321-f7d4-432d-8497-8aa8acf35b29/blog_socialButtonsTweet.jpg"></span><span class="st_sharethis_custom"><img src="http://cdn.content.compendiumblog.com/uploads/user/1a556805-affb-4faf-9619-39be7a281e1a/d8067321-f7d4-432d-8497-8aa8acf35b29/blog_socialButtonsShare.jpg"></span>',
v_counters: '<span class="st_sharethis_vcount"></span><span class="st_facebook_vcount"></span><span class="st_twitter_vcount"></span>',
h_counters: '<span class="st_plusone_hcount"></span><span class="st_twitter_hcount"></span><span  class="st_linkedin_hcount"></span><span class="st_fblike"></span>'
}
}
},
AddThis: {
username: 'kfeatherston',
services_exclude: 'myspace',
data_track_callback: true,
compendium: {
template: 'krystal',
finder: USE_FINDER,
appender: USE_APPENDER,
templates: {
basic: '<a class="addthis_button"></a>',
preferred: '<div class="addthis_toolbox addthis_default_style"><a class="addthis_button_preferred_1" style="cursor:pointer"></a><a class="addthis_button_preferred_2" style="cursor:pointer"></a><a class="addthis_button_preferred_3" style="cursor:pointer"></a></div>',
specific: '<div class="addthis_toolbox addthis_default_style"><a class="addthis_button_facebook" style="cursor:pointer"></a><a class="addthis_button_twitter" style="cursor:pointer"></a><a class="addthis_button_email" style="cursor:pointer"></a></div>',
krystal: '<div class="addthis_toolbox addthis_default_style"><a class="addthis_button_compact" style="cursor:pointer">Share</a><span class="addthis_separator">|</span><a class="addthis_button_preferred_1"></a><a class="addthis_button_preferred_2"></a><a class="addthis_button_preferred_3"></a><a class="addthis_button_preferred_4"></a><a></a></div>'
}
}
}
};


var AddThisProvider = function(config) {
var options = config,
compendium = config.compendium,
finder = config.compendium.finder,
appender = config.compendium.appender,
shared = false,
intervalCount = 0,
interval,
initialize,
self;

// remove so we don't send to AddThis
delete options.compendium;
for ( var key in options ) {
if ( options.hasOwnProperty(key) ) {
addthis_config[key] = options[key];
}
}

initialize = function() {
if ( typeof addthis !== 'undefined' ) {
clearInterval(interval);
/*
* http://www.addthis.com/help/client-api#events
*
* We dispatch key events asynchronously for your consumption.
* N.B. Events are dispatched globally; we do not currently
* support distinguishing between multiple menus on a page.
* ...
* Upon the even being fired, the provided function will be
* called with the event object as its single argument. Any
* relevant event metadata will be attached to the event object.
*/
addthis.addEventListener(
'addthis.menu.share',
function (evt) {
var svc = evt.data.service;
if (typeof allTrackersEvent === 'function') {
allTrackersEvent(['_trackSocial', svc, 'share']);
}
}
);
return;
}
if ( intervalCount > 500 ) {
throw new Error('Unable to load AddThis javascript code');
clearInterval(interval);
return;
}
if ( typeof interval !== 'undefined' ) {
intervalCount++;
return;
}
var s = document.createElement('script');
s.setAttribute('src', 'http://s7.addthis.com/js/250/addthis_widget.js#domready=1&#username=' + options.username);
document.body.appendChild(s);
interval = setInterval(initialize, 10);
};

var share = function(e, url, title) {
if ( !shared ) {
shared = true;
finder(self);
return;
}
e.attr('addthis:url', url);
e.attr('addthis:title', title);
};

self = {
share: share,
run: initialize,
config: options,
compendium: compendium,
append: appender
};

return self;
};


var ShareThisProvider = function(config) {
var options = config,
finder = config.compendium.finder,
appender = config.compendium.appender,
compendium = config.compendium,
shared = false,
intervalCount = 0,
interval,
initialize,
self;

delete options.compendium;

initialize = function() {
if ( typeof stLight !== 'undefined' ) {
console.log('stLight is defined');
clearInterval(interval);
stLight.options(options);
return;
}
if ( intervalCount > 500 ) {
console.log("Probably an error, clearing interval");
clearInterval(interval);
return;
}
if ( typeof interval !== 'undefined' ) {
console.log('Interval is not undefined');
intervalCount++;
return;
}
var s = document.createElement('script');
s.setAttribute('src', 'http://w.sharethis.com/button/buttons.js');
document.body.appendChild(s);
interval = setInterval(initialize, 10);
};

var share = function(e, url, title) {
if ( !shared ) {
shared = true;
finder(self);
return;
}
e.attr('st_title', title);
e.attr('st_url', url);
};

self = {
share: share,
run: initialize,
config: options,
append: appender,
compendium: compendium
};

return self;
};

// Load up provider core
var provider = null;
switch ( WIDGET_PROVIDER ) {
default:
throw new Error("Invalid widget provider specified");
break;
case "ShareThis":
provider = new ShareThisProvider(PROVIDER_DEFAULTS.ShareThis);
break;
case "AddThis":
provider = new AddThisProvider(PROVIDER_DEFAULTS.AddThis);
break;
}

// Runs until jQuery loads, then calls provider.share() (get DOM in good
// state for provider) then provider.run() (load provider code asynchronously)
var interval;
interval = setInterval(function() {
if ( typeof jQuery !== 'undefined' ) {
clearInterval(interval);
provider.share();
provider.run();
}
}, 10);
})();
</script></div>
			</div>
<!-- close the 'sidebar' structural unit -->

		</div>
<!-- close the 'columns' structural unit -->

