     $("#offgeoif").lazyload({}); 
   }
        else if (geoip.county=='Varsinais-Suomi') {
            iframe_src = 'http://ko.snstatic.fi/turunsanomat-is-iframe/';
        } else if (geoip.areacode=='08') {
            iframe_src = 'http://ko.snstatic.fi/kaleva-is-iframe/';
        } else if (geoip.county == 'Kymenlaakso') {
			if(geoip.city == "Kouvola" || geoip.city == "Kuusankoski" || geoip.city == "Elimäki" || geoip.city == "Iitti") {
				iframe_src = 'http://ko.snstatic.fi/is-sale-kousa/';
			}
			else {
				iframe_src = 'http://ko.snstatic.fi/is-sale-kysa/';
			}
            
        } else if (geoip.county == 'Etelä-Karjala') {
            iframe_src = 'http://ko.snstatic.fi/is-sale-esaimaa/';
        }

        if (iframe_src != '') {
            $('#geouutiset').html($('<iframe />', {"name":"geoiframe","id":"geoiframe","src":iframe_src,"height":375,"width":658,"scrolling":"no","border":0,"frameborder":0}));
        }
    }
});
</script>
</div>
</div>

