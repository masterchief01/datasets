<script type="text/javascript">
	jQuery(document).ready(function($) {
		var currentPath = window.location.pathname.split( '/' );
		var page = currentPath[1];
		var user;
		

		// all page slug, NOTE: first array is default page, home or anything thats not here in the pages array
		var pages = [
		"",  // default page
		"fooddiet", 
		"fitness",
		"health",
		"healthystars", 
		"beauty", 
		"sexrelationships"]; 
		
		
		if(page==pages[0]){ //if page home
			user = ["selfmagazine","SELFyStars","YahooHealth", "msnhealth", "lucydanziger", "CBirndorfMD", "sarahjaneRD", "CJNutrition", "meaghanbmurphy", "DrOz", "JillianMichaels", "serenawiliams", "KimKardashian", "mariamenounos"];
			
		}else if(page==pages[1]){ //if page fooddiet
			user = ["fitmenu", "GDeLaurentiis", "Twye", "HungryGirl", "jamie_oliver", "ellie_krieger", "JillianMichaels", "RememberTheMilk", "aroundtheplate", "veganvegetarian","snack_girl_", "tom_colicchio", "shesafatchick", "alwayshungry"];
			
		}else if(page==pages[2]){ //if fitness
			user = ["serenawiliams", "ShawneyJ", "Gunnar", "haleypasternak", "karagoucher", "daratorresswims", "JillianMichaels", "activenetwork", "GoTracy", "Tony_Horton", "lancearmstrong", "fitmenu", "apoloohno", "jessistensland", "lanes0220"];
			
		}else if(page==pages[3]){ //if health
			user =["lucydanziger" , "YahooHealth" , "DalaiLama" , "adventuregirl" , "dailyhealthtip" , "daily_strength" , "StressLess" , "lifestyleoracle" , "CBirndorfMD" , "KeepingCalm" , "PsychToday" , "DrOz" , "soulpancake" , "USATODAYHealth"];
			
			
		}else if(page==pages[4]){ //if healthystars
			user = ["SELFyStars" , "popsugar" , "yahooOmg" , "KimKardashian" , "taylorswift13" , "JnettlesMusic" , "anna_paquin" , "vanessa minnillo" , "sherylcrow" , "colbiecaillat" , "lauren2go" , "thejenaniston" , "amandapeet" , "themandymoore"];
			
			
		}else if(page==pages[5]){ //if beauty
			user = ["evyanmetzner", "hedygold", "lindsaythuggins", "kristensaladino","lidamooremusso","amberlherring","jackierisser","aesam","elainedfarley","cnfashion","bellasugar","fabsugar","mbfashionweek","refinery29","dailycandy","whowhatwear","stylecaster","thecoveteur","theglamourai","intothegloss","fashionista_com","stylelist","womensweardaily","fashionologie"];
			
			
			
		}else if(page==pages[6]){ //if sexrelationships
			user = ["nytimeshealth" , "theglossdotcom" , "cheapdatecomedy" , "nytimeswell" , "yahooshine" , "msn_life" , "msnrelationshpz" , "okcupid" , "match" , "drmichellexo" , "andreasyrtash" , "theedatingcoach" , "eharmony_jack"];
			
		}else{ //default
			user =["SELFmagazine" , "lucydanziger"]; 
		}
		
		$("#twitter").tweet({
			avatar_size: 38,
			count: 4,
			username: user,
			loading_text: "loading tweets...",
			refresh_interval: 60
		});
		
	});
</script>

