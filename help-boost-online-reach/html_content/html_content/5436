    <!-- end feature --></div><div id="ps5_fs2" class="features set16"><!-- start feature --><div class="feature manual first"><div class="feature-content-wrapper"><div class="feature-content">
    
    <h3 class="header">
        
            <img src="/images/headers/hed-blogroll-ho296.gif" alt="Newest Blog Posts" class="featurehed" />
        
    </h3>
        <div class="blurb">
            

            
                <div class="text">
                    
                         <script type="text/javascript" src="/images/widgets/jquery.jfeed.js"></script>
<script type="text/javascript">
jQuery(document).ready(function($)
{
	var parseStatus = 0;
	var feedLimit = 6;
	var postsLimit = 6;
	var blogRollDiv = '#blogRoll';
	var blogRoll = new Array();
	var blogFeeds = new Array(
		'/magazine/blogs/lucysblog/jfeed.xml',
		'/beauty/blogs/selfstylesecrets/jfeed.xml',
		'/beauty/blogs/beyondthebeautypages/jfeed.xml',
		'/fooddiet/blogs/eatlikeme/jfeed.xml', 
		'/fitness/blogs/freshfitnesstips/jfeed.xml',
		'/health/blogs/healthyself/jfeed.xml'
	);
	var month_names = new Array(
		"January",
		"February",
		"March",
		"April",
		"May",
		"June",
		"July",
		"August",
		"September",
		"October",
		"November",
		"December"
	);
	var day_names = new Array(
		"Monday",
		"Tuesday",
		"Wednesday",
		"Thursday",
		"Friday",
		"Saturday",
		"Sunday"
	);

	function stripHTML(string) {
		return string.replace(/(<([^>]+)>)/gi, '');
	}

	function trimWhitespace(string) {
		return string.replace(/^\s+|\s+$/g, '').replace(/[\s(&nbsp;)]+$/g, '');
	}

	function truncate(string, length, ellipsis)
	{
	    if (string.length < length) return string;
	    for (var i = length-1; string.charAt(i) != ' '; i--) {
	        length--;
	    }
	    return string.substr(0, length) + ellipsis;
	}

	function formatPostDate(date) {
		var post_day = date.getDay();
		var post_date = date.getDate();
		var post_month = date.getMonth();
		var post_year = date.getFullYear();
		var post_hour = date.getHours();
		var post_minute = date.getMinutes();
		if(post_minute < 10) { post_minute = '0' + post_minute; }

		var post_dayName = day_names[post_day];
		var post_monthName = month_names[post_month];
		var post_monthNumber = (post_month + 1);

/*		var post_am_pm = '';
		if (post_hour < 12) { post_am_pm = "AM"; } else { post_am_pm = "PM"; }
		if (post_hour == 0) { post_hour = 12; }
		if (post_hour > 12) { post_hour = post_hour - 12; } */

/*		var post_time = (post_hour + ':' + post_minute + ' ' + post_am_pm);*/
		var post_longdate = (post_dayName + ', ' + post_monthName + ' ' + post_date + ', ' + post_year);
		var post_shortdate = (post_monthNumber + "/" + post_date + "/" + post_year);

		var post_today = new Date();
		post_today.setHours(0,0,0,0);
		var post_compare = new Date(0,0,0,0,0,0,0);
		post_compare.setFullYear(post_year, post_month, post_date);
		var post_d1 = post_today.getTime();
		var post_d2 = post_compare.getTime();
		if (post_d1 == post_d2) { return( "Today " /*+ post_time */ ); }
		else if (post_d1 == post_d2 + 86400000) { return( 'Yesterday ' /*+ post_time*/ ); }
		else if (post_d2 > post_d1 - 604800000) { return( post_dayName /*+ ' at ' + post_time*/ ); }
		else { return( post_shortdate /*+ ' at ' + post_time*/ ); }
	}

	function displayFeed(array)
	{
		/* <AA> sort posts */
		blogRoll.sort( function(a, b)
		{
			return b.sequence - a.sequence;
		});

		$(blogRollDiv).append(
			'<style type="text/css">' +
				'#blogRoll #post-loading {' +
					'display:none;' +
				'}' +
			'<\/style>'
		);

		/* <AA> display blogRoll */
		for(var k = 0; k < array.length && k < feedLimit; k++)
		{

/*			if(array[k].image != '') */
			{
				thpost = '<div class="post-thumb">' + '<a href="' + array[k].link + '">' + '<img src="' + array[k].image + '" \/>' + '<\/a>' + '<\/div>'; 
/*			} else { */
				thpost = '';
			}

			$(blogRollDiv).append(
			'<div id="post-' + k + '" class="post-entry">' +
				'<div class="post-infoline">' +
					'<a href="' + array[k]./*blog*/link + '">' + array[k].blogname + '<\/a>' + ' &raquo; ' +
					'<span class="post-date">' + array[k].updated + '<\/span>' +
				'<\/div>' +

				thpost +

				'<div class="post-title">' +
					'<a href="' + array[k].link + '">' + array[k].title + '<\/a>' +
				'<\/div>' +
/*
				'<div class="post-description">' +
					array[k].description + ' ' +
					'<a class="post-readmore" href="' + array[k].link + '">read more &raquo;<\/a>' +
				'<\/div>' +
*/
				'<div class="post-footer">' + '<\/div>' +
			'<\/div>'
			);
		}
	}

	for(var i = 0; i < blogFeeds.length; i++)
	{
		jQuery.getFeed({
			url: blogFeeds[i],
			success: function(feed)
			{
				/* <AA> before feed's items are parsed */

				/* <AA> parse feed's items */
				for(var j = 0; j < feed.items.length && j < postsLimit; j++)
				{
					feed.items[j].sequence = Date.parse(feed.items[j].updated);

					var postDateString = feed.items[j].updated;
					var postDate = new Date(postDateString);
					feed.items[j].updated = formatPostDate(postDate);
					feed.items[j].description = trimWhitespace(feed.items[j].description);
					feed.items[j].description = stripHTML(feed.items[j].description);
					feed.items[j].description = truncate(feed.items[j].description, 65, '...');

					feed.items[j].blogname = feed.title;
					feed.items[j].bloglink = feed.link;

					/* <AA> add this feed's items to blogRoll array */
					blogRoll.push(feed.items[j]);
				}

				/* <AA> after feed's items are parsed */
				parseStatus++;
				if(parseStatus == blogFeeds.length)
				{
					/* <AA> done loading all blogRoll's feeds */
					displayFeed(blogRoll);
				}
			}
		});
	}


});
</script>

<!--
		'/beauty/blogs/beyondthebeautypages/jfeed.xml',
		'/beauty/blogs/selfstylesecrets/jfeed.xml',
		'/fitness/blogs/freshfitnesstips/jfeed.xml',
		'/fooddiet/blogs/eatlikeme/jfeed.xml',
		'/fooddiet/blogs/healthybites/jfeed.xml',
		'/health/blogs/healthyself/jfeed.xml',
		'/magazine/blogs/lucysblog/jfeed.xml',
		'/magazine/blogs/selfystars/jfeed.xml'

		'/images/widgets/blogs/beauty.xml',
		'/images/widgets/blogs/style.xml',
		'/images/widgets/blogs/fitness.xml',
		'/images/widgets/blogs/eatlikeme.xml'
-->

