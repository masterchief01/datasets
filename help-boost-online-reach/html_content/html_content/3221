nts div').hide();				$('.artcomments').show();				return false;			});						$('.articlecomments').click(function() {				$('#comments div').hide();				$('.artcomments').show();				return false;			});						if($('.content').length > 1) {				$('.content').hide();				$('#content1').show();								$('.showcontent').click(function() {					$('.showcontent').css('color', '#333');					$(this).css('color', '#ac0101');					var content = $(this).attr('title');					$('.content').hide();					$('#'+ content).show();					return false;				});							}						$('a.final').click(function() {				if($('.promoslide').length == 0) {										$('<div />').addClass('promoslide').load("/mhlists/promo/index.php .content2", function() {						$('#slide1 > img').hide();											}).appendTo($('#slide1'));										$('<div />').addClass('promocontent').load("/mhlists/promo/index.php .content", function() {						if($('.promocontent').text().length > 1) {							$('.origcontent').hide();						} else {							$(this).hide();						}					}).prependTo($('#zmodcontent'));									} else {					$('#slide1 > img').hide();					$('.promoslide').show();					if($('.promocontent').find('p').text().length > 1) {						$('.origcontent').hide();						$('.promocontent').show();					}				}				$(this).addClass('disabled');				$('.prev a').addClass('fakeprev');				return false;			});						$('a.fakeprev').live('click', function() {				$('#slide1 > img').show();				$('.origcontent').show();				$('.promoslide').hide();				$('.promocontent').hide();				$('.next a').removeClass('disabled');				$(this).removeClass('fakeprev');				return false;			});		});	</script>			<noscript>		<style type="text/css" media="screen">			#zmod, #zmodpagination {				visibility: visible;			}		</style>	</noscript>	<script language="Javascript">	      <!--              var ord=Math.random()*10000000000000000;	      //-->	</script>		<script type="text/javascript" charset="utf