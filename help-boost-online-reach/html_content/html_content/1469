Reuters.utils.adjustColumnistAttribution = function() {
	var count = $('#homepageColumnists h2').length;
	for (var i=0; i<count; i++) {
		$('#homepageColumnists h2').eq(i).before($('#homepageColumnists div.contributor').eq(i));
	}
}
Reuters.utils.replaceContent("columnistsData", "/assets/sharedModuleLoader?view=RSM-Global-HP-Columnists", null, Reuters.utils.adjustColumnistAttribution);
