	<div id="trendWatch" class="focus"><script type="text/javascript">
Reuters.namespace("social");
Reuters.social.parsePulse = function(obj){
	var pulseobj = obj[0];
	var maxitems = pulseobj.response.length;
	var className = $("#pulseArticles ul").attr("class");
	var limitFormat =  /limit-([0-9]{1,2})/;
	if (typeof className !== 'undefined' && className.match(limitFormat)){
		maxitems = parseInt(className.match(limitFormat)[1]);
	} 
	trace("parsing pulse, maxitems: " + maxitems);
	if (pulseobj.status == "ok" ){
		var pHTML = "";
		var article_arr = pulseobj.response;
		var count = 0;
		for (p=0; p<article_arr.length; p++){
				curr = article_arr[p].page;
			if (curr.url.indexOf("video") == -1 && count < maxitems){
				pHTML += '<li>';
				if (curr.thumbnail && curr.thumbnail != "") pHTML += '<div class="photo"><a href="' +curr.url + '" ><img src="http://s1.reutersmedia.net/resources/r/?t=2&w=85&' + curr.thumbnail +  '"/></a></div>';
				pHTML += '<a href="' +curr.url + '" >' + curr.title + '</a></li>';
				count++;
			}
		}
		$("#pulseArticles ul").html(pHTML);
		$("#pulseArticles").show();
	}
}

</script>

