<div id="medalsCount"></div>
<script>
Reuters.medalsList = function(obj){
		Reuters.medalsList.medalcount = obj.response;
		var oMedalsTotal = Reuters.medalsList.medalcount;
		for (i = 0; i < oMedalsTotal.length; i++) {
			var medalCountry = oMedalsTotal[i].NOC;
			var goldCount = oMedalsTotal[i].gold;
			var silverCount = oMedalsTotal[i].silver;
			var bronzeCount = oMedalsTotal[i].bronze;
			if (medalCountry == "United States") {
				if (goldCount || silverCount || bronzeCount) {
				var html = '<div id="medalTallyHeader">USA Medals</div>';
				html += '<div id="medalTally">';
				html += '<div id="goldmedal"><img src="/resources_v2/images/olympic_gold.gif" border="0" /><div class="oly-text"><span id="goldcount">' + goldCount + '</span>&nbsp;|&nbsp;</div></div>';
				html += '<div id="silvermedal"><img src="/resources_v2/images/olympic_silver.gif" border="0" /><div class="oly-text"><span id="silvercount">' + silverCount + '</span>&nbsp;|&nbsp;</div></div>';
				html += '<div id="bronzemedal"><img src="/resources_v2/images/olympic_bronze.gif" border="0" /><div class="oly-text"><span id="bronzecount">' + bronzeCount + '</span>&nbsp;&nbsp;</div></div>';
				html += '</div>'
				document.getElementById("medalsCount").innerHTML = html;
			}  else  {
				var html = '<div id="medalTallyHeader">USA Medals</div>';
				html += '<div id="medalTally">';
				html += '<div id="goldmedal"><img src="/resources_v2/images/olympic_gold.gif" border="0" /><div class="oly-text"><span id="goldcount"> 0 </span>&nbsp;|&nbsp;</div></div>';
				html += '<div id="silvermedal"><img src="/resources_v2/images/olympic_silver.gif" border="0" /><div class="oly-text"><span id="silvercount"> 0 </span>&nbsp;|&nbsp;</div></div>';
				html += '<div id="bronzemedal"><img src="/resources_v2/images/olympic_bronze.gif" border="0" /><div class="oly-text"><span id="bronzecount"> 0 </span></div></div>';
				html += '</div>'
				document.getElementById("medalsCount").innerHTML = html;
		} 
}
}
};
Reuters.utils.loadScript('medalcount', 'http://infos.pulseapi.reuters.com/api/0.15/olympics/jsonp/medals?callback=Reuters.medalsList&SeasonID=482');
</script>

