<script language="javascript">
wwd.slideshow.jqueryCall = 'http://result.wwd.com/spring/event/most.go?day=5&event=most_viewed&type=all&site=WWS&env=PROD&callback=?';

var mostViewedContentIds = [];
function callMostViewedSlideshowArticles(jsonArticles) {
  for (i = 0; i < mostViewedContentIds.length; i++) {
    for (j = 0; j < jsonArticles.length; j++) {
        if (jsonArticles[j].articleId == mostViewedContentIds[i]) {
            $("#tab2-div").append("<div class=\"slideshow-tout\"><a href=\"" + jsonArticles[j].articleUrl + "#/slideshow/article/" + jsonArticles[j].articleId + "/" + jsonArticles[j].imageId + "\"><img src=\""+unescape(jsonArticles[j].mediaFile.toolLandscapeUri)+"\" /></a><div class=\"slideshow-title\"><a href=\"" + jsonArticles[j].articleUrl + "#/slideshow/article/" + jsonArticles[j].articleId + "/" + jsonArticles[j].imageId + "\"> " + unescape(jsonArticles[j].articleHed) + "</a></div><div class=\"related-date\">Posted " + jsonArticles[j].articleLiveDate + "</div></div>");
            break;
        }
    }
  }
}
</script>

        <div id="tab2-div" class="tab_content" style="display:none;">

        <ul id="viewed"></ul>

        </div>
    </div>
</div>

