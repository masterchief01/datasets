<script>
function slideshowMicrositeAd(section, seasonYear) {


    var section = $("#wwd-section").html();
    timeout = setTimeout("renderSlideshowAd('" + section + "', '"+seasonYear+"')",0);
 }
function slideshowAd(section) {


    if ( section == "homepage" ) {

        $("#wwd-section").remove();
        $("#slideshow").append("<div id=\'wwd-section\' style=\'display:none;\'>homepage</div>");
    } else if (section == "null") {
        $("#wwd-section").remove();
        $("#slideshow").append("<div id=\'wwd-section\' style=\'display:none;\'>homepage-features</div>");
    } else if (section == "features") {
        section = ("homepage-features");
        $("#wwd-section").remove();
        $("#slideshow").append("<div id=\'wwd-section\' style=\'display:none;\'>homepage-features</div>");
    } else {
        var section = $("#wwd-section").html();
    }
     // this timeout has to be here for IE6
         if(wwd.slideshow.season != null)
         {
            timeout = setTimeout("renderSlideshowAd('" + section + "', '"+wwd.slideshow.season+"-"+wwd.slideshow.year+"')",0);
         }
         else
         {

             timeout = setTimeout("renderSlideshowAd('" + section + "', '')",0);
         }
}

$(document).ready(function() {
    slideshowAd("");
});</script>





