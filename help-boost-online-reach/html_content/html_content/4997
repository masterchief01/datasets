<script type="text/javascript" language="javascript">
$(document).ready(function() {
    authorizeUser();
});


function authorizeUser() {
    PublicationsController.authorizeUser(1875840, authCompleteCallback);
}

function authCompleteCallback(accessAllowed) {
    if (accessAllowed) {
        $(".pdf").attr("href","/wwd-publications/pdf/wwd-2008-12-03.pdf?id=1875840");
        $(".pdf").attr("target","_blank");
    } else {
        // put roadblocks here, in two different flavors
        isArchiveOnly = true;
        if (isArchiveOnly) {
            //display the archive roadblock
            $(".pdf").attr("href","javascript:showRoadBlock()");
        } else {
            //display the online roadblock
            $(".pdf").attr("href","javascript:showRoadBlock()");
        }
    }
}

</script>


