<script type="text/javascript">




var ENGINE_URL = 'http://ubokia.com/cms2-public';
var SITE_URL = 'http://ubokia.com';
var CDN_URL = 'http://cdn.ubokia.com';
var SECURE_URL = 'https://ubokia.com';





// API for category picker -- also handles subcat showing/hiding
function categoryPickerSet(id, target, initial) {
	if (initial==null) {
		initial = false;
	}

	// if we are in expanded mode, react as such:
	if ($("#browse-categories-full").height() > 20) {

		if (id == '') {
		  id = 'ANY';
		}
		//Check if parent or child
		var isParent = false;
		if ($("#category-" + id).hasClass("category-parent")) {
			isParent = true;
		}
		
		// grab the element that I clicked
		$trigger = $("#category-" + id);
		
		// derive the parent of the clicked element (if it was a child)
		var parent = "";
		relparts = $trigger.attr("rel").split(" ");
		if (!isParent && relparts[1] != undefined) {
			parent = relparts[1];
			if ($(parent).hasClass("category-selected") == false) {
				// click the parent programmatically so that it highlights itself and shows all children
				$(parent).trigger("click");
		   }
		}
		
		// grab the hidden form element in which to store the category value
		$target = $(target);
	
		//store value in the target
		if (id == 'ANY') {
		  $target.val('');
		} else {
		  $target.val(id);
		}
		
		// fire "change" event; when you're not signed in there are events on want/post bound to 'change' events
		// of each field to keep the login modal's mirror elements in sync.
		$target.trigger("change");
		
		// set name on the "mini picker"
		$("#selected-category-name").html($trigger.html());
		
		// clear styles on all
		$(".category-name").removeClass("category-selected");
		$(".category-name").removeClass("category-sub-selected");
		
		
		// style the one that was clicked
		if (isParent) {
			$("#category-" + id).addClass("category-selected");
		} else {
			$("#category-" + id).addClass("category-sub-selected");
		}
		$(parent).addClass("category-selected");
		
		//Hide all children
		if (isParent) {
			$(".browse-categories ul li ul").hide();
		}
		
		var currentHeight = $(".browse-categories").height();
		var childrenHeight = $("#category-" + id).parent().children("ul").height();
		//Determine parent height based on number of parent li
		var parentHeight = 0;
		$(".category-parent").each(function(e) {
			parentHeight += 18;
		});
		
			
		
		
		//Show proper children if we clicked a parent
		if (isParent) {
		
			if (currentHeight < childrenHeight) {
			
				currentHeight = childrenHeight;
				$("#category-" + id).parent().children("ul").show();
				$(".browse-categories").animate({ height: currentHeight }, 500);
				$(".parent-category-ul").animate({ height: currentHeight }, 500);
			
			} else if ((currentHeight > childrenHeight) && (childrenHeight > parentHeight)) {
			
				currentHeight = childrenHeight;
				$("#category-" + id).parent().children("ul").show();
				$(".browse-categories").animate({ height: currentHeight }, 500);
				$(".parent-category-ul").animate({ height: currentHeight }, 500);
			
			} else if ((currentHeight > childrenHeight) && (childrenHeight < parentHeight)) {
			
				currentHeight = parentHeight;
				$("#category-" + id).parent().children("ul").show();
				$(".browse-categories").animate({ height: currentHeight }, 500);
				$(".parent-category-ul").animate({ height: currentHeight }, 500);
			
			}
		
		}
		
	}
	
	if (initial) {
	
		}
}

function hideShowCategories(action) {
	//if ($("#browse-categories-full").height() > 20) {
	if (action == "show") {
		$(".parent-category-ul").animate({ marginTop: 0 }, 500);
		$(".child-category-ul").animate({ marginTop: 0 }, 500);
		var parentHeight = $(".parent-category-ul").height();
		var childHeight = $(".child-category-ul").height();
		var newHeight = 0;
		if (parentHeight < childHeight) {
			newHeight = childHeight;
		} else {
			newHeight = parentHeight;
		}
		$("#browse-categories-full").animate({ height: newHeight}, 500, function() { 
			$("#categories-hide-show-img").attr("src", "http://cdn.ubokia.com/images/area-hide.png");
			$("#categories-hide-show-content").text($("#categories-hide-show-content").text().replace(/show/ig, "hide"));
			$("#categories-hide-show-content").html("hide categories");
		});
	} else if (action == "hide") {
		var parentPos = $(".category-selected").parent().position();
		var parentPosPx = 0 - parentPos.top;
		var subPos = $(".category-sub-selected").parent().position();
		$(".parent-category-ul").animate({ marginTop: parentPosPx }, 500);
		if (subPos != null) {
			childPosPx = (0 - subPos.top) + Math.abs(parentPosPx);
			$(".child-category-ul").animate({ marginTop: childPosPx }, 500);
		} else {
			var hideHeight = 1 - $(".child-category-ul").height();
			$(".child-category-ul").animate({ marginTop: hideHeight });
		}
		$("#browse-categories-full").animate({ height: "18px"}, 500, function() { 
			$("#categories-hide-show-img").attr("src", "http://cdn.ubokia.com/images/area-show.png");
			$("#categories-hide-show-content").text($("#categories-hide-show-content").text().replace(/hide/ig, "show"));
		});
	}
}





// we post to this after clicking the Facebook Connect button(s)
function fbPostLogin() {
  FB.getLoginStatus(function(response) {
	location.href = SECURE_URL + "/?page=login&action=fbPostAuth&path=http%3A%2F%2Fubokia.com%2Ffashion";
  });
}

function updateDashboardNotificationsCount() {
	$.getJSON(SITE_URL, { page: 'process', action: 'updateDashboardNotificationsCount'}, function(json) {

						
		// update the count in the sidebar
		$countElem = $(".notification-count");
		if (json.requiresAttentionCount > 0) {
			// we may need to re-create the element, as it may have been previously removed.
			if ($countElem.length == 0) {
				$("#notification-count-container").html('<span class="notification-count"></span>');
			}
			$(".notification-count").html(json.requiresAttentionCount);
		} else {
			$(".notification-count").remove();
		}
	});
}



jQuery().ready(function() {




	/*
	var secureForms = $(".secure-form");
	$.each(secureForms, function(i, elem) {
		$.get(SITE_URL + "/?page=secure_form", function(txt) {
			$(elem).append('<input type="hidden" name="_secure_form_timestamp" value="'+txt+'" />');
			$(elem).append('<input type="hidden" name="_secure_form" value="1" />');
		});
	});
	*/





// handle category picker click event
$(".category-name").click(function(e) {
	// extract the target
	relparts = $(this).attr("rel").split(" ");
	target = relparts[0];
	
	// extract the ID
	parts = $(this).attr("id").split("-");
	id = parts[1];
	
	categoryPickerSet(id, target);
	
});

// show/hide categories
$("#categories-hide-show").hover(
	function() {
		if ($("#categories-hide-show-img").attr("src") == "http://cdn.ubokia.com/images/area-hide.png") {
			$("#categories-hide-show-img").attr("src", "http://cdn.ubokia.com/images/area-hide-hover.png");
		} else {
			$("#categories-hide-show-img").attr("src", "http://cdn.ubokia.com/images/area-show-hover.png");
		}
		$("#categories-hide-show-content").css("text-decoration", "underline");
	}, function() {
		if ($("#categories-hide-show-img").attr("src") == "http://cdn.ubokia.com/images/area-hide-hover.png") {
			$("#categories-hide-show-img").attr("src", "http://cdn.ubokia.com/images/area-hide.png");
		} else {
			$("#categories-hide-show-img").attr("src", "http://cdn.ubokia.com/images/area-show.png");
		}
		$("#categories-hide-show-content").css("text-decoration", "none");
	}
);

$("#categories-hide-show").click(function() {
	if ($("#browse-categories-full").height() < 20) {
		hideShowCategories("show");
	} else {
		hideShowCategories("hide");
	}
});

$("#browse-categories-full").click(function() {
	if ($("#browse-categories-full").height() < 20) {
		hideShowCategories("show");
	}
});

