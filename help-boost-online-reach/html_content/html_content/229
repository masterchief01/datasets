// update notification count every 5 minutes
setInterval("updateDashboardNotificationsCount()", 1000*60*5);


// make internal links scroll smoothly.
$.localScroll();

// handle the slide-up/slide-down functionality for the left hand sidebar
$(".side-nav-link-slider").click(function() {
	target = $(this).attr("rel").substr(21);
	if ($("#side-nav-sub-" + target).is(":hidden")) {
		$(this).css("background-image", "url('http://cdn.ubokia.com/images/side-nav-slider-bg-up.png')");
		$("#side-nav-sub-" + target).slideDown(500, function() {
			$("#side-nav-sub-" + target).show();
		});
	} else {
		$(this).css("background-image", "url('http://cdn.ubokia.com/images/side-nav-slider-bg.png')");
		$("#side-nav-sub-" + target).slideUp(500, function() {
			$("#side-nav-sub-" + target).hide();
		});
	}
});
// activate any links that should load modals
$("a.modal").fancybox();
$("a.fancybox").fancybox();

// activate the "close" link image for the modals
$("a.fb-close").click(function(e) {
	e.preventDefault();
	$.fancybox.close();
});
// activate the defaultValue plugin for text inputs
$(".defaultvalue type=[text]").defaultValue();
// activate the help dropdown menu in the main nav
$("#nav ul li").hoverIntent(function(e) {
	$sub = $(this).children("ul");
	$sub.stop().fadeIn(300, function() {
	  $sub.fadeTo(1, 1);
	});
},
function(e) {
	$sub = $(this).children("ul");
	$sub.stop().hide();
});
$("#nav ul li ul").hoverIntent(function() {
	$(this).show();
},
function(e) {
	$(this).fadeOut(500)
});
// Activate the login slider
$(".login-button").click(function() {
	if ($("#login-area").is(":hidden")) {
		$("#login-area").slideDown(500);
		$(".login-button").attr("id", "login-button-clicked");
		$("#login-area-bg").show(function() {
			var height = $(window).height();
			var width = $(window).width();
			$("#login-area-bg").height(height);
			$("#login-area-bg").width(width);
		});
	} else {
		$("#login-area").slideUp(500, function() {
			$(".login-button").attr("id", "");
			$("#login-area-bg").hide();
		});
	}
});
$("#login-area-bg").click(function() {
	$("#login-area").slideUp(500, function() {
		$(".login-button").attr("id", "");
		$("#login-area-bg").hide();
	});
});
// Better user experience for the mini-login form in the slider
$(".user-login-username").focusin(function() {
	$(this).attr("id", "user-login-value");
	$(this).css("background-color", "#fff");
});
$(".user-login-password").focusin(function() {
	$(this).attr("id", "user-login-value");
	$(this).css("background-color", "#fff");
});
$(".user-login input").focusout(function() {
	if ($(this).hasClass("user-login-username") && $(this).val() == "") {
		$(this).attr("id", "");
	} else if ($(this).hasClass("user-login-password") && $(this).val() == "") {
		$(this).attr("id", "");
	}
});
// Fearns removed password javascript and added forgot username link.
$(".forgot-username").click(function() {
        var email = prompt("Forgot your username? Enter your email address below and we will email it to you.  Remember you can also login by entering your email instead of your username.");
        if (email == '' || email == null) {
                alert("No email address provided. Please try again.");
        } else {
                window.location.href = SITE_URL + '/?page=login&action=forgotUsername&email=' + email;
        }
});
// Better user experience for the 'change my location' functionality in the login slider
$("#change-zip").click(function(e) {
	$("#change-location-link").hide();
	$("#change-location-form").show();
	$("#change-location-input").focus();
});







$("#intro-button-link").hover(
	function() {
		$(".bob-container").css( { backgroundPosition: "0 -74px" } );
	}, function() {
		$(".bob-container").css( { backgroundPosition: "0 0" } );
	}
);



});
</script>







