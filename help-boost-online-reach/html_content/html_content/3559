<script language="javascript">
var rated_img = new Image();
rated_img.src = "/modules/news/images/ratings/star_rated_state.gif";
var unrated_img = new Image();
unrated_img.src = "/modules/news/images/ratings/star_unrated_state.gif";

var max_stars = 5;

function do_on_over_star( i_star )
{
	if( i_star >= 1 && i_star <= max_stars )
	{
		for( i=1; i<=max_stars; i++ )
		{
			img_elem = document.getElementById( "_star" + i );
			if( i <= i_star )
				img_elem.src = rated_img.src;
			else
				img_elem.src = unrated_img.src;
		}
	}
}

function do_on_out_of_star( i_star )
{
	if( i_star >= 1 && i_star <= max_stars )
	{
		for( i=1; i<=max_stars; i++ )
		{
			img_elem = document.getElementById( "_star" + i );
			img_elem.src = "/modules/news/images/ratings/star_unknown_state.gif";
		}
	}
}

function do_rate_article( form_action )
{
	var f = document.forms['rateform'];
	f.action = form_action;
	f.submit();
}
</script>



