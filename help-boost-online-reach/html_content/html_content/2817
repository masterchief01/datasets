<script language="javascript">
function fpsShowUserInfo(){//Here we do the user info loading
	stmt='(new com.fps.ajax.CurrentUserInfo(document.getElementById("userInfo"))).loadsInfo()';
	fpsDependExecute("fl:com.fps.ajax.js,fl:com.fps.ajax.CurrentUserInfo.js,ob:userInfo",stmt);
}
</script>
