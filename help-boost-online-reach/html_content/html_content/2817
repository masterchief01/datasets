<script language="javascript">
// global function that will check to make sure the dependency
// is okay then execute the function
// functionCall can be a String , it will be eval()
// or a function object, it will be call()
function fpsDependExecute(objectID, functionCall) {
    // break up objectID
    var entries=null;
    if (objectID.indexOf(',') == -1) {
        // single entry
        entries = new Array(objectID);
    } else {
		entries = objectID.split(/, ?/);
    }

    var check;
    for (var i=0; i<entries.length; i++) {

        check = entries[i].substring(3,entries[i].length);
        // check files, then js vars, then html ids
        if (entries[i].indexOf('fl:') != -1) {
            if ( (typeof(loaded_scripts) != "object") || (loaded_scripts[check] != true) ) {
                setTimeout("fpsDependExecute('" + objectID + "', '" + functionCall + "')", 100);
                return;//---------
            }
        } else if (entries[i].indexOf('js:') != -1) {
            if ( typeof (check) == "undefined" || check==null ) {
                setTimeout("fpsDependExecute('" + objectID + "', '" + functionCall + "')", 100);
                return;//---------
            }
        } else if (entries[i].indexOf('ob:') != -1) {
            if (!document.getElementById(check)) {
                setTimeout("fpsDependExecute('" + objectID + "', '" + functionCall + "')", 100);
                return;//-------
            }
        }
    }
   //real work
	if(typeof functionCall =="function") {
		//functionCall.call();
   	}else {
		//eval(functionCall);
    }
    return true;
}
</script>
