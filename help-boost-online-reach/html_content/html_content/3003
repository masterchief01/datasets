        window.refreshLinks = function() {
            jive.rte.multiRTE.forEach(function(rteID) {
                if ($j('#' + rteID + '_parent').length < 1) {
                    // safari 2, text only
                    $j('#' + rteID + '_toggle_html').hide();
                    $j('#jivePreferredEditorModeLinkHREF').hide();
