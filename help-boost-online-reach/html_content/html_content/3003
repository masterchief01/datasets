                    $j('#' + rteID + '_toggle_html').hide();
                }

                if (preferredMode == currentMode) {
                    $j('#jivePreferredEditorModeLinkHREF').hide();
                } else {
                    $j('#jivePreferredEditorModeLinkHREF').css('display', 'block');
