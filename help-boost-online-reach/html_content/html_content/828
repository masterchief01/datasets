			$("a.dynamic_embed").one('click',function(){
		      var anchor = $(this);
		      anchor.html(anchor.html().replace('<!--','').replace('-->',''));
		      $(this).find('span.play_button').hide();
		      $(this).find('img').hide();
		      $allVideos = $("#ajax-vid iframe");
		      $allVideos.each(function() {
			  	  $(this).data('aspectRatio', this.height / this.width).removeAttr('height').removeAttr('width');
			  });
		      $(window).resize();
		      return false;
		    });
			
			$(".bbp-replies").on("click","a.bbp-reply-link", function(){
				$linkid = $(this).attr('id');
				$linkid = "@"+$linkid+", ";
				$replycontent = $("#bbp_reply_content").val();
				$replycontent += $linkid;
				$("#bbp_reply_content").focus().val($replycontent);
				$('html,body').animate({scrollTop: $("#bbp_reply_content").offset().top},'slow');
				return false;
			});
			
			$("a.tooltip").tooltip({ effect: 'slide', delay: 250, offset: [15,0], relative: true, position: 'bottom center'});
			
			function ajax_spam(object,url,action,link){
				$(object).fadeOut();
				$.post(url, function(){
					$parent = $(object).parent();
					if(action == 'spam'){
						$(object).replaceWith("<a style='display:none' class='no-ajaxy spam unspam' onclick='return ajax_spam(this,\"" + link + "\",\"publish\", \"" + url + "\");' title='Unmark this as spam'></a>");
						$parent.children('a.spam:first').fadeIn();
					}
					else{
						$(object).replaceWith("<a style='display:none' class='no-ajaxy spam' onclick='return ajax_spam(this,\"" +  link + "\",\"spam\", \"" + url + "\");' title='Mark this as spam'></a>");
						$parent.children('a.spam:first').fadeIn();
					}
				});			
			}

			$('form#new-post').bind('submit', function(e) {
			     $(this).find('input:submit').attr('disabled', 'disabled');
			});
			
			function getfileextension(filename) 
			{ 
			    var dot = filename.lastIndexOf("."); 
			    if( dot == -1 ) return ""; 
			    var extension = filename.substr(dot,filename.length); 
			    return extension; 
			}
			
			$('#content .bbp-reply-content a').each(function(){
			    var ext = getfileextension(this.href);
			    switch(ext){
			        case '.gif':
			        case '.png':
			        case '.jpg':
			        case '.jpg':
			            var img = $('<img>',{src: this.href});
			            $(this).replaceWith(img);
			            break;
			    }
			
			});
			
			$(".filter").on("click","li.dropdown ul li a", function(){
				newcat = $(this).attr('title');
				$(this).parent().parent().parent().find('a.displayed').html(newcat + '<span>&#x25BC;</span>');
				$('li.dropdown ul li').show();
				$(this).parent().hide();
			});
			
			$("ul.filter > li").not('.category').children().click(function(){
				$('li.category a.displayed').html('Category<span>&#x25BC;</span>');
				$('li.category ul li').show();
			});
			
			$("ul.filter > li").not('.sort').children().click(function(){
				$('li.sort a.displayed').html('Sort<span>&#x25BC;</span>');
				$('li.sort ul li').show();
			});
			
