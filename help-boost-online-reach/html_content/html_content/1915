<script>
function splatRequest(){
ajaxRequest('', '/modules/splat/ajaxSplat.jhtml?itemID=1640963&splattype=plain', '', 'plainSplatResponse');
}
function slideSplatRequest(rating){
ajaxRequest('', '/modules/splat/slideSplat.jhtml?itemID=1640963&slide=null&rating=' + rating , '', 'slideSplatResponse');
if(rating >= 3){
var mainSplatUpdateFunction = 'function(){}'; // if you want this to update the splat on the page, change to 'plainSplatResponse'
ajaxRequest('', '/modules/splat/ajaxSplat.jhtml?itemID=1640963&splattype=plain&subItemId=null&rating=' + rating , '', mainSplatUpdateFunction);	
}
}
</script>

<script language="javascript">
function URLDecodeIt (encodedString) 
{
var output = encodedString;
var plus = "+";
var f = output.indexOf(plus);
while (f>-1)
{
output=output.substring(0,f)+"%20"+output.substring(f+1);
f = output.indexOf(plus);
}
return output;
}
</script>

