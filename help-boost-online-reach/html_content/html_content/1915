<script>
var resetStar = new Array("one-star", "two-stars", "three-stars", "four-stars", "five-stars");

function setStarRating(rating){
for(x in resetStar){
if(x > rating){
document.getElementById(resetStar[x]).style.backgroundPosition = "left top";
}else{
document.getElementById(resetStar[x]).style.backgroundPosition = "left center";
}
}
document.getElementById(resetStar[rating]).style.backgroundPosition = "left center";
}

function resetRating(rating){
for(x in resetStar){
if(x > rating){
document.getElementById(resetStar[x]).style.backgroundPosition = "left top";
}else{
document.getElementById(resetStar[x]).style.backgroundPosition = "left center";
}
}
}

function defalutRating(){
var curRating = document.getElementById("ratingValue").value - 1;
for(x in resetStar){
if(x > curRating){
document.getElementById(resetStar[x]).style.backgroundPosition = "left top";
}else{
document.getElementById(resetStar[x]).style.backgroundPosition = "left center";
}
}

}

function checkMessage(target, message,goAheadFucName,itsArgs){
if(document.getElementById(target).value == ""){
alert("Please Fill in the " + message + " section.");	
}else{
eval(goAheadFucName+"('"+itsArgs+"')");
//setComments('theComment');
}
}
</script>

