<script language="javascript" type="text/javascript">
//Create your sharelet with desired properties and set button element to false
var theTitle = 'Creative+School+Lunch+Ideas';
var object = SHARETHIS.addEntry({
title:URLDecodeIt(theTitle)
}, {button:false});
//Output your customized button
document.write('<div id="shareBottom"><a href="javascript:void(0);" class="shortcuts"><img src="/images-pclocal/parentspicks10/sharethis_h_button.GIF" border="0" /></a></div>');
//Tie customized button to ShareThis button functionality.
