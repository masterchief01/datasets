*/
var s = '';
var i;
/*
* Verify that there are actually ads to display.
*/

if (google_ads.length == 0) {
//alert("0");
return;
}
/*
* If an image or flash ad is returned, display that ad.
* Otherwise, build a string containing all of the ads and
* then use a document.write() command to print that string.
*/
//alert(google_ads.length);
if (google_ads.length == 1) {
s += '<a href=\"' + google_info.feedback_url + '\" style="color:#666666">Ads by Google</a><br> <div class="gader" style="width:100%; float:none;"><a style="color:#fff;text-decoration:none" href="' + google_ads[0].url + '" onmouseout="window.status=\'\'"onmouseover="window.status=\'go to ' +google_ads[0].visible_url + '\';return true"> <span style="textdecoration:underline;"> <b>' +google_ads[0].line1 + '</b><br></span></a> <span style="color:#d7d8d6;">' + google_ads[0].line2 + '&nbsp;' + google_ads[0].line3 + '<br></span> <span><a style="text-decoration:none" class="adbottom" href="' + google_ads[0].url + '" onmouseout="window.status=\'\'" onmouseover="window.status=\'go to ' + google_ads[0].visible_url + '\';return true">' + google_ads[0].visible_url + '</span></a><br></div>';
}
else
{
s += '<a href=\"' + google_info.feedback_url + '\" style="color:#666666;margin-left:2px;">Ads by Google</a><br>';
//alert("2");;
/*
* For text ads, append each ad to the string.
*/
for(i = 0; i < google_ads.length; i++) {
s += '<div class="gader"><a style="color:#fff;text-decoration:none" href="' +google_ads[i].url + '" onmouseout="window.status=\'\'" onmouseover="window.status=\'go to ' + google_ads[i].visible_url + '\';return true"> <span style="textdecoration: underline"> <b>' +  google_ads[i].line1 + '</b><br></span></a> <span>' + google_ads[i].line2 + ' ' + google_ads[i].line3 + '<br></span> <span><a style="text-decoration:none" class="adbottom" href="' + google_ads[i].url + '" onmouseout="window.status=\'\'" onmouseover="window.status=\'go to ' + google_ads[i].visible_url + '\';return true">' + google_ads[i].visible_url + '</span></a></div>';
//alert(s);
}
if (google_ads[0].bidtype == "CPC") { /* insert this snippet
for each ad call */
google_adnum = google_adnum + google_ads.length;
}
}
document.write(s);
return;
}
google_ad_client = 'pub-8257082012913054'; // substitute your client_id (pub-#)
google_ad_channel = '6387043655';
google_ad_output = 'js';
google_max_num_ads = '3';
google_ad_type = 'text';
google_image_size = '200x300';
//google_adtest='off';
google_feedback = 'off';
google_skip = google_adnum; /* insert this snippet for each ad call */
// -->
</script>
<script language="JavaScript" src="http://pagead2.googlesyndication.com/pagead/show_ads.js">
</script>
