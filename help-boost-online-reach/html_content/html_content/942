    function ShowControls()
    {
      document.getElementById('pnlMortgage').style.display="none";
      document.getElementById('pnlCDs').style.display="none"; 
      document.getElementById('pnlHomeEquity').style.display="none"; 
      document.getElementById('pnlChecking').style.display="none"; 
      document.getElementById('pnlAuto').style.display="none"; 
      document.getElementById('pnlCards').style.display="none"; 
      document.getElementById('pnlInsurance').style.display="none";  
      document.getElementById('pnlStudentLoan').style.display="none"; 
      var displayedpanel = document.getElementById('ddlProductType').value;
      switch(eval(displayedpanel))
      {        
        case 0:
           document.getElementById('pnlMortgage').style.display="block";           
           break;   
        case 2:
            document.getElementById('pnlHomeEquity').style.display="block";  
            break;   
        case 4:
           document.getElementById('pnlCDs').style.display="block"; 
           break;
        case 11:
           document.getElementById('pnlChecking').style.display="block";
           break;
        case 6:
           document.getElementById('pnlAuto').style.display="block";
           break;
        case 7:
           document.getElementById('pnlCards').style.display="block";
           break;
        case 12:
           document.getElementById('pnlInsurance').style.display="block";
           break;
        case 9:
           document.getElementById('pnlStudentLoan').style.display="block";
           break;
        default:            
            break;
      }
    }
       
    function CheckCheckingProduct(sender, args)
    {
        var selectedvar = document.getElementById('ddlCheckingProduct').value;
        if(selectedvar == "-- select --" || selectedvar == "")
        {
            args.IsValid = false;
            return;
        }
    }
    
    function BindCheckingProducts_MultiRateSearchSmall(cat,list)
    {
        list.options.length=0;
        if(cat.value == "Traditional checking")
        {
            list.options.add(new Option('Interest checking','31'));
            list.options.add(new Option('Non-Interest checking','274'));
        }
        else
        {
            list.options.add(new Option('Interest checking','31'));
        }
    }
    

     
           
