        function SetProductList_MultiRateSearchSmall(ProductName, ListControlID, type)
        {   
            var JSON = new Hash();
            JSON.set("gadgetID","9db781c7-c64a-4dc6-9036-896b0f3c77da");
            JSON.set("productName",ProductName);
            JSON.set("productTypes",type);
            var struct = new Object();
            struct.ProductsList = ListControlID;
            AjaxManager.Send(struct,'/ProductService/ProductAJAXJSONEndpoint.ajx',JSON.toJSON(),Success_Function_MultiRateSearchSmall,"get",false);            
        }
 
    
var Success_Function_MultiRateSearchSmall = function GetProducts_success(token,e)
{    
        if(e.Status == 'SUCCESS')
        {        
            var List=$(token.ProductsList);  
            List.options.length = 0;    
            e.Response.Buffer.products[0].evalJSON().each(function(product){
                List.options.add(new Option(product.Name,product.ID));
            });
        }
        else
        {
            ExceptionManager.HandleException("MultiRateSearchSmall","Success_Function_MultiRateSearchSmall","Status",e.AllErrors.toJSON());        
        }
}     
   
