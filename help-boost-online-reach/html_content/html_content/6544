var currImage = 0;
var nextImage = currImage + 1;

function LoadGallery (index, skip) {
        currImage = index;
        nextImage = currImage + 1;
        
        if(currImage >= gal_info.length) { nextImage = 0;   }
        
        if(typeof skip == 'object' || (typeof skip == 'boolean' && !skip)) {
   
            if(typeof YWA != 'undefined') {
                var YWATracker = YWA.getTracker("1000563892833");
                YWATracker.setDocumentGroup("More Sports");
                YWATracker.setUrl('http://www.nydailynews.com/sports/more-sports/sports-gruesome-injuries-gallery-1.53540');
                YWATracker.submit();
            }
            
            if(typeof _gaq != 'undefined') {
                _gaq.push(['_trackPageview', 'http://www.nydailynews.com/sports/more-sports/sports-gruesome-injuries-gallery-1.53540']);
            }
             if(typeof _vrtrack != 'undefined') {
                _vrtrack();
            }
            
            var d = new Image(1, 1);
            d.onerror = d.onload = function () { d.onerror = d.onload = null; };
            d.src = ["//secure-us.imrworldwide.com/cgi-bin/m?ci=us-805104h&cg=0&cc=1&si=", escape(window.location.href), "&rp=", escape(document.referrer), "&ts=compact&rnd=", (new Date()).getTime()].join('');
            
        }
        
        
        if( index <= gal_size-1 && jQuery("#middle_area #nydn_moregal").html() != null) {
            jQuery("#middle_area #nydn_moregal").remove();
            jQuery("#middle_area").html(parseTemplate(jQuery("#photo_area_tpl").html(), {}) );
        }
        
        // do the last frame
        if(index == gal_size) {
            jQuery("#middle_area").html(parseTemplate(jQuery("#moregal_latest_tpl").html(), {}) ); 
                                                            
                                                      jQuery("#prev_next .nav_btn .next").replaceWith('<a class="next" href="http://www.nydailynews.com/sports/more-sports/sports-gruesome-injuries-gallery-1.53540"></a>');
                                                           jQuery("#prev_next .nav_btn .next").replaceWith('<a class="next" href=""></a>');
            jQuery("#pagecount").text( (gal_size+1) + " of " + (gal_size+1));
            jQuery(".img_credits").html('');
            return;
        }
        
        var img = gal_info[index];
        jQuery("#photo_area > p").html(img.caption);
        
        
        if(img.credit != null && img.credit != '') { jQuery(".img_credits").html('Credits: ' + img.credit); }
        
        jQuery.each(jQuery("iframe"), function(i, item) { 
                if(item.id.indexOf('nydn') >= 0)  {
                    var s = item.src;
                    var pos = s.indexOf('&cb=')
                    if(pos >= 0) { s = s.substring(0, pos) + '&cb=' + Math.floor(Math.random() * 100000); }
                    item.src = s;
                }
            }
        );
        
        jQuery.each(jQuery("iframe"), function(i, item) { if(item.id.indexOf('nydn') >= 0)  {var s =item.src; item.src = s + 'a'} });       
        
        jQuery("#pagecount").text((currImage +1) + " of " + (gal_size+1));
        
        jQuery(".nav_btn").pagination(gal_size+1, {
            num_display_entries: 0,
            items_per_page: 1,
            num_edge_entries: 0,
            ellipse_text: "",
            next_text: " ",
            prev_text: " ",
            callback:LoadGallery,
            current_page: currImage,
            prev_extra_class: "prev",
            next_extra_class: "next",
            nav_reverse: true,
            next_show_always: true,
            prev_show_always: true
        });
        
        var jsimg = new Image();
        jQuery(jsimg).load(function () {
            jQuery(this).hide();
            jQuery('#photo_area img').replaceWith(this);
            jQuery('#photo_area img').attr('alt', img.alt).attr('title', img.caption);
            jQuery(this).show();
        }).attr('src', img.src);    
    }     

    LoadGallery(0, true);

