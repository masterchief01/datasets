						function submit911(){
							submitUrl = 'http://earth911.org/search-recycle?';
							whatValue = document.earth911_form.what.options[document.earth911_form.what.selectedIndex].value;
							whereValue = passZip;
							parameters = 'what='+whatValue+'&where='+whereValue+'&max_distance=25&goRecycle=';
							submitUrl = submitUrl + parameters;					
							window.open (submitUrl,"mywindow");
						}
					
