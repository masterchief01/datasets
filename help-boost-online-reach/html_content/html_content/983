<ul id="login_links"> </ul>

<script language="javascript" type="text/javascript" src="/cm/shared/assets/build/3/scripts/hearst.js"></script>
<script language="javascript" type="text/javascript" src="/cm/shared/assets/build/3/scripts/hearst.tempUtils.js"></script>

<script language="javascript" type="text/javascript">

HEARST.userLinks = function() {
	
	var user, name, displayName, HTML, _location, $login;
		/*signOut = HEARST.utils.getQueryVariable('signout'),
		nextURL = HEARST.utils.getQueryVariable('next_url') || document.location.href,*/
	// Redirect to www, if on games.
	if (document.location.href.indexOf('http://games') == -1) {
		if (mag_user.logged_in){
			var	URL = {	
				login: '/login/',
				logout: '/login/?signout=1',
				reg: '/registration/editProfile.html'			
			};
		}
		else {
			var	URL = {	
				login: '/login/',
				logout: '/login/?signout=1',
				reg: '/registration/'			
			};
		}
	} else {
		if (mag_user.logged_in){
			var URL = {			
				login: 'http://www.thedailygreen.com/login/',
				logout: 'http://www.thedailygreen.com/login/?signout=1',
				reg: 'http://www.thedailygreen.com/registration/editProfile.html'			
			};
		} 
		else {
	    	var URL = {			
				login: 'http://www.thedailygreen.com/login/',
				logout: 'http://www.thedailygreen.com/login/?signout=1',
				reg: 'http://www.thedailygreen.com/registration/'
			};
		}
	}
	if (mag_user && mag_user.logged_in) {
		user = mag_user;
		name = user.first_name || user.user_name;
		displayName = (name.length > 12) ? name.substring(0,12) + '&hellip;' : name ;
	}
	
	var hello_text = "Hi";
	var sign_out_text = "Sign Out";
	var join_text = "Join Free";
	var sign_in_text = "Sign In";
	
	
	
	HTML = {
		logout: '\
			<li><a href="#" class="sign_in_link" >' + hello_text + ' ' + displayName + '</a></li>\n\
			<li class="last"><a href="' + URL.logout + '" class="sign_in_link" >' + sign_out_text + '</a></li>\n',
		login: '\
			<li><a href="#" class="sign_in_link" >'+ sign_in_text +'</a></li>\n\
			<li class="last"><a href="' + URL.reg + '" class="sign_in_link" >' + join_text + '</a></li>\n'
	};

	$login = $( (user) ? HTML.logout : HTML.login );		
	_location = (user) ? URL.reg : URL.login ;
		
	$('#login_links').append($login);
	
	$('a.sign_in_link').bind("click", function(){
		// Set next url cookie
		if ( (document.location.href.indexOf('registration') == -1) && (document.location.href.indexOf('login') == -1) ) {
			var _nexturl = document.location.href;
		} else {
			var _nexturl = "http://" + document.domain;
		}
		
		HEARST.tempUtils.writeCookie('nexturl', _nexturl, 1);
		window.location = _location;
		
	});
	
	//return user || { logged_in:false, toString: function() { return '' } };
	
};

HEARST.userLinks();

</script>

<!-- /TMPL xs_login_links -->

