<script type="text/javascript">
            function doSearch() {
                document.searchform.q.value=document.searchform.searchquery.value +
                ' site:www.cbc.cavideo/#/Shows/Shows/In+the+Kitchen+with+Stefano+Faita/ OR site:www.cbc.ca/inthekitchen';
                document.searchform.submit();
            }

            function checkEnter(e){ 
                var characterCode;
                if(e && e.which){
                    e = e;
                    characterCode = e.which;
                } else {
                    e = event;
                    characterCode = e.keyCode;
                }
                if(characterCode == 13){
                    doSearch();
                    return false;
                } else {
                    return true;
                }
            }
    </script>
