    OutbrainStart();
    else {
    var OB_Script = true;
    var str = '<script src="http://widgets.outbrain.com/OutbrainRater.js" type="text/javascript"><\/script>';
    document.write(str);
    var heading = document.getElementById('outbrain_ext_element');
    if(heading != null) {
      heading.style.paddingTop = '0px';
    }
    }
  </script>
</div>
