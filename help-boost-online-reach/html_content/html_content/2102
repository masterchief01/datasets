<script>
(function(){
	var wildCardMode=false;
	if(wildCardMode){
		$("#standings_link_wc").click(function(){
			$("#standings_data, #wildcard_data").toggle();
		}).toggle(
			function(){$(this).text("Show Division")},
			function(){$(this).text("Show Wildcard")}
		).show();		
		$("#standings_link_all").addClass("wc_bottom_links").find("a").text("Expanded Standings");		
		$("#standings_data").show();	
	}else{
		$("#standings_data").show();	
	}
})();
</script>


					
						
						
							

