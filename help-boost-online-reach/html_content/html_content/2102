		<div class="row">
			



 






<script type="text/javascript">
$(document).ready(function () {
	bam.loadSync(bam.homePath + 'bam.datetime.js');
	$.ajax({
		type: "GET",
      	url: "/lookup/json/named.schedule_next_x_series.bam?season=2011&team_id=112&game_date=%272011/07/23%27&results=3&game_type=%27R%27",
		dataType: "json",
		success: function(json) {
			var results = bam.util.getQueryResults(json, "schedule_next_x_series");
			$.each(results, function(i,item){
				var series_nbr = item.series_nbr;				
				var hs_start_time = item.series_start;	// Ex: 2011-03-31T00:00:00
				var hs_end_time = item.series_end;		// Ex: 2011-04-03T00:00:00				
				var hs_start_month = hs_start_time.slice('6','7');
				var hs_end_month = hs_end_time.slice('6','7');				
				var hs_start_formatDate = bam.datetime.parseISODate(hs_start_time);
				hs_start_time = hs_start_formatDate.formatDate("MMMM d");				
				var hs_end_formatDate = bam.datetime.parseISODate(hs_end_time);					
				var time_display = "";						
				if(hs_start_month === hs_end_month){
					hs_end_time = hs_end_formatDate.formatDate("d");
					time_display = hs_start_time+'-'+hs_end_time;	
				}else{					
					hs_end_time = hs_end_formatDate.formatDate("MMMM d");
					time_display = hs_start_time+' - '+hs_end_time;
				}				
				var opponent = item.opponent.toLowerCase();
				var opponent_name = item.opponent_brief;
				if (i<3) {			
					$('<li></li>')
					.html('<img height="56" width="66" src="/images/homepage/team/y2011/logos/'+opponent+'.png" class="png seriesLogo" /><div class="middle_column"><h4>vs '+opponent_name+' <br />'+time_display+'</h4></div><ul class="last_column"><li class="tickets"><a href="/ticketing/singlegame.jsp?c_id=chc#game_series_container&series='+series_nbr+'">Tickets</a></li><li class="promotions"><a href="/schedule/promotions.jsp?c_id=chc&y=2011#m='+hs_end_month+'">Promotions</a></li></ul>')
					.appendTo('#homestand_items');
				}
			});
			$("#homestand_items").find("li:even").addClass("odd");
    	}
   	});
});
</script>

<div id="homestand_container" class="module">
	<h3>Next Homestand</h3>
	<div id="homestand_content">
		<ul id="homestand_items"></ul>
		<ul>
			<li class="more seriesloge"><a href="/schedule/promotions.jsp?c_id=chc">View Promotions Schedule</a></li>
			<li class="more seriesloge"><a href="/ticketing/singlegame.jsp?c_id=chc">View More Games</a></li>
			<li class="more seriesloge"><a href="/schedule/sortable.jsp?c_id=chc&year=2011">View Sortable Schedule</a></li>
		</ul>
	</div>
</div>





		</div>
