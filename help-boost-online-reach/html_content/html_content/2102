<script type="text/javascript">
$("#value-menu").ValueMenu().find('ul');
$("#header_teams").ValueMenu().find('ul');


	$('#value-menu li.first').slice(-4).addClass('last_four');


 $(document).ready(function(){ 
     $("#value-menu").ValueMenu({		
		onShow: function(){						
			$(this).find("img[thumb]").each(function(){		
				var $t = $(this), newSrc = $t.attr("thumb");						
				$t.load(function(){			
					$t.fadeIn();			
				})
				.attr("src", newSrc)
				.removeAttr("thumb");				
			});								
		}
     });			
 }); 
</script>




