	 };
jQuery(document).ready(function() {
    jQuery("#smooth_slider").jcarousel({
        size: 6,
		scroll: 1,
		visible: 1,
		auto:4,
		wrap:"last",
		animation:10,
        initCallback: smooth_slider_initCallback,
		itemVisibleInCallback:{ onAfterAnimation:smooth_slider_itemFirstInCallback }
