    <script type="text/javascript">
var moreButtonState = 'hideMore';
Reuters.utils.lastVideoTab = 72;
Reuters.utils.loadVideoTab = function(videoChannel) {
	if (document.getElementById("ch" + Reuters.utils.lastVideoTab)) {
		document.getElementById("ch" + Reuters.utils.lastVideoTab).className = '';
	}
	document.getElementById("ch" + videoChannel).className = 'current';
	Reuters.utils.lastVideoTab = videoChannel;
    if (moreButtonState == 'showMore') {
	lessVideoEvent(videoChannel);
    }
}
moreVideoEvent = function() {
document.getElementById('videoTabs').height = '1160';
if (Reuters.utils.lastVideoTab != 72) {
document.getElementById('videoTabs').src = '/assets/videoTabLg?videoChannel='+Reuters.utils.lastVideoTab+'';
} else {
document.getElementById('videoTabs').src = '/assets/videoTabLg?videoChannel=72';
}
document.getElementById('moreVideoBtn').className = 'hidden';
}
lessVideoEvent = function(videoChannel) {
document.getElementById('videoTabs').height = '650';
document.getElementById('videoTabs').src = '/assets/videoTabNarrow?videoChannel='+videoChannel+'';
document.getElementById('moreVideoBtn').className = '';
}

</script>

