<script type="text/javascript">
	var priviledge = 0;
	jQuery.ajax({
		type: 'POST',
		url: '/users/conversations/check_opt.php',
		dataType:'json',
		success: function(data){
			loadConversations(data);
		}
	});
	jQuery('.invitation_box_note .btn').live('click',
		function(e){
			jQuery('.invitation_box_note').css('opacity', 0.5); 
			jQuery.ajax({
			type: 'POST',
			data: {'action':jQuery(this).attr('rel')},
			url: '/users/conversations/invite.php',
			dataType:'json',
			success: function(data){
				window.location = window.location.href;
			}
		});
		e.preventDefault();
		return false;
	})
	function loadConversations(priv){
		priviledge = priv;
		switch (priv){
			case 6:
				huff.use("conversations/conversations");
			break;
			case 5:
				huff.use("conversations/conversations");
				break;
			case 0:
			break;
		}
	}
</script>



