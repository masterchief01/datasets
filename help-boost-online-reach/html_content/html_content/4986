<script language="javascript" type="text/javascript">
	document.getElementById("go-widget-type").style.display = 'none';
	document.getElementById("go-widget-varietal").style.display = 'none';
	document.getElementById("go-widget-region").style.display = 'none';
	document.getElementById("go-widget-main").style.display = 'none';
	
	function showgodiv(id){
		if (id=="go-widget-type"){
			document.getElementById("go-widget-type").style.display = 'block';
			document.getElementById("go-widget-varietal").style.display = 'none';
			document.getElementById("go-widget-region").style.display = 'none';
			document.getElementById("go-widget-main").style.display = 'none';
		}
		if (id=="go-widget-varietal"){
			document.getElementById("go-widget-type").style.display = 'none';
			document.getElementById("go-widget-varietal").style.display = 'block';
			document.getElementById("go-widget-region").style.display = 'none';
			document.getElementById("go-widget-main").style.display = 'none';
		}	
		if (id=="go-widget-region"){
			document.getElementById("go-widget-type").style.display = 'none';
			document.getElementById("go-widget-varietal").style.display = 'none';
			document.getElementById("go-widget-region").style.display = 'block';
			document.getElementById("go-widget-main").style.display = 'none';
		}	
		if (id=="go-widget-main"){
			document.getElementById("go-widget-type").style.display = 'none';
			document.getElementById("go-widget-varietal").style.display = 'none';
			document.getElementById("go-widget-region").style.display = 'none';
			document.getElementById("go-widget-main").style.display = 'block';
		}		
		if (id==""){
			document.getElementById("go-widget-type").style.display = 'none';
			document.getElementById("go-widget-varietal").style.display = 'none';
			document.getElementById("go-widget-region").style.display = 'none';
			document.getElementById("go-widget-main").style.display = 'none';
		}
	}
</script>

