function ventananueva(direccion)
							{
							LeftPosition = (screen.width) ? (screen.width-800)/2 : 0;
								TopPosition = (screen.height) ? (screen.height-440)/2 : 0;
							window.open(direccion,'pagina','top=' + TopPosition +',left=' + LeftPosition + ',width=800,height=440,directories=no,location=no,menubar=no,scrollbars=no,status=no,toolbar=no,resizable=no');
							} 
							
function setdiv(tab)
{
  if (tab==1)
  {
    document.getElementById("lomasvisto").style.display = "block";
    document.getElementById("lomasenviado").style.display = "none";
		document.getElementById("lomasbuscado").style.display = "none";
  }
  else
      {
        if (tab==2)
        {
            document.getElementById("lomasvisto").style.display = "none";
            document.getElementById("lomasenviado").style.display = "block";
		        document.getElementById("lomasbuscado").style.display = "none";
        }
        else
            {
              if (tab==3)
              {
                document.getElementById("lomasvisto").style.display = "none";
                document.getElementById("lomasenviado").style.display = "none";
		            document.getElementById("lomasbuscado").style.display = "block";
              }
            }
      }
}

