        function llenadiv(activeTabhome,ConsecutivoId)
        {
          switch(ConsecutivoId)
          {
            case 1:
              document.getElementById("fotografialiga1").innerHTML = divpanel1[0][1];
              document.getElementById("texto1").innerHTML = divpanel1[0][2];
              document.getElementById("fotografialiga2").innerHTML = divpanel1[1][1];
              document.getElementById("texto2").innerHTML = divpanel1[1][2];
              document.getElementById("fotografialiga3").innerHTML = divpanel1[2][1];
              document.getElementById("texto3").innerHTML = divpanel1[2][2];
              break;

            case 2:
             document.getElementById("fotografialiga1").innerHTML = divpanel2[0][1];
             document.getElementById("texto1").innerHTML = divpanel2[0][2];
             document.getElementById("fotografialiga2").innerHTML = divpanel2[1][1];
              document.getElementById("texto2").innerHTML = divpanel2[1][2];
              document.getElementById("fotografialiga3").innerHTML = divpanel2[2][1];
              document.getElementById("texto3").innerHTML = divpanel2[2][2];
              break;

            case 3:
             document.getElementById("fotografialiga1").innerHTML = divpanel3[0][1];
             document.getElementById("texto1").innerHTML = divpanel3[0][2];
             document.getElementById("fotografialiga2").innerHTML = divpanel3[1][1];
              document.getElementById("texto2").innerHTML = divpanel3[1][2];
              document.getElementById("fotografialiga3").innerHTML = divpanel3[2][1];
              document.getElementById("texto3").innerHTML = divpanel3[2][2];
              break;

            case 4:
             document.getElementById("fotografialiga1").innerHTML = divpanel4[0][1];
             document.getElementById("texto1").innerHTML = divpanel4[0][2];
             document.getElementById("fotografialiga2").innerHTML = divpanel4[1][1];
              document.getElementById("texto2").innerHTML = divpanel4[1][2];
              document.getElementById("fotografialiga3").innerHTML = divpanel4[2][1];
              document.getElementById("texto3").innerHTML = divpanel4[2][2];
              break;
            case 5:
             document.getElementById("fotografialiga1").innerHTML = divpanel5[0][1];
             document.getElementById("texto1").innerHTML = divpanel5[0][2];
             document.getElementById("fotografialiga2").innerHTML = divpanel5[1][1];
              document.getElementById("texto2").innerHTML = divpanel5[1][2];
              document.getElementById("fotografialiga3").innerHTML = divpanel5[2][1];
              document.getElementById("texto3").innerHTML = divpanel5[2][2];
              break;
          }
         
        }

		
function showPane1(activeTabhome,ImagenId,ConsecutivoId,zonascount) {
  // make tab active class
  // hide other panes (siblings)
  // make pane visible
  // alert(activeTabhome);
  // alert('consecutivo ' + ConsecutivoId);
  // alert(ImagenId);
  // alert(zonascount);
   var i=1;
  for (i=1;i<=zonascount;i++)
    {
      if (i==ConsecutivoId)
        { 
          if (i==1)  
          {
            activeTabhome.className = "tab-active1";
          }
          else
          {
            if (i==zonascount)  
            {
              activeTabhome.className = "tab-activelast";
            }
            else
            {
              activeTabhome.className = "tab-active";
            }
          }   
          var imagenact= document.getElementById("img"+i+"home").src;
          if (imagenact.indexOf("_off")>-1)
            {
              //alert(imagenact);
              var nuevaimagen= imagenact.substring(0,imagenact.indexOf("_off")); 
              //alert(nuevaimagen);
              document.getElementById("img"+i+"home").src=nuevaimagen + '_on.gif'; 
            } 
          llenadiv(activeTabhome,ConsecutivoId);
        }
    }     
    
		var containerhome = document.getElementById("container1home");
		var tabshome = containerhome.getElementsByTagName("ul")[0];
		var tabListhome = tabshome.getElementsByTagName("a")
		for (var x=1; x<=tabListhome.length; x++ ) 
			{ 
				var tabhome = tabListhome[x-1];
				if (tabhome != activeTabhome)
        {
          var imagenact= document.getElementById("img"+x+"home").src;
          if (imagenact.indexOf("_on")>-1)
          {
            //alert(imagenact);
            var nuevaimagen= imagenact.substring(0,imagenact.indexOf("_on")); 
            //alert(nuevaimagen);
            document.getElementById("img"+x+"home").src=nuevaimagen + '_off.gif';
          }
          
          if (x==zonascount)
          {
          tabhome.className = "tab-disabledlast";
          }
          else
          {
            if (x==1)
            {
              tabhome.className = "tab-disabled1";
            }
            else
              {
              tabhome.className = "tab-disabled";
              }
          }
        }
		  }				
  return false;    
}


