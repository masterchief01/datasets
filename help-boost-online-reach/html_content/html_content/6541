<script language="javascript">	
function verArchivoTuEspacio(idtipo,idregistro, strnombre,modo,autor,titulo,descripcion,inc,oplaza)
{
	if (idtipo==3) // Para los blogs
	{
		window.open("http://www.elnorte.com/libre/offlines/tuespacio/visita.asp?intIdTipo="+idtipo+"&idRegistro="+idregistro+"&strnombre="+strnombre+"&idIdplaza=2&Origenplaza="+oplaza+"&modo="+modo+"&autor="+autor+"&strtitulo="+titulo+"&strdescripcion="+descripcion+"&inc="+inc, "TuEspacioFoto","width=700,height=400");
	}
	else
	{
		window.open("http://www.elnorte.com/offlines/tuespacio/visita.asp?intIdTipo="+idtipo+"&idRegistro="+idregistro+"&strnombre="+strnombre+"&idIdplaza=2&Origenplaza="+oplaza+"&modo="+modo+"&autor="+autor+"&strtitulo="+titulo+"&strdescripcion="+descripcion+"&inc="+inc, "TuEspacioFoto","width=700,height=400");
	}
}
