		function cambiac()
		{
		
		document.images['ImgCart1'].src = arrayimgcart[thisPicfac];
		document.images['ImgCart2'].src = arrayimgcart[thisPicfacdos];
		document.images['ImgCart3'].src = arrayimgcart[thisPicfactres];
		
		}
		
		function verliga(parametro)
		{window.parent.location.href = arrayimgligacart[parametro];}
		
		function DateAdd (fecha,dias) 
		{
			var buffer = Date.parse(fecha);
			dias *= 24; //dias
			dias *= 60; //minutos
			dias *= 60; //segundos
			dias *= 1000; //milisegundos
			fecha = new Date(buffer+dias);
			buffer = null;
			return fecha;
			}
		function fechaArreglada (fecha,periocidad) 
		{
			var fechaAux = new Date();
			var weekdayAux = fecha.getDay();
			var diasAtras = 0;
			fechaEncontrada = false;
			do {
			if (parseInt(Math.pow(2,weekdayAux)&periocidad) > 0) {
			fechaEncontrada = true;
			} else {
			diasAtras++;
			weekdayAux-=1;
			weekdayAux%=7;
			if(weekdayAux < 0)weekdayAux+=7;
    }
    }
    while(!fechaEncontrada)
    fechaAux=DateAdd(fecha,-diasAtras);
    return fechaAux;
    }

    cambiac();

    function switchColorIni()
    {
    document.getElementById('celda1').bgColor = '#e6ddcf';
    document.getElementById('celda1').style.color='#ffffff';

    if (document.getElementById)
    {
    document.getElementById("TituloArtEdt").innerHTML = '<b>' + arraytituloartedt[0] + '</b>';
					document.getElementById("AticoArtEdt").innerHTML = '<img src="libre/online07/imggc/aztec.gif" width="19" height="13" align="absmiddle"/>' + '<a href="' + arrayligaartedt[0] + '">' + arrayaticoartedt[0] + '</a>';
				}

			else if (document.all)//Explorer 5 o menos
				{
					document.all("TituloArtEdt").innerHTML = '<b>' + arraytituloartedt[0] + '</b>';
					document.getElementById("AticoArtEdt").innerHTML = '<img src="libre/online07/imggc/aztec.gif" width="19" height="13" align="absmiddle"/>' + '<a href="' + arrayligaartedt[0] + '">' + arrayaticoartedt[0] + '</a>';
				}			
		}
		
		switchColorIni()
		
	--></script><map name="Ligas"><area coords="4,4,70,20" href="libre/online07/editoriales/nacional/default.shtm"><area coords="4,23,80,42" href="editoriales/nacional/626/1251010"><area coords="80,4,350,42" href="editoriales/nacional/626/1251010"></map></td></tr><tr><td bgcolor="ffffff"><img alt="" src="libre/online07/imggc/pix.gif" width="1" height="10"></td></tr></table><div class="zonaCUATRO"></div><table width="355" cellspacing="0" cellpadding="0" border="0" bordercolor="red" bgcolor="cccccc"><tr><td valign="top">    <!--TUESPACIO-->
