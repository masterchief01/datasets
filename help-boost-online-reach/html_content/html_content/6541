            function escogerliga(ind,liga)
            {   strc = new String(liga);
            if(strc.indexOf("?")>-1)
            {caract="&";}
            else
            {caract="?";}
            if(plazaConsulta(liga)==1)
            {   str = new String(document.location.href);
            if(str.search("palabra.com")>1)
            {liga = liga + caract + "plazaconsulta=palabra";}
            else{
            if(str.search("reforma.com")>1)
            {liga = liga + caract + "plazaconsulta=reforma";}
            else{
            if(str.search("mural.com")>1)
            {liga = liga + caract + "plazaconsulta=mural";}
            else{
            if(str.search("elnorte.com")>1)
            {liga = liga + caract + "plazaconsulta=elnorte";}
            }
            }
            }
            }

            if(ind==1)
            {herramienta(liga);}
            if(ind==2)
            {herramientascroll(liga);}
            if(ind==0)
            {window.parent.location.href=liga;}
            }

            function plazaConsulta(lp)
            {
            str = new String(lp);
            if(str.search("plazaconsulta=elnorte")>1)
            {return 0;}
            if(str.search("plazaconsulta=reforma")>1)
            {return 0;}
            if(str.search("plazaconsulta=mural")>1)
            {return 0;}
            if(str.search("plazaconsulta=palabra")>1)
            {return 0;}
            return 1;
            }
