                  if (parseInt(Total) > 9)
                    document.getElementById('art1251122comentario').innerHTML = '<a href="javascript:comentariosArt(&quot;http://www.elnorte.com/general/Comentario/TodosComentarios.asp?DestinoId=1251122&ComentarioHerrId=503191&BasePath=../../&rutaArt=nacional/articulo/626/1251122/&tituloArt=Cae director de Polic&iacute;a; lo delata narco&PlazaConsulta=elnorte&correoSecc=nacional.com@elnorte.com&nombreSecc=Secci&oacute;n Nacional&quot;)"><img alt="" src="libre/online07/imggc/cuadro_comentarios.gif" height="15" border="0" align="absmiddle" /><span class="arherr11"><b>[' + Total + ']</b></span></a>';
                }
            }
            else
            {
              //alert("There was a problem retrieving the XML data:\n" + xml1251122.statusText);
              document.getElementById('art1251122comentario').innerHTML = '';
            }
          }
        }
