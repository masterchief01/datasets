<SCRIPT LANGUAGE=javascript>
	function offline_herramientasudoku(direccion)
	{
		LeftPosition = (screen.width) ? (screen.width-800)/2 : 0; 
		TopPosition = (screen.height) ? (screen.height-440)/2 : 0;
		window.open(direccion,'pagina','top=' + TopPosition + ',left=' + LeftPosition + ',directories=no,location=no,menubar=no,scrollbars=no,status=no,toolbar=no');
	}
	
	function offline_herramienta(direccion)
	{
		LeftPosition = (screen.width) ? (screen.width-800)/2 : 0; 
		TopPosition = (screen.height) ? (screen.height-440)/2 : 0;
		window.open(direccion,'pagina','top=' + TopPosition + ',left=' + LeftPosition + ',width=800,height=440,directories=no,location=no,menubar=no,scrollbars=no,status=no,toolbar=no');
	}
</SCRIPT>
