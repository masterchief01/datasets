        function Blogs(titulo, autor, link){
            this.titulo = titulo;this.autor = autor;this.link = link;return this;
        }
        function randArray ( myArray ) {
          var i = myArray.length;
          if ( i == 0 ) return false;
          while ( --i ) {
             var j = Math.floor( Math.random() * ( i + 1 ) );
             var tempi = myArray[i];var tempj = myArray[j];
             myArray[i] = tempj;myArray[j] = tempi;
           }
        }
        /*INFORMACIÓN SOBRE LOS BLOGS
        1. Se crea un objeto llamado Blogs con 3 propiedades: titulo, autor y link.
        2. Si se desea crear un nuevo Blog se agrega aqui con el siguiente índice
           y la definición con el orden indicado en el punto 1, si el autor es nulo
           hay q respetar de cualquier manera la posición en la definición, dejandolo
           como "".
        3. Si se desea borrar un Blog, asegurarse de reordenar los indices de arrBlogs ya
           que siempre deben quedar consecutivos, empezando por 0.*/
        var arrBlogs = new Array();
        arrBlogs[0] = new Blogs("Alt-Neg","Tendencias y an&aacute;lisis sobre <br>las empresas en M&eacute;xico.<br> Por Ver&oacute;nica Baz","http://www.reforma.com/blogs/altneg/");
        arrBlogs[1] = new Blogs("El mensaje en la botella","Un cat&aacute;logo de asombros <br>y curiosidades. <br>Por Juan E. Pardinas","http://www.reforma.com/blogs/juanpardinas/");
        arrBlogs[2] = new Blogs("AlConsumidor","La voz del consumidor ya tiene <br>su espacio en la Web. Te asistimos,<br> te informamos, te facultamos.","http://www.reforma.com/blogs/alconsumidor/");
        arrBlogs[3] = new Blogs("El blog de Jes&uacute;s Silva-Herzog M&aacute;rquez","","http://www.elnorte.com/blogs/silvaherzog/");
        arrBlogs[4] = new Blogs("Las Ni&ntilde;as Bien... <br>&iquest;abuelas bien?","Descubre la respuesta. <br>Por Guadalupe Loaeza","http://www.reforma.com/blogs/guadalupeloaeza/");
        arrBlogs[5] = new Blogs("La comedia pol&iacute;tica","Para reir de los pol&iacute;ticos <br>que nos hacen llorar. <br>Por Dan T.","http://www.reforma.com/blogs/dant/");
		arrBlogs[6] = new Blogs("Legal Alien","Cr&oacute;nicas de un mexicano en Miami","http://www.elnorte.com/blogs/legalalien/");
		arrBlogs[7] = new Blogs("M&aacute;quina de sue&ntilde;os","A pesar de todo...piensa positivo","http://www.reforma.com/blogs/maquinasuenos/");

        randArray(arrBlogs);
  </script>
