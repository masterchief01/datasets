    var tablet="null";
    var orientation = window.orientation;
    var detecttabletOrientation = function () {
      if (orientation == 0 || orientation == 180) {
        tablet = "portrait";
      } else if (orientation == 90 || orientation == -90) {
        tablet = "landscape";
      }
    }
  window.onorientationchange = detecttabletOrientation;
