<script language="JavaScript">
$("#block-block-xx").removeClass("js-off").addClass("js-on");var d=$(".slide").length;var e=$(".morefeat").width();var c=d*$(".slide").width();var a=$(".slide:first-child");$(".totalart").text(d);$(".slider").width(c+"px");function b_hpdyn(f){$("#lead .featimage").hide();currart=f.attr("id").replace(/D/g,"");
title2=f.find(".arttitle").html();
artlink=f.find(".arttitle").attr("href");imgsrc=f.find(".artimage").attr("src");imgtitle=f.find(".artimage").attr("title");imgalt=f.find(".artimage").attr("title");artclass=f.find(".lead h1").attr("class");dek=f.find(".artdek").html();section=f.find(".artsection").text();sectionlink=f.find(".artsection").attr("href");related2=f.find(".related").html();
$("#leadtitle h1 a").attr("href",artlink).html(title2);
if(artclass){$("#lead h1").attr("class",artclass)}else{$("#lead h1").attr("class","")}$("#leaddek a").attr("href",artlink).html(dek);if(sectionlink){$("#leadsection a").attr("href",sectionlink).text(section);$("#leadsection").show()}else{$("#leadsection").hide()}$(".featimage a").attr("href",artlink);$("#lead .featimage img").attr("src",imgsrc);$("#lead .featimage img").attr("title",imgtitle);$("#lead .featimage img").attr("alt",imgalt);$("#relatedart").html(related2);$("#relatedart").slideDown("fast");$(".currentart").text(currart);if(Cufon){Cufon.refresh("#lead h1 a")}$("#relatedart li").each(function(h){var g=$(this).position();if((g.top+$(this).height())>($("#featarticles").height()-10)){$(this).hide()}});$("#lead .featimage").show()}$(".slide").hover(function(){var f=$(this);if(f.attr("id").replace(/D/g,"")!=$(".currentart").text()){$("#relatedart").hide();$("#featarticles").animate({opacity:"0"},250,function(){b_hpdyn(f)});$("#featarticles").animate({opacity:"1"},250)}},function(){null});
b_hpdyn(a);$("#lead .featimage").animate({opacity:"1"},250);
if(!$.browser.msie) {
$(".slidenext").click(function(){
	var f=$(".slider").css("margin-left");
	f=parseInt(f);f=f-e;if(-f<c&&!$(".slider").is(":animated")){$(".slider").animate({marginLeft:f+"px"},400);
	$(".slideprev").css("color","#555")}if(-f>=(c-e)){$(this).css("color","#aaa")}return false});
$(".slideprev").click(function(){var f=$(".slider").css("margin-left");f=parseInt(f);
	if(f<0&&!$(".slider").is(":animated")){f=f+e;$(".slider").animate({marginLeft:f+"px"},400);
	$(".slidenext").css("color","#555")}if(f==0){$(this).css("color","#aaa")}
	if(f>0){$(".slider").css("margin-left","0px")}return false});	
}
Cufon.replace("#lead h1 a");
Cufon.now();
$("#lead").show();
</script>
  </div>

  
  </div>
  </div>
