<script language="javascript" type="text/javascript">
    $(document).ready(function () {

        $(".txtbox").focus(function () {
            if ($(this).val() == this.defaultValue) {
                $(this).val("");
            }
        });

        $(".txtbox").blur(function () {
            if ($.trim($(this).val()) == "") {
                $(this).val(this.defaultValue);
            }
        });


        $("input[id$='btnSignUp']").click(function () {
            if ($("input[id$='txtEmail']").val() != 'Enter your email to sign up') {
                $.ajax({
                    type: 'POST',
                    url: '/includes/webmethods/methods.aspx/SubscribeNewsletter',
                    contentType: 'application/json; charset=utf-8',
                    data: '{Email:" ' + $("input[id$='txtEmail']").val() + '", ID: "' + $("input[id$='hidNLID']").val() + '"}',
                    dataType: 'json',
                    success: function (msg) {
                        var successful = msg.d
                        if (successful) {
                            alert("You're subscribed!");
                        }

                    },
                    error: function (XMLHttpRequest, textStatus, errorThrown) {

                        alert(errorThrown);

                    }
                })

                return false;
            }
        });

    });
</script>
