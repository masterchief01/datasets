<script src="/includes/js/jquery.isotope.min.js?wpc=3" type="text/javascript"></script>
<script type="text/javascript">
    $(this).ready(function () {

        $("#printDisplayHorizontal").dialog({
            autoOpen: false,
            zIndex: 11000,
            width: 700,
            height: 700,
            resizable: false,
            modal: true

        });

        $("#printDisplayVertical").dialog({
            autoOpen: false,
            zIndex: 11000,
            width: 800,
            height: 700,
            resizable: false,
            modal: true

        });

        $("#divMap").dialog({
            autoOpen: false,
            zIndex: 11000,
            width: 400,
            height: 400,
            resizable: false,
            modal: true

        });

        $("#divCopyLink").dialog({
            autoOpen: false,
            zIndex: 11000,
            width: 500,
            resizable: false,
            modal: true

        });

        $("#divShare").dialog({
            autoOpen: false,
            zIndex: 11000,
            width: 500,
            resizable: false,
            modal: true,
            close: function (event, ui) { $("span[id$='lblMessage']").text(''); }
        });



    });

    
        
      //Call WebMethod to return images
        GetPostAdImages();

    $(window).load(function () {
        //This sets isotope up to re-arange images to fit
        $('#containerModule').imagesLoaded(function () {
            $('#containerModule').isotope({
            
                masonry: {
                    columnWidth: Math.floor($('#container').width() / Math.floor($('#container').width() / 1))

                }

                
            });

        });
    });

   
    //Show map when map link is clicked
    function ShowMap() {    
        $("#divMap").dialog("open");
        $("#divMap").attr("style", "visibility:visible");
        $("#divMap").parent().appendTo($("form:first"));
        LogTrackingInfo($("input[id$='hfAdid']").val(), 1, document.URL.toString(), 0, 0, 0, 0, 0, 0, 1, 0, 0);
    }

    //Show copy link when link is clicked
    function ShowCopyLink() {
        $("#divCopyLink").dialog("open");
        $("#divCopyLink").attr("style", "visibility:visible");
        $("#divCopyLink").parent().appendTo($("form:first"));

        LogTrackingInfo($("input[id$='hfAdid']").val(), 1, document.URL.toString(), 0, 0, 0, 0, 0, 0, 0, 1, 0);
    }

    //Show share when link is clicked
    function ShowShare() {

        $("#divShare").dialog("open");
        $("#divShare").attr("style", "visibility:visible");
        $("#divShare").parent().appendTo($("form:first"));     

    }

    
    function SendShareEmail() {


        $.ajax({
            type: 'POST',
            url: '/includes/webmethods/methods.aspx/ShareEmail',
            contentType: 'application/json; charset=utf-8',
            data: '{SenderName:" ' + $("input[id$='txtName']").val() + '", ToEmail:"' + $("input[id$='txtToEmail']").val() + '", FromEmail:"' + $("input[id$='txtFromEmail']").val() + '", EmailMessage:"' + $("textarea[id$='txtMessage']").val() + '", ShareLink:"' + $("span[id$='lblCopyLink']").text() + '"}',
            dataType: 'json',
            success: function (msg) {
                var successful = msg.d

                if (successful) {
                    $("span[id$='lblMessage']").attr("style", "color:#004000");
                    $("span[id$='lblMessage']").text("Sent Successfully!");
                    $("input[id$='txtName']").val('');
                    $("input[id$='txtToEmail']").val('');
                    $("input[id$='txtFromEmail']").val('');
                    $("textarea[id$='txtMessage']").val('');

                }
                else {

                    $("span[id$='lblMessage']").attr("style", "color:#FF0000");
                    $("span[id$='lblMessage']").text("Sent Failed!");
                }

                LogTrackingInfo($("input[id$='hfAdid']").val(), 1, document.URL.toString(), 0, 0, 0, 0, 0, 1, 0, 0, 0);
            },
            error: function (XMLHttpRequest, textStatus, errorThrown) {
                alert(errorThrown);

            }
        })

       return false;
    }


    //Call webmethod to get ad information by adid
    function GetPostAd(Adid) {


        $("input[id$='hfAdid']").val(Adid);

        $.ajax({
            type: 'POST',
            url: '/includes/webmethods/methods.aspx/GetPostAd',
            contentType: 'application/json; charset=utf-8',
            data: JSON.stringify({
                Adid: Adid
            }),
            dataType: 'json',
            success: function (msg) {
                var AdInfo = msg.d

                //This will determine if the ad is horizontal or vertical based on the width and height
                var img = new Image();
                img.onload = function () {

                    if (this.height - this.width < 250) {

                        $("span[id$='lblHeader']").text(AdInfo.Name);
                        $("span[id$='lblAddress']").text(AdInfo.Address);
                        $("span[id$='lblCityStateZip']").text(AdInfo.Name);
                        $("span[id$='lblUrl']").text(AdInfo.URL);
                        $("span[id$='lblPhone']").text(AdInfo.Phone);
                        $("span[id$='lblDescription']").text(AdInfo.Description);
                        $("span[id$='lblCopyLink']").text('http://' + location.host + '/site/printpost/default.aspx?id=' + Adid);
                        $("img[id$='imgWeb']").attr('src', AdInfo.OriginalImage);
                        $("a[id$='hrefImage']").attr('href', AdInfo.OriginalImage);
                        $("a[id$='hrefPDF']").attr('href', AdInfo.PDF);
                        $("a[id$='hrefUrl']").attr('href', AdInfo.URL);
                        $("a[id$='hrefFacebook']").attr('href', AdInfo.ShareFacebook);
                        $("a[id$='hrefTwitter']").attr('href', AdInfo.ShareTwitter);
                        $("iframe[id$='iFrameMap']").attr('src', AdInfo.MapLink);
                        $("#printDisplayHorizontal").dialog("open");
                        $("#printDisplayHorizontal").attr("style", "visibility:visible");
                        $("#printDisplayHorizontal").parent().appendTo($("form:first"));
                    }
                    else {

                        $("span[id$='lblVHeader']").text(AdInfo.Name);
                        $("span[id$='lblvAddress']").text(AdInfo.Address);
                        $("span[id$='lblvCityStateZip']").text(AdInfo.Name);
                        $("span[id$='lblvUrl']").text(AdInfo.URL);
                        $("span[id$='lblvPhone']").text(AdInfo.Phone);
                        $("div[id$='vDescription']").text(AdInfo.Description);
                        $("span[id$='lblCopyLink']").text('http://' + location.host + '/site/printpost/default.aspx?id=' + Adid);
                        $("img[id$='imgvWeb']").attr('src', AdInfo.OriginalImage);
                        $("a[id$='hrefvImage']").attr('href', AdInfo.OriginalImage);
                        $("a[id$='hrefFacebook']").attr('href', AdInfo.ShareFacebook);
                        $("a[id$='hrefTwitter']").attr('href', AdInfo.ShareTwitter);
                        $("a[id$='hrefvUrl']").attr('href', AdInfo.URL);
                        $("a[id$='hrefvPDF']").attr('href', AdInfo.PDF);
                        $("iframe[id$='iFrameMap']").attr('src', AdInfo.MapLink);
                        $("#printDisplayVertical").dialog("open");                        
                        $("#printDisplayVertical").attr("style", "visibility:visible");
                        $("#printDisplayVertical").parent().appendTo($("form:first"));
                    }

                }
                img.src = AdInfo.OriginalImage;

                LogTrackingInfo(Adid, 1, document.URL.toString(), 1, 0, 0, 0, 0, 0, 0, 0, 0);
            },
            error: function (XMLHttpRequest, textStatus, errorThrown) {
                alert(errorThrown);

            }
        })

        return false;

    }


    function LogLargerImage() {

        LogTrackingInfo($("input[id$='hfAdid']").val(), 1, document.URL.toString(), 0, 1, 0, 0, 0, 0, 0, 0, 0);

    }

    function LogPrintImage() {

        LogTrackingInfo($("input[id$='hfAdid']").val(), 1, document.URL.toString(), 0, 0, 0, 0, 0, 0, 0, 0, 1);

    }

    function LogURLClick() {

        LogTrackingInfo($("input[id$='hfAdid']").val(), 1, document.URL.toString(), 0, 0, 1, 0, 0, 0, 0, 0, 0);

    }

    function LogTwitterClick() {

        LogTrackingInfo($("input[id$='hfAdid']").val(), 1, document.URL.toString(), 0, 0, 0, 1, 0, 0, 0, 0, 0);

    }

    function LogFaceBookClick() {

        LogTrackingInfo($("input[id$='hfAdid']").val(), 1, document.URL.toString(), 0, 0, 0, 0, 1, 0, 0, 0, 0);

    }


    //Call webmethod to get all images for display
    function GetPostAdImages() {
        $.ajax({
            type: 'POST',
            url: '/includes/webmethods/methods.aspx/GetAdPostImages',
            contentType: 'application/json; charset=utf-8',
            data: JSON.stringify({
                ImageCount: '6' //Number of Ads to return
            }),
            dataType: 'json',
            success: function (msg) {
                var AdInfo = msg.d              
                 $.each(AdInfo, function (i, val) {                   
                  
                   //This will create the image by the path and add it to the container div area
                   var image =  $(document.createElement("img"))
                    image.attr({ src: val.ThumbImage, style: 'cursor:pointer' })
                //image.appendTo("#containerModule")

                    $('#containerModule').append(image)
                                   .isotope('reloadItems').isotope({ sortBy: 'original-order' })
                                  
                      image.click(function () {
                        //This will add the click event to each image ad created
                        GetPostAd(val.Adid);
                    })
                    
                });
                               
            },
            error: function (XMLHttpRequest, textStatus, errorThrown) {
                alert(errorThrown);

            }
        })

        return false;

        
    }

    function LogTrackingInfo(Adid, SourceTypeId, LocationPage, DetailViews, ClickedLargerView, ClickedUrl, ClickedTwitter, ClickedFaceBook, SharedLink, MapViewed, CopyLinkViewed, Printed) {

        $.ajax({
            type: 'POST',
            url: '/includes/webmethods/methods.aspx/LogAdpostDetails',
            contentType: 'application/json; charset=utf-8',
            data: '{Adid:" ' + Adid +
                       '", SourceTypeID:"' + SourceTypeId +
                       '", LocationPage:"' + LocationPage +
                       '", DetailViews:"' + DetailViews +
                       '", ClickedLargerView:"' + ClickedLargerView +
                       '", ClickedUrl:"' + ClickedUrl +
                       '", ClickedTwitter:"' + ClickedTwitter +
                       '", ClickedFaceBook:"' + ClickedFaceBook +
                       '", SharedLink:"' + SharedLink +
                       '", MapViewed:"' + MapViewed +
                       '", CopyLinkViewed:"' + CopyLinkViewed +
                       '", Printed:"' + Printed + '"}',
            dataType: 'json',
            success: function (msg) {


            },
            error: function (XMLHttpRequest, textStatus, errorThrown) {
                alert(errorThrown);

            }
        })

        return false;

    }
   
</script>

