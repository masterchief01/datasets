<script  type="text/javascript" language="javascript">

    $(document).ready(function () {
        $("#divMostcommented").hide();
        $("#divMostViewed").show();
        $("a#bottomlink").attr("href", "../news/popularstoriestoplist.aspx?actiontype=viewed");

        $("#MV").click(function () {
            $("#MV").removeClass('NormalTab').addClass('ActiveTab');
            $("#MC").removeClass('ActiveTab').addClass('NormalTab');
            $("#divMostcommented").hide();
            $("#divMostViewed").show();
            $("a#bottomlink").attr("href", "../news/popularstoriestoplist.aspx?actiontype=viewed");
        })
        $("#MC").click(function () {
            $("#MC").removeClass('NormalTab').addClass('ActiveTab');
            $("#MV").removeClass('ActiveTab').addClass('NormalTab');
            $("#divMostcommented").show();
            $("#divMostViewed").hide();
            $("a#bottomlink").attr("href", "../news/popularstoriestoplist.aspx?actiontype=commented");
        })
    })
</script>

