function kalleanka(element)
{
        if ($(element).css("display") == "block")
        {
                $(element).slideUp("fast"); } else {
                $(".box").slideUp("fast");
                $(element).slideToggle("fast"); }
        }

        $(document).ready(function(){
        var match = /added$/.test(window.location.href);
        if (match == true)
        {
                kalleanka('#added');
        }
});
  

function com0()
{
        $.cookie("com", "0", { expires: 7 });
        comcom();
        return false;
}

function com1()
{
        $.cookie("com", "1", { expires: 7 });
        comcom();
        return false;
}

function comcom()
{
        if( $.cookie("com") == "1" )
        {
                $('.com1').css("display",'inline');
                $('.com0').css("display",'none');
        }
        else
        {
                $('.com0').css("display",'inline');
                $('.com1').css("display",'none');
        }
}

