        window.registered = cookie_exists('auth_token');
      }
      var loadIframe = function() {
        var elem = document.getElementById('med_rectangle');
        if (elem && elem.contentWindow) {
          elem.contentWindow.location.replace('http://ad.doubleclick.net/adi/caring.health/vision;pos=mr;kw=stroke;kw=stroke-symptoms;kw=vision-problems;kw=cataracts;kw=glaucoma;kw=vision;kw=macular-degeneration;kw=general-decline;kw=migraines;i=article;tile=5;sz=300x250;dc_ref=' + encodeURIComponent(location.href) + ';ord=' + ord + ';reg=' + registered + '?');
        }
        else if (elem.contentDocument) {
          elem.contentDocument.location.replace('http://ad.doubleclick.net/adi/caring.health/vision;pos=mr;kw=stroke;kw=stroke-symptoms;kw=vision-problems;kw=cataracts;kw=glaucoma;kw=vision;kw=macular-degeneration;kw=general-decline;kw=migraines;i=article;tile=5;sz=300x250;dc_ref=' + encodeURIComponent(location.href) + ';ord=' + ord + ';reg=' + registered + '?');
        }
      };
      if (true) { // load_later
        PAGESETUP.addControl(loadIframe, 'low');
      }
      else { // load now
        loadIframe();
      }
    })(window);
  //]]>
</script>

<noscript>
<a href="http://ad.doubleclick.net/jump/caring.health/vision;pos=mr;kw=stroke;kw=stroke-symptoms;kw=vision-problems;kw=cataracts;kw=glaucoma;kw=vision;kw=macular-degeneration;kw=general-decline;kw=migraines;i=article;tile=5;sz=300x250;dc_ref=' + encodeURIComponent(location.href) + ';ord=' + ord + ';reg=' + registered + '?" target='_blank'>
