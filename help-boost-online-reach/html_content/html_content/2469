    <script type="text/javascript">
         window._ghearst_vars = (function() {
        var
            metaKeyWords = (function(){
                var
                    i                  = 0,
                    metaKeywordContent = '',
                    nameAttribute      = null,
                    metaCollection     = document.getElementsByTagName('meta');

                for (i = 0; i < metaCollection.length; i++) {
                    nameAttribute = metaCollection[i].name.search(/keywords/);
                    if (nameAttribute!= -1) {
                        metaKeywordContent = metaCollection[i].content;
                        metaKeywordContent = metaKeywordContent.replace(/ /g,''); // to replace space
                    }
                }
                return encodeURIComponent(metaKeywordContent);
            })();

        if( (navigator.userAgent.match(/iPad/i) !== null) && (CN.cookie.get('ipadTakeover') !== 'true') ){
            return {
                'ams_ads_script_src': 'http://subscribe.newyorker.com/ams/page-ads.js?ad_category_prefix=online&browser_path=%2Fonline%2Fblogs%2Fshouts%2F2012%2F12%2Fthe-hundred-best-lists-of-all-time.html&cat_prefixes=%2Conline%2Cblogs%2Cshouts%2C2012%2C12%2Cthe-hundred-best-lists-of-all-time.html&keywords='+metaKeyWords+'&position_list=AMS_NYR_POPUP_FLOATINGAD%2CAMS_NYR_GLOBAL_NAVBAR_ROLLOVER%2CAMS_NYR_GLOBAL_RIGHTRAIL_A%2CAMS_NYR_GLOBAL_EMBEDDEDFORM%2CAMS_NYR_GLOBAL_RIGHTRAIL_C%2CAMS_NYR_GLOBAL_RIGHTRAIL_B%2CAMS_NYR_GLOBAL_HEADER%2CAMS_NYR_GLOBAL_NAVBAR%2CAMS_NYR_GLOBAL_HEADERCALLOUT%2CAMS_NYR_CONTENTPAGE_TEXTOFFER,AMS_NYR_GLOBAL_IPAD_TAKEOVER&site_prefix=newyorker&subdomain=www&url_name=the-hundred-best-lists-of-all-time'
            }
        } else {
            return {
                'ams_ads_script_src': 'http://subscribe.newyorker.com/ams/page-ads.js?ad_category_prefix=online&browser_path=%2Fonline%2Fblogs%2Fshouts%2F2012%2F12%2Fthe-hundred-best-lists-of-all-time.html&cat_prefixes=%2Conline%2Cblogs%2Cshouts%2C2012%2C12%2Cthe-hundred-best-lists-of-all-time.html&keywords='+metaKeyWords+'&position_list=AMS_NYR_POPUP_FLOATINGAD%2CAMS_NYR_GLOBAL_NAVBAR_ROLLOVER%2CAMS_NYR_GLOBAL_RIGHTRAIL_A%2CAMS_NYR_GLOBAL_EMBEDDEDFORM%2CAMS_NYR_GLOBAL_RIGHTRAIL_C%2CAMS_NYR_GLOBAL_RIGHTRAIL_B%2CAMS_NYR_GLOBAL_HEADER%2CAMS_NYR_GLOBAL_NAVBAR%2CAMS_NYR_GLOBAL_HEADERCALLOUT%2CAMS_NYR_CONTENTPAGE_TEXTOFFER&site_prefix=newyorker&subdomain=www&url_name=the-hundred-best-lists-of-all-time'
            }
        };
    })();

    jQuery(document).ready(function(){
        CN.hearstQue.register(function(pageAds){
            var div;
            if ((typeof pageAds !== 'undefined') && (typeof pageAds.AMS_NYR_GLOBAL_IPAD_TAKEOVER !== 'undefined') && (CN.url.params('nojoy') != 1) ) {
                div = jQuery('<div id="ipad-takeover-wrapper"></div>');
                jQuery('body').append(div);
                div.html(pageAds.AMS_NYR_GLOBAL_IPAD_TAKEOVER.replace(/document.write\(.*\)/gi,"/*filtered by amg-magnet:document.write(...)*/"));
            }
        });
    });
         (function($) {
             var show = function() {
                 $('.ecom-placement').each(function() {
                     $(this).css('visibility', 'visible');
                 });
             };
             
             $LAB.script(_ghearst_vars.ams_ads_script_src)
                 .wait(function() {
                     
                     CN.hearstQue.run(window.pageAds);
                 });
             
             setTimeout(show, 10 * 1000);
         }(jQuery));
    </script>





    











































        
        
    


        
            
            
            
    


    
    
        



















 































    
