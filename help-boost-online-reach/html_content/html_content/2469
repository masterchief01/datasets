    <!-- START: TNY 3inch stats - SocialSharing - NEWYORKER-1555 -->
    <script type="text/javascript">
        /**
         * Prevent Omniture from firing until SocialShare has a chance to load
         * which is located in scripts bottom.
         */
         (function() {
            var oldTracking = CN.stats.omniture.doPageTracking;
            CN.stats.omniture.doPageTracking = function(s) {
                if (s == 'track') {
                    CN.stats.omniture.doPageTracking = oldTracking;
                    CN.stats.omniture.doPageTracking();
                }
            };
        }());
    </script>
    <!-- END: TNY 3inch stats - SocialSharing - NEWYORKER-1555 -->
    





















        
        
            
        
            
        
            
        
            
        
            
        
    


        
        
        
                
            
    
