<script type="text/javascript">
(function() {
	if( HPBrowser.isIE6() ) return false;

	if( $('featured_content') ) {
		// top content
		$('featured_content').onmouseover = function(event){
		
			event = event || window.event;
			
			//IE uses srcElement as the target
			var target = event.target || event.srcElement;    
			
			if( "IMG" != target.tagName )
				return false;
			
			QV.onIMGMouseOver(target.tagName, target);
		};
	}
	if($('sidebar_right')) {
		// right column
		$('sidebar_right').onmouseover = function(event) {
			
			event = event || window.event;
			
			//IE uses srcElement as the target
			var target = event.target || event.srcElement;    
			
			if( "IMG" != target.tagName )
				return false;
				
			QV.onIMGMouseOver(target.tagName, target);
		};
	}	
	

})();


</script>


