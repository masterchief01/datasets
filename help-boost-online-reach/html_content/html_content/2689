<script type="text/javascript">


				

function performLocalPagesSearch()
{
	var objLocation = $('#concertinaLocalSearchLocation');
    var objName = $('#concertinaLocalSearchCompany');
    var objCategory = $('#concertinaLocalSearchCategory');
    var strUrl = "http://www.localpagestoday.co.uk/results.aspx?Radius=10";
	//alert();

	if ((objLocation != null) && (objLocation.val() != null) && (objLocation.val().length > 0)) {
        if ((objName != null) && (objName.val() != null) && (objName.val().length > 0)) {
            strUrl = strUrl + "&BusinessName=" + escape(objName.val());
        }
        if ((objCategory != null) && (objCategory.val() != null) && (objCategory.val().length > 0)) {
            strUrl = strUrl + "&Category=" + escape(objCategory.val());
        }
        strUrl = strUrl + "&Location=" + escape(objLocation.val());
		strUrl = strUrl + "&SiteKey=" + siteKey;

		window.open(strUrl);
    } 
}

function performJobSearch()
{
    var strUrl = "http://www.jobstoday.co.uk/cgi-bin/advsearch?search_type=quick&location_within=10&reqd_salary_low=ANY&reqd_salary_hifh=ANY&daysback=7&location_include=Sheffield,South Yorkshire&fp_skill_include=";
	
    window.open(strUrl + $('#jobSearchFrm #keywords').val() + "&SiteKey=" + siteKey);
}

function performCarSearch()
{
    var manufactureId = $('#carSearchFrm #make option:selected').val();
    var maufactureName = $('#carSearchFrm #make option:selected').text();
    var modelId = $('#carSearchFrm #model option:selected').val();
    var modelName = $('#carSearchFrm #model option:selected').text();
    if(modelName == "Any Model")
    {
        modelName = "";
    }
    var minPrice = $('#carSearchFrm #priceFrom option:selected').val();
    var maxPrice = $('#carSearchFrm #priceTo option:selected').val();
    
    var strUrl = "http://www.motorstoday.co.uk/results.aspx?location=Sheffield&keywords=&manufacturerid=" + manufactureId + "&manufacturername=" + maufactureName + "&modelid=" + modelId + "&modelname=" + modelName + "&minage=-1&maxage=-1&minprice=" + minPrice + "&maxprice=" + maxPrice + "&maxmileage=-1&radius=10&type=3&sortorder=2&page=1&searchtype=1&derivitive=&fueltype=-1&bodytype=-1&doors=-1&colour=-1&enginesize=-1&transmission=-1";
	strUrl = strUrl + "&SiteKey=" + siteKey;
	
    window.open(strUrl);
}

function performPropertySearch()
{
    var orderType = $('#propertySearchFrm input[name=buyRent]:checked').val();
    var maxPrice = (orderType == 0)
                    ? $('#propertySearchFrm #priceToBuy option:selected').val()
                    : $('#propertySearchFrm #priceToRent option:selected').val();
    var minPrice = (orderType == 0)
                    ? $('#propertySearchFrm #priceFromBuy option:selected').val()
                    : $('#propertySearchFrm #priceFromRent option:selected').val();
    var bedrooms = $('#propertySearchFrm #bedrooms option:selected').val();
    
    var strUrl = "http://www.propertytoday.co.uk/propertyresults.aspx?Town=Sheffield&MaxPrice=" + maxPrice + "&MinPrice=" + minPrice + "&Radius=5&OrderType=1&Bedrooms=" + bedrooms;
    if(orderType == 1)
    {
        strUrl += "&Rent=true";
    }
    strUrl = strUrl + "&SiteKey=" + siteKey;
	
    window.open(strUrl);
}

	
</script>
                        
