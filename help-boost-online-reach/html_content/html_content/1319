    function playClip(tag, mediaId, mobileMediaId, isAvailable) {    
        if(isAvailable){
            var width = "416px";
            var height = "230px";

            setupPlayerForMedia(tag, width, height, mediaId, mobileMediaId);
            
        }else{
            $(".dnse-webtvinit").remove();
            $(".clip-deleted-message", this).show();
        }
    }
    $(document).ready(function() {
        $(".dnse-webtvinit").hover(function() {
            var flasharea = $(this).parent(".dnse-webtvclip");
            flasharea.addClass("dnse-webtvclip-hover");
        }, function() {
            var flasharea = $(this).parent(".dnse-webtvclip");
            flasharea.removeClass("dnse-webtvclip-hover");
        });
    });
