        $(document).ready(function() {
        
            if (typeof ShowDelayedAds != 'undefined') {
                ShowDelayedAds();
            };
            var ad = "";
            $(function() {
                document.write = function(adsource) {
                    ad += adsource;
                }
            });
            if (typeof enirocenter != 'undefined') {
                enirocenter();
            };
            $('div#enirocenter').html(ad);
            ad = "";
            if (typeof eniroright != 'undefined') {
                eniroright();
            };
            $('div#eniroright').html(ad);
            ad = "";
            if (typeof enirohook != 'undefined') {
                enirohook();
            };
            $('div#enirohook').html(ad);
        });
        
