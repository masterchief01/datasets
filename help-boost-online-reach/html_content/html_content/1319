<!--fix for equal colums -->
    <script type="text/javascript">
        $(document).ready(function() {
            var leftEqCol = $('.fh-left-column-wrap, .iframe-span-2').height();
            var rightEqCol = $('#right').height();
            if (rightEqCol >= leftEqCol) {
                $('.fh-left-column-wrap, .iframe-span-2').height(rightEqCol);
            }
            if ($('.fh-control-wrap').length) {
                $('.fh-left-column-wrap').css('background', '#FFFFFF');
                $('.fh-left-column-wrap').find('.teaser-divider').hide();
            }
        });
        $(".section-teasers-holder").prev('.teaser, .teaser-first').find('.teaser-divider').css({ 'border': 'none' });
   </script>
