        <script type="text/javascript">
// function to add dropdown to ANNONSERA link in page header.
$(document).ready(function() {
	$("#header-verticals .dropdown a:first").toggle(function () {
    	$(this).siblings("ul.dropdown-holder").slideDown(250).width(($("#header-verticals .dropdown a:first").width() + 20) + "px").addClass("dropopen");
    }, function () {
        $(this).siblings("ul.dropdown-holder").slideUp(500).removeClass("dropopen");
    });
    $("#header-verticals .dropdown ul.dropdown-holder a").click(function () {
        $("#header-verticals .dropdown a:first").click();
    });
});
</script>
    
    
    
      
