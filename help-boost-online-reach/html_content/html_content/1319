<script language="javascript" type="text/javascript">
    function OnSearchStringKeyPressed_ctl00_ctl00_ExtraWideContentRegion_RightContentRegion_rightTeaserList_teasers_ctl13_ctl00_ShortSearch(e) {
         var keyCodeEntered = (e.which) ? e.which : window.event.keyCode;
         if ( keyCodeEntered == 13 )   {
            document.getElementById('ctl00_ctl00_ExtraWideContentRegion_RightContentRegion_rightTeaserList_teasers_ctl13_ctl00_ShortSearch_btnSearch').click();
            if(e.preventDefault) { e.preventDefault(); }
            return false;
        }
    }
</script>

