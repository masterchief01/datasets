<script language="javascript">
    function showResultsDialog2279(results) {
        
        var poll = $("#poll2279");
        var template = poll.find("#rowTemplate").html();

        poll.find("#rows").setTemplate(template);
        poll.find("#rows").processTemplate(results.Votes);
        poll.find("#totalAnswers").html(results.Total);
        
        $.nyroModalManual(
            { 
                content: poll.find("#dialogTemplate").html(), 
                title: 'Webbfrågan',
                autoSizable: true,
                width:400,
                minHeight:50
            });
        
    }
</script>
</div>


<script language="javascript">
    $().ready(function() {
        $("#poll2279 #showResults").click(function() {
            $.ajax({
                url: '/WebServices/PollService.ashx',
                data: "pollId=2279",
                type: "POST",
                timeout: 30000,
                dataType: "json",
                success: function(results) {
                    showResultsDialog2279(results);
                },
                error: GenericAjaxErrorHandler
            });
        });

        $("#poll2279 .answer").click(function() {
            $("#poll2279 .answer").attr('disabled', 'disabled');
            $.ajax({
                url: '/WebServices/PollService.ashx',
                data: "pollId=2279&&choiceId=" + this.value,
                type: "POST",
                timeout: 30000,
                dataType: "json",
                success: function(results) {
                    
                        showResultsDialog2279(results);
                    
                },
                error: GenericAjaxErrorHandler
            });
        });
    });
</script>


