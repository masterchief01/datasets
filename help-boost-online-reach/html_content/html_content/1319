    <div id="ctl00_ctl00_ExtraWideContentRegion_RightContentRegion_rightTeaserList_teasers_ctl13_ctl00_TemperatureChartWidget_chart_chartContainer" class="fh-chart-placeholder">
    <img id="ctl00_ctl00_ExtraWideContentRegion_RightContentRegion_rightTeaserList_teasers_ctl13_ctl00_TemperatureChartWidget_chart_ChartImg" src="/chartcache/201106060437__160__130__absolute__6486__none.png" alt=""/>
</div>


<script language="javascript" type="text/javascript">
    setTimeout(updateImage_ctl00_ctl00_ExtraWideContentRegion_RightContentRegion_rightTeaserList_teasers_ctl13_ctl00_TemperatureChartWidget_chart, 1000);
    counterctl00_ctl00_ExtraWideContentRegion_RightContentRegion_rightTeaserList_teasers_ctl13_ctl00_TemperatureChartWidget_chart = 0;
    function updateImage_ctl00_ctl00_ExtraWideContentRegion_RightContentRegion_rightTeaserList_teasers_ctl13_ctl00_TemperatureChartWidget_chart() {
        var img = document.getElementById('ctl00_ctl00_ExtraWideContentRegion_RightContentRegion_rightTeaserList_teasers_ctl13_ctl00_TemperatureChartWidget_chart_ChartImg');        
        if(img) {           
            if(img.width < 20) {
                if(counterctl00_ctl00_ExtraWideContentRegion_RightContentRegion_rightTeaserList_teasers_ctl13_ctl00_TemperatureChartWidget_chart < 5)
                {
                    img.src = setUrlKey("randparam", GetGuid(), img.src);
                    counterctl00_ctl00_ExtraWideContentRegion_RightContentRegion_rightTeaserList_teasers_ctl13_ctl00_TemperatureChartWidget_chart++;
                    setTimeout(updateImage_ctl00_ctl00_ExtraWideContentRegion_RightContentRegion_rightTeaserList_teasers_ctl13_ctl00_TemperatureChartWidget_chart, counterctl00_ctl00_ExtraWideContentRegion_RightContentRegion_rightTeaserList_teasers_ctl13_ctl00_TemperatureChartWidget_chart * 1000);
                }
                else {
                    img.src = "/img/FinancialHub/error_chart.png";
                }
            }
        }
    }
</script>
