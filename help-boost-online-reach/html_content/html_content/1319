<script type="text/javascript">
    $(function() {
        $('#slideshowQuote').cycle({
            fx: 'fade',
            timeout: 10000,
            prev: '#prevQuote',
            next: '#nextQuote'
        });
        $("#quoteTeaserContainer").show();
    });

    $().ready(function() {
        $('#prevQuote').hide();
        $('#nextQuote').hide();

        $('#quoteTeaserContainer').mouseenter(function() {
            $('#slideshowQuote').cycle('pause');
            $('#prevQuote').stop(true, true).fadeIn();
            $('#nextQuote').stop(true, true).fadeIn();
        });
        $('#quoteTeaserContainer').mouseleave(function() {
            $('#slideshowQuote').cycle('resume');
            $('#prevQuote').stop(true, true).fadeOut();
            $('#nextQuote').stop(true, true).fadeOut();
        });
    });

    $("div.blogspace-loop .item").livequery(function() {
        var link = $(this).find("a.item-url:first");
        $(this).click(function() {            
            if (link.attr("href") != undefined) {
                window.open(link.attr("href"), link.attr("target"));
            }
            return false;
        }).hover(function() {
            $(this).addClass("item-hover");
            if (link.attr("href") == undefined) {
                $(this).css('cursor', 'default');
            } 
        }, function() {
            $(this).removeClass("item-hover");
        });
    });
        
</script>

                            
                        </div>
                        

<div id="header-searchbox" class="header-searchbox">
    <div class="row">
        <input type="text" id="searchTextBox" class="user-input field inactived" value="Sök" 
        onkeypress="if (event.keyCode == 13) { DoSearch(); return false; } return true;" />
        <input type="image" name="ctl00$ctl00$searcher$searchButton" id="ctl00_ctl00_searcher_searchButton" class="btn" alt="" src="/img/buttons/searchbtn.gif" onclick="DoSearch(); return false;" style="border-width:0px;" />
    </div>
</div>    
<script type="text/javascript">
    String.prototype.trim = function() {
        return this.replace(/^\s*/, "").replace(/\s*$/, "");
    }

    $(document).ready(function() {
        var defaultSearchValue = 'Sök'
        $('#searchTextBox').focus(function() {
            $(this).removeClass("inactived");
            $(this).addClass("actived");
            if ($(this).val() == defaultSearchValue) $(this).val('');
        });

        $('#searchTextBox').blur(function() {
            $(this).removeClass("actived");
            $(this).addClass("inactived");
            if ($(this).val() == '') $(this).val(defaultSearchValue);
        });
    });

    function DoSearch() {
        var searchText = $('#searchTextBox').val();
        if (searchText != 'Sök') {
            
                location.href = '/Pages/SolrSearch.aspx?id=328&s=' + encodeURIComponent(searchText.replace(/ /g, '+'));
            
        }
    }
</script>
