<script type="text/javascript">
    $(function() {
        $('#rotationSlideshow').cycle({
            fx: 'scrollHorz',
            delay: 2000,
            speed: 500,
            timeout: 10000,
            prev: '#prevRotation',
            next: '#nextRotation',
            pager: '#rotation-navigation-numbers',
            pagerAnchorBuilder: function(idx, slide) {
                return '#rotation-navigation-numbers span:nth-child(' + idx + ')';
            },
            //before: onBefore,
            after: fixPagerColors
        });
    });


    function onBefore(opt1, opt2, opt3) {
        /*var className = $(opt2).find('#sectionName').attr('class');
        var nextSlide = opt3.nextSlide;
        if (opt1 == opt2) {
            nextSlide = opt3.currSlide;
        }
        $('#rotation-navigation-numbers').find('span').removeClass();
        $('#rotation-navigation-numbers').find('span:eq(' + nextSlide + ')').addClass(className);*/
        /*  The above code doesn't seem to work, since the rgba value is printed straight onto the span as a style tag.
            Added the after: instead to keep track of which one is visible at the moment.
        */
    }

    function fixPagerColors(arColors) {
        $('#rotation-navigation-numbers span').addClass('noRotationBg');
        $('#rotationSlideshow > div').each(function(i) {
            if ($(this).is(':visible')) {
                var currRotationItem = $('#rotation-navigation-numbers').find('span:eq(' + i + ')');
                currRotationItem.removeClass('noRotationBg');
            }
        });
    }

    window.onload = function() {

        $('#rotationTeaserContainer').mouseenter(function() {
            $('#rotationSlideshow').cycle('pause');    
        });
        $('#rotationTeaserContainer').mouseleave(function() {
            $('#rotationSlideshow').cycle('resume');                                     
        });
    }

    $("#rotation-loop .item").livequery(function() {
        $(this).click(function() {
            document.location = $(this).find("a.item-url:first").attr("href");
        }).hover(function() {
            $(this).addClass("item-hover");
        }, function() {
            $(this).removeClass("item-hover");
        });
    });
        
</script>

