<script language="javascript">
    function showResultsDialog2273(results) {
        
        var poll = $("#poll2273");
        var template = poll.find("#rowTemplate").html();

        poll.find("#rows").setTemplate(template);
        poll.find("#rows").processTemplate(results.Votes);
        poll.find("#totalAnswers").html(results.Total);
        
        $.nyroModalManual(
            { 
                content: poll.find("#dialogTemplate").html(), 
                title: 'Webbfrågan',
                autoSizable: true,
                width:400,
                minHeight:50
            });
        
    }
</script>
</div>


<script language="javascript">
    $().ready(function() {
        $("#poll2273 #showResults").click(function() {
            $.ajax({
                url: '/WebServices/PollService.ashx',
                data: "pollId=2273",
                type: "POST",
                timeout: 30000,
                dataType: "json",
                success: function(results) {
                    showResultsDialog2273(results);
                },
                error: GenericAjaxErrorHandler
            });
        });

        $("#poll2273 .answer").click(function() {
            $("#poll2273 .answer").attr('disabled', 'disabled');
            $.ajax({
                url: '/WebServices/PollService.ashx',
                data: "pollId=2273&&choiceId=" + this.value,
                type: "POST",
                timeout: 30000,
                dataType: "json",
                success: function(results) {
                    
                        showResultsDialog2273(results);
                    
                },
                error: GenericAjaxErrorHandler
            });
        });
    });
</script>


