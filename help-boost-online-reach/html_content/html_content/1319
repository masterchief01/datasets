<div class="teaser html-teaser bi3dPos bi3dTeasId-1019__Teaser">
    <div class="teaser-container-inner">
        <script type="text/javascript">
var COUNT_ACTIVE = true;
var COUNT_DIRECTION = 1;
var SetTimeoutPeriod = (Math.abs(COUNT_DIRECTION) - 1) * 1000 + 990;
var startDate = new Date(2001, 08, 23, 09, 37, 30);
var currentDate = new Date();

function calculateDatePart(secs, f1, f2) {
  s = ((Math.floor(secs/f1))%f2).toString();
  //if (s.length < 2)
    //s = "0" + s;
  return s;
}

function update(secs) {
	var days = calculateDatePart(secs, 86400, 100000);
	var hours = calculateDatePart(secs, 3600, 24);
	var minutes = calculateDatePart(secs, 60, 60);
	var seconds = calculateDatePart(secs, 1, 60);
	
	document.getElementById('dawitDays').innerHTML = days;
	document.getElementById('dawitHours').innerHTML = hours;
	document.getElementById('dawitMinutes').innerHTML = minutes;
	document.getElementById('dawitSeconds').innerHTML = seconds;
	
	if (COUNT_ACTIVE) {
		setTimeout("update(" + (secs + COUNT_DIRECTION) + ")", SetTimeoutPeriod);
	}
}
dateDiff = currentDate.getTime() - startDate.getTime();
secs = Math.floor(dateDiff.valueOf()/1000);
</script>
<div style="line-height:1.4em;border-bottom:1px solid #cccccc; padding-bottom:6px;"><p><a href="/tema/dawit-isaak" title="Läs mer om Dawit Isaak på vår temasida" alt="Läs mer om Dawit Isaak på vår temasida"><span style="text-transform: uppercase;font-weight: bold;color:#bf0005;">Dawit Isaak har suttit fängslad</span></a><br/><span id="dawitDays"></span>&nbsp;dagar, <span id="dawitHours"></span>&nbsp;tim, <span id="dawitMinutes"></span>&nbsp;min och <span id="dawitSeconds"></span>&nbsp;sek. <script type="text/javascript">update(secs);</script></p></div>
        
    </div>
</div>

