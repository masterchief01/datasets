	<script type="text/javascript">

	  var _gaq = _gaq || [];
	  _gaq.push(['_setAccount', 'UA-1342607-1']);
	  _gaq.push(['_setCustomVar', 1, 'story', '1', 3], ['_trackPageview']);
	  
	  (function() {
	    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
