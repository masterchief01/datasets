<script type="text/javascript">
function $(i){return (document.getElementById)?document.getElementById(i):null}	

objRounder = {
   $:function(i){return (document.getElementById)?document.getElementById(i):null},
   match_classes:function(b){;return (b.className=="nav_title");},
   get_markup:function() {
        dinadiv = document.createElement("div");
        dinadiv.className="dinadiv";
        return dinadiv;
   },
   round_this_box:function(b){
        b.parentNode.insertBefore(this.get_markup(), b);
   },
   init:function(){
        this.round_this_box(this.$("nav_title_in_recipesmenus"));
        this.round_this_box(this.$("nav_title_in_articlesguides"));
        this.round_this_box(this.$("nav_title_in_community"));
        this.round_this_box(this.$("nav_title_in_tools"));
   } 
}

    objRounder.init();
</script>
</div>

    
    
    
