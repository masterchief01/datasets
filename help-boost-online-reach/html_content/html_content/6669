<script language="JavaScript" type="text/javascript">
function toggleMe(a){
  var e=document.getElementById(a);
  if(!e)return true;
  if(e.style.display=="none"){
    e.style.display="block"
  } else {
    e.style.display="none"
  }
  return true;
}
</script>

<script language="JavaScript" type="text/javascript">
function add_tags_multi(headerfield, checkform, mask) {
	topics_list = "";
	for (var i = 0; i < checkform.length; i++)
	{
		if (typeof(checkform[i].name) == "undefined" || (typeof(mask) != "undefined" && checkform[i].name.substr(0, mask.length) != mask))
			continue;

		if (!checkform[i].disabled)
			if ( checkform[i].checked )
				topics_list = topics_list + checkform[i].value + ",";
	}
	if ( topics_list.length > 0 ) {
		topics_list = topics_list.substring(0,topics_list.length-1);
		pop_window("http://www.craftster.org/forum/tagactions.php?action=showaddform;topic="+topics_list,500,550,"AddTag");
	}
}
</script>
