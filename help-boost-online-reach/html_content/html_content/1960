			s.linkTrackVars="events";
			s.linkTrackEvents="event9";
			s.events="event9";
			s.tl(this,'o','Subscription link clicked');
		}
		
		function linkSubSubmitCode(obj){
			s.linkTrackVars="events";
			s.linkTrackEvents="event10";
			s.events="event10";
			s.tl(this,'o','Subscription submit button clicked');
		}		
	</script>





