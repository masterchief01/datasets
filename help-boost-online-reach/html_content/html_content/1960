     } 

         // This method is activated when page is loaded  
         function onLoad() {  		 
             gigya.services.socialize.addEventHandlers(conf, {   
					onLogin: onLoginHandler   
                     , onLogout: onLogoutHandler
					, onConnectionAdded: onConnectAddedHandler
                     });  
   
         }  
   
		function renderVerified()
		{
			document.getElementById('status').innerHTML = "Welcome!&nbsp;|&nbsp;<a href='' onclick='logoutFromGS()'>Logout</a>";	
		}		
			

         function renderUI(res) {  
			if(typeof(res)=='undefined')
			{
				document.getElementById('connect-icons').innerHTML ="";	
				document.getElementById('status').innerHTML ="Login&nbsp;With:&nbsp;";	
				gigya.services.socialize.showLoginUI(conf,login_params);
				
			}
			 else
			{
				if (res.user != null && res.user.isConnected) 
				{  
					document.getElementById('status').innerHTML = "Welcome!&nbsp;|&nbsp;<a href='' onclick='logoutFromGS()'>Logout</a>"; 
					document.getElementById('login-icons').innerHTML ="";	
					gigya.services.socialize.showAddConnectionsUI(conf,connect_params);
				} else 
				{ 
					document.getElementById('status').innerHTML ="Login&nbsp;With:&nbsp;";	
					document.getElementById('connect-icons').innerHTML ="";					
					gigya.services.socialize.showLoginUI(conf,login_params);
				} 
			} 
         }         
 
  
 
         function verifyTheSignature(UID, timestamp, signature) {  
             encodedUID = encodeURIComponent(UID);                          
			xajax_wd(encodedUID, timestamp, signature);
			
         }   
   
         // onLogin Event handler  
         function onLoginHandler(eventObj) {   

             verifyTheSignature(eventObj.UID, eventObj.timestamp, eventObj.signature);  
          
         }  
         // onConnectAdded Event handler  
         function onConnectAddedHandler(eventObj) {   

             verifyTheSignature(eventObj.UID, eventObj.timestamp, eventObj.signature);  
          
         }  		 

           
         // Logout from Gigya platform. This method is activated when "Logout" button is clicked   
         function logoutFromGS() {  
             gigya.services.socialize.logout(conf, {/*no required params*/}); 
         }  
           
         // onLogout Event handler  
         function onLogoutHandler(eventObj) {  
            document.getElementById('status').innerHTML = "Login&nbsp;With:&nbsp;"; 
			document.getElementById('connect-icons').innerHTML ="";					
			gigya.services.socialize.showLoginUI(conf,login_params);
         }  
</script>
	
		
	
