		$(".primary-links li a").mouseover(function(){
		  var src = $(this).children("img").attr("src").match(/[^\.]+/) + "_active.png";
		  alt = $(this).children("img").attr('alt').toLowerCase();		  
		  src = '/'+path+'/images/menu_'+alt+'_active.png';
