<script type="text/javascript">
  SBN.carousel = new Carousel($('c-scroller'), $$('.c-slide'), $$('a.c-carousel-control'),{
    visibleSlides:1,
    circular:false,
    wheel:false
  });
  SBN.carousel.deactivateControls = function () {
    this.controls.invoke('addClassName', this.options.disabledClassName);
    this.classifyHandles();
  };
  SBN.carousel.classifyHandles = function() {
    var current = this.current;
    var slides = this.slides;
    if (!current || current._index == 0) {
      $('pprev').addClassName('disabled');      
    } else {
      $('pprev').removeClassName('disabled');
    }
    if (current && current._index == (slides.length -1)) {
      $('ppnext').addClassName('disabled');
    } else {
      $('ppnext').removeClassName('disabled');
    }
  };
</script>
   



<div id="blog-header" class="cbranding blog-header header-fix clearfix">
  <h2 class="fonted"><a href="/commentary_archive">From Our Editors</a></h2>
  <p class="tagline-editable" id="commentary_title">Updated throughout the day with quick takes from staff. </p>
