<script type="text/javascript">
  jQuery(document).ready(function() {
    var focused = false;
    var IE7 = (navigator.appVersion.indexOf("MSIE 7.")==-1) ? false : true;
    jQuery('#subscriptions').hover(function() {
      if (IE7){jQuery("#banner-main,#content").css("z-index","-1");}
      clearTimeout(jQuery(this).data('timeout'));
      jQuery('#subscriptions div.nav-categories, #subscriptions div.nav-categories-chute').show();
      jQuery('p#button-close').click(function(event){
      event.preventDefault();
      jQuery('#subscriptions div.nav-categories, #subscriptions div.nav-categories-chute').hide();
      if (IE7){jQuery("#banner-main,#content").css("z-index","1");}
    });
    }, function() {
      if (focused === false){
      var t = setTimeout(function() {
      if (IE7){jQuery("#banner-main,#content").css("z-index","1");}
      jQuery('#subscriptions div.nav-categories, #subscriptions div.nav-categories-chute').hide();
      }, 2000);
      jQuery(this).data('timeout', t);
      }
    });
jQuery('.default-value').each(function() {
   var default_value = this.value;
   jQuery(this).focus(function() {
       if(this.value == default_value) {
           this.value = '';
       }
       focused = true;
   });
   jQuery(this).blur(function() {
       if(this.value == '') {
           this.value = default_value;
       }
       focused = false;
   });
});
});
</script> 

<script type="text/javascript"> 

function validateSubscribeForm() {  

    var alertMsg = "The following REQUIRED fields\nhave been left empty:\n";

    if (document.getElementById("StreetAdr").value == "" || document.getElementById("StreetAdr").value == "Address 1")
	{	
		alertMsg += "\nStreet Address";
	}
    if (document.getElementById("SuppAdr").value == "Address 2") 
	{
		document.getElementById("SuppAdr").value = "";
	}
    if (document.getElementById("City").value == "" || document.getElementById("City").value == "City") 
	{
		alertMsg += "\nCity";  
	}
    if (document.getElementById("PostalCode").value == "" || document.getElementById("PostalCode").value == "Zip")
	{
		alertMsg += "\nZip Code"; 
	}
    if (document.getElementById("EmailAdr").value == "" || document.getElementById("EmailAdr").value == "E-mail" )
	{	
		alertMsg += "\nEmail Address";
	}
    if (document.getElementById("FirstName").value == "" || document.getElementById("FirstName").value == "First Name" ) 
	{
		alertMsg += "\nFirst Name";
	}
	if (document.getElementById("LastName").value == "" || document.getElementById("LastName").value == "Last Name" ) 
	{
		alertMsg += "\nLast Name";
	}
	


 if (alertMsg != "The following REQUIRED fields\nhave been left empty:\n") {
      alert(alertMsg);
      return false;
    } else {
      //document.subscribe_form.submit() ;
      return true;
    }
 
}
</script>


	


	


