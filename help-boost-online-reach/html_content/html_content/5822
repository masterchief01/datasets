<script type="text/javascript">
	jQuery(function() {

		
		jQuery("#nav-sub ul").hide();

		/* Inactivated */
		jQuery("#nav-sub.inactivated-sub ul").show();
		
		

		
		/* Ticker */
		var newsitems;
		var curritem = 0; 

		newsitems = jQuery("#nav-sub li").hide().size();
		jQuery("#nav-sub li:eq("+curritem+")").show();
		setInterval(ticknews,4000);

		function ticknews() {
			jQuery("#nav-sub li:eq("+curritem+")").hide();
			curritem = ++curritem%newsitems;
			jQuery("#nav-sub li:eq("+curritem+")").show();
		} 
		
		

		jQuery('#subscriptions input[type="text"]').focus(function() {
			if (this.value == "Zip") {
				this.value = "";
			}
		});		

		

	});
</script>
<script type="text/javascript" language="javascript">
function setHomepage() 
{
	 if (document.all) 
	 {
	  document.body.style.behavior='url(#default#homepage)';
	  document.body.setHomePage('http://www.womansday.com'); 
	 }
 else 
     {
			  alert("This is not an Internet Explorer. Please follow the instructions for your browser.");
	 }
	
 } 
</script>


	

	<script language="JavaScript" type="text/javascript">
		function linkSubCode(obj){
			s.linkTrackVars="events";
			s.linkTrackEvents="event9";
			s.events="event9";
			s.tl(this,'o','Subscription link clicked');
		}
		
		function linkSubSubmitCode(obj){
			s.linkTrackVars="events";
			s.linkTrackEvents="event10";
			s.events="event10";
			s.tl(this,'o','Subscription submit button clicked');
		}		
	</script>


	

	<script language="JavaScript" type="text/javascript">
		function doSearch(){
            var chosen;
            var searchUrl;
            var len = document.getElementsByName('searchradio').length;

            for (i = 0; i<len; i++){
                        if (document.getElementById('top_search_form').searchradio[i].checked){
                                    chosen = document.getElementById('top_search_form').searchradio[i].value;
                        }
            }
            if (chosen=='recipes'){
                        searchUrl='/recipe-search/find/keyword-'+document.getElementById('top_search_form').SearchText.value;
            } else {
                        searchUrl='http://find.womansday.com/search/'+document.getElementById('top_search_form').SearchText.value;
            }

            window.location = searchUrl; 
		} 
	</script>
	




