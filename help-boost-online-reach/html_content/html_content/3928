		    	function highlight(item) {		
					var topNewsItems = $(".topNewsWidget").find(".topNewsStory");
					id = item.split("_")[1];
					for (i=0; i<topNewsItems.length; i++) {
						x = topNewsItems[i].id.split("_")[1];
						if (x == id) {
							document.getElementById("hpi_"+x).className = "homePageTopImage active";
							var textElement = document.getElementById("text_"+x);
							var textClasses = textElement.className;
							textElement.className = textClasses.replace("text_inactive", "text_active");
							document.getElementById("sidebar_"+x).className = "side_active";
						} else {
							document.getElementById("hpi_"+x).className = "homePageTopImage inactive";
							var textElement = document.getElementById("text_"+x);
							var textClasses = textElement.className;
							textElement.className = textClasses.replace("text_active", "text_inactive");
							document.getElementById("sidebar_"+x).className = "side_inactive";
						}
					}
				}			
			$(document).ready(function() {
			  $(".topNewsWidget").find(".topNewsStory").bind("mouseover", function() {
