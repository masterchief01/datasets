	_gaq.push(['_setAccount', 'UA-872493-1']);
	_gaq.push(['_trackPageview']);

	(function() {
		var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
		ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
		var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
	})();

	function popupWindow(url, height, width, lockdown, center) {
		height 		= height == undefined ? 100 : height;
		width		= width == undefined ? 100 : width;
		lockdown 	= lockdown != undefined ? lockdown : 'toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=no,resizable=yes,copyhistory=no';
		lockdown   += ',height='+height+',width='+width;
	
		if(center === true) { 
			var w = document.all ? document.body.clientWidth : window.innerWidth;
			var h = document.all ? document.body.clientHeight : window.innerHeight;
			var l = (w-width)/2;
			var t = (h-height)/2;
			
			lockdown += ',screenX='+l+',screenY='+t+',top='+t+',left='+l;
		} else { 
			lockdown += ',screenX=150,screenY=150,top=150,left=150';
		};
	
		window.open(url,'popupWindow',lockdown);
	};
</script>

