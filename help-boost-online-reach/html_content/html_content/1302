	<script type="text/javascript">
		FB.init("b2d7fd07b7f8b00cf503a357503b0dc0", "/xd_receiver.htm", {"ifUserConnected":onConnected,'doNotUseCachedConnectState': true});
	
	function onConnected()
	{
		// Check if the user is linked to an account here - if they aren't, redirect them to the places they need to go.
					
			jQuery.get("/ajax/get_user_id/", function(data)
			{
				if (parseInt(data) > 0)
				{
					if (typeof(ignoreReturnURL) == 'undefined')
					{
						var ignoreReturnURL = false;
					}
					
					//jQuery("#header_controls_container").load('/ajax/get_site_header_control/', {}, function() { FB.XFBML.Host.parseDomTree(); }); 	
					if (jQuery('#return_url').length > 0 && !ignoreReturnURL)
					{
						// Is there a popup login box visible? Use that return URL if there is. 
						if (jQuery('#loginPopup:visible').length > 0)
						{
							 var urlDestination = unescape(jQuery('#return_url', jQuery('#loginPopup:visible')).val());
						}
						else
						{
							// use what ever you can find.
							var urlDestination = unescape(jQuery('#return_url').val());
						}
						
						
						// If the URL has a #anchor on it, remove it before we carry on or the page might 
						// not reload, which wouldnt be very helpful.
						var anchor_index = urlDestination.indexOf('#');
						if (anchor_index != -1) 
						{
							urlDestination = urlDestination.substring(0, anchor_index);
						}
						
						document.location.href = urlDestination;
					}
					else
					{
						document.location.reload();	
					}
				}
				else if (fbConnectInitiated == true)
				{
					document.location.href = MPORA.globalVars.global_base_url+'register/fbconnect/';
				}
			});
				}
	</script>

	<script type='text/javascript' src='http://mpora.com/js/load/globaljs/init_expand_box/1297249202/'></script>
<script type='text/javascript' src='http://mpora.com/js/load/globaljs/init_tab_control/1297249202/'></script>

  </body>
</html>