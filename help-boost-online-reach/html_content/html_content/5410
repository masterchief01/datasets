	var ad_check_counter = 0; 
	$(document).ready(function(){
		if($.browser.msie){
			var t; 							  
			function checkIframe(){
				ad_check_counter++;
				if(ad_check_counter == 5){
					if(typeof(t)!='undefined'){ clearTimeout(t);}
					return;
				}
	
				if($('.bcwrapper iframe').length > 0 ){
					var iframe = $(".bcwrapper iframe").get(0);
					if (iframe.readyState == "complete"){
		        			$(".bcwrapper #atwAdFrame1").contents().find("body").css("background-color", "#F4F4F4");
						$("#ad215").show();
						if(typeof(t)!='undefined'){ clearTimeout(t);}
					}else{
						iframe.onreadystatechange = function(){
							if (iframe.readyState == "complete"){
				        		$(".bcwrapper #atwAdFrame1").contents().find("body").css("background-color", "#F4F4F4");
								$("#ad215").show();
								if(typeof(t)!='undefined'){ clearTimeout(t);}
							}
						}
					}
				}else{
					t = setTimeout(function(){checkIframe()}, 2000);
				}
		        }
		        checkIframe();
		}else{
			$("#ad215").show();
		}
	});
var carousel_speed = 5000;
var carousel_interval = setInterval('carousel_rotate()', carousel_speed);   

$(document).ready(function() {
  $(".maindl-article").hover(function() {
	clearInterval(carousel_interval);

	$(this).addClass('maindl-active').siblings().removeClass('maindl-active');
	$("#"+$(this).attr('id')+"-mainspot").show().siblings().hide();	
  }, 
  function() {
	carousel_interval = setInterval('carousel_rotate()', carousel_speed); 	
  });

  $("#maindl-mainspot").hover(function() {
	clearInterval(carousel_interval);
  }, 
  function() {
	carousel_interval = setInterval('carousel_rotate()', carousel_speed); 	
  });
});

function carousel_rotate() {
	if($('.maindl-active').next('.maindl-article').length){
		next_id = $('.maindl-active').next().attr('id');
		$('.maindl-article').removeClass('maindl-active');
		$('#'+next_id).addClass('maindl-active');
		$("#"+next_id+"-mainspot").show().siblings().hide();	
	}else{
		$('.maindl-article').removeClass('maindl-active');
		$('.maindl-article:first').addClass('maindl-active');
		$("#"+$('.maindl-active:first').attr('id')+"-mainspot").show().siblings().hide();	
	}
}	
		
