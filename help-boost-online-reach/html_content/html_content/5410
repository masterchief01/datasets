        count: '10',
        directURL : 'http://c.aol.com/',
        proxyPath : '/aolc/',
        create_topic : '',
        fancyScroll : true,
        adsDivs : '',
        referrer_url: 'http://jobs.aol.com/articles/2011/09/08/what-to-wear-and-not-to-wear-to-an-interview-infographic/?ncid=dynalduscare00000002',
