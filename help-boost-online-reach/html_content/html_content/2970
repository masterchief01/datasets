</script>
<script src="http://ajax.microsoft.com/ajax/jquery.validate/1.7/jquery.validate.min.js" type="text/javascript"></script>

<script type="text/javascript">
//<![CDATA[
$(function () {
	$("#newsletter-email").val("Enter your email address");
	$("#newsletter-email").focus(function() {
		$("#newsletter-email").val("");
	});
	$("#newsletter-email").blur(function() {
		if ($("#newsletter-email").val() == "") {
			$("#newsletter-email").val("Enter your email address");
		}
	});

	var validator = $("#newsletter-form").validate({
		rules: {
			email: {
				required: true,
				email: true
			}
		},
		messages: {
			email: ""
		},
		submitHandler: function(form) {			
			dataString = $(form).serialize();
			$.ajax({
				type: "GET",
				url: "/submit-sweeps-form/",
				data: dataString,
				success: function() {
					$("#newsletter-form").hide();
					$("#newsletter-success").show();
				}
			}); 
		},
		errorLabelContainer: $("#email-error"),
		invalidHandler: function(form, validator) {
		      var errors = validator.numberOfInvalids();
		      if (errors) {
		        var message = errors == 1
		          ? 'Please enter a valid email address.'
		          : 'Please enter a valid email address.';

		        $("#email-error").show();
		        		        $("#email-error").html(message);
		      } else {
		        $("#email-error").hide();
		      }
		    }
	});
	var validator2 = $("#register").validate({
		rules: {
			email: {
				required: true,
				email: true
			}
		},
		messages: {
			email: ""
		},
		submitHandler: function(form) {			
			dataString = $(form).serialize();
			$.ajax({
				type: "GET",
				url: "/submit-summerrun-sweeps/",
				data: dataString,
				success: function() {
					$("#register .form, #sweeps-submit, #form-intro").hide();
					$("#successContainer").show();
				}
			}); 
		},
		errorLabelContainer: $("#email-error-sweeps"),
		invalidHandler: function(form, validator2) {
		      var errors = validator2.numberOfInvalids();
		      if (errors) {
		        var message = errors == 1
		          ? 'Please enter a valid email address.'
		          : 'Please enter a valid email address.';

		        $("#email-error-sweeps").show();
		        		        $("#email-error-sweeps").html(message);
		      } else {
		        $("#email-error-sweeps").hide();
		      }
		    }
	});

        var validator3 = $("#register-runway").validate({
		rules: {
			email: {
				required: true,
				email: true
			}
		},
		messages: {
			email: ""
		},
		submitHandler: function(form) {			
			dataString = $(form).serialize();
			$.ajax({
				type: "GET",
				url: "/submit-project-runway-sweeps/",
				data: dataString,
				success: function() {
					$("#register-runway .form, #sweeps-submit, #form-intro, p#lifetime-promo").hide();
					$("#successContainer").show();
				}
			}); 
		},
		errorLabelContainer: $("#email-error-sweeps"),
		invalidHandler: function(form, validator3) {
		      var errors = validator3.numberOfInvalids();
		      if (errors) {
		        var message = errors == 1
		          ? 'Please enter a valid email address.'
		          : 'Please enter a valid email address.';

		        $("#email-error-sweeps").show();
		        		        $("#email-error-sweeps").html(message);
		      } else {
		        $("#email-error-sweeps").hide();
		      }
		    }
	});


	$("#query").val("Search Heidi Klum on AOL");
	$("#query").focus(function() {
		$("#query").val("");
	});
	$("#query").blur(function() {
		if ($("#query").val() == "") {
			$("#query").val("Search Heidi Klum on AOL");
		}
	});
	
});
//]]>
</script>

<script type="text/javascript">
//<![CDATA[
 $(document).ready(function(){ 
  $("input#add-profile").click(function(){ 
    $("input#input-weight").val($("input#user-weight").val());
    $("input#input-birthday").val($("select#select-year").val()+'-'+$("select#select-month").val()+'-'+$("select#select-day").val());
   
    var height_feet = parseFloat($("input#user-feet").val());
     if (!isNaN(height_feet)) {
       var height_feet_inches = height_feet * 12;
       $("input#input-height").val(height_feet * 12);
       var height_inches = parseFloat($("input#user-inches").val());
       var total_inches = height_feet_inches + height_inches;
       $("input#input-height").val(total_inches);
     } else {
        alert("Please enter a whole number for height");
    }
  });
 });
//]]>
</script>

<script type="text/javascript">
//<![CDATA[ 
 $("#user-update").submit(function() {
     var startTime = this.start_hrs.value+":"+this.start_mins.value;
     var durationMillis = (parseInt("0"+this.duration_hrs.value,10)*60+parseInt("0"+this.duration_mins.value,10))*60000;
	$.post("/fitbit-activity-submit/?callback=?",
		{
		"distance":this.distance.value,
                "startTime":startTime+"",
                "durationMillis":durationMillis+"",
                "activityId":this.activityId.value,
                "date":this.date.value
		},function(data) {
			  if(data.status == 'error'){
			     $("#activity-message").html(data.message);
			     $("#activity-message").fadeIn('slow');
			  } else {
				$("#activity-message").html(data.message);
      			        $("#activity-message").fadeIn('slow');
                              myForm=$("#user-update")[0];
                              myForm.start_hrs.value = "";
                              myForm.start_mins.value = "";
                              myForm.duration_hrs.value = "";
                              myForm.duration_mins.value = "";
                              myForm.distance.value = "";
                              myForm.activityId.value = "";
                              myForm.date.value = "";
			  }
		},"json");
	return false;
 });
//]]>
</script>

<script type="text/javascript">
//<![CDATA[ 
/*jQuery JavaScript Library jcarousellite */

(function($){
    $.fn.jCarouselLite=function(o){
        o=$.extend({
            btnPrev:null,
            btnNext:null,
            btnGo:null,
            mouseWheel:false,
            auto:null,
            speed:200,
            easing:null,
            vertical:false,
            circular:true,
            visible:3,
            start:0,
            scroll:1,
            beforeStart:null,
            afterEnd:null
        },o||{});
        return this.each(function(){
            var b=false,animCss=o.vertical?"top":"left",sizeCss=o.vertical?"height":"width";
            var c=$(this),ul=$("ul",c),tLi=$("li",ul),tl=tLi.size(),v=o.visible;
            if(o.circular){
                ul.prepend(tLi.slice(tl-v-1+1).clone()).append(tLi.slice(0,v).clone());
                o.start+=v
            }
            var f=$("li",ul),itemLength=f.size(),curr=o.start;
            c.css("visibility","visible");
            f.css({
                overflow:"hidden",
                float:o.vertical?"none":"left"
            });
            ul.css({
                margin:"0",
                padding:"0",
                position:"relative",
                "list-style-type":"none",
                "z-index":"1"
            });
            c.css({
                overflow:"hidden",
                position:"relative",
                "z-index":"2",
                left:"0px"
            });
            var g=o.vertical?height(f):width(f);
            var h=g*itemLength;
            var j=g*v;
            f.css({
                width:f.width(),
                height:f.height()
            });
            ul.css(sizeCss,h+"px").css(animCss,-(curr*g));
            c.css(sizeCss,j+"px");
            if(o.btnPrev)$(o.btnPrev).click(function(){
                return go(curr-o.scroll)
            });
            if(o.btnNext)$(o.btnNext).click(function(){
                return go(curr+o.scroll)
            });
            if(o.btnGo)$.each(o.btnGo,function(i,a){
                $(a).click(function(){
                    return go(o.circular?o.visible+i:i)
                })
            });
            if(o.mouseWheel&&c.mousewheel)c.mousewheel(function(e,d){
                return d>0?go(curr-o.scroll):go(curr+o.scroll)
            });
            if(o.auto)setInterval(function(){
                go(curr+o.scroll)
            },o.auto+o.speed);
            function vis(){
                return f.slice(curr).slice(0,v)
            };

            function go(a){
                if(!b){
                    if(o.beforeStart)o.beforeStart.call(this,vis());
                    if(o.circular){
                        if(a<=o.start-v-1){
                            ul.css(animCss,-((itemLength-(v*2))*g)+"px");
                            curr=a==o.start-v-1?itemLength-(v*2)-1:itemLength-(v*2)-o.scroll
                        }else if(a>=itemLength-v+1){
                            ul.css(animCss,-((v)*g)+"px");
                            curr=a==itemLength-v+1?v+1:v+o.scroll
                        }else curr=a
                    }else{
                        if(a<0||a>itemLength-v)return;else curr=a
                    }
                    b=true;
                    ul.animate(animCss=="left"?{
                        left:-(curr*g)
                    }:{
                        top:-(curr*g)
                    },o.speed,o.easing,function(){
                        if(o.afterEnd)o.afterEnd.call(this,vis());
                        b=false
                    });
                    if(!o.circular){
                        $(o.btnPrev+","+o.btnNext).removeClass("disabled");
                        $((curr-o.scroll<0&&o.btnPrev)||(curr+o.scroll>itemLength-v&&o.btnNext)||[]).addClass("disabled")
                    }
                }
                return false
            }
        })
    };

    function css(a,b){
        return parseInt($.css(a[0],b))||0
    };

    function width(a){
        return a[0].offsetWidth+css(a,'marginLeft')+css(a,'marginRight')
    };

    function height(a){
        return a[0].offsetHeight+css(a,'marginTop')+css(a,'marginBottom')
    }
})(jQuery);

