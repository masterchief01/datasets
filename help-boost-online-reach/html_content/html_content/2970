<script type="text/javascript">
//<![CDATA[
//Popular Tab
function selectPopularTab(id) {
    $("#popular ul.tabbedList li a").removeClass("sel");
    $("#popular ul.tabbedList li a#"+id).addClass("sel");
}

// Popular module
try{
$("#popular ul.tabbedList li a").click(function(){
    $("#popular .dataCont").removeClass("dataContSelected");
    $("#popular div#"+this.id+"_cont").addClass("dataContSelected");
    selectPopularTab(this.id);
    return false;
});
}catch(e){}

// GCP Comment count in Post title
var gcpRequestCount=0;

function getDiscussCmtCount() {
    if($('.comments-meta h4').length != 0) {
        var cmntCnt=0;
        var cmntTxt = $('.comments-meta h4').text();
        if(!isNaN(parseInt(cmntTxt))) {
            cmntCnt = parseInt(cmntTxt);
        }
        if(cmntCnt >1)
           cmntCnt += ' Comments';
        else
           cmntCnt += ' Comment';

        $('#post-cmt-count').html('&nbsp;| <a href="#aol-comments">' + cmntCnt + '</a>');
        $('#post-header .cmt-count').show();
        clearTimeout(window.gcpDiscussTimer);
    }else if(gcpRequestCount>=10){
        clearTimeout(window.gcpDiscussTimer);
    }
    gcpRequestCount++;
}

window.gcpDiscussTimer = setInterval("getDiscussCmtCount()", 1000);

