<script type="text/javascript"><!--//--><![CDATA[//><!--
sfHover = function() {

	var sfEls = document.getElementById("nav").getElementsByTagName("LI");
	var selectTagsNav = document.getElementsByTagName("SELECT");
	var optionTagsNav = document.getElementsByTagName("OPTION");
	for (var i=0; i<sfEls.length; i++) {
		sfEls[i].onmousemove=function() {
		if (this.className.indexOf(" sfhover") == -1) {
			this.className += " sfhover";
		}
			
			
			//hides the form elements when menu is rolled in IE
			if (selectTagsNav) {
			for (var x=0; x<selectTagsNav.length; x++) {
			selectTagsNav[x].style.visibility = "hidden";
			}
			}
			if (optionTagsNav) {
			for (var x=0; x<optionTagsNav.length; x++) {
			optionTagsNav[x].style.visibility = "hidden";
			}
			}

		}
		sfEls[i].onmouseout=function() {
			this.className=this.className.replace(new RegExp(" sfhover\\b"), "");
			
			//shows the form elements when menu is rolled in IE
			if (selectTagsNav) {
			for (var x=0; x<selectTagsNav.length; x++) {
			selectTagsNav[x].style.visibility = "visible";
			}
			}
			if (optionTagsNav) {
			for (var x=0; x<optionTagsNav.length; x++) {
			optionTagsNav[x].style.visibility = "visible";
			}
			}
			
		}
	}

}
if (window.attachEvent) window.attachEvent("onload", sfHover);

//--><!]]></script>

