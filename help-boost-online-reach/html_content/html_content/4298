<script language="JavaScript1.2">
setInterval("UpdateNums_TODAY()",500);
var vDivCal_TODAY = document.getElementById? document.getElementById("calories_TODAY") : document.all.calories_TODAY;
var iDivCal_TODAY = 24574475290.2283;

var vDivCups_TODAY = document.getElementById? document.getElementById("cups_TODAY") : document.all.cups_TODAY;
var iDivCups_TODAY = 418274421.331042;

var vDivMins_TODAY = document.getElementById? document.getElementById("Mins_TODAY") : document.all.Mins_TODAY;
var iDivMins_TODAY = 2555785121.38699;

var vDivPounds_TODAY = document.getElementById? document.getElementById("pounds_TODAY") : document.all.pounds_TODAY;
var iDivPounds_TODAY = 19519056.5257176;

function UpdateNums_TODAY()
{
	iDivCal_TODAY = iDivCal_TODAY + (51.8816782407407);
	vDivCal_TODAY.innerHTML = '<span>Calories Burned: </span>' + addCommas_TODAY(parseInt(iDivCal_TODAY));

	iDivCups_TODAY = iDivCups_TODAY + (1.24286458333333);
	vDivCups_TODAY.innerHTML = '<span>Cups Water Drunk: </span>' + addCommas_TODAY(parseInt(iDivCups_TODAY));
	
	iDivMins_TODAY = iDivMins_TODAY + (9.32700231481481);
	vDivMins_TODAY.innerHTML = '<span>SparkAmerica Minutes: </span>' + addCommas_TODAY(parseInt(iDivMins_TODAY));

	iDivPounds_TODAY = iDivPounds_TODAY + (7.54456018518519E-02);
	vDivPounds_TODAY.innerHTML = '<span>Pounds Lost: </span>' + addCommas_TODAY(parseInt(iDivPounds_TODAY));
}

function addCommas_TODAY(nStr)
{
	nStr += '';
	x = nStr.split('.');
	x1 = x[0];
	x2 = x.length > 1 ? '.' + x[1] : '';
	var rgx = /(\d+)(\d{3})/;
	while (rgx.test(x1)) {
		x1 = x1.replace(rgx, '$1' + ',' + '$2');
	}
	return x1 + x2;
}
</script>
