<script>
	function setMHCookieValue(name, value, expires, path, domain) {
		var today = new Date();
	  	today.setTime( today.getTime() );
	  	if ( expires ) {
	   		expires = expires * 1000 * 60 * 60 * 24;
		}
	  	var expires_date = new Date( today.getTime() + (expires) );
	  
	  	document.cookie = name + "=" + value + ( ( expires ) ? ";expires=" + expires_date.toGMTString() : "" ) + 
		  ( ( path ) ? ";path=" + path : "" ) + 
	  	( ( domain ) ? ";domain=" + domain : "" );
	} 

	function getMHCookieValue(NameOfCookie) {
		if (document.cookie.length > 0) {
			begin = document.cookie.indexOf(NameOfCookie+"=");
			if (begin != -1) { // Note: != means "is not equal to"
				begin += NameOfCookie.length+1;
				end = document.cookie.indexOf(";", begin);
				if (end == -1) end = document.cookie.length;
				rawValue = unescape(document.cookie.substring(begin, end));
			   	return rawValue;
			}
		}
		return null;
	}
	
   	try
   	{
		var callBackURL = window.location.href;
    	setMHCookieValue('MHCURURL', callBackURL, 1, '/','.menshealth.com');
	}
	catch(e)
	{}
	
	
	function replaceAll(oldStr,findStr,repStr) {
		var srchNdx = 0;  // srchNdx will keep track of where in the whole line of oldStr are we searching.
		var newStr = "";  // newStr will hold the altered version of oldStr.
		while (oldStr.indexOf(findStr,srchNdx) != -1)  {
			newStr += oldStr.substring(srchNdx,oldStr.indexOf(findStr,srchNdx));
		    newStr += repStr;
		    srchNdx = (oldStr.indexOf(findStr,srchNdx) + findStr.length);
		}
		newStr += oldStr.substring(srchNdx,oldStr.length); 
		return newStr;
	}
	
	var HDCookie = getMHCookieValue("HD");
	var isCasSet = getMHCookieValue("CASSET");	

				
		/*
		 * If the call is being made from an IFRAME, we would not have access to the
		 * window href, so the calling page will pass in the current calling URL as a 
		 * parameter to the IFRAME. So if that is available, set that as the callback
		 * URL for login and logout.
		 * Otherwise set the window href as the callback URL
		 *
		 */
		var callBackURL2 = ''
		var queryString2 = window.location.search.substring(1);
		var vars2 = queryString2.split("&");
		for (var i=0;i<vars2.length;i++) {
			var pair2 = vars2[i].split("=");
			if (pair2[0] == 'currentCallingURL') {
				callBackURL2 =  pair2[1];
			}
		}				
		
		var lgCallBackURL2 = callBackURL2;

		try{
			if((logInLandingPage != null) && (logInLandingPage != ''))
			{
				callBackURL2 = logInLandingPage;
			}
		}
		catch(e)
		{}
		
		try{
			if((logOutLandingPage != null) && (logOutLandingPage != ''))
			{
				lgCallBackURL2 = logOutLandingPage;
			}
		}
		catch(e)
		{}

			if(isCasSet!=null)
			{
				setMHCookieValue('CASSET', '0', -1, '/','.menshealth.com');
			}
			/*if (HDCookie == null)
			{
					if(isCasSet=="1")
					{
						top.location.href='http://www.menshealth.com/cda/initLogin.do?gwyParam=Y&callback=' + callBackURL2;
					}
			}
			else
			{
					if(isCasSet=="2")
					{
						top.location.href='http://www.menshealth.com/cda/initLogout.do?callback=' + lgCallBackURL2;
					}
			}*/
	
</script>




	
	
