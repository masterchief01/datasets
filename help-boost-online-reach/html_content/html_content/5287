    var div = null;

    if ((typeof pageAds !== 'undefined') && (typeof pageAds.AMS_GMT_GLOBAL_HEADERCALLOUT !== 'undefined') && (CN.url.params('nojoy') != 1) ) {

        div = document.getElementById("AMS_GMT_GLOBAL_HEADERCALLOUT");
