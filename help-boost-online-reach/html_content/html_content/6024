CN.features.render({
    features : (function() {
        var
            i,
            feature = null,
            render = [],
            featuresOnPage = "thumbnail-grid black-bg small-thumbnail-grid  align-left-georgia-16 center-align   divider3 thumbnail-grid".replace(/\s{2,}/g, ' ').split(' '),
            len = featuresOnPage.length,
            cssClass;

        featuresOnPage = jQuery.grep(featuresOnPage,function(el,idx){
            return jQuery.inArray(el, featuresOnPage) === idx;
        });

        for(i = 0; i < len; i++){
            cssClass = featuresOnPage[i];
            if (CN.features.types.hasOwnProperty(cssClass)) {
                render.push(cssClass);
            }
        }

        return render;
    }())
});

