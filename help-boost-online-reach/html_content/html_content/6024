<script type="text/javascript">
    var _ghearst_vars = (function() {
        var metaKeyWords = '', metaCollection;
        metaCollection = document.getElementsByTagName('meta');
        for (i = 0; i < metaCollection.length; i++) {
            nameAttribute = metaCollection[i].name.search(/keywords/);
            if (nameAttribute != -1) {
                metaKeyWords = metaCollection[i].content;
                metaKeyWords = metaKeyWords.replace(/,/g, '%2C'); // to replace ,
                metaKeyWords = metaKeyWords.replace(/ /g, ''); // to replace space
            }
        }
        return {
            'ams_ads_script_src' : 'http://subscribe.vanityfair.com/ams/page-ads.js?ad_category_prefix=content&browser_path=%2Fcontent%2Fvanityfair%2Fculture%2Ffeatures%2F2011%2F08%2Felvis-kiss-concert-photographs.html&cat_prefixes=%2Ccontent%2Cvanityfair%2Cculture%2Cfeatures%2C2011%2C08%2Celvis-kiss-concert-photographs.html&keywords=' + metaKeyWords + '&position_list=AMS_VYF_GLOBAL_NAVBAR%2CAMS_VYF_GLOBAL_NAVBAR_ROLLOVER%2CAMS_VYF_MAGAZINE_SUBNAV%2CAMS_VYF_GLOBAL_HEADER%2CAMS_VYF_GLOBAL_HEADERCALLOUT%2CAMS_VYF_GLOBAL_ZRAIL%2CAMS_VYF_POPUP_ENTRY%2CAMS_VYF_POPUP_EXIT%2CAMS_VYF_SITE_FOOTER_SUBSCRIBE%2CAMS_VYF_GLOBAL_SLIDESHOW&site_prefix=vanityfair&subdomain=www&url_name=elvis-kiss-concert-photographs'
        };
    })();
    if (!window.$h) {
        window.$h = {}
    }
    (function() {
        if (!$h.util) {
            $h.util = {buildScriptTag: function(src) {
                if (!!src) {
                    document.write('<scr' + 'ipt src="' + src + '" language="javascript"></scr' + 'ipt>')
                }
            }}
        }
        $h.util.buildScriptTag(_ghearst_vars.ams_ads_script_src)
    })();

    /***************************************
     * Validate Registration URL
     * and Show Popup when validated success
     *
     */
    CN.validateRegistration = (function() {
        var url_Param_ecomUpsellURL, url_window = window.location.href;
        return {
            checkSuccessURL : function() {
                if (CN.url.params("ecomupsell", url_window) == "true") {
                    url_Param_ecomUpsellURL = decodeURIComponent(decodeURIComponent(CN.url.params("ecomupsellurl", url_window)));
                    url_Param_ecomUpsellURL = url_Param_ecomUpsellURL.replace(/^https*:\/\//g, window.location.protocol + "//");
                    window.open(url_Param_ecomUpsellURL, "PaymentPage", "menubar=1,status=0,resizable=0,toolbar=0,scrollbars=0,width=910,height=560,left=" + ((screen.width) / 3) + ",top=" + ((screen.height) / 3));
                }
            }
        };
    })();

    jQuery(document).ready(function() {
        CN.validateRegistration.checkSuccessURL();
    });

