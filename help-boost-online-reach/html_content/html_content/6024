<script>
    jQuery(document).ready(function () {
        if ((typeof pageAds !== 'undefined') && (typeof pageAds.AMS_VYF_POPUP_FLOATINGAD !== 'undefined') && (CN.url.params('nojoy') != 1)) {
            jQuery('#AMS_VYF_POPUP_FLOATINGAD').html('');
            jQuery('#AMS_VYF_POPUP_FLOATINGAD').html(pageAds.AMS_VYF_POPUP_FLOATINGAD);

            if ('AMS_VYF_POPUP_FLOATINGAD' === 'AMS_VYF_MAGAZINE_SUBNAV') {
                if (jQuery('#AMS_VYF_POPUP_FLOATINGAD').length > 0) {
                    jQuery('#mag_subnav_subscribe').parent().html(pageAds.AMS_VYF_POPUP_FLOATINGAD);
                    jQuery('#AMS_VYF_POPUP_FLOATINGAD').hide();
                }
            }
        } else {
             if ('AMS_VYF_POPUP_FLOATINGAD' === 'AMS_VYF_MAGAZINE_SUBNAV') {
                // For failsafe content.
                jQuery('#mag_subnav_subscribe')
                    .attr('href',   jQuery('#AMS_VYF_POPUP_FLOATINGAD').find('a').attr('href'))
                    .attr('target', jQuery('#AMS_VYF_POPUP_FLOATINGAD').find('a').attr('target'))
                    .attr('title',  jQuery('#AMS_VYF_POPUP_FLOATINGAD').find('a').attr('title'));
            }
        }
    });
</script>
