							updateAdWrapperPosition()
						})
						updateAdWrapperPosition()
					});
			
					function updateAdWrapperPosition(){
						$y = $(window).scrollTop();
						if($y > $toolbar_height){
							$adWrapper.addClass('adWrapperFixed');
						} else {
							$adWrapper.removeClass('adWrapperFixed');
						}
					}
