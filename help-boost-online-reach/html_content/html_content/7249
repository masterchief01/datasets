		upi_pt_c('/story/photos/fill/UPI-79011325055631/'+o+'/', '', 'story_w_photos', uid);
		jQuery.ajax({
			type: 'GET', cache: false, 
			url: '/story/photos/fill/UPI-79011325055631/'+o+'/', 
			dataType: 'json', 
			success: function(d) {
				var sp_pn = jQuery('#sp .pn');
				var sp_photo = jQuery('#sp .photo');
				var sp_cap = jQuery('#sp .caption');
				sp_pn.html(d.pn);
				sp_photo.fadeOut('fast', function() {sp_photo.html(d.photo);}).fadeIn('fast');
				sp_cap.fadeOut('fast', function() {sp_cap.html(d.caption);}).fadeIn('fast');
			}
		});
	}
