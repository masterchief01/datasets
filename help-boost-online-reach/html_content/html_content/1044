function openMenu( el ) {

var blockedBrowser = 0;

var browserName=navigator.appName;
var browserVersion=navigator.appVersion; 

//disabling Opera 8 for Mac (Engine: Macintosh MSIE)
if ((browserName.indexOf("Explorer") != -1) && (browserVersion.indexOf("Macintosh") != -1)) {
	//alert ("MSIE");
	var blockedBrowser = 1;
}

//disabling Opera 9
else if (browserName.indexOf("Opera") != -1) {
	//alert ("Opera");
	var blockedBrowser = 1;
}

//disabling Safari
else if (browserVersion.indexOf("Safari") != -1) {
	//alert ("Safari");
	var blockedBrowser = 1;
}

if (blockedBrowser == 0) {

	for ( var i = 0; i < el.childNodes.length; i++ ) {
	    if ( el.childNodes[i] && el.childNodes[i].className == 'popupmenu' )
	    {
		el.childNodes[i].style.display = 'block';
	    }
	}
} else {
	alert ("This feature is not supported by your browser.\r\nPlease use a new version of Firefox, Mozilla, Netscape, or Microsoft Internet Explorer.\r\nWe apologize for the inconvenience.");
}

}

function closeMenu( el ) {
	for ( var i = 0; i < el.childNodes.length; i++ ) {
	    if ( el.childNodes[i] && el.childNodes[i].className == 'popupmenu')
	    {
		el.childNodes[i].style.display = 'none';
	    }
	}
}
