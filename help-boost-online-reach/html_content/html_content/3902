			   title  : "Brownie bonanza: A collection of brownies, blondies, and bars",
			   share_url_transforms : { add : { cmpid : 'addthis_{{code}}'} } 
			   };
		</script>
		<div id="storyToolbar" class="storyToolbar">
			
		
					<div id="sTul_0" class="addthis_toolbox addthis_default_style addthis_32x32_style">
				<a style="width:88px;"class="addthis_button_facebook_like" fb:like:layout="box_count" fb:like:action="recommend" fb:like:width="80" fb:like:show_faces="false"></a>
				<a class="addthis_button_tweet" tw:count="vertical" tw:via="CSMonitor" tw:counturl="http://www.csmonitor.com/The-Culture/Food/2012/1015/Brownie-bonanza-12-recipes-of-goodness/White-and-dark-chocolate-chip-blondies"></a>
				<a class="addthis_button_google_plusone" g:plusone:size="tall"></a>
								<a id="tStumbleUpon_0" class="addthis_button_stumbleupon"></a>
				<a id="tEmail_0" class="addthis_button_email"></a>
									<a class="addthis_default_style at300m" href="/layout/set/print/The-Culture/Food/2012/1015/Brownie-bonanza-12-recipes-of-goodness/Kentucky-Derby-bars"><img src="http://cache.addthiscdn.com/icons/v1/thumbs/32x32/print.png" width="32" height="32" /></a>
							</div>
				
		
					
