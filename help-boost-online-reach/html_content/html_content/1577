<script type="text/javascript">
    CN.config.set({
        hasIntro   : false,
        playTimer  : 5,
        transition : 'fade',
        contentId  : 2000000001294811,
        viewAll    : true,
        carousel   : false,
        imagelist  : false,
        viewLarger : false
    });

            // Adds classes for rendering based on intro and hash state
    (function() {
        var fragment = CN.url.getFragment(),
            slide    = CN.url.params().slide;

                // For page rendering purposes, we have a visible intro if we hit the page without a fragment id
        if (CN.page.config.hasIntro === true && ((fragment === false || fragment === 'intro') && !slide)) {
            jQuery('html').addClass('slideshow-hasintro-true');
        }

                // If DCT turns off intro page, we show intro if it exists
        if (CN.page.config.hasIntro === false) {
            jQuery('html').addClass('slideshow-showintro-true');
        }

                // Showing the initial JSP-provided slide depends on whether the URL agrees with the JSP provided slide
        if (fragment === false || fragment === 'slide=1' || fragment === 'intro' || (fragment === false && slide)) {
            jQuery('html').addClass('slideshow-showinitial-true');
        } else {
            jQuery('html').addClass('slideshow-showinitial-false');
        }
    })();
</script>



    


