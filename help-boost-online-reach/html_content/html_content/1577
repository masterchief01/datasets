function filterMostViewed() {
    // Filter out the Miley Cyrus Timeline
    jQuery('#most-viewed').find('a').each(function(){
        if(this.href.indexOf('miley-cyrus-fashion-timeline') !== -1) {
            jQuery(this).closest('li').remove();
        }
    });
    CN.debug.info('Most viewed filtering complete');
}
