    if ((typeof pageAds !== 'undefined') && (typeof pageAds.AMS_TNV_CONTENTPAGE_ZRAIL !== 'undefined') && (CN.url.params('nojoy') != 1) ) {

        div = document.getElementById("AMS_TNV_CONTENTPAGE_ZRAIL");
        div.innerHTML = "";

        
        jQuery("#AMS_TNV_CONTENTPAGE_ZRAIL").html(pageAds.AMS_TNV_CONTENTPAGE_ZRAIL.replace(/document.write\(.*\)/gi,"/*filtered by amg-magnet:document.write(...)*/"));
        jQuery(div).css({
            visibility: 'visible'
        });
    }

    
});

</script>



        </div>
    


        
        
    

    </div>

    
        

