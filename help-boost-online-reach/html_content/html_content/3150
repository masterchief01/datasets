    <div class="poll-question" id="poll-error">
        <h3>Please Login</h3>
        <p>You must <a href="javascript:void(0)" onclick="return mtSignInOnClick('signin-widget-content')">log in</a> before you can answer the poll. If you don't have an account, you can <a href="http://slice.seriouseats.com/cgi-bin/mte/mt-cp.cgi?__mode=register&blog_id=25&return_to=http%3A%2F%2Fwww.seriouseats.com%2Fwelcome%2F">sign up</a>. It's free and it only takes a moment.</p> 
    </div>
    <div class="poll-question" id="poll-results-container">
        <h3>You already answered this poll.</h3>
        <p></p>
        <ul class="poll-results">

        </ul>
        
        <div class="return-link">Back to <a href="http://slice.seriouseats.com/archives/2010/05/do-you-eat-hawaiian-pineapple-on-a-pizza.html">'Slice' Poll: Pineapple Pizza - Way or No Way?</a></div>
    </div>
    <input class="poll-submit" type="submit" value="Submit" />
  </form>

<script>
    $.ajaxSetup({cache:false});
    // they can only answer a poll if they're logged in
    var u = mtGetUser();
    if (!u || !u.is_authenticated) {
        // show the loading div
        $('#poll-loading').show();
        // check to see if the poll is closed
        $.getJSON('http://slice.seriouseats.com/cgi-bin/mte/mt-cp.cgi?__mode=check_if_taken&question_id=332&callback_name=closed_cb');
    } else {
        // show the loading div
        $('#poll-loading').show();
        // check to see if they've taken this poll
        $.getJSON('http://slice.seriouseats.com/cgi-bin/mte/mt-cp.cgi?__mode=check_if_taken&question_id=332&callback_name=taken_cb');
    }
    
    $('#view-results-link').click(function(){
        $('#poll-results-container h3').text('Results');
        $('#poll-results-container').slideToggle('fast');
        $('#view-results-link span').toggle();
    });   
    
    function closed_cb(res) {
        $('#poll-loading').hide();
        if (res.closed) {
            if (res.current_answers){
                $.each(res.current_answers, function(key,value) {
                    var barValue = value * 2;
                    barValue = parseInt(barValue);
                    $('.poll-results').append('<li><span class="label">' + key + '</span><div class="ratings-container-200"><div class="ratings-bar" style="width: ' + barValue + 'px;"></div><div class="ratings-value">' + value + '%</div></div></li>')
                });            
            }
            $('#poll-results-container').show();
            $('#poll-results-container h3').text('This poll is closed.');
            $('#poll-results-container p').text( res.num_takers + ' people took this poll. Here are the final results:');
        } else {
            $('#poll-error').show();
        }
    }
    
    function taken_cb(res) {
        $('#poll-results-container p').text(res.num_takers + ' people have taken this poll. Here are the results so far:');
        $('#poll-loading').hide();
        if (res.current_answers){
            $.each(res.current_answers, function(key,value) {
                var barValue = value * 2;
                barValue = parseInt(barValue);
                $('.poll-results').append('<li><span class="label">' + key + '</span><div class="ratings-container-200"><div class="ratings-bar" style="width: ' + barValue + 'px;"></div><div class="ratings-value">' + value + '%</div></div></li>')
            });            
        }
        if (res.closed) {
                $('#poll-results-container').show();
                $('#poll-results-container h3').text('This poll is closed.');
                $('#poll-results-container p').text( res.num_takers + ' people took this poll. Here are the final results:');
        } else {
            if (res.taken) {
                $('#poll-results-container').show();
                $('#poll-results-container p').text(res.num_takers + ' people have taken this poll. Here are the results so far:');
            } else {
                $('.poll-container div:first').show();
                $('.poll-submit').show();
            }
        }
    }
</script>


	



