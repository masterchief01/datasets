<script type="text/javascript">
$(function(){
	$("div#header input.txt-subscribe").defaultvalue("Email Address");
	$("div#footer input.txt-subscribe").defaultvalue("Email Address");
	$('a.folding-nav-header').bind('click',function(){
		$this = $(this).next();
		if(!$this.hasClass('active')){
			$('ul.folding-nav:not(#'+$this.attr("id")+')').slideUp(400,function(){
				$(this).removeClass('active');
				if(!$this.hasClass('active')){
					$this.addClass('active').slideDown();
				}
			});
		}
		return false;
	});
});
</script>
