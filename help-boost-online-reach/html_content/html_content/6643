        <div id="fb-root"></div>
        <script>
        window.fbAsyncInit = function() {
            FB.init({
            appId : "133109973370101",
            authResponse : 0,
            status : true, // check login status
            oauth  : true, // enable OAuth 2.0
            cookie : true, // enable cookies to allow the server to access the session
            xfbml : true, // parse XFBML
            channelUrl: 'http://www.justapinch.com/xd_receiver.html'
            });
            

            if($.browser.msie){
                //FB.UIServer.setLoadedNode:  function (a, b){FB.UIServer._loadedNodes[a.id] = b;if(a.params)b.fbCallID=a.id;}
                FB.UIServer.setLoadedNode = function (a, b){FB.UIServer._loadedNodes[a.id] = b;} // Hack for IE8
            }
            FB.Event.subscribe("edge.create", function(href, widget) {
                _gaq.push(["_trackEvent", "Facebook", "like", href]);
            });

        };

        var fb_login = function(){
            FB.login(function(response) {
                if (response.authResponse) {
                    // add some google event tracking here.
                    parent.window.location = "/member/login/rpx/";
                    $('#fb-container').hide();
                    $('#fb-loader').show();
                }
            },{
                scope: "user_about_me,email,user_hometown,user_location"
            });
        };

        var fb_logout = function(){
            FB.logout(function(response) {
            });
        };

        var fb_dialog = function(options) {
            FB.ui(
                {
                    method: "stream.publish",
                    message: options.message,
                    link: options.url,
                    picture: options.picture
                }, function(response) {
                    if (response && response.post_id) {
                        $("#fb-ui-flash").hide();
                        // add some google event tracking here.
                        _gaq.push(["_trackEvent","Facebook", "Share success", options.url]);
                    } else {
                        _gaq.push(["_trackEvent","Facebook", "Share declined", options.url]);
                    }
                }
            );
        };

        var fb_isAuth = function() {
            FB.ui({
                method:"auth.status",
                display:"hidden"
            }, function(obj) {
                if (obj.scope) {
                    return true;
                } else {
                    return false;
                }
            });
        }
        
        $(".fb_a").click(function(){
            $('#fb-container').hide();
            $('#fb-loader').show();
        });

        (function() {
            var e = document.createElement("script");
            e.src = document.location.protocol + "//connect.facebook.net/en_US/all.js";
            e.async = true;
            document.getElementById("fb-root").appendChild(e);
        }());
        </script>
        

