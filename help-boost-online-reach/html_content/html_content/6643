<script type="text/javascript">
$(document).ready(function(){

	$('#comment-label').append('<a href="#" class="add_recipe_url right" field="comment" style="text-decoration:none;"><img src="http://justapinchassets.com/images/icons/link.png" style="width:16px;height:16px;vertical-align:middle" /> Insert Recipe Link</a><br class="clear" />');

	build_tooltips();

	$('#comment-form').ajaxForm({
		dataType:'html',
        error: function (xhr, textStatus, errorThrown) {
            $("#comment-form :submit").removeAttr("disabled");
            alert('Oops! There was a problem posting your comment. Please try again.');
            errorTrap('comment: ' + textStatus + '|' + errorThrown, '', 0);
        },
		success: loadComment,
		beforeSubmit: preprocess
	});

	function preprocess(formData, jqForm, options) {
		$("#comment-form :submit").attr("disabled", "true");
		return true;
	}

	function loadComment(data) {
		//scrollToTag('#comments_target');
		//$('#comment-posts').prepend('<div id="slideIn" style="display:none">'+data+'</div>');
		var random = new Date().getTime();
		$('#comment-posts').append('<div id="slideIn'+random+'" style="display:none">'+data+'</div>');
		$('#slideIn'+random).show();//slideDown('slow');
					$('#comment-form').hide();
			$('#rate-recipe-comment').remove();
			$('#comment-posts').append('<div id="rate-recipe-comment" class="group_discussion_list_item"><span class="loading-16"></span></div>');
			$.ajax({
				url: '/recipes/cook/26957/rate?a=1&format=json',
				success: function(data) {
					$('#rate-recipe-comment').html(data.html);
				}
			});
				//resetForm('comment-form');
		$('#comment-form #comment').val('');
		$('#comment-form :submit').removeAttr('disabled');
		$('#comment-subscribe-container').hide();
		$('input[type=radio].star').rating();
		build_tooltips();

	}

	$('input[type=radio].star').rating();

    $(document).delegate('.subscribe-to-comment', 'click',function() {
		var this_element = $(this);
		this_element.html('<span class="loading-16">&nbsp;</span>');
		$.ajax({
			data: {'object_type' : '1',
					'user_id':'-1',
					'object_id':'26957'},
			url: '/comments/subscribe/format/json',
			success: function(data){
				if(data.success) {
					$('#comment-subscribe-container').html('<div>You have successfully subscribed and will be notified when new comments are made.</div>');
					$('#subscribe-toggle').hide();
					//this_element.replaceWith('<div>You will be notified when comments are made.</div>');
				} else {
					$('#comment-subscribe-container').html('<div>Error.</div>');
					//this_element.replaceWith('<div>Error.</div>');
				}
			}
		});
		return false;
	});

    $(document).delegate('.unsubscribe-to-comment', 'click',function(){
		var this_element = $(this);
		this_element.html('<span class="loading-16">&nbsp;</span>');
		$.ajax({
			data: {'object_type' : '1',
				   'user_id' : '-1',
				   'object_id' : '26957'},
			url: '/comments/unsubscribe/format/json',
			success: function(data){
				if(data.success) {
					$('#comment-subscribe-container').html('<div>You have successfully unsubscribed from this notification.</div>');
					$('#subscribe-toggle').show();
					$('#comment-form #subscribe').attr('checked',false);
					//this_element.replaceWith('<div>Notification has been cancelled.</div>');
				} else {
					$('#comment-subscribe-container').html('<div>Error.</div>');
					//this_element.replaceWith('<div>Error.</div>');
				}
			}
		});
		return false;
	});

});
</script>





<script type="text/javascript">

    
    $(document).ready(function(){

        /*
