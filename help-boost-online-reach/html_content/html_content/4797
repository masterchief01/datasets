<script type="text/javascript">
	var prepop = '';
	if(!location.href.match('news/careers'))
	{
		if(HPConfig.current_vertical_name == 'business') prepop = 'General Business ';
		else if(HPConfig.current_vertical_name == 'college') prepop = 'Entry Level';
		else if(HPConfig.current_vertical_name == 'politics') prepop = 'Government';
		else if(HPConfig.current_vertical_name == 'media') prepop = 'Broadcast/Journalism';
		else if(HPConfig.current_vertical_name == 'education') prepop = 'Education';
		else if(HPConfig.current_vertical_name == 'sports') prepop = 'Marketing';
		else if(HPConfig.current_vertical_name == 'technology') prepop = 'Information Technology';
		document.getElementById('careerbuilder_widget_keywords').value = prepop;
	}
</script><!--

		
