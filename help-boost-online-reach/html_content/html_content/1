	function posttimeline_setupbuttons() {
		jQuery("#the_timeline li.prev_link a,#the_timeline li.next_link a").unbind('click').click(function(){
			jQuery.get('http://theberry.com/wp-admin/admin-ajax.php', {
					action: 'posttimeline',
					date: jQuery(this).attr("rel")
				}, function(data){
					jQuery("#the_timeline").html( data );
				});
			return false;
		});
	}
		posttimeline_setupbuttons();
