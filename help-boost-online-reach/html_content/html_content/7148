<script src="http://aht.seriouseats.com/archives/2011/05/the-burger-lab-building-a-better-big-mac-comments.js?20120303064826" type="text/javascript"></script>

<script type="text/javascript">
  	var commentsJSLoaded = false;
  	var commentsDisplayed = false;
  	var commentCount = '';
  	var commentCountText = '';
  	var intervalID = 0;
  	var numCommentsPerPage = 100;
  	//100;

  	// For printing comments with Recipe posts   
  	var recipeCommentsToInclude = new Array(); 
  	var jumpToTopOfComments = true;
    
  	if (numComments > 0) {
		commentCount = numComments + " Comment";
        if (numComments > 1) {
              commentCount += "s";
        }
   	} else {
        commentCount = "Post a Comment";
   	}
	  

      commentCountText = '<a href="http://aht.seriouseats.com/archives/2011/05/the-burger-lab-building-a-better-big-mac.html#comments"  class="commentCount">';
      commentCountText += commentCount + '</a>';               



      commentsJSLoaded = true;
	  
	  intervalID = setInterval("showCommentSection()", 1000);
  
function showCommentSection()
{
	if (!commentsDisplayed)
	{
		if (commentsJSLoaded && ($('#commentSection').length) && ($('#postFooterCommentCount').length))  {
			$("#postFooterCommentCount").html(commentCountText);
			if($(".entryCommentCount").length > 0) {
				$(".entryCommentCount").each(function() {
				 	$(this).html(commentCount);
				});
			}	
	   
			if (numComments <= numCommentsPerPage) {
				showComments(0, numComments);
			} else {
				jumpToTopOfComments = false;
	
				$(".pagination").pagination(numComments, {
					items_per_page:numCommentsPerPage,
					num_edge_entries:1,
					num_display_entries:9,
					next_show_always:false,
					prev_show_always:false,
					link_to:"#comments",
					callback:handlePaginationClick
				});
				
				jumpToTopOfComments = true;
			}
			commentsDisplayed = true;
		} 
	} else {
		clearInterval(intervalID);
	} 
}
  
function showComments(startNum, endNum)
{
    var commentContent = '';
	var regex = new RegExp("\n", "g"); 

    for (var i = startNum;i < endNum;i++) {
        commentContent += '<a name="' + commentJSON[i]["comment_id"] + '"></a>';
    	if (i%2) {
    		commentContent += '<div class="comment yellow">';
    	} else {
    		commentContent += '<div class="comment green">';
    	}
        commentContent += '<div class="comment-body">';
        
                commentContent += '<p>' + commentJSON[i]["text"].replace(regex, "<br />") + '</p><div class="comment-body-bottom"></div></div><p class="commenter postFooter"><a href="http://www.seriouseats.com/user/profile/' + commentJSON[i]["author_username"] + '">' + decodeURIComponent(escape(commentJSON[i]["author_nickname"])) + '</a> at <a href="http://aht.seriouseats.com/archives/2011/05/the-burger-lab-building-a-better-big-mac.html#' + commentJSON[i]["comment_id"] + '">';
        
 		
 		var date = commentJSON[i]["created_on"];
 		
 		var commentDate = new Date(date.substr(0,4), date.substr(4,2)-1, date.substr(6,2), date.substr(8,2), date.substr(10,2), date.substr(12,2));
 		
 		commentContent += commentDate.toString("h:mmtt on MM/dd/yy") + '</a></p></div>'; 		
    }
	
    $('#commentSection').html(commentContent);
	
	// Recipe post - checked comments for printing
	$('input[name$="include-comment"]').change(function() {
		if ($(this).is(':checked')) { 
			var arrayIndex = (jQuery.inArray($(this).attr('value'), recipeCommentsToInclude));
			if (arrayIndex < 0) {
				recipeCommentsToInclude.push($(this).attr('value'));
				comments_list = recipeCommentsToInclude.join();
			}
		}
		else {
			var arrayIndex = (jQuery.inArray($(this).attr('value'), recipeCommentsToInclude));
			if (arrayIndex >= 0) {
				recipeCommentsToInclude.splice(arrayIndex,1);
				comments_list = recipeCommentsToInclude.join();
			}
		}
	});
}

function handlePaginationClick(page_index, pagination_container) {
    var max_num = Math.min((page_index+1) * numCommentsPerPage, numComments);
    var commentRange = '';
    if ((max_num < numComments) || (max_num > numCommentsPerPage)) {
         commentRange += "(" + (page_index*numCommentsPerPage + 1) + " - " + max_num + ") of ";
    }

    $('#commentRange').html(commentRange);

	showComments(page_index*numCommentsPerPage, max_num);
    
	if (jumpToTopOfComments) {
    	document.getElementById('comments').scrollIntoView();
	}
    return false;
}

$(document).ready(function()
{
	if (!commentsDisplayed)
	{
		showCommentSection();
	}
});
</script>


