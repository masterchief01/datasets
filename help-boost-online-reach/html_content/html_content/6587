   jQuery(this).addClass('section-hover');
    }, function() {
    jQuery(this).removeClass('section-hover');
    });
    
    (function($) {
		$(document).ready(function() {
	    	$('.hot-links-title. .hot-links-title a').mouseover(function() {
				var $curr = $(this);
				$curr = $curr.next();
	    		$curr.css('opacity', '0.5');
	    	}).mouseout(function() {
	    		$('.hot-links-image').css('opacity', '1');
	    	});
	    });
	})(jQuery);
