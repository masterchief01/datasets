		$("#aux_results").addClass("autoComplete_top_result");
	});
	$("#gen_search_bottom").click(function() {		
		setAutoComplete("gen_search_bottom", "aux_results", "/ramp/autocomplete/realbeauty?q=");
		setIsTypingClass();
	});
	$("#aux_search_top").click(function() {		
		setAutoComplete("aux_search_top", "aux_results", "/ramp/autocomplete/realbeauty?q=");
		setIsTypingClass();
	});
	$("#aux_search_bottom").click(function() {		
		setAutoComplete("aux_search_bottom", "aux_results", "/ramp/autocomplete/realbeauty?q=");
		setIsTypingClass();
	});
	
	function setIsTypingClass(){
		$(".pagination").attr("autoComplete","isTyping");
		$("#aux_results").removeClass("autoComplete_top_result");
	}

});

</script>

