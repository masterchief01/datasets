<script type="text/javascript">
	$(document).ready(
		function() {
		
			var cookie_impressions= parseInt($.cookie("NewsweekSubscriptionsOffer-Count"));
			if ( !cookie_impressions ) { cookie_impressions= 0; }
			
			var cookie_last= $.cookie("NewsweekSubscriptionsOffer-Last");
			
			if ( ( cookie_impressions < 6 ) && !cookie_last ) {
				$.cookie("NewsweekSubscriptionsOffer-Count", (cookie_impressions + 1), { path: '/', expires: 365 } );
				$.cookie("NewsweekSubscriptionsOffer-Last", 1, { path: '/', expires: 7 } );

				$('.overlay_newsweek_close').bind('click', function(e){$('#overlay_newsweek_container').hide()})
				$('#overlay_newsweek_content_inner').html('<a href="http:\/\/ad.doubleclick.net\/jump\/5480.iac.thedailybeast\/blogs_stories\/article;s1=;url=;kw=;db=ad;pos=sub;tile=1;sz=702x493;ord=123456789?"; target="_blank"><img src="http:\/\/ad.doubleclick.net\/ad\/5480.iac.thedailybeast\/blogs_stories\/article;s1=;url=;kw=;db=ad;pos=sub;tile=1;sz=702x493;ord=123456789?"; width="702" height="493" border="0" alt=""><\/a>');
				$('#overlay_newsweek_container').show();
			}
		}
	);
</script>
