<script type="text/javascript">
		function clearForm()
		{
		document.forms.dealsearch.query.value = '';
		}
		function formURL()
		{
			var destVar = '';
			var urlValVar = "http://travel.aol.com/travel-deals/";
			var selectvar = '';
			if(document.getElementById('dest_interest').value != 'Enter Destination' && document.getElementById('dest_interest').value != "" && document.getElementById('dest_interest').value != null)
			{
				destVar = document.getElementById('dest_interest').value;
				var destVarVal = destVar.replace(" ", "+");
				urlValVar = urlValVar + destVarVal;
			}
			var optObj = document.getElementById('categorydropdown').options;
			for(i=0;i<optObj.length;i++)
			{
				if(optObj[i].selected == true)
				{
					if(i == 0)
					{
						urlValVar = urlValVar + "-all";
						selectvar = " All";
					}
					if(i == 1)
					{
						urlValVar = urlValVar + "-Flight";
						selectvar = " Flight";
					}
					if(i == 2)
					{
						urlValVar = urlValVar + "-Hotel";
						selectvar = " Hotel";
					}
					if(i == 3)
					{
						urlValVar = urlValVar + "-Vacation";
						selectvar = " Vacation";
					}
					if(i == 4)
					{
						urlValVar = urlValVar + "-Cruise";
						selectvar = " Cruise";
					}
					if(i == 5)
					{
						urlValVar = urlValVar + "-Car";
						selectvar = " Car Rentals";
					}
				}
			}
			urlValVar = urlValVar + "--deals";
			document.dealsearch.action = urlValVar;
			document.dealsearch.submit();
		}
	</script><div class="comments" id="comments"><!-- <a id="cmt_sub_link" href="comments.xml">Subscribe to these comments</a> --> <h2 id="readercomments">Reader Comments <span>(Page 1 of 2)</span></h2><div class="commentlinks" id="commentlinks10015343-936680"><div class="commentclass1  level2 parent" id="c10015343"><span class="cmt_tools" id="tools_10015343"><a href="#" class="voteLink" id="vu10015343" onclick="voteComment('10015343','up'); return false;" title="Vote This Comment Up"><img src="/media/thumb_up.png" alt="vote up" /></a> <a href="#" class="voteLink" id="vd10015343" onclick="voteComment('10015343','down'); return false;" title="Vote This Comment Down"><img src="/media/thumb_down.png" alt="vote down" /></a><a href="#" class="reportLink" id="r10015343" onclick="reportComment(10015343); return false" title="Report This Comment"><img src="http://www.blogsmithmedia.com/www.gadling.com/media/btn-report.gif" alt="Report" /></a></span><span class="cmt_label label_level2"><img src="http://www.gadling.com/media/stars2.gif" alt="2 hearts" /></span><div class="avatar"><a href="http://www.gadling.com/profile/936680/"><img src="http://www.blogsmithcdn.com/avatar/images/13/936680_64.jpg" alt="Faz" /></a></div><h4><a href="http://www.gadling.com/profile/936680/">Faz</a> <span class="cmt_time"> <a href="./comments/10015343/">Jan 23rd 2008 8:54PM</a></span></h4><p class="cmt_contents">I want one!</p><div class="cmt_reply_show"><a href="#commentform" onclick="replyToCmt('10015343','Faz');">Reply</a></div></div></div><div class="commentlinks" id="commentlinks10100146-1721916"><div class="commentclass2  level2 child" id="c10100146"><span class="cmt_tools" id="tools_10100146"><a href="#" class="voteLink" id="vu10100146" onclick="voteComment('10100146','up'); return false;" title="Vote This Comment Up"><img src="/media/thumb_up.png" alt="vote up" /></a> <a href="#" class="voteLink" id="vd10100146" onclick="voteComment('10100146','down'); return false;" title="Vote This Comment 