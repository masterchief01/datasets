    var VideoPlayer1089201380 = {
      content: null,
      exp: null,   
      player: null,
      social: null,
      video: null,
      firstTime: true,
      localID: "1089201380",
      init: function() {
        jQuery(document).bind('videoTemplateLoaded', function(e, data) {
          if ((data.id.indexOf(VideoPlayer1089201380.localID) !== -1) && (VideoPlayer1089201380.firstTime === true)) {
            VideoPlayer1089201380.firstTime = false;
            VideoPlayer1089201380.registerEvents(data.id);
            VideoPlayer1089201380.constructShareLink();
            return false;
          }
        });
        return false;
      },
      registerEvents: function(id) {
        VideoPlayer1089201380.player  = brightcove.getExperience(id);
        VideoPlayer1089201380.content = VideoPlayer1089201380.player.getModule(APIModules.CONTENT);
        VideoPlayer1089201380.exp     = VideoPlayer1089201380.player.getModule(APIModules.EXPERIENCE);
        VideoPlayer1089201380.video   = VideoPlayer1089201380.player.getModule(APIModules.VIDEO_PLAYER);
        VideoPlayer1089201380.social  = VideoPlayer1089201380.player.getModule(APIModules.SOCIAL);
        VideoPlayer1089201380.video.addEventListener(BCMediaEvent.CHANGE, VideoPlayer1089201380.onMediaChange);
        
        return id;
      },
      onMediaChange: function(e) {
        VideoPlayer1089201380.constructShareLink();
        return false;
      },
      constructShareLink: function() {
        var playlistItems = VideoPlayer1089201380.content.getAllMediaCollections("playlist");
        var playlistItemsLength = playlistItems.length;
        var currentVideo = VideoPlayer1089201380.video.getCurrentVideo();
        var title = currentVideo.displayName.replace(/\s/g, "-");
        var id = currentVideo.referenceId;
        var bc_id = currentVideo.id.toString();
        var classification = currentVideo.customFields.classification;
        var rootPath = "http://www.sun-sentinel.com/";   
        var match = false;
        var currentPlaylist = "";
        var shareLink = rootPath + "videogallery/" + id + "/" + classification + "/" + title;
        
        for (var i = 0; i < playlistItemsLength; i++) {
          var mediaIDs = playlistItems[i].mediaIds;
          var mediaIDsLength = playlistItems[i].mediaIds.length;
          
          for (var n = 0; n < mediaIDsLength; n++) {
            match = (bc_id === mediaIDs[n].toString());
            currentPlaylist = playlistItems[i].referenceId;
            if (match) { break; }
          }
          
          if (match) { break; }
        }
        
        if (currentPlaylist !== "") {
          shareLink = shareLink + "#pl-" + currentPlaylist;
        }
        
        VideoPlayer1089201380.social.setLink(shareLink);
        
        return currentVideo;
      }
    };
    
    VideoPlayer1089201380.init();
    
