    // Check whether the user is logged in.
    var loggedIn = false;
    if (window["FD"]) {
        loggedIn = FD.isLoggedIn();
    }
    if (loggedIn) {
        _gaq.push(['_setCustomVar', 5, 'Login Status', 'Logged In', 2]);
    } else {
        _gaq.push(['_setCustomVar', 5, 'Login Status', ' ', 2]);
    }
    _gaq.push(['_trackPageview']);
