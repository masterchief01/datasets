<div class="clear fL" id="pushdownAdWrapper" style="width:970px; padding-left:10px;">
    <input type="hidden" value="" id="quizPushDownAd" />
    <input type="hidden" value="" id="articlePushDownAd" />
    <input type="hidden" value="false" id="matchItem" />
    <input type="hidden" value="false" id="isValidDate" />
    
    <script type="text/javascript" language="javascript">
      function ShowPushDownAd(page) {
        var isArticlePage = false;
        var articlePages = "";
        var isQuizPage = false;
        var quizPages = "";

        var isValidDate = false;
        var matchItem = false;
        var maxViewPerVisit = "0";
        maxViewPerVisit = maxViewPerVisit * 1;
        var visitDuration = "0";

        var isChannelPage = true;
        if (isArticlePage || isQuizPage)
            isChannelPage = false;
        
        if (!isValidDate)
            return false;
        else if (!matchItem)
            return false;

        var cookieVal = fnGetAlphaCookie("lsPushDownAd");

        if (!fnCookieThere("lsPushDownAd") || (cookieVal != null && cookieVal > 0)) {
            if (cookieVal == null)
                cookieVal = maxViewPerVisit;

            if (isChannelPage) {
                fnCreateNewCookieV2("lsPushDownAd", cookieVal - 1, visitDuration);
                return true;
            }
            else if (isArticlePage) {
                var articlePages = articlePages.split(",");
                for (var i = 0; i < articlePages.length; i++) {
                    if (articlePages[i] == page) {
                        fnCreateNewCookieV2("lsPushDownAd", cookieVal - 1, visitDuration);
                        return true;
                    }
                }
               
            }
            else if (isQuizPage) {
            var quizPages = quizPages.split(",");
                for (var i = 0; i < quizPages.length; i++) {
                    if (quizPages[i] == page) {
                        fnCreateNewCookieV2("lsPushDownAd", cookieVal - 1, visitDuration);
                        return true;
                    }
                }
            }
        }
        return false;
    }
  
 </script>
    <div id="pushdownAdContainer" class="fL" style="padding-bottom: 30px; *padding-bottom: 10px;">
      <script type="text/javascript" language="javascript">
          var page;
          if ($.query.get('page') != "")
              page = $.query.get('page');
          else if ($.query.get('Page') != "")
              page = $.query.get('Page');
          else
              page = 1;

          if (ShowPushDownAd(page)) {
              try {
                  var javascriptSrc = GetAd('15', 'pushdown', gDartTag, gContexts, gCampaigns, gAdFlag, '', '0', '0', 'pushdownAdContainer');
                  document.write('<scr' + 'ipt type="text/javascript" language="javascript" src="' + javascriptSrc + '"></scr' + 'ipt>');
              } catch (e) { }
              document.getElementById("pushdownAdWrapper").style.display = "block";
          }
          else {
              document.getElementById("pushdownAdWrapper").style.display = "none";
          }
      </script>
    </div>
</div>
           </div>
        </div>
         
