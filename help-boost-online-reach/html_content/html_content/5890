    var pnlQuizSponsored = "content_0_QuizSponsored";
    var showSponsoredContent = ""

    var cPage = getURLParam("Page");
    if (cPage == "" || cPage == null)
        cPage = "1";
    
    if ( parseInt(gQuizCount) + 1 == parseInt(cPage))
    {
        if (showSponsoredContent == "show") {
            document.getElementById(pnlQuizSponsored).className = "QuizSponsoredBox hide";
        }
    }
   
    if ( parseInt(gQuizCount) + 2 == parseInt(cPage))
    {
        if (showSponsoredContent == "show") {
            document.getElementById(pnlQuizSponsored).className = "QuizSponsoredBox hide";
        }
    }

    function SeeQuizAnswerResults()
    {
        var quizItemID = "{9CB46A02-A0F3-4DC3-9FB6-5215E92DB3F2}";
         var quizTitle = document.getElementById("content_0_lblQuizTopic").firstChild.nodeValue;
         var quizDescElm = document.getElementById("content_0_lblQuizDescription");
         
         if(quizDescElm)
            quizDescElm.style.display="none";

        var answerName = "";
        var answerIndex = 0;
         var rb1 = document.getElementById("content_0_rbAnswer1");
         var rb2 = document.getElementById("content_0_rbAnswer2");
         var rb3 = document.getElementById("content_0_rbAnswer3");
         var rb4 = document.getElementById("content_0_rbAnswer4");
         var rb5 = document.getElementById("content_0_rbAnswer5");
         
         if (rb1.checked)
         {
            answerName = "Answer 1";
            answerIndex = 1;
            document.getElementById("lblYourAnswer").innerHTML = rb1.nextSibling.innerHTML;
         }
         else if (rb2.checked)
         {
            answerName = "Answer 2";
            answerIndex = 2;
            document.getElementById("lblYourAnswer").innerHTML = rb2.nextSibling.innerHTML;
         }
         else if ( rb3 != null && rb3.checked)
         {
            answerName = "Answer 3";
            answerIndex = 3;
            document.getElementById("lblYourAnswer").innerHTML = rb3.nextSibling.innerHTML;
         }
         else if (rb4 != null && rb4.checked)
         {
            answerName = "Answer 4";
            answerIndex = 4;
            document.getElementById("lblYourAnswer").innerHTML = rb4.nextSibling.innerHTML;
         }
         else if (rb5 != null && rb5.checked)
         {
            answerName = "Answer 5";
            answerIndex = 5;
            document.getElementById("lblYourAnswer").innerHTML = rb5.nextSibling.innerHTML;
         }
         else
         {
            document.getElementById("lbErrorMsg").style.display="inline";
            return false;
         }
         
         if (answerName != "")
         {
           var pageNum = getURLParam("Page");
           if (pageNum == "")
                pageNum = 1;

            GetQuizAnswersInfo(quizItemID, quizTitle, answerName, pageNum, gQuizCount, answerIndex);
         }
         
         DisplayComScore();
         OptifyTracking();
         RefreshQuizpageAds();
         TrackPageView(true);
         EF_TrackQuizPageView();
         AW_TrackQuizPageView();
         return false;
    }
    
    
    function DisplayQuizNL()
    {
        document.getElementById("NewsletterSignUpBox").className = "display";
        document.getElementById(pnlQuizSponsored).className = "QuizSponsoredBox hide";
        DisplayComScore();
    }
    
    function RedirectQuizNL()
    {
        document.getElementById("content_0_pnlNewsletterQA").className = "hide";
        document.getElementById("content_0_pnlQuizQA").className = "show";
        document.getElementById("content_0_QuizDescBox").className = "show";
        document.getElementById("content_0_lblQuestionFooter").className = "blue mt5 show";
        if (showSponsoredContent == "show") {
            document.getElementById(pnlQuizSponsored).className = "QuizSponsoredBox show";
        }
        DisplayComScore();
    }
    
    function LoadPopUp()
    {
        NewWindow('/layouts/SponsoredContentPopUp.aspx', 'win', '511px', '550px');
    }
    
</script>

