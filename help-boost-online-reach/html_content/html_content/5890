<script type="text/javascript">
function GetContent(type)
{
    if (type == 'popular') {
        document.getElementById('tab-2').className = "show";
        document.getElementById('tab-1').className = "hide";
        document.getElementById('headerId').className = "boxheader2";
    } else {
        document.getElementById('tab-2').className = "hide";
        document.getElementById('tab-1').className = "show";
        document.getElementById('headerId').className = "boxheader";
    } 
}
</script>
