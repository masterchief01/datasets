<script type="text/javascript" src="/JavaScript/cookie.js"></script>
<script type="text/javascript">
    var DART_OVERRIDE_OFF_AD_HEIGHT = 250;
    var DART_OVERRIDE_ON_AD_HEIGHT = 600; 
    var DART_OVERRIDE_AD_WIDTH = 300;
    
    var DartOverride_alwaysShow = 600 == DART_OVERRIDE_ON_AD_HEIGHT ;
    var DartOverride_height = 600;
    var DartOverride_width = DART_OVERRIDE_AD_WIDTH;


    function DartOverride_ToggleAdB()
    {
        var div = document.getElementById("DivAdB");
        if (DartOverride_height != DART_OVERRIDE_OFF_AD_HEIGHT)
        {
            if (div != null)
            {
                $('#DivAdB').hide();
            }
        }
        else
        {
            if (div != null)
            {
                $('#DivAdB').show();
            }
        }
    }
</script>

<div style="float:right; margin-bottom: 10px;width:300px;">
