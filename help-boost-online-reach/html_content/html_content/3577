<script type="text/javascript">
var currTheme = 4;
var currAvarage = 5;

function chTheme(theme) {
	if (theme != currTheme) {
		ChangeTheme(theme);
		currTheme = theme;
	}
}

function chAvarage(avarage, text) {
	if (avarage != currAvarage) {
		ChangeAvarage(avarage, text);
		currAvarage = avarage;
	}
}

