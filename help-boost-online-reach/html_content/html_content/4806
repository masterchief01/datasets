<script type="text/cjs">
var onFBInit = [];
// Async load FB lib and trigger init when loaded
window.fbAsyncInit = function(){
	if(typeof FB != "undefined"){
		FB.init({appId: '63203377906', status: true, cookie: true, xfbml: true,
			channelUrl : (window.location.protocol + '//' + window.location.host + '/xd_receiver.htm')
		});
		FB.XFBML.parse();
		for (var i = 0, c = dmjs.onFBInit.length; i < c; i++) {
			dmjs.onFBInit[i]();
		}
		$(".facebookLike").css('background','transparent');
		FB.getLoginStatus(function(response){
			if (response.session){
				updateFBLogin();
			} else {
				updateFBLogin(true);
			}
		});

		function updateFBLogin(login, logify) {
			if(login) {
				/* hide/show for IE... *sigh* */
				$('#FacebookLogin').html('<a id="FBLogin" class="login"><span class="text">Sign In or Join</span></a>').hide().show();
				return;
			}
			FB.api('/me', function(response){
				if(logify) {
					$.post('http://www.ehow.com/services/fb/login/', {
						uid: response.id,
						email: response.email
					});
				}
				$('#FacebookLogin').html('Welcome, '+response.name+' | <a id="FBLogout">logout</a>');
				$('#FBLogout').one('click', function() {
					FB.logout(function(response){
						updateFBLogin(true);
					});
				});
				$('#FacebookFacepile').appendTo('#FacebookLogin');
				$('#FacebookLogin').hover(function(){
					$('#FacebookFacepile').show();
				}, function(){
					$('#FacebookFacepile').hide();
				});
			});
		}

		$('#FBLogin').live('click', function() {
			FB.login(function(response) {
				if (response.session) {
					updateFBLogin(false, true);
				}
			}, {perms:'email'});
		});

		FB.Event.subscribe('auth.sessionChange', function(response) {
		  	if (response.session) {
				updateFBLogin();
			}
		});
	}
};
dmjs.addLoadEvent(document.location.protocol + '//connect.facebook.net/en_US/all.js', 4);
</script>
