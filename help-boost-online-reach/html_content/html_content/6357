<script type="text/javascript">
	if ( $pathname.search(/^\/vault\/result\//) === 0 ){
	
		cnn_metadata.section			= 'search:';
		cnn_metadata.template_type 	= 'other:search results';
		cnn_metadata.content_type 	= 'none';
			
		var type =	( $pathname.indexOf('/article/') > -1 )	? 'news' :
					( $pathname.indexOf('/video/') > -1 )	? 'video': 
					( $pathname.indexOf('/gallery/') > -1 )	? 'gallery':
					( $pathname.indexOf('/cover/') > -1 )	? 'cover': 'all';
		cnn_metadata.search =  {
			results:	"-1",
			term:		"",
			type:		type
		};
	} 
</script>

