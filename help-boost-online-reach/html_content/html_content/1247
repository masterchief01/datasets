document.write("<scri"+"pt src='http://j.imwx.com/v."+wxDateArray[0]+wxDateArray[1]+wxDateArray[2]+"."+wxTimeArray[0]+"/common/ads/contextuallinks/providerHomepage_pco2.js'></scri"+"pt>");
}else if(timeframe == 'today') {
