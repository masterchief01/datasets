if (typeof(timeframe)!="undefined"){


if(timeframe == 'rightnow') {
	document.write("<scri"+"pt src='http://j.imwx.com/v."+wxDateArray[0]+wxDateArray[1]+wxDateArray[2]+"."+wxTimeArray[0]+"/common/ads/contextuallinks/provider_rightnow_pco2.js'></scri"+"pt>");
