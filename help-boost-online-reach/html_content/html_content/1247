}else if(timeframe == 'tenday' || timeframe.indexOf('dailydetails')!=-1) {
document.write("<scri"+"pt src='http://j.imwx.com/v."+wxDateArray[0]+wxDateArray[1]+wxDateArray[2]+"."+wxTimeArray[0]+"/common/ads/contextuallinks/provider_tenday_dailydetails_pco2.js'></scri"+"pt>");
}else if(timeframe == 'tomorrow') {
document.write("<scri"+"pt src='http://j.imwx.com/v."+wxDateArray[0]+wxDateArray[1]+wxDateArray[2]+"."+wxTimeArray[0]+"/common/ads/contextuallinks/provider_tomorrow_pco2.js'></scri"+"pt>");
