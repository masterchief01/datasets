<!-- js / ajax fact loading -->

<script language="javascript" type="text/javascript">

if(typeof String.prototype.trim !== 'function') {
  String.prototype.trim = function() {
    return this.replace(/^\s+|\s+$/g, ''); 
  }
}
jQuery(function() {
    var postid = window.location.hash.replace('#','');
    if(!postid || postid=="scroll") postid=969;
    loadFact(postid);
});

function loadFact( id ) {
		var u_prefix = 'http://www.mentalfloss.com/amazingfactgenerator/?p=';
	// show the loading image
	jQuery("#amazing_fact_content").html("<p><img border=0 src='/amazingfactgenerator/wp-content/plugins/wp-postratings/images/loading.gif'></p>");
    jQuery("#submitter").html('');
	// load the new fact, write it 
	jQuery.getJSON( '/amazingfactgenerator/load-fact.php', { id: id, url_prefix: u_prefix }, function(result){
		circ_ad_html = '';

		circ_ad_html = "<br/><br/>Want more fun facts and amazing trivia? <br>"+
			"<a href='https://secure.palmcoastd.com/pcd/eSv?iMagId=20501&i4Ky=IRAH'>Click here</a> "+
			"for a trial issue of mental_floss magazine!";
		if(result.circ_ad==1) result.post_content += circ_ad_html;

		jQuery("#amazing_fact_content").html("<p>"+result.post_content+"</p>");

        if(result.author) {
            jQuery("#submitter").html("Submitted by: "+result.author);
        }
		// write the next fact id to load to the hitme button
		jQuery("#hitmelink").unbind('click');
		jQuery("#hitmelink").click( function() { loadFact(result.randomid); });
		jQuery("#hitmelink").attr("name", result.randomid);
		location.hash=result.id;	
		// write the ratings markup 
        jQuery("#ratingdiv").html(result.rating_html);
		// load share buttons and ads
        loadShareLinks(result);
        refreshAds();
		// track a fact load as a new pageview w/ GA
        _gaq.push(['_trackPageview']); 
	});
}

function loadShareLinks( fact ) {
	var factTitle = "@mental_floss Amazing fact #"+fact.id;
	var summaryLength = 139 - factTitle.length; // - fact.short_url.length;
	var factSummary = fact.post_content.trim().substring(0, summaryLength).split(" ").slice(0, -1).join(" ") + "...";
    var shareElement;

    jQuery("#sharediv").html('');

    jQuery.each(["facebook", "twitter", "stumbleupon", "plusone"], function(idx, service){
		if(service=="twitter") factTitle = factTitle + ": " + factSummary;
		if(service=="facebook") factTitle = factTitle +": "+fact.post_content;
        shareElement = "af_"+service;
        jQuery("#sharediv").append('<span id="'+shareElement+'" style="float:left; margin-left:15px"></span>');
        stWidget.addEntry({
            "service":service
            ,"element": document.getElementById(shareElement)
            ,"url": fact.short_url
            ,"title": factTitle
            ,"type":"chicklet"
            ,"summary": factSummary
        });
    });	
	// add opengraph tags to the head
	jQuery('title').html(factTitle);
}


function refreshAds() {
	googletag.cmd.push(function() { 
		googletag.pubads().refresh([slot0,slot1]);
	});
}
</script>


