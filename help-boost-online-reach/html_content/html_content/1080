<script language="javascript">
    function goEscape(intPag1, intIdGuia1, intPag2, intIdGuia2, intPag3, intIdGuia3, strFecha)
    {
		if (intIdGuia1 == 9)
		{
			document.location.href = "http://www.reforma.com/offlines/guias/cineguia/pelicula.asp?bandera=0&idpelicula=" + intPag1 + "&fecha=" + strFecha;
		}
		else
		{
			var intCountEventos = 0
			for(x=0;x<arrIncEscape.length;x++)
			{
				if(arrIncEscape[x][8] != 9)
				{
					intCountEventos++;
					intIndexEscape1 = x;
					var iddatoguia = '?iddatoguia='+arrIncEscape[intIndexEscape1][0];
				}											                    
			}
			if (intCountEventos > 2)
			{
				if (intIdGuia2 == 9)
				{
					intIndexEscape2 = aleatorioEscape(0,arrIncEscape.length-1);
					intPag2 = arrIncEscape[intIndexEscape2][0];
					while ((intPag1 == intPag2) || (arrIncEscape[intIndexEscape2][8] == 9))
					{
						intIndexEscape2 = aleatorioEscape(0,arrIncEscape.length-1);
						intPag2 = arrIncEscape[intIndexEscape2][0];
					}
				}
				
				if (intIdGuia3 == 9)
				{
					intIndexEscape3 = aleatorioEscape(0,arrIncEscape.length-1);
					intPag3 = arrIncEscape[intIndexEscape3][0];
					while ((intPag1 == intPag3) || (intPag2 == intPag3) || (arrIncEscape[intIndexEscape3][8] == 9))
					{
						intIndexEscape3 = aleatorioEscape(0,arrIncEscape.length-1);
						intPag3 = arrIncEscape[intIndexEscape3][0];
					}
				}
				document.location.href = "http://www.reforma.com/offlines/guias/escape/default.htm?iddatoguia=" + intPag1 + "&pag1=" + intPag1 + "&pag2=" + intPag2 + "&pag3=" + intPag3;
			}
			else
			{
				var iddatoguia = '?iddatoguia='+intPag1;
				document.location.href = "http://www.reforma.com/offlines/guias/escape/default.htm" + iddatoguia;
			}
		}
    }
