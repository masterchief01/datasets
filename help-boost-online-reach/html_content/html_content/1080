		function cambiaTab(celda){
			
			for(i=1;i <=Atic;i++)
			{
				document.getElementById('divcelda' + i).style.display = "none";
			}				
			
			var nombreDiv = 'div' + celda;
			
			
			document.getElementById(nombreDiv).style.display = "block";
			document.getElementById(celda).style.cursor = "arrow";
		}
		
		var herractual = '0';
		function seleccionaherr(numherr,flag)
			{
				
				if (herractual !== numherr)
				{
					celda= 'celda' + flag
					cambiaTab(celda);
				}	
			}	
						
			colorbackRollSel="#e6ddcf";
			colorbackRoll="#ffffff";	


			function switchColor(o)
			{	
				for(i=1;i<=Atic;i++)
				{
				celda = 'celda'+i;
				if(celda==o.id)
					{
					if (o.bgColor.toLowerCase()!=colorbackRollSel.toLowerCase())
						{	
							cambiaimgart(i)
							o.bgColor = colorbackRollSel;
							o.style.color='#ffffff';
						}
					}
					else
					{
						document.getElementById('celda'+i).bgColor = '#ffffff';
						document.getElementById('celda'+i).style.color='#ffffff';
					}
				}
			}
			
			function cambiaimgart(x)
			{
				
				if (document.getElementById)
					{
						document.getElementById("TituloArtEdt").innerHTML = '<b>' + arraytituloartedt[x-1] + '</b>';
						document.getElementById("AticoArtEdt").innerHTML = '<img src="libre/online07/imggc/aztec.gif" width="19" height="13" align="absmiddle">' + '<a href="' + arrayligaartedt[x-1] + '">' + arrayaticoartedt[x-1] + '</a>';
					}

				else if (document.all)//Explorer 5 o menos
					{
						document.all("TituloArtEdt").innerHTML = '<b>' + arraytituloartedt[x-1] + '</b>';
						document.getElementById("AticoArtEdt").innerHTML = '<img src="libre/online07/imggc/aztec.gif" width="19" height="13" align="absmiddle">' + '<a href="' + arrayligaartedt[x-1] + '">' + arrayaticoartedt[x-1] + '</a>';
					}
			}
			
	
