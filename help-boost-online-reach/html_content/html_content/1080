    function entra(obj, directorio,alto,ancho)
    {document.getElementById("imgsuple").src="libre/online07/paginas/estilos/" + directorio;
     document.getElementById("imgsuple").style.widht=ancho;
     document.getElementById("imgsuple").style.height=alto;
     var antes = document.estilos.anterior.value;
     if (antes != obj)
     {document.estilos.anterior.value = obj;}}
    function veasuburb()
    {document.location = document.estilos.combosub.options[document.estilos.combosub.selectedIndex].value;}
