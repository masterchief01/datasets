            function plazaConsulta(lp)
            {
            str = new String(lp);
            if(str.search("plazaconsulta=elnorte")>1)
            {return 0;}
            if(str.search("plazaconsulta=reforma")>1)
            {return 0;}
            if(str.search("plazaconsulta=mural")>1)
            {return 0;}
            if(str.search("plazaconsulta=palabra")>1)
            {return 0;}
            return 1;
            }
