<script>
  //<![CDATA[
    $(function(){
      $(function(){
      $('#reservation_date.datepicker').datepicker({
        minDate: '0',
        maxDate: '+90D',
        dateFormat: 'yy/mm/dd',
        onClose: function(dateText, inst) {
          var today = $(this).data('today');
          if (dateText === today) {
            $('#reservation_time_today').show().removeAttr('disabled');
            $('#reservation_time_future').hide().attr('disabled', 'disabled');
          } else {
            $('#reservation_time_today').hide().attr('disabled', 'disabled');
            $('#reservation_time_future').show().removeAttr('disabled');
          }
        }
      });
    
      /* There is a bug in the jqueryui datepicker widget. It doesn't actually use the
         widget factory. Because of this, I can't bind on the create method to fire off
         an event. I have to check the values manually and then hide the elements. This
         should be fixed in jqueryui-1.9 but all of this should ultimatly be wrapped up
         into a jquery widget.
      */
      var today = $('#reservation_date.datepicker').data('today');
      var dateText = $('#reservation_date.datepicker').val();
      if (dateText === today) {
        $('#reservation_time_today').show().removeAttr('disabled');
        $('#reservation_time_future').hide().attr('disabled', 'disabled');
      } else {
        $('#reservation_time_today').hide().attr('disabled', 'disabled');
        $('#reservation_time_future').show().removeAttr('disabled');
      }
    });
    
    });
  //]]>
</script>

