<script>
  //<![CDATA[
    $('#submit_feedback').click(function() {
      text = $('#message').val();
      if (text != null && text !== undefined && text != '') {
        $.blockUI({ message: '<h2><img src="/images/busy.gif" /> Sending...</h2>' });
        $.ajax({
          'url': '/send_mail',
          'type': 'post',
          'data': {
            'web_contact_message[message]': text,
            'web_contact_message[postal]': $('#postal').val(),
            'web_contact_message[email]': 'no-reply@cityeats.com',
            'web_contact_message[ip_address]': '38.99.102.236',
            'web_contact_message[source]': 'Your Feedback'
          },
          'success': function(result) {
            $.unblockUI();
            $.blockUI({ 
              message: "<h2>Thank you.</h2><h3>Your message has been received</h3>",
              timeout: 2000 
            }); 
          }, 
          'error': function(result) {
            $.unblockUI();
            $.blockUI({ 
              message: "<h2>We're sorry.</h2><h3>An error prevented your message from being sent. Please try again later.</h3>", 
              timeout: 2000 
            }); 
          },
          'complete': function(result) {
            $('#message').val('');
          }
       });
      }
    });
    
    $(function(){
      CityEats.Followings.setEventsOnAllButtons();
    });
  //]]>
</script>


