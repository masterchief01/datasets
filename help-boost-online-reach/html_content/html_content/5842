                if (typeof DartRandomOrd == 'undefined') {
                    var DartRandomOrd = new Date(); DartRandomOrd = Math.ceil(DartRandomOrd.getTime() / 1000);
                    DartRandomOrd = Math.floor(Math.random() * 100000) * DartRandomOrd;
                }
				dartParameters = dartParameters.replace(/ord=[0-9]+/, 'ord='+DartRandomOrd);

				if(typeof dartAdditionalParameters === "string") {
					dartParameters = dartAdditionalParameters + dartParameters;
				}
				

									var _url = '<iframe src="/ads/banner.php?ad=slideshow;'+dartParameters+'" width="300" height="250" marginwidth="0" marginheight="0" frameborder="0" scrolling="no"><\/iframe>';
								
				_url = _url.replace('%dx%', (typeof dexQS !== "undefined" && dexQS.length > 0) ? dexQS : '');
				
				document.write(_url);
