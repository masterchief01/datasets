<script type="text/javascript">
    
    $(function () {
        $('a[href="#share_overlay"]').fancybox({ overlayColor: '#553e3f', overlayOpacity: 0.9, padding: 0, width: 560, height: 430,
                                                 autoDimensions: false, scrolling: 'no', autoScale: false, onComplete: fancyboxLoadComplete });
        $('a[href="#share_overlay"]').click( initShareDialog );

        $('#share-email_form .submit_button').click(doShareRecipeEmail);
        $('#share-email_form #share-your_name').keydown(updateShareMessage);
        $('#share-email_form #share-your_name').keyup(updateShareMessage);

        // Set recipe name in share recipe popup
        var recipeName = $(document).attr('title').substring(0, $(document).attr('title').indexOf(' |'));
        $('#share-message_content .recipe').text(recipeName + " ");
    });

    function initShareDialog()
    {
        clearForm( $('#share_overlay') );
        $('#share-message_content .name').text("Your friend ");
    }

    function updateShareMessage()
    {
        var currentName = $('#share-email_form #share-your_name').val();
        
        if (currentName != "")
            $('#share-message_content .name').text(currentName + " ");
        else
            $('#share-message_content .name').text("Your friend ");
    }

    function doShareRecipeEmail() {
        clearFormErrors($('#share-email_form'));
        requestData = {
            recipeId: recipeId,
            senderName: $('#share-email_form #share-your_name').val(),
            senderEmail: $('#share-email_form #share-your_email').val(),
            recipientEmail: $('#share-email_form #share-friends_email').val()
        };

        onSuccess = partial(CreateAskBeckyFormSuccess);
        SendRecipeShareEmail(requestData);
        googleReportEvent('recipe', 'email', window.location.pathname);

        return false;
    }

    function SendRecipeShareEmail(requestData) {
        AjaxCall(requestData);
    }

    function SendRecipeShareEmailReply(reply) {
        if (reply.Success == "false") {
            displayErrors(reply.Errors, $('#share-email_form'));
        } else {
            confirmationPopup('shareRecipeConfirmationPopup', 400, 200);
        }
    }

    function SendRecipeShareEmailSuccess() {

    }



</script>

