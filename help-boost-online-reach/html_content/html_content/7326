<script type="text/javascript">
function localize(t, format)
{
	var replacements = {1: "at"};
	var tz = Date.today().isDaylightSavingTime() ? "EDT" : "EST";
	var d = Date.parse(t).setTimezone(tz);
	var time = d.toString(format);
	$.each(replacements, function(i,v) {
		time = time.replace('{'+i+'}', v);
	});
	document.write(time);
}
</script>
