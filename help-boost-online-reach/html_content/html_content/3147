              <div class="panes2">
              	<div id="flickr_feed"></div>
                <script>
                  $.getJSON("http://api.flickr.com/services/feeds/photos_public.gne?id=45409925@N07&tags=indy500&lang=en-us&format=json&jsoncallback=?", function(data){
                    $.each(data.items, function(i,item){
                      if(i < 4) {
                      $("<img/>").attr("src", item.media.m).attr("width", 91).attr("height", 91).appendTo("#flickr_feed")
                        .wrap("<a class='group' rel='gallery' href='" + item.media.m.replace('_m', '') + "'></a>");
                      }
                    });
                  });
                </script>

              	<div id="youtube_feed"></div>
                <script type='text/javascript'>
                  //YOUTUBE FEED
                  $.getJSON("http://gdata.youtube.com/feeds/api/videos?author=indycars&orderby=published&alt=json-in-script&callback=?&max-results=4", function(data){
                    if(data.feed.entry) {
                      $.each(data.feed.entry, function(i,item){
                      var parts = item.id.$t.split('/');
                      var videoId = parts[parts.length-1];

                      $("<img/>")
                        .attr("src", 'http://i.ytimg.com/vi/' + videoId + '/2.jpg')
                        .attr("width", 91)
                        .attr("height", 91)
                        .appendTo("#youtube_feed")
                        .wrap('<a target="_blank" class="fancyvideo" href="' + item.link[0].href + '"></a>');
                      });
                    }
                  });
                </script>
              </div>
      </div>
      
      

