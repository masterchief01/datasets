<script type='text/javascript'>
  $(document).ready(function(){
    //OUR TWEETS
    $.ajax({
      url: 'http://search.twitter.com/search.json?q=from:ims&rpp=1&callback=?',
      dataType: 'json',
      success: function(data) {
          $.each(data.results, function(index, value) {
            var createdAtString = twitterDateTimeString(value.created_at);

            $('#our_tweets').html('<p><b><a target="_blank" href="http://twitter.com/'+value.from_user+'">@'+ value.from_user +'</a></b> ' + replaceURLWithHTMLLinks(value.text) + '<br/><span class="tweetdate">'+ createdAtString +'</span></p>');
          });
        }
    });

    //YOUR TWEETS
    $.ajax({
      url: 'http://search.twitter.com/search.json?q=%23indycar+OR+%23izodindycar+OR+%23indy500&rpp=1&callback=?',
      dataType: 'json',
      success: function(data) {
          $.each(data.results, function(index, value) {
            var createdAtString = twitterDateTimeString(value.created_at);
            $('#your_tweets').html('<p><b><a target="_blank" href="http://twitter.com/'+value.from_user+'">@'+ value.from_user +'</a></b> ' + replaceURLWithHTMLLinks(value.text) + '<br/><span class="tweetdate">'+ createdAtString +'</span></p>');
          });
        }
    });

    //BLOG FEED


    //COUNTDOWN TIMER
	//NOTE: Months are base zero, so should be one less than 'real date'
    var dates = {
      'indy500':new Date(2013,4,26,12,0), //12pm eastern
      'brickyard':new Date(2012,6,29,13,0), //1pm eastern
      'redbull':new Date(2012,7,19,14,0) //2pm eastern
    };
//{ts '2012-06-18 18:53:23'}
    var event_date = dates['indy500'];
    $('#countdown_wrapper').countdown({until: event_date, layout: '<div class="days">{dnn}</div><div class="hrs">{hnn}</div><div class="min">{mnn}</div><div class="secs">{snn}</div>'});

    //TOP NEWS
    $("div.scrollable").scrollable({
      size: 1,
      clickable: false
    }).navigator({
      navi: '#myController',
      activeClass: 'jFlowSelected'
    });


  });

</script>


