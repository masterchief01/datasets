  $(document).ready(function() {
    $("#modal_div").dialog({modal: true, autoOpen: false, draggable: false, resizeable: false});
    $("#submitVote").submit(function(event){
      event.preventDefault();
      submitVote();
    });
    //Close Captcha Popup Box
    $("#closePopup").click(function(){
      $(".popup").addClass('hide');
    });
    $('#main-footer .wrapper').css({"padding-bottom":"100px"});
    $.get("/_view_recipe?id=22956");
    updateVoteStatus();
  });
  
  function submitVote(){
    $.post("/recipes/contestvote", $("#submitVote").serialize(), function(data){
      response = $.parseJSON(data);
      if(response["success"]){
        $("#vote_msg").hide();
        $("#captcha_container").hide();
        $("#vote_sharebar_container").show();
        updateVoteStatus();    
      } else {
        ACPuzzle.reload();
        $("#vote_msg").show();
      }
    });
  }
  function updateVoteStatus() {
    $.getJSON("/_update_vote_status/22956", function(data) {
      if(data["is_voted"]){
        $("#vote_button_container").html("<span title=\"VOTE\" class=\"opac50 cursDef\">VOTE</span>");
        recipeVoteOmniture();
      } else { 
        $("#vote_button_container").html("<a title=\"VOTE\" id=\"vote\">VOTE</a>");
        $("#vote").click(function(){
          $(".popup").removeClass('hide');
          $(".popup").css('height', $(document).height());
        });
      }
      fireOmniture();
    });
  }
  function closeMe() {
    $("#modal_div").dialog("close");
    window.location.href = window.location.href;
  }
  function cancelModal() {
    $("#modal_div").dialog("close");
  }
  
  function addPhoto() {
    $("#modal_div").dialog("open");
    $("#modal_div").dialog("option", {"title": "Add a photo to \"Santa's Cookie Sleds\"", "width": 600, "height": 600});
    $("#modal_iframe").attr("src","/recipes/photos/new/22956");
    return false;
  }  
  function rate(rating) {
    $("#rating_container").load("/recipes/rate", {id:22956, rating:rating}, 
    function() {$("#rating_bottom").html($("#rating_top").html());
    });
  }  
  function addToSaved() {
    $(".save_button").load("/recipes/addtosaved", {id:22956});
  }
</script>

