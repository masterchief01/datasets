<script language='JavaScript' type='text/javascript'>
function aliry_checkInputs() {
    var something = jQuery('#aliry_something').val();
    var uname = jQuery('#aliry_uname').val();
    var uemail = jQuery('#aliry_uemail').val();
    if (something == 'Your DIY question?' || something == '')
        return false;
    if (uname == 'Enter your name' || uname == '')
        return false;
    if (uemail == 'Enter your E-mail' || uemail == '')
        return false;
    return true;
}

function aliry_doSubmit() {
    if (aliry_checkInputs()) {
        jQuery('#aliry_something').hide();
        jQuery('#aliry_uname').hide();
        jQuery('#aliry_uemail').hide();
        jQuery('#aliry_go').hide();
        jQuery('div#aliry-saving').show();
        jQuery.post(
            'http://www.skiptomylou.org/wp-content/themes/wpskiplou/ccForm-backend.php',
            { aliry_action: 'save', aliry_something: jQuery('#aliry_something').val(), aliry_uname: jQuery('#aliry_uname').val(), aliry_uemail: jQuery('input#aliry_uemail').val() },
            function(data, p_result) {
                if (p_result == 'success' && data > 0) {
                    alert('Your data are saved!');
                    jQuery('#aliry_something').val('Your DIY question?');
                    jQuery('#aliry_something').removeClass('aliry-unshadowColor');
                    jQuery('#aliry_something').addClass('aliry-shadowColor');
                    jQuery('#aliry_uname').val('Enter your name');
                    jQuery('#aliry_uname').removeClass('aliry-unshadowColor');
                    jQuery('#aliry_uname').addClass('aliry-shadowColor');
                    jQuery('#aliry_uemail').val('Enter your E-mail');
                    jQuery('#aliry_uemail').removeClass('aliry-unshadowColor');
                    jQuery('#aliry_uemail').addClass('aliry-shadowColor');
                }
                else
                    alert('Please fill in fields properly!');

                jQuery('div#aliry-saving').hide();
                jQuery('#aliry_something').show();
                jQuery('#aliry_uname').show();
                jQuery('#aliry_uemail').show();
                jQuery('#aliry_go').show();
            }
        );
        
        return false;
    }
    else
    {
        alert('Please fill in fields properly!');
        return false;
    }
}
				</script><style type='text/css'>
/* Aliry widgets style */
.aliry-saving {
display:none;
}
.aliry-shadowColor {
font-style: italic;
color:#7f7f7f;
}
.aliry-unshadowColor {
font-style:normal;
color:#000000;
}
.aliry-inputs {
padding:5px;
}
.aliry-bottom-line {
padding-bottom:30px;
}
.aliry-go-btn {
text-align:right;
margin-right:37px;
position:relative;
top:-23px;
}
/* End Aliry widgets style */
</style>
