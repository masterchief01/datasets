<script type="text/javascript">
// <![CDATA[
	submission_controller.PageLoaded();

			var is_ie = (navigator.userAgent.indexOf("MSIE") > 0);

		var xmouse, ymouse;
					var layout_width = 1024;
		
		if(is_ie) {
			var xflux = 38;
			var yflux = 342;
		} else {
			var xflux = 28;
			var yflux = 344;
		}

		var xoffset = 20;
		var yoffset = 30;

		var checkhover = false;
					var buttons_left = $('size_and_lightbox_options').offsetLeft;
			var buttons_width = $('size_and_lightbox_options').offsetWidth;
			var buttons_right = buttons_left+buttons_width;

			var buttons_top = $('size_and_lightbox_options').offsetTop;
			var buttons_height = $('size_and_lightbox_options').offsetHeight;
			var buttons_bottom = buttons_top+buttons_height;
				function GetMousePosition(e)
		{
			if(is_ie)
			{
				xmouse = document.documentElement.scrollLeft+window.event.clientX;
				ymouse = document.documentElement.scrollTop+window.event.clientY;
			} else
			{
				xmouse = e.pageX;
				ymouse = e.pageY;
			}

			var left_offset = Math.round((getWindowWidth() - layout_width)/2);

			var my_left = xmouse - left_offset - xflux;
			var my_top = ymouse - yflux;

			if (left_offset < 1) {
				left_offset = 0;
			}
			var my_width = (xmouse+xoffset) - left_offset;
			// if the api hovers extend offscreen, move it to the left of the mouse


			var x_offset = my_width >= 800 ? 260:0;

						
							if (checkhover && (my_left < buttons_left || my_left > buttons_right || my_top < buttons_top || my_top > buttons_bottom)) {
					$('button_hover_text').innerHTML = "";
					checkhover = false;
				}
					}

		function getWindowWidth() {
			var x = 0;
			if (self.innerHeight) {
				x = self.innerWidth;
			} else if (document.documentElement && document.documentElement.clientHeight) {
				x = document.documentElement.clientWidth;
			} else if (document.body) {
				x = document.body.clientWidth;
			}

			return x;
		}

		document.onmousemove = GetMousePosition;
	
	// ]]>
</script>

<script type="text/javascript" src="http://server.cpmstar.com/view.aspx?poolid=1134&amp;script=1"></script>

<script type="text/javascript">
// <![CDATA[
		// if cpmstar is blocked use this text:
		if (user_is_a_leech) {
			cpmStarAdInfo = {
				title:"Newgrounds Store",
				desc:"Cool shirts, toys, keychains, DVDs, stickers and more. BUY SOMETHING!!!!",
				link:"http://www.newgrounds.com/store"
			};
		}

		var textlink = ("<a class=\"cpmstarHeadline\" href=\"" + cpmStarAdInfo["link"] + "\" target=\"_blank\">" + cpmStarAdInfo["title"] + "</a>");
		textlink += ("<a class=\"cpmstarText\" href=\"" + cpmStarAdInfo["link"] + "\" target=\"_blank\">" + cpmStarAdInfo["desc"] + "</a>");
		$('textunderflash').innerHTML = textlink;

		// just because the ad space isn't the right size by default....
		setSizing();


		// this is a list of all the possible api divs
		var api_pods = [
			{name:'files', label:"User Generated Files"},
			{name:'medals', label:"Medals"},
			{name:'scores', label:"Scores"}
		];

		// check the html and see what pods actually exist for this submission
		var has_pods = [];
		for(var i=0; i<api_pods.length; i++) {
			var pod = api_pods[i].name;
			if ($('api_pod_'+pod)) {
				has_pods.push(api_pods[i]);
			}
		}

		// build the inter-pod tabs for each pod
		if (has_pods.length > 0) {

			for(var i=0; i<has_pods.length; i++) {
				var pod = has_pods[i].name;
				if ($(pod+'_nav_tabs')) {
					var tabs = [];

					for(var p=0; p<has_pods.length; p++) {
						has_pods[p].pod = pod;
						tabs.unshift(has_pods[p]);
					}

					var template = new TemplateWriter(pod+'_nav_tabs');
					template.AssignVarToLoop('tabs', tabs.length > 1 ? tabs : []);
					template.Render();

					if (tabs.length > 1) {
						var btn = new Button(pod+"_btn_"+pod);
						btn.Disable();
					}
				}
			}

			if (save_file_id) {
				viewAPIPod('files');
			} else {
				viewAPIPod(has_pods[0].name);
			}
		}


		// hide all pods then unhide the slecetd one
		function viewAPIPod(pod) {
			for(var i=0; i<has_pods.length; i++) {
				$('api_pod_'+has_pods[i].name).style.display = "none";
			}
			$('api_pod_'+pod).style.display = "";
		}

	// ]]>
</script>

