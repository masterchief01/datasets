  function login_box() {
	FB.api('/me', function(response) {
		var profile_image = 'http://graph.facebook.com/'+response.id+'/picture';
		jQuery('div#subheader-social-box').empty().addClass('logged-in').append('<img src="'+profile_image+'" style="float: left;"/>').append('<h4>Hi, '+response.first_name+'</h4>').append('<a href="javascript:logout();">Logout of Facebook</a>').append('<div class="clear"></div>');	
	});
	
  }

  function logout() {
	FB.logout(function(response) {
		window.location.reload();
	});
  }

