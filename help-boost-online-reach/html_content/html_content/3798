<div id="tres_sugar" class="tres_sugar feed-widget module" style="display:none;">
	<div class="head">
		<p>today's top stories from our friends at <strong>TresSugar</strong></p>
	</div>
	<ul id="tres_sugar_stories"></ul>
</div>

<script type="text/javascript">
	$(document).ready(function(){ 
		$.ajax({
			type: 		"GET",
			url: 			"/feeds/partner/tressugar",
			dataType: "jsonp",
			success:   function(response){
        if (response && response.rss.channel.item.length > 0){
          feed_items(response.rss.channel.item);
        }
			},
			error:     function(response){
		    /* ruh roh */
			}
		});
	});
	
	function feed_items(response){ 
    if (typeof response.items != 'undefined' && response.items.length > 0){
      var items = response.items;
    } else {
      var items = response;
    }
    var item, li, last_css = "";
	  var limit = 5;
	  for (var i = 0; i < items.length && i < limit; i++){
	    if (i == (limit-1)){
	      last_css = 'last';
	    }
	    item = items[i];
			li_html = '<a href="' + item.link + '" class="number no-' + (i+1) + '">' + (i+1) + '</a>' + 
				       	'<a href="' + item.link + '" class="title">' + $("<span />").html(item.title).text()  + '</a>' +
				       	'<span class="clear"></span>';
			li = $('<li class="' + last_css + '">' + li_html + '</li>');
			
			// ga event tracking
			li.bind('click', function(){
				_gaq.push(['_trackEvent', 'TresSugar Feed Widget', 'Click', item.title]);
			});
			
			$("#tres_sugar_stories").append(li);
	  }
	  $("#tres_sugar").show();
	}
</script>
