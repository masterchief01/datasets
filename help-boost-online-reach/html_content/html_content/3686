//<![CDATA[
      document.observe('fb_connect:loaded', function(event) {        
        FB.getLoginStatus(function(response) {
          if (response.session) {
            if ((User.isLoggedIn()) && (currentUser.fb_login)) {
              $('fod_logout_link').hide();
              $('fb_logout_link').show();
            } else if (!User.isLoggedIn()) {
              
              $$('span.facebook_connect_text_18x18').each(function(el) { el.update('Log in'); });
              $('fb_connect').style.display = 'inline';
            } 
          } else $('fb_connect').style.display = 'inline';
        });
      });

//]]>
</script>
