<script type="text/javascript">
//<![CDATA[
  var next_page = 1;
  var last = 1288980669
  var comments_total = 13
  
  function increment_comments_total() {
    comments_total += 1;
    var comments_str = comments_total.toString();
    var rgx = /(\d+)(\d{3})/;
  	while (rgx.test(comments_str)) {
  		comments_str = comments_str.replace(rgx, '$1' + ',' + '$2');
  	}
    $$('.comments_total').invoke('update', comments_str);
  }

  function get_comments() {
    var sort = Cookie.get('_comments_sort') || 'recent';
    if($('comments_sort')) {
      $('comments_sort').select('a').invoke('show');
      $('comments_sort').select('span.current').invoke('hide');
      $('comments_sort_'+sort).down('a').hide();
      $('comments_sort_'+sort).down('span').show();
    }
    
    new Ajax.Request('/media/more_comments/a012185a77', {asynchronous:true, evalScripts:true, method:'get', onComplete:function(request){$('more_comments_waiting').hide();}, parameters:'page=' + next_page + '&sort=' + sort + '&last=' + last})
  }
  
  function check_live_comments() {
    if($('live_comments') && $('live_comments').checked) {
      new_comments();
    }
  }
  
  function new_comments() {
    new Ajax.Request('/media/new_comments/a012185a77', {asynchronous:true, evalScripts:true, method:'get', parameters:'last=' + last})
  }
  
  function resort_comments(sort) {
    Cookie.set('_comments_sort', sort)
    next_page = 1;
    $('comments_list').update('');
    $('more_button').hide();
    $('more_comments_waiting').show();
    get_comments();
  }
  
  document.observe('dom:loaded', function(event) {
    get_comments();
  });
  
  Scroll.init(function() {
    check_live_comments();
    setInterval("check_live_comments();", 60000);
  }, $('comments_box').cumulativeOffset().top)

//]]>
</script>
