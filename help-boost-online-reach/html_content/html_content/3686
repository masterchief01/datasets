<script type="text/javascript">
//<![CDATA[
  window.fbAsyncInit = function() {
    FB.init({
      appId: '8cfaaac2ff6772d8e0d6f1788726ee09', 
      status: true, 
      cookie: true, 
      xfbml: true,
      channelUrl: document.location.protocol + '//www.funnyordie.com/fb_channel.html'
    });
    $('fb-root').fire('fb_connect:loaded');
  };
  jQuery("body").bind('lastAd.loaded', function() {
    var e = document.createElement('script'); e.async = true;
    e.src = document.location.protocol +
      '//connect.facebook.net/en_US/all.js#xfbml=1';
    document.getElementById('fb-root').appendChild(e);
  });
  
  document.observe('fb_connect:loaded', function() {
    $('fb_connect_href').observe('click', function(event) {
      event.stop();
      $("fb_connect").hide();
      $("fb_spinner").show();
      FB.login(handleFBSessionResponse, {perms:'email'});
      GA.event("fb_connect", "login", "a012185a77-josh-and-trevor-cook");
    });
    
    FB.Event.subscribe('edge.create', function(href, widget) {
      GA.event("sharing", "facebook_like", href);
    });
    
  });
  
  function handleFBSessionResponse(response) {
    if (response.session) {
      new Ajax.Request("/users/fb_connect_login", {method: "post"});
    }
  }
  

//]]>
</script>

    
<span id="twt_anywhere_span"></span>

<script type="text/javascript">
//<![CDATA[

  jQuery("body").bind('lastAd.loaded', function() {  
    
    var twt_anywhere = document.createElement("script"), done = false;
    twt_anywhere.type = 'text/javascript';
    twt_anywhere.async = true;
    twt_anywhere.src = 'http://platform.twitter.com/anywhere.js?id=EONRLjsUyz8LtqQtUGhMg&v=1';
    
    twt_anywhere.onload = twt_anywhere.onreadystatechange = function(){
      if ( !done && (!this.readyState || this.readyState == "loaded" || this.readyState == "complete") ) {
        done = true;
        $('twt_anywhere_span').fire('twitter:loaded');
      }
    };
    document.getElementsByTagName("head")[0].appendChild(twt_anywhere);

    if ($('tweet_medium_button')) {
      Event.observe($('tweet_medium_button'), 'click', function(event){
        twttr.anywhere('1', function(twitter) {
          twitter('#twitter_box_placeholder').tweetBox({
            defaultContent: 'http://FunnyOrDie.com/m/2wzr', 
            onTweet: function (tweet, renderedTweet) { 
              $('twitter_action_area').innerHTML = 'Thanks for your tweet';
            }
          });
        });
        $('twitter_action_area').show();
        GA.event('sharing','twitter_share','"a012185a77-josh-and-trevor-cook"');
      });
    }
  });
  
  function populateTwitterForm(user) {
    $('twitter_name').value = user.data('screen_name');
    $('twitter_id').value = user.data('id');
    $('twitter_image').value = user.data('profile_image_url');
  }
  
  var handleTwitterLogin;
  document.observe("twitter:loaded", function() {
    twttr.anywhere(function (T) {
      if (T.isConnected()) {
        handleTwitterLogin = function (val) {
          new Ajax.Request("/users/twitter_connect_login", {method: "post", parameters: { 'twitter_id': T.currentUser.data('id'), 'post_auth':  val }});
          populateTwitterForm(T.currentUser);
        } 
      } else { 
        if ($('twitter_login')) {
          $('twitter_login').innerHTML = "<span id='twitter_login'></span>";  
          T("#twitter_login").connectButton({
            authComplete: function(user) {
              populateTwitterForm(user);
              new Ajax.Request("/users/twitter_connect_login", {method: "post", parameters: { 'twitter_id': T.currentUser.data('id'), 'post_auth': '' }});
            }
          });
        } 
        if ($('twitter_login_comments')) {
          $('twitter_login_comments').innerHTML = "<span id='twitter_login_comments'></span>";  
          T("#twitter_login_comments").connectButton({
            authComplete: function(user) {
              populateTwitterForm(user);
              new Ajax.Request("/users/twitter_connect_login", {method: "post", parameters: { 'twitter_id': T.currentUser.data('id'), 'post_auth': '' }});
            }
          });
        }
      }
    });
  });

//]]>
</script>

    
