    $(this).parents('.expandable').toggleClass('collapsed');
    if ($.data(document.body, $(this).attr('data-remote')) != true)
      $.get($(this).attr('data-remote'));
    $.data(document.body, $(this).attr('data-remote'), true);
    return false;
  });

    });
  
