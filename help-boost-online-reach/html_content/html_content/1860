			<script>
				( function ( $ ) {
					$(document).ready(function() {
						$('#saveForm').click(function() {
							if( $('#valid').attr("checked") ) {
								// update user interface
								$('#sub-prompt').css('color','black');
								$('#sub-prompt').html('Adding email address...');
								
								// Prepare query string and send AJAX request
								$.ajax({
									url: 'http://fashionindie.com/wp-content/themes/fashionindie/inc/store-address.php',
									data: 'ajax=true&email=' + escape($('#sub-mail').val()),
									success: function(msg) {
										$('#sub-prompt').html(msg);
										if( msg.indexOf( 'Error' ) < 0 ){
											$( '#subscribe' ).mouseleave( function () { $( '#subscribe' ).fadeOut( 'slow' ) } );
										}else{
											$( '#sub-prompt' ).css( 'color', 'red' );
										}
									}
								});
							}else{
								
								$('#sub-prompt').css('color','red');
								$('#sub-prompt').html('Please select a newsletter to receive.');
							}
							return false;
						});
						if($('#gallery-image').length){
							$(document).keydown(function(e) {
								if(e.keyCode == 39){	//right arrow
									var gn = $('#next-img a').attr("href");
									if(gn) window.location = gn;
								}
								if(e.keyCode == 37){	//left arrow
									var gp = $('#prev-img a').attr("href");
									if(gp) window.location = gp;
								}
						 	});
					    }
					});
				} ) ( jQuery );
			</script>
