    <script language="javascript">
		var $ = jQuery;
		
		function addEvents () {
			$('.accordion-cell' ).each( function () {
				var cell = $(this);
				var bar = cell.children().eq(0);
				( bar.attr('class').indexOf('closed') >= 0 ) ?
					bar.click( function () {
						$( this ).attr( 'class', 'accordion-bar-open');
						$( this ).parent().siblings().each( function () {
							$( $( this ).children()[ 0 ] ).attr( 'class', 'accordion-bar-closed');
						} );
						update();
					} )
				 : bar.unbind('click');
			} );
		}
		function update () {
			$('.accordion-pane').each( function () {
				var pane = $(this);
				var bar = $( pane.siblings()[0] )
				if( bar.attr('class').indexOf('closed') === -1 ){
					pane.show();
				} else {
					pane.hide();
				}
			} );
			
			addEvents();
		}
       	update();
    </script>
