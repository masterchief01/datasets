//event tracking
jQuery.gaTracker('UA-4967829-1');
function extractParamFromUri(uri, paramName) {
  if (!uri) {
    return;
  }
  var uri = uri.split('#')[0];  // Remove anchor.
  var parts = uri.split('?');  // Check for query params.
  if (parts.length == 1) {
    return;
  }
  var query = decodeURI(parts[1]);

  // Find url param.
  paramName += '=';
  var params = query.split('&');
  for (var i = 0, param; param = params[i]; ++i) {
    if (param.indexOf(paramName) === 0) {
      return unescape(param.split('=')[1]);
    }
  }
}
//facebook
function facebook_login(user){
	var data = {action:'wp_fb_ajax_signon', user:user};
	if($('#facebook-user').is(':visible')){
		data.username = $('#facebook-user').val();
		data.pw = $('#facebook-user-pw').val();
	}
	$.post('/wp-admin/admin-ajax.php', data, function(data){
		if(data.length){
			var error = eval('('+data+')');
			if(typeof error.error != 'undefined'){
				$('#account-error').find('div').text(error.error);
				$('#account-error').stop(true).fadeIn().delay(10000).fadeOut();
				if(error.code == 'admin'){
					$('#signin form').show();
					$('#facebook').hide();
				}
				return;
			}
		}else{
			_gaq.push(['_trackEvent', 'User', 'login', 'facebook']);
			if(typeof data.username != undefined)
				_gaq.push(['_trackEvent', 'User', 'register', 'facebook']);
			window.location.reload();
		}
	});
}
function facebook_login_check(){
	FB.api('/me', function(response){
		$.post('/wp-admin/admin-ajax.php', {action:'wp_fb_has_meta', user: response}, function(is_user){
			$('.fb-login-btn, #signin form, #signin form + div').hide();
			$('#signin #facebook').css('float', 'left');
			$('#facebook-user').val('');
			if($.trim(is_user) == 'no'){
				$('#has-account').show();
				$('#has-account-yes').click(function(){
					$('#has-account').hide();
					$('#account-username').show();
					$('#fb-account-usrname').click(function(){
						facebook_login(response);
						return false;
					});
					return false;
				});
				$('#has-account-no').click(function(){
					facebook_login(response);
					return false;
				});
			}else
				facebook_login(response);
		});
	});
}
window.fbAsyncInit = function() {
	FB.init({
		appId      : '174021895985194',
		status     : true, 
		cookie     : true, 
		oauth      : true,
		xfbml      : true	
	});
	$(".smp-fb-share, .fb_share").live('click', function(){ 
	  var a=this; 
	  FB.ui({method:'feed', 
	     name:$(a).attr("fbs-name"), 
	     link:$(a).attr("fbs-link"), 
	     caption:$(a).attr("fbs-caption"), 
	     description:$(a).attr("fbs-description"), 
	     picture:$(a).attr("fbs-picture")
	    }); 
	    return false;
	});
	$(".fb-login-btn").click(function(){
		FB.getLoginStatus(function(response){
			if(response.authResponse)
				facebook_login_check();
			else{
				FB.login(function(response){
					if(response.authResponse)
						facebook_login_check();
				}, {scope: 'email'});
			}
		}); 
	});
	FB.Event.subscribe('edge.create', function(targetUrl) {
		_gaq.push(['_trackSocial', 'facebook', 'like', targetUrl]);
			});
	/*twttr.events.bind('tweet', function(event) {
		if (event) {
			var targetUrl;
			if (event.target && event.target.nodeName == 'IFRAME') {
				targetUrl = extractParamFromUri(event.target.src, 'url');
			}
			_gaq.push(['_trackSocial', 'twitter', 'tweet', targetUrl]);
					}
	});*/

};
(function() {
  var e = document.createElement('script'); e.async = true;
    e.src = document.location.protocol +
      '//connect.facebook.net/en_US/all.js';
    document.getElementById('fb-root').appendChild(e);
 }());


function stop_bottom(){
	jQuery('#column > div').css({'position': 'fixed', 'bottom': '0px'});
}

function stop_top(){
	jQuery('#column > div').css({'position': 'fixed', 'top': '0px'});
}

function mid_scroll(){
	var offset = jQuery('#column div').offset(),
		pos =  offset.top;
	jQuery('#column > div').css({'position': 'absolute', 'top': pos+'px'});
}

(function(){
	var push_performance = false;
	function loadPerformance() {
		if(typeof window.performance != 'undefined') {
			var p = document.createElement('script');
			p.setAttribute('src', '/wp-content/themes/SMP-BoF-Theme/js/scripts/performance.js?v=2');
			document.body.appendChild(p);
		}
	}
	if(push_performance){
	    var preonload = window.onload;
	    window.onload = (typeof window.onload != 'function')?loadPerformance:function(){preonload();loadPerformance();};
	 }
})();

