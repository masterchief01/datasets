{
	s.channel = 'Food';
}
if($('body').hasClass('homeandgarden'))
{
	s.channel = 'Home & Garden';
}
if($('body').hasClass('love') || $('body').hasClass('love-sex'))
{
	s.channel = 'Love & Sex';
}



