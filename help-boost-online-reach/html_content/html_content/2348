</script><script type="text/javascript">
(function() {
    //var $ = Mobify.$;

    $.extend($.support, {
        touch: 'ontouchend' in document,
        //transitions: 'WebKitTransitionEvent' in window,
        transitions3d: !! (window.WebKitCSSMatrix && 'm11' in new WebKitCSSMatrix())
    });

    var abs = Math.abs;
    var has = $.support;

    // TODO: These are useful and should be factored out!
    var getXY = (has.touch)
         ? function (e) {e = e.originalEvent; return {x: e.touches[0].clientX, y: e.touches[0].clientY}}
         : function (e) {return {x: e.clientX, y: e.clientY}};

    var events = (has.touch)
        ? {down: 'touchstart', move: 'touchmove', up: 'touchend'}
        : {down: 'mousedown', move: 'mousemove', up: 'mouseup'};
    
    $.support.events = events;

    var translateOpen = 'translate' + (has.transitions3d ? '3d(' : '(');
    var translateClose = has.transitions3d ? 'px,0,0)' : 'px,0)';
    var transformProperty = (function() {
        var props = ['transformProperty', 'WebkitTransform', 'MozTransform', 'OTransform', 'msTransform'];
        var test = document.createElement('div');
        var testStyle = test.style;
        for (var i in props) {
            if (testStyle[props[i]] !== undefined) {
                return props[i];
            }
        }
    })();
    var hasTransform = !!transformProperty;

    var slide = function (options) {
        var $slide = $(this);
        // Don't slide if we're already bound.
        if (!$slide.length || $slide.data('slide')) return;
        $slide.data('slide', true);

        var $nodes = $slide.children();
        var length = $nodes.length;
        var slideStyle = $slide[0].style;
        var opts = $.extend({}, slide.defaults, options);
        var index = 0;
        var transitionOffset = 0;
        var transition = hasTransform ?
             function (v) {slideStyle[transformProperty] = translateOpen + (v || transitionOffset) + translateClose} : 
             function (v) {slideStyle.left = (v || transitionOffset) + 'px'};
        
        transition();

        function move(direction, force) {
            var newIndex = index + (direction * -1);
            moveTo(newIndex, force);
        };

        // TODO: Force could stand to be thought through further.
        function moveTo(newIndex, force) {

			// prevent out of range values
            if(newIndex < 0) newIndex = 0;				
			if(newIndex >= length) newIndex = length - 1;
	
            if (force || newIndex != index) {
                if (!force) $slide.trigger('indexchange', [newIndex, index]);

                // Focus on an element while moving.
                // TODO: Calculate a natural animation time from the current position.
                if (opts.focus && !force) $('body').animate({scrollTop: $(opts.focus).offset().top}, 250);

                index = newIndex;

                var $cur = $nodes.eq(index);
                var $start = $nodes.eq(0);
                var curLeft = $cur.position().left;
                var startLeft = $start.position().left;
                var curWidth = $cur.width();
                var startWidth = $start.width();
                var initialOffset = parseInt($start.parent().css('marginLeft'));
                
                transitionOffset = -(curLeft - startLeft - (curWidth - startWidth) * ( initialOffset / startWidth));
                
                if (!force) {
                    $nodes.slice(0, newIndex + 1 + opts.lazyLookahead).find('img[lazysrc]').each(function(i, el) {
                        this.setAttribute('src', this.getAttribute('lazysrc'));
                        this.removeAttribute('lazysrc');
                    });
                }
            }

            slideStyle.webkitTransitionDuration = opts.duration;
            slideStyle.MozTransitionDuration = opts.duration;
            transition();
        }

        var dragging = false;
        var canceled = false;
        var xy, dx, dy, dragThresholdMet;

        function start(e) {
            if (!has.touch) e.preventDefault();

            dragging = true;
            canceled = false;
            xy = getXY(e);
            dx = 0;
            dy = 0;
            dragThresholdMet = false;
            slideStyle.webkitTransitionDuration = 0;
            slideStyle.MozTransitionDuration = 0;
        }

        function drag(e) {
            if (!dragging || canceled) return;
                
            var cXY = getXY(e);
            dx = xy.x - cXY.x;
            dy = xy.y - cXY.y;

            if (dragThresholdMet || abs(dx) > abs(dy) && (abs(dx) > opts.minDragDelta)) {
                dragThresholdMet = true;

                e.preventDefault();

                // Slow movement when out of bounds.
                if ((index == 0 && dx < 0) || ((index == length - 1) && dx > 0)) dx *= 0.4;

                transition(transitionOffset - dx);
            
            } else if ((abs(dy) > abs(dx)) && (abs(dy) > opts.minDragDelta)) {
                canceled = true;
            }
        }

        // Calculate # of steps to slide by checking x delta.
        function end(e) {
            dragging = false;

            var adx = abs(dx);
            var steps = 0;
            if (adx > abs(dy) && adx > opts.minMoveDelta) {
                steps = adx > $nodes.eq(index).width() ? 2 : 1;
                steps *= dx > 0 ? -1 : 1;
            }
            move(steps);
        }

        function click(e) {
            if (dragThresholdMet) e.preventDefault();
        }

        function mouseout() {
            if (dragging) end();
        }

        // Reset transition offset if item width changes on rotation.
        function orientationchange() {
            setTimeout(function() {
                move(null, true);
            }, 1);
        }


        // orientationchange can only be bound on window.
        $(window).bind('orientationchange.slide', orientationchange);

        $slide
            .bind(events.down + '.slide', start)
            .bind(events.move + '.slide', drag)
            .bind(events.up + '.slide', end)
            .bind('click.slide', click)
            .bind('slidemove', function(e, direction) { 
                move(direction); 
            })
            .bind('indexchange', function(e, newIndex, oldIndex) {
                $(opts.dots).children(':nth-child(1)')
                    .removeClass(opts.dotsClass)
                    .eq(newIndex).addClass(opts.dotsClass);
            })
            .trigger('indexchange', [index, index]);

        if (!has.touch) $slide.bind('mouseout.slide', mouseout);
        
        if (length <= opts.maxDots) {

	if(length == 5) $(opts.dots).addClass('items4');
	if(length == 6) $(opts.dots).addClass('items5');

	 // create list item for the dots
			for(i=0; i < length; i++){
				if(i == 1){
					$(opts.dots).append('<li class="active">&bull;</li>');
				} else {
					$(opts.dots).append('<li>&#176;</li>');
				}			
}


            $(opts.dots).addClass('on')
			.children().bind(events.up + '.slide', function() {
				$(opts.dots+' li').html('&#176;').removeClass('active');
				$(this).html('&bull;').addClass('active');
				moveTo($(this).index());
            });
		}
                
        return this;
    }

    slide.defaults = {
        minDragDelta: 10,
        minMoveDelta: 20,
        dots: '.x-slide-dots',
        dotsClass: 'x-current',
        maxDots: 12,
        duration: '0.5s',
        lazyLookahead: 1
    };

    $.fn.slide = slide;
    
    document.addEventListener('DOMContentLoaded', function DOMContentLoaded() {
        document.removeEventListener('DOMContentLoaded', DOMContentLoaded, false);
        $('.container.right-tabs.tabs-5').slide();
    }, false);

    document.addEventListener('DOMContentLoaded', function DOMContentLoaded() {
        document.removeEventListener('DOMContentLoaded', DOMContentLoaded, false);
        $('.container.right-tabs.tabs-4').slide();
    }, false);

    document.addEventListener('DOMContentLoaded', function DOMContentLoaded() {
        document.removeEventListener('DOMContentLoaded', DOMContentLoaded, false);
        $('body.mob-photos #TabsCarousel').slide();
    }, false);
	
})();

</script><script src="http://www.ivillage.com/public/javascripts/jquery/jquery.cookie.js" type="text/javascript"></script><script type="text/javascript">
$(document).ready(function(){

if( !$.cookie("iv_id") )
{
	$('.if-logged-out').show();
	$('.if-logged-in').remove();
} else {
        // else user is signed in
	$('.if-logged-in').show();
	$('.if-logged-out').remove();
	
	$('.username').each(function(){
	
		var HTML    =  $(this).text(),
		cookie  =  $.cookie("iv_id");
		  
		if( HTML.toLowerCase() != cookie ) {
			$(this).text(cookie.capitalize());
	 	}
	 	$(this).show();
	});
}

});
</script></div>
