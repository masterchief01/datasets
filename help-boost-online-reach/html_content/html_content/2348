/*
s.channel = ""; // site section
s.prop2   = ""; // article title
s.prop3	  = ""; // content type
*/

/* SET CHANNEL VAR (s.channel) */
if($('body').hasClass('ivillage-home')) 
{
	s.channel = 'Homepage';
}
