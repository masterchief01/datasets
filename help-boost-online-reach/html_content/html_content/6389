			return {  
				log : function(v){  
				}  
			}  
		}  
		var console = new _console();  
	}  

	jQuery("img.logo").dblclick(function(){
		    jQuery("#serverName").show();
			setTimeout(function() {
			    jQuery("#serverName").hide();
			}, 2500);
	});
});
</script>

