<script>	
function battleMouseOver(name) {
	//console.log('Mouseover '+name);
	jQuery('.'+name+'Image').removeClass(name+'Image').addClass(name+'ImageHover');
	//console.log('remove: '+ name+'Image,  add: '+ name+'ImageHover');
	jQuery('.'+name).removeClass(name).addClass(name+'Hover');
	//console.log('remove: '+ name+ ',  add: '+ name+'Hover');
	jQuery('.'+name+'dek').addClass('aelectionBattleDEKHover');
	//console.log('add: aelectionBattleDEKHover');
	jQuery('.'+name+'morePollsImage').removeClass(name+'morePollsImage').addClass(name+'morePollsImageHover');
}
function battleMouseOut(name) {
	//console.log('Mouseout '+name);
	//jQuery('.'+name+'hed').attr('src', hed);
	jQuery('.'+name+'ImageHover').removeClass(name+'ImageHover').addClass(name+'Image');
	//console.log('remove: '+ name+'ImageHover,  add: '+ name+'Image');
	jQuery('.'+name+'Hover').removeClass(name+'Hover').addClass(name);
	//console.log('remove: '+ name + 'Hover,  add: '+ name);
	jQuery('.'+name+'dek').removeClass('aelectionBattleDEKHover');
	//console.log('remove: aelectionBattleDEKHover');
	jQuery('.'+name+'morePollsImageHover').removeClass(name+'morePollsImageHover').addClass(name+'morePollsImage');
}
</script>

