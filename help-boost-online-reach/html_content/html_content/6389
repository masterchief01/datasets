		jQuery(function() {
			// record an event for which version of the buttons was displayed
			_gaq.push(['_trackEvent', buttonVersion, 'displayed', '', '', true]);
			// and display the proper button version
			jQuery("."+buttonVersion).show();
		});

		console.log("showing button version: "+buttonVersion);
		function recordShareClick(e, svc) {
			if(svc != "unlike") {
				console.log("Got a click for "+buttonVersion+" button: "+svc);
				_gaq.push(['_trackEvent', buttonVersion, 'click', svc, '', true]);
			}
		}

		// A/B test callbacks for social buttons
		function gplusClickCallback(json) {
			if(json.state=='on') recordShareClick('click', 'plusone');
		}
		stLight.subscribe("click",recordShareClick); //register the callback function with sharethis 

	  (function() {
	    var po = document.createElement('script'); po.type = 'text/javascript'; po.async = true;
	    po.src = 'https://apis.google.com/js/plusone.js';
