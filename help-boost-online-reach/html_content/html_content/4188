<p style="display: none;"><input type="hidden" id="akismet_comment_nonce" name="akismet_comment_nonce" value="9504174470" /></p><h3 style='text-align: center;'>Click to Insert Smiley</h3>	<script language="javascript" type="text/rocketscript">
	//<![CDATA[
	function tseCTIcomment(tseClicked) {
		var target;
		if (document.getElementById('comment') && document.getElementById('comment').type == 'textarea') {
			target = document.getElementById('comment');
			scroll = target.scrollTop;
		}
		else {
			return false;
		}
		if (document.selection) {
			target.focus();
			sel = document.selection.createRange();
			sel.text = tseClicked;
			target.focus();
		}
		else if (target.selectionStart || target.selectionStart == '0') {
			var startPos = target.selectionStart;
			var endPos = target.selectionEnd;
			var cursorPos = endPos;
			target.value = target.value.substring(0, startPos) + tseClicked + target.value.substring(endPos, target.value.length);
			cursorPos += tseClicked.length;
			target.focus();
			target.selectionStart = cursorPos;
			target.selectionEnd = cursorPos;
		}
		else {
			target.value += tseClicked;
			target.focus();
		}
		target.scrollTop = scroll;
	}
	//]]>
