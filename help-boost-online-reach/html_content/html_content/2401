<script type="text/javascript">
    (function($){ 
        $("ul#front_door_spots").delegate("a","click", function(e) { 
                var photoTitle = escape($(this).find("span.spot_title").text().replace(/\s/g),"_");
                
                if ( window.location.href.match(/\/photos(?=\/|$)/) ) {
                    eol.track.page("events","editors_picks","photo_gallery",photoTitle);
                } else if ( window.location.href.match(/b\d{3,}_/) ) { 
                    eol.track.page("events","editors_picks","detail",photoTitle);
                } else { 
                    eol.track.page("events","editors_picks","front_door",photoTitle);
                }
            });
    })(jQuery);
</script>
