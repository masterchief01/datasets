var cbsiad16_100 = {
    'SP' : '16',
    'POS' : '100',
    'WIDTH' : '300',
    'HEIGHT' : '250',
    'CALLBACK' : {onload: mpuOnLoadCallback} 
};
if (location.search.substring(1).indexOf('DCLK')>-1) $('<input type="text" value="cbsiad16_100" style="width:300px"><br>').insertBefore('#cbsiad16_100');
cbsiSetupAdDiv("cbsiad16_100", cbsiad16_100);
function mpuOnLoadCallback(callbackArr){
    if (callbackArr.isBlank == 0){$('#mpu_spacer').show();}
    else if (callbackArr.isBlank == 1){$('#cbsiad16_100').hide();}
}
</script>
