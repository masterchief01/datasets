#livefyre .lf_logo { display: none !important; }

#livefyre .lf_mod_options a[name=delete] { display: none !important; }

#livefyre .lf_main_avatar { cursor: default !important; }

#livefyre .lf_comment_attachments { display: none !important; }

/* If more than 10 listeners, don't blow out column */
