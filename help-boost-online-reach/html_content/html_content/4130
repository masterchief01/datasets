var _cc_sm_counter_top = $('.sm_counters').offset().top;
$(document).ready( function() {
	$(window).scroll( function() {
		if ($(window).scrollTop() > _cc_sm_counter_top) {
			$('.sm_counters').addClass('floating');
		} else {
			$('.sm_counters').removeClass('floating');
		}
	});
});
</script>
