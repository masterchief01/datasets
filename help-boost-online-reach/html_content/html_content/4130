    if ($("#cse-search-box input[type=text]").val().trim() == '') {
        $("#cse-search-box input[type=text]").addClass("empty");
    }
    $("#cse-search-box input[type=text]").focus(function() {
        //alert("focus");
        $("#cse-search-box input[type=text]").removeClass("empty");
    });
    $("#cse-search-box input[type=text]").blur(function(){
        //alert("blur");
        if ($("#cse-search-box input[type=text]").val().trim() == '') {
            $("#cse-search-box input[type=text]").addClass("empty");
        }
    });

