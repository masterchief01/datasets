function emptyAdCollapse(){
jQuery("div.cxAdvertisement").not(":has(script)").remove();
var listAdDivs = jQuery("div.cxAdvertisement");
for (var j=0;j<listAdDivs.length;j++){
if(listAdDivs[j].id.substring(0,2) == "RP"){
if(listAdDivs[j].offsetHeight == 20){ 
jQuery(listAdDivs[j]).removeClass().addClass("cxAdvertisementEmpty");
}
}
}
}
