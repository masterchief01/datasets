$(document).ready(function(){	$("nav.menuGroup > ul > li.tab").each(function(index) {
var dropElement = $(this).find(".drop");
if(dropElement) {
var loadingElement = dropElement.find(".loading");
if(loadingElement) {
var ajaxURL = loadingElement.attr("rel");
if(ajaxURL) {
// alert("ajaxURL: " + ajaxURL);
jQuery.ajax({
url: ajaxURL,
dataType: 'html',
success: function(data){
dropElement.append(data);
loadingElement.remove();
}
}); }
}
}
});
if (!Modernizr.touch){	$(".nav-main .tab").hoverIntent({
sensitivity: 4,
interval: 100, over: drop_show, timeout: 500, out: drop_hide
});
} else {
$(".nav-main .tab").hover(function() {
if($(this).find(".drop").length>0) {
$(this).addClass("drop-hover");
}
}, function() {
$(this).removeClass("drop-hover");
});
}
});
//]]></script>
