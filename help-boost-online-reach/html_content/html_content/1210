function swapStars(star) {

	for(i=1;i<6;i++){
		document.getElementById("star"+i).src='stars/star.png';
	}
	for(i=1;i<=star;i++){
		document.getElementById("star"+i).src='stars/star1.png';
	}
}
function stopStars(star) {
	
	swapStars(star);
	for(i=1;i<6;i++){
		document.getElementById("star"+i).onmouseout='';
		document.getElementById("star"+i).onmouseover='';
	}
}
</script>

