<script type="text/javascript">  
function pageLoad()  
{
    var menu = $find("ctl00_CPHTopmenu_ctl00_RadMenu1");
    var href = window.location.href;  
    var index = href.lastIndexOf("/");  
    var pageURL = href.substring(index + 1);

    var items = menu.get_items();
    var item = null;
      
    for (var i = 0; i < items.get_count(); i++)  
    {
        
        // Look into 1st level items
        var url = items.getItem(i).get_navigateUrl();
        var lindex = url.lastIndexOf("/");
        var nurl = url.substring(lindex + 1);
        if (nurl == pageURL)  
        {
            item = items.getItem(i);  
            break;
        }

        // look into 2nd level items
        if (item == null) {
            var subItems = items.getItem(i).get_items();

            for (var j = 0; j < subItems.get_count(); j++) {
                var urlSub = subItems.getItem(j).get_navigateUrl();
                //alert(urlSub);
				if (urlSub != null)
				{
					var lindexSub = urlSub.lastIndexOf("/");
					var nurlSub = urlSub.substring(lindexSub + 1);
					if (nurlSub == pageURL) {
						item = subItems.getItem(j);
						break;
					}
				}
				// look into 3rd level items
				if (item == null) {
					var subsubItems = subItems.getItem(j).get_items();  //items.getItem(i).getItem(j).get_items();

					for (var k = 0; k < subsubItems.get_count(); k++) {
						var urlSubSub = subsubItems.getItem(k).get_navigateUrl();
						var lindexSubSub = urlSubSub.lastIndexOf("/");
						var nurlSubSub = urlSubSub.substring(lindexSubSub + 1);
						if (nurlSubSub == pageURL) {
							item = subsubItems.getItem(k);
							break;
						}       
					}
					   
				}		
            }
               
        }
		       
              
    }         
      
    if (item)  
    {  
        while (item != menu)  
        {  
            var element = item.get_linkElement();  
            element.className += " rmFocused";  
            item = item.get_parent();  
        }  
    }       
}  
</script> 
                    
            </div>
