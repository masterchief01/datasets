<script type="text/javascript">
    
    // Hide unused divs specially in IE
    
	if (/MSIE (\d+\.\d+);/.test(navigator.userAgent)){ //test for MSIE x.x;
		
		var ieversion=new Number(RegExp.$1) // capture x.x portion and store as a number
		if (ieversion == 7)
		{
			//alert(ieversion);
			if (!document.getElementById('CPHDocumentationTutorials').hasChildNodes())
				document.getElementById('CPHDocumentationTutorials').style.display = "none";

			if (!document.getElementById('PartnersSection').hasChildNodes())
				document.getElementById('PartnersSection').style.display = "none";

			if (!document.getElementById('LeftMainContent').hasChildNodes() && !document.getElementById('rightContent').hasChildNodes())
				document.getElementById('blogsandflashcontainer').style.display = "none";

			if (!document.getElementById('LeftKBContent').hasChildNodes() && !document.getElementById('rightKBContent').hasChildNodes())
				document.getElementById('kbMainContainer').style.display = "none";
			
			if (!document.getElementById('hpPanel1').hasChildNodes() && !document.getElementById('hpPanel2').hasChildNodes() && !document.getElementById('hpPanel3').hasChildNodes())
				document.getElementById('HomeCenterPanels').style.display = "none";
		}
	}

</script>

