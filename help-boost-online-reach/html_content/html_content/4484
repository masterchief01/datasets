var rscriptloaded = 0;
function displayEmailForm() {
	if(rscriptloaded) {
		mi_story_tool.storyTool("email", "///.html");
	} else {
		$.getScript("http://api.recaptcha.net/js/recaptcha_ajax.js", function() {
			mi_story_tool.storyTool("email", "///.html");
			rscriptloaded = 1;
		});
	}
}
</script>

