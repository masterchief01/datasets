<script type="text/javascript">
$(document).ready(function(){
	document.domain="abc.net.au";	
	var UserRequest = new PluckSDK.UserRequest();	
	var userKey = new PluckSDK.UserKey();
	var cookieKey = getCookie('u');
	userKey.Key = cookieKey;
	UserRequest.UserKey = userKey;

	PluckSDK.SendRequests([UserRequest], success);
			

	// Full image script
		
	var xmlDoc = $.ajax({
	  url:"3472198-mediarss.xml",
	  context: document.body,
	  success: function(data){		
	  
		  var currentImgHeight =  $('#related_photo img').height();
		  var fullImgHeight = 394 + 30;	
		  
		  if (currentImgHeight > 191) {
			  var fullImg = $(data).find('*[width="700"][height="933"]');
			  fullImg = fullImg.attr('url');	
			  fullImgHeight = 500 + 30;	
		  } else {
			  var fullImg = $(data).find('*[width="700"][height="394"]');
			  fullImg = fullImg.attr('url');
		  }			

		
		if (typeof fullImg != 'undefined') {										
			 $('#related_photo img').css('cursor','pointer');
			$('#related_photo img').click( function() {										
				   //window.open(fullImg,'_blank','width=730');	
				   imgWindow(fullImg,null,700,fullImgHeight,$('#related_photo img').attr("title"))							
			});							  	
		}

	  }						
	});	
	
	// END Full image script	
	

	if ($('.heading .abcopen').length > 0) {
	
		$('.heading .abcopen').after('<div class="byline_logo"><a href="http://open.abc.net.au"><img src="/local/global_img/open/open_logo_sm.jpg" alt="ABC Open" title="ABC Open"/></a></div>');
		
	}
	



	
	if ($('.timeline_container').length > 0) {
	
	$('.byline').remove();
	$('#col1 .date').remove();		
	var timelineContent = $('.timeline_container').html();
	var storyHeading = $('#col1 .heading').html();	
	$('#col1 .heading').remove();
	$('.timeline_container').remove();
	$('#col1').before('<div class="heading">'+storyHeading+'</div><div class="timeline_container" style="margin-left: -20px;">'+timelineContent+'</div>');

	}
	

	

	
});

function success(data) {
		
		//Login function
		userLogin(data[0], 'brokenhill');
		
		
		if ($('.competitions.media').length > 0) {
			formPopulate(data[0]);
		}

}
</script>

