<script type="text/javascript">

$(function() {
      var isiDevice = /iPad|iPhone|iPod/i.test(navigator.userAgent);

		// setup overlay actions to buttons
		$("button[rel]").overlay({	
			onLoad: function(content) {
			// find the player contained inside this overlay and load it
            if (!isiDevice){
               this.getOverlay().find("a.player").flowplayer(0).load();
            }
			},
		
			onClose: function(content) {
            //$("a.shotlistTab").removeClass("current");
            if ($(".downloadPane").find("#content")){
		         $(".downloadPane").find("#content").remove();
               $(".downloadPane").hide(); 
               $("a.player").show();
            }

            $(".shotlistPane").hide(); 

            if (!isiDevice)
            $f().unload();
			}
		});
		
		//hide / show overlay tabs
		$(".shotlistPane").hide();

		$("a.shotlistTab").toggle(
		   function(){$(".shotlistPane").show();},
		   function(){$(".shotlistPane").hide();}
		);
			
		$("a.downloadTab").toggle(
         function(){
            $("a.player").hide();
            $(".downloadPane").append("<div id=\"content\"></div>");
            $(".downloadPane").find("#content").load("/?lid="+this.id+"&tmpl=shoppertypeselection&noga=1");
		      $(".downloadPane").show();
         },
         function(){
		      $(".downloadPane").find("#content").remove();
		      $(".downloadPane").hide();
            $("a.player").show();
         }
      );

      $(".buydownload[rel]").overlay({
         onBeforeLoad: function(content){
            $("#paneTabs").hide(); 
         },

         onLoad: function(content){
            var lid = this.getTrigger().attr("id");

            $("a.player").hide();
            $(".downloadPane").append("<div id=\"content\"></div>");
            $(".downloadPane").find("#content").load("/?lid="+lid+"&tmpl=shoppertypeselection&noga=1");
            $(".downloadPane").show();
         },

         onClose: function(content){
            $("#paneTabs").show(); 
            $("a.player").show();
            $(".downloadPane").find("#content").remove();
            $(".downloadPane").hide();
         }
      });

		// install flowplayers (depends on if the user-agent is a PC browser

      if (!isiDevice)
		$("a.player").flowplayer("/player/flowplayer.commercial-3.2.5.swf", {
		key: "#$b5b4b6eedbb628d0100",
		clip: { 
  			onStart: function(clip) { 
    		var wrap = jQuery(this.getParent()); 
    		wrap.css({width: clip.metaData.width, height: clip.metaData.height}); 
  			} 
			}
		});		
});
</script>

