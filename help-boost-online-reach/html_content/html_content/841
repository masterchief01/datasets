#page {padding:80px; font-family:verdana}

div.overlay {display: none; padding:20px; border: 1px solid #333333; background-color: #484848; -moz-box-shadow: 0 0 40px rgba(0,0,0,.9); -webkit-box-shadow: 0 0 40px rgba(0,0,0,.9); box-shadow: 0 0 40px rgba(0,0,0,.9)}

div.overlay a.close {position: absolute; right: -15px; top: -15px; cursor:pointer; height: 35px; width: 35px; background-image: url(/images/player_close.png)}

div.overlay a.player {display: block; /*height: 380px; width: 565px;*/ height: 360px; width: 640px }

