        <li class="clearfix">
            <div class="image">
                <a id="content_0_rightcolumn_3_ctlNewRecipeSidebarList_rptRecipes_ctl00_lnkImage" href="/recipes/gooey-honey-cake-recipe/1/"><img id="content_0_rightcolumn_3_ctlNewRecipeSidebarList_rptRecipes_ctl00_imgRecipe" class="imgthumb" title="Gooey Honey Cake" src="/assets/recipe/honeycakehero1_586-634639560188512812.jpg" alt="Gooey Honey Cake" style="border-width:0px;" /></a>
