							{
								document.getElementById("website_channel").value = "yzime97jm8o";
								document.getElementById("cx").value = "000052645401690375061:yzime97jm8o";
								document.searching.id = "searchbox_000052645401690375061:yzime97jm8o"
								document.getElementById("sa").value = "";
							}
							else if (selected == "Forum")
							{
								document.getElementById("website_channel").value = "encnazrldeo";
								document.getElementById("cx").value = "000052645401690375061:encnazrldeo";
								document.searching.id = "searchbox_000052645401690375061:encnazrldeo"
								document.getElementById("sa").value = "";
							}
							else if (selected == "Shop")
							{
								document.getElementById("website_channel").value = "gnfrbi9lceg";
								document.getElementById("cx").value = "000052645401690375061:gnfrbi9lceg";
								document.searching.id = "searchbox_000052645401690375061:gnfrbi9lceg"
								document.getElementById("sa").value = "";
							}
							else if (selected == "Travel")
							{
								document.getElementById("website_channel").value = "";
								document.getElementById("cx").value = "000052645401690375061:";
								document.searching.id = "searchbox_000052645401690375061:"
								document.getElementById("sa").value = "";
							}
							document.searching.action = "/global-search.asp";
							document.searching.submit();
						}
