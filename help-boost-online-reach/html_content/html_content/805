                  <td colspan="3"><font color="#fff799" face="arial" size="2"><img src="spacer.gif" height="1" width="20"><b>Results</b></font></td>
                </tr>
                <tr>
                  <td align="right" bgcolor="gray"><font face="arial" size="2"><b>Protein</b> </font></td>

                  <td colspan="3" bgcolor="#3d3d3d"><b><font face="arial" size="2"><div id="tprotein"></div></font></b></td>
                </tr>


