                <script type="text/javascript">
                    (function($) {
                        $('#bb-login-form').bind('submit', function() {
                            var formobj = this;
                            if (formobj.username.value === '') {
                                window.alert('Please enter your Username or Email Address.');
                                return false;
                            }
                            else if (formobj.password.value === '') {
                                return false;
                            }
                            else {
                                var junk_output;
                                if (!BB.Util.md5) {
                                    window.alert('Could not check your password');
                                    return false;
                                } else {
                                    BB.Util.md5.md5hashAuto(formobj.password, formobj.password_md5, junk_output, 0);
                                }
                                return true;
                            }
                        });
                    }(jQuery));
                </script>
