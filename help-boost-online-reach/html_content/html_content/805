    <script type="text/javascript" id="com-bodybuilding-content-web-page-fun-legacy-ArticlePage-2-0">

        var notifyReplies = false;

        function handleErr(x, e) {
            if (x.status == 0) {
                alert('You are offline!!\n Please Check Your Network.');
            } else if (x.status == 404) {
                alert('Requested URL not found.');
            } else if (x.status == 500) {
                alert('Internel Server Error.');
            } else if (e == 'parsererror') {
                alert('Error.\n Parsing JSON Request failed.');
            } else if (e == 'timeout') {
                alert('Request Time out.');
            } else {
                //alert('Unknow Error.\n' + x.responseText);
            }
        }

     function getMaxCommentsPerPage(){
          maxComments = parseInt($('var.max-comments-per-page').html());
         return maxComments;
     }
     
     function getMaxCommentsOnCurrentPage(){
         maxComments = parseInt($('var.max-comments-on-current-page').html());
        return maxComments;
     }
     
     function getMaxCommentsOnFirstPage(){
         maxComments = parseInt($('var.max-comments-on-first-page').html());
        return maxComments;
     }
     
     function getCurrentPage(){
         currentPage = parseInt($('var.current-page').html());
         return currentPage;    	 
     }

     function getCommentsCount(){
         commentsCount = parseInt($('var.comments-counter').html());
         return isNaN(commentsCount)? 0:commentsCount
     }
     
     function getCurrentPageCommentsCount(){
    	 if ( getCommentsCount() > getMaxCommentsOnFirstPage() ){
    		 var commentsAfterFirstPage = getCommentsCount() - getMaxCommentsOnFirstPage(); 
    		 var totalPages = Math.ceil( commentsAfterFirstPage / getMaxCommentsPerPage() ) + 1;
    		 if ( getCurrentPage() == totalPages ){
    			 return commentsAfterFirstPage % getMaxCommentsPerPage();
    		 }else{
    			 if ( getCurrentPage() == 1 )
    				return getMaxCommentsOnFirstPage();
    			 else
    			 	return getMaxCommentsPerPage();
    		 }
    	 }else{
    		 return getCommentsCount();    		 
    	 }
     }

     function getCurrentHost()
     {
         var currentHost = $('var.current-host').html();
         return "http://www."+currentHost;
     }



     function incCommentsCount(){
          commentsCount = getCommentsCount();
          if ((commentsCount - getMaxCommentsOnFirstPage()) % getMaxCommentsPerPage() == 0 ){ 
        	  location.reload();
          }else{
         	$('var.comments-counter').html(commentsCount+1);
         	$('span.counter-comments').html(commentsCount+1);
         	$('span.total').html(commentsCount+1);
            var stringLabel =   (commentsCount+1)==1?"Comment":"Comments";
            $('span.total-count-label').html(stringLabel);
            $('#count-header-label').html(stringLabel);
          }
     }

     function setNotifyReplies()
     {
         notifyReplies = !notifyReplies;
     }

     function showError(obj, postItem, isReply)
     {
    	 $('.success').hide();
         var errorCode = (obj == 'default')?0:obj.errorCode;
         var message = 'Due to technical difficulties, your comment was unable to post. If this problem persists, please <strong><a href="http://www.bodybuilding.com/store/email.htm ">contact us</a></strong>.';
		 var title = 'ERROR';
         
         switch(errorCode)
         {
            case 701:
                message = '<li class="last">You can only post one comment per 60 seconds. Please wait and try again.</li>';
                errorOnPost(message, title, isReply, postItem, false);
                break;
            case 702:
            	title = 'NOTICE';
                message = '<li class="last">Due to our spam filter, your comment has not yet been posted. If not spam, your comment will be posted within 48 hours.</li>';
                errorOnPost(message, title, isReply, postItem, true);
                break;
            case 703:
                    if (obj.errorMessage != null && obj.errorMessage != undefined)
                    {
                        if (obj.errorMessage == 8)
                            message = '<li class="last">'+BB.Instances.currentUser.userName+', your account has been banned. If you believe this is an error, please feel free to <a href="http://www.bodybuilding.com/store/email.htm ">contact us</a>.</li>';

                        if (obj.errorMessage == 3 || obj.errorMessage == 4)
                            message = '<li class="last">In order to continue, you must validate your BodySpace account from the email that has been sent to you. If you no longer have this email,&nbsp;<a href="http://my.bodybuilding.com/register.php?a=ver">view the activation page</a>.</li>';

                        errorOnPost(message, title, isReply, postItem, true);
                    }
                break;
            default:
                break;
        }

     }

     function showSuccess(postItem, isReply)
     {
    	 $('.error-box').hide();
         var message = '<li>Your comment has been posted! Because comments are displayed from oldest to newest, it will appear on the last page. </li>';

         if (notifyReplies && !isReply)
         {
            message += '<li class="last">You will be notified when others reply to your comment.</li>';
            resetNotificationControls();
         }else{
        	 message = message.split('<li>').join('<li class="last">');
         }

    	 messageBox(message, "SUCCESS", isReply, postItem, false, ".success" );
    	 $('textarea.autosize').val('');
     }
     
     function messageBox(message, title, isReply, postItem, replace, className){
    	 $(className).find('h3').html(title + ":");
    	 $(className).find('ul').html(message);
    	 
    	 if (!isReply){
    	 	if (replace){
    	 		postItem.replaceWith( $(className) );
    	 		postItem.closest('.feed-comment').find('.keep-pos').hide();
    	 	}else{
    			postItem.closest('.feed-comment').before( $(className) );
    	 	}
    	 }else{
     	 	if (replace){
    	 		postItem.find('.feed-comment-text').html("");
     	 		postItem.find('.feed-comment-text').prepend( $(className) );
     	 	}else{
    			postItem.prepend( $(className) );
     	 	}
    	 }


    	 
    	 $(className).removeClass('out-replay');    		 
    	 if (replace){
    		 $(className).removeClass('out');
    		 if (isReply){
    		 	$(className).addClass('replay');
    		 }else{
    		 	$(className).removeClass('replay');
    		 }
    	 }else{
    		 $(className).removeClass('replay');
    		 if (isReply){
    			 $(className).removeClass('out');
    		 	$(className).addClass('out-replay');
    		 }else{ 
    		 	$(className).addClass('out');
    		 }
    	 }    	 
    	 
    	 $(className).show();  	 
     }     
     
     function errorOnPost(message, title, isReply, postItem, replace){
    	 $('.error-box').hide();
    	 messageBox(message, title, isReply, postItem, replace, '.inline-message.error-box');
     }


    function replaceBadCharacters(text)
    {
        text= text.replace("ä", "a");
        text= text.replace("ö", "o");
        text= text.replace("ü", "u");
        text= text.replace("Ä", "A");
        text= text.replace("Ö", "O");
        text= text.replace("Ü", "U");
        text= text.replace("ß", "B");
        return text;
    }

     
     function postComment(e){
         var commentText = replaceBadCharacters($('textarea.autosize').val());
         var hash = $('.main-comment-box #entity-hash').val();
         var notifyRepliesParam = notifyReplies?"true":"false";

         if ($.trim(commentText).length < 5){
            if (!$(e).data('tipswift').shown){
               BB.Instances.tsTip.doShow($(e).data('tipswift'));
               $('.tipswift-inner .close').bind('click.remove', function(){
                  BB.Instances.tsTip.doHide($(e).data('tipswift'));
               });
               var tipswift = $('.tipswift.tip'),
               position = $(tipswift[tipswift.length -1 ]).offset();
               $(tipswift[tipswift.length -1 ]).offset({ top: position.top-2, left: position.left-15});
            }
            return false;
         }else{
        	 if ($(e).data('tipswift').shown)
        		 BB.Instances.tsTip.doHide( $(e).data('tipswift') );
         }



         
          $.ajax({
		        type: 'POST',
		        url: getCurrentHost()+"/comments/post",
		        dataType: 'html',
		        data: 'text='+commentText+'&notifyReplies='+notifyRepliesParam+'&commentHash='+hash,
		        success: function(result) {

                if (result == null || result== undefined || result == "")
                        showError('default', $(e).closest('form'), false);
                else
                {
                    var obj = $.parseJSON(result);
                    if( obj.errorCode != undefined ){
                        showError(obj, $(e).closest('form'), false);
                    }
                    else {
                      showSuccess($(e).closest('form'), false);
                      $.extend(obj, obj, {'postHandler': 'postSecondLevelComment(this); return false;'});
                      // FIX: Accept paragraphs
                      obj.text = obj.text.replace(new RegExp( "\\n", "g" )," <br/> ");
                      var maxCommentsPerPage = getMaxCommentsOnCurrentPage();
                      var commentsCount = getCommentsCount();
                      if(getCurrentPageCommentsCount() < maxCommentsPerPage){
                    	 var hash = '<input class="comment-hash" value="'+obj.hash+'" name="commentsPanel:comments:pages-dataview:2:comment:comment-hash" type="hidden">';
                         BB.UI.GenerateMK.displayComment(e, formatBodyStats(obj), 'firstLevel',$('div.articles-commets'));
                         $('.last .principal').prepend(hash);
                         $('.last .feed-comment .grey-x').tipSwift({ plugins: [ $.tipSwift.plugins.removeDialog({'action':'removeComment'}) ]});
                      }else{
                          var commentTextArea = $(e).closest('.feed-comment-text').find('textarea.comment');
                          if (commentTextArea[0] !== undefined){
                              commentTextArea.css('height','90px');
                          }
                      }
                      incCommentsCount();
                    }
                }
		        },
                error:function (x,e) {
                   handleErr(x,e);
                }
		    });
     };

     function postSecondLevelComment(e){
         var parentHash =  $(e).closest('.article-comment-container').find('.feed-comment-post.principal').find('.comment-hash').val();
         // save a ref on the text area
         var commentTextArea = $(e).closest('.feed-comment-text').find('textarea.comment');
         if ($.trim(commentTextArea.val()).length < 5){
            if (!$(e).data('tipswift').shown){
              BB.Instances.tsTip.doShow($(e).data('tipswift'));
              $('.tipswift-inner .close').bind('click.remove', function(){
                 BB.Instances.tsTip.doHide($(e).data('tipswift'));
              });
              var tipswift = $('.tipswift.tip'),
              position = $(tipswift[tipswift.length -1 ]).offset();
              $(tipswift[tipswift.length -1 ]).offset({ top: position.top-2, left: position.left-15});
            }
            return false;
         }else{
        	 if ($(e).data('tipswift').shown)
        		 BB.Instances.tsTip.doHide( $(e).data('tipswift') );
         }
         
          $.ajax({
		        type: 'POST',
		        url: getCurrentHost()+'/comments/reply',
		        dataType: 'html',
		        data: 'text='+commentTextArea.val()+'&commentHash='+parentHash,
		        success: function(result) {

                if (result == null || result== undefined || result == "")
                        showError('default', $(e).closest('.feed-comment'), true);
                else
                {
                    var obj = $.parseJSON(result);
                    // FIX: Accept paragraphs

                    if( obj.errorCode != undefined ) {
                      showError(obj, $(e).closest('.feed-comment'), true);
                    } else {
                    	$('.error-box').hide();
                      obj.text = obj.text.replace(new RegExp( "\\n", "g" )," <br/> ");
                      commentTextArea.val('');

                      var hash = '<input class="reply-hash" value="'+obj.hash+'" name="commentsPanel:comments:page:1:feed-comment-post-panel:feed-comment:0:reply-hash" type="hidden">';
                      BB.UI.GenerateMK.displayComment(e, formatBodyStats(obj), 'secondLevel',$(e).closest('.feed-comment'));
                      $(e).closest('.feed-comment').parent().find('.grey-x').last().before(hash);
                      $(e).closest('.feed-comment').parent().find('.grey-x').last().tipSwift({ plugins: [ $.tipSwift.plugins.removeDialog({'action':'removeComment'}) ]});
                      incCommentsCount();
                      $(e).closest('.replay-feed-comment').remove();
                    }
                }
		        },
                error:function (x,e) {
                   handleErr(x,e);
                }
		  });
     };

     function moderateComment(e){
    	 e.doclick();
     }
     
     //e is the x button
     function removeComment(e){
    	var parentHash;
    	var element;
    	if ( $(e).parent().find('.reply-hash').length == 0 ){
    		//First Level
    		parentHash = $(e).closest('.article-comment-container').find('.feed-comment-post.principal').find('.comment-hash').val();
    		element = $(e).closest('.article-comment-container');
    	}else{
    		//Reply
    		parentHash = $(e).parent().find('.reply-hash').val();
    		if ( $(e).parent().parent().find('.feed-comment').length == 1 ){
    			element = $(e).parent(); 
    		}else{
    			element = $(e).parent().parent();
    		}
    	}
          $.ajax({
		        type: 'POST',
		        url: getCurrentHost()+'/comments/remove',
		        dataType: 'html',
		        data: 'commentHash='+parentHash,
		        success: function(result) {

                    if (result == null || result== undefined)
                        showError('default');

                    var obj = $.parseJSON(result);
                    if( obj.errorCode != undefined )
                        showError(obj);
                    else
                    {
                       //Remove comment
                       //element.remove();
                       location.href=$('var.reload-page').html();
                       location.reload();
                    }
		        },
                error:function (x,e) {
                   handleErr(x,e);
                }
		  });
     };
     
     function formatBodyStats(obj){
    	 if (obj.height != undefined) obj.height = formatHeight(obj.height);
    	 if (obj.weight != undefined) obj.weight = formatWeight(obj.weight);
    	 if (obj.upBy != undefined) obj.upBy = formatWeight(obj.upBy);
    	 if (obj.bodyFat != undefined) obj.bodyFat = formatBodyFat(obj.bodyFat);
    	 return obj;
     }
     
     function formatHeight(value){
    	 if (value == 0) return value;
         var inches = Math.floor(value);
         var feet = Math.floor(inches / 12);
         var leftover = inches % 12;
         return feet.toString() + "'" + (leftover > 0 ? leftover.toString() : "") + (leftover > 0 ? "\"" : "");
     }

     function formatWeight(value){
    	 if (value == 0) return value;
         var result = Number(Math.round(value * 100) / 100).toString() + " lbs";
    	 return result;
     }

     function formatBodyFat(value){
    	 if (value == 0) return value;
    	 var result = Number(Math.round(value * 100) / 100).toString() + "%";
    	 return result;
     }

     function resetNotificationControls()
     {
         notifyReplies = false;
         $('#notify-options').attr('checked', false);
     }
     
     function postSecondLevelReply(e){
         var user = {};
         $.extend(user, BB.Instances.currentUser, {'postHandler': 'postSecondLevelComment(this)'});
         BB.UI.GenerateMK.displayComment(e, user,'secondLevelReplay',$(e).closest('.article-comment-container'));
     };

     jQuery.fn.limitMaxlength = function(options){
    	 	 
   	 	    var settings = jQuery.extend({
   	 	        attribute: "maxlength",
   	 	        onLimit: function(){},
   	 	        onEdit: function(){}
   	 	    }, options);
   	 	 
   	 	    // Event handler to limit the textarea
   	 	    var onEdit = function(){
   	 	        var textarea = jQuery(this);
   	 	        var maxlength = 2500;
   	 	 
   	 	        if(textarea.val().length > maxlength){
   	 	        	//alert(textarea.val().length);
   	 	            textarea.val(textarea.val().substr(0, maxlength));
   	 	 
   	 	            // Call the onlimit handler within the scope of the textarea
   	 	            jQuery.proxy(settings.onLimit, this)();
   	 	        }
   	 	 
   	 	        // Call the onEdit handler within the scope of the textarea
   	 	        jQuery.proxy(settings.onEdit, this)(maxlength - textarea.val().length);
   	 	    }
   	 	 
   	 	    this.each(onEdit);
   	 	 
   	 	    return this.keyup(onEdit)
   	 	                .keydown(onEdit)
   	 	                .focus(onEdit)
   	 	                .live('input paste', onEdit);
	}     
    </script>
