    </span>
    </div>
    <script type="text/javascript">
        (function($) {
            //var $ = jQuery.noConflict();
            var source = $('span#crumb-source');
            var target = $(source).parent();
            var targetWidth = target.width();

            source.prependTo('body');
            if (source.width() > targetWidth) {
                $($('ul.bread-crumbs li', source).get().reverse()).each(function() {
                    // If this is a space, remove it and get another one
                    if ($('span.side-chevron', this).length > 0) {
                        $(this).remove();
                        return true;
                    }
                    // If the text to put in is smaller than the target, don't do anything.
                    if (source.width() <= targetWidth) {
                        return false;
                    }
                    // Get down, turn around, go to town
                    if ($('span', this).length > 0) {
                        var crumb = $('span', this).eq(0);
                    } else {
                        var crumb = $('a', this).eq(0);
                    }
                    var string = $(crumb).text();
                    string = string.slice(0, string.length - 3);
                    $(crumb).text(string + '...');
                    while (source.width() >= targetWidth && string.length > 0) {
                        string = string.slice(0, string.length - 1);
                        $(crumb).text(string + '...');
                    }
                    if (string.length == 0) {
                        $(this).remove();
                        return true;
                    }
                    return false;
                });
            }
            source.prependTo(target);
            source.css('display', 'block');
        })(jQuery);
    </script>

