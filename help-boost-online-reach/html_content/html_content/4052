pageLength = 0;
msie = false;

var avgCommentHeight = 55;
var numComments = 0;

if (typeof navigator.userAgent != 'undefined' && navigator.userAgent.indexOf('MSIE') != -1) {
  msie = true;
}

if (msie == true && (typeof colBLength == 'undefined' || colBLength == null || colBLength < 1)) {
  
  pageLength = document.body.scrollHeight;

  numComments = 0;

  
  

 pageLength = pageLength + numComments * avgCommentHeight;

}

