							curr = getCurrentItem();

							if(curr <= 17) {
								$('#lessOn').hide();
							}
							if((totItems >= 16 && curr < 17) || (totItems >= 34 && curr < 35)) {
								$('#moreOn').show();
							}
					}
				}

			
