<style type="text/css"
          media="screen"><!-- @import url(http://blogs.discovermagazine.com/80beats/wp-content/themes/galleria/themes/galleria.classic.css); --></style>
<script type="text/javascript" src="http://blogs.discovermagazine.com/80beats/wp-content/themes/galleria/galleria.js"></script>
<script type="text/javascript">
    jQuery(document).ready(function() {
        Galleria.loadTheme("http://blogs.discovermagazine.com/80beats/wp-content/themes/galleria/themes/galleria.classic.js");

        //giving unique ids to the galleries so that we can create them
        //individually
        var galleries = jQuery(".galleriaImages");
        for (i=0; i< galleries.length; i++){
            var gallery = galleries[i];
            var gallery_id = "gallery-" + i;
            jQuery(".galleriaImages:eq("+i+")").attr("id", gallery_id);
            jQuery("#"+ gallery_id).galleria(); 
        }

        //hooking up the previous/next and thumbnail buttons
        jQuery('.galleria-image-nav-right').live('click', function(){
            var parent_id = jQuery(this).parents(".galleriaImages").attr("id");
            whatImage(1, parent_id);
        });
        jQuery('.galleria-image-nav-left').live('click', function(){
            var parent_id = jQuery(this).parents(".galleriaImages").attr("id");
            whatImage(-1, parent_id);
        });
        jQuery('.galleria-image img').live('click', function(){
            var parent_id = jQuery(this).parents(".galleriaImages").attr("id");
            var post_url = jQuery("#" + parent_id).parents(".weblog-entry").children("h2").children("a").attr("href");
            var url = jQuery(this).attr('src');
            onNextImage(post_url + url.split('/').pop());
        });

        //we look at what is the next/previous image url
        function whatImage(change, parent_id){
            var post_url = jQuery("#" + parent_id).parents(".weblog-entry").children("h2").children("a").attr("href");
            var current = parseInt(jQuery('#' + parent_id +' .galleria-counter .current').text());
            var total = parseInt(jQuery('#' + parent_id + ' .galleria-counter .total').text());
            if(current==0 && change==-1)
                current = total;
            if(current==total && change==1)
                current = 0;
            var next = current + change;
            var next_url = jQuery('#' + parent_id + ' .galleria-thumbnails .galleria-image:eq('+(next-1)+') img').attr('src');
            onNextImage(post_url + next_url.split('/').pop());
        };

        //Update the ads/GA 
        function onNextImage(next_url){
            jQuery(".adframe").each(function(){
              this.contentWindow.location.reload(true);
            });
            _gaq.push(['_trackPageview', next_url]);
        }

    });
</script>
<!-- end Galleria libraries -->
