<script type="text/javascript">
    $(document).bind("ApplicationReady", function() {
        var _udcSrc = 'http://assets1c.msnbc.msn.com/rendering/msnbc/html40/assets/Js/UdcTag.js';
        var _rawTaxonomy = 'NC=1255&NA=1154&PS=70037&PI=7329&DI=305';

        function deserialize(formEncodedString) {
            var result = {},
			parts = formEncodedString.split("&"),
			pair;

            while (parts.length) {
                pair = parts.shift().split("=");
                if (pair.length < 2) { continue; }
                result[pair[0].toLowerCase()] = pair[1];
            };

            return result;
        }

        function getParams() {

            var taxonomy = deserialize(_rawTaxonomy);

            ijv.log("taxonomy:", taxonomy);

            return jQuery.extend(taxonomy, {
                pageName: ijv.model.trackAs,
                tp: ijv.model.shareUri,
                guid: createGuid(),
                rf: document.referrer
            });
        }

        function initializeTracking(params) {
            $.track({
                trackInfoOpts: {
                    sitePage: {
                        pageName: params.pageName,
                        domainId: params.di,
                        propertyId: params.pi,
                        tp: params.tp,
                        propertySpecific: params.ps,
                        sourceUrl: document.URL
                    },
                    userStatic: {
                        beginRequestTicks: '',
                        requestId: params.guid
                    },
                    client: {
                        referrer: params.rf
                    }
                },
                spinTimeout: 150
            }).register(new $.track.genericTracking({
                base: "http://udc.msn.com/c.gif?",
                linkTrack: 0,
                commonMap: {
                    sitePage:
	            {
	                di: 'domainId',
	                pn: 'pageName',
	                pi: 'propertyId',
	                ps: 'propertySpecific',
	                su: 'sourceUrl',
	                cu: 'tp'
	            },
                    userStatic: {
                        rid: 'requestId',
                        clid: 'requestId'
                    },
                    client: {
                        rf: 'referrer',
                        bh: 'height',
                        bw: 'width',
                        scr: 'screenResolution',
                        sd: 'colorDepth'
                    },
                    report: {
                        ce: 'contentElement',
                        cm: 'contentModule',
                        hl: 'headline',
                        gt1: 'campaignId',
                        du: 'destinationUrl'
                    }
                },
                impr: {
                    param: {
                        evt: 'impr',
                        js: '1'
                    }
                }
            }));
        }

        function createGuid() {
            return 'xxxxxxxxxxxx4xxxyxxxxxxxxxxxxxxx'.replace(/[xy]/g, function(c) {
                var r = Math.random() * 16 | 0, v = c == 'x' ? r : (r & 0x3 | 0x8);
                return v.toString(16);
            });
        }

        function cTag() {
            var cTagImage = "";
            var imageSrc = "http://c.msn.com/c.gif?",
            tp = "http://msnbc.msn.com/",
            wbId = ijv.model.workbenchId;
            var referrer = "";

            if (document.referrer) {
                var check = /http|https|ftp:\/\/[a-zA-Z0-9\.\-]+(\:\d{1,5})?/gi.test(document.referrer);
                if (!check)
                    referrer = "";
                else
                    referrer = "&rf=".concat(encodeURIComponent(document.referrer));
            }

            if (wbId != '3053415')
                tp = tp.concat('id/', wbId);
            tp = '&TP='.concat(encodeURIComponent(tp));
            var guid = createGuid();
            imageSrc = imageSrc.concat(_rawTaxonomy, referrer, tp);
            cTagImage = "<img src=" + imageSrc + "&RID=" + guid + " width=\"0\" height=\"0\" border=\"0\" alt=\"\" />";
            $('body').append(cTagImage);
        }

        $.getScript(_udcSrc, function(data) {
            try {
                cTag();
                var params = getParams();
                initializeTracking(params);
                $.track.trackPage();
            } catch (Error) {
                eval(data);
                var params = getParams();
                initializeTracking(params);
                $.track.trackPage();
            }
        });
    });
</script>


