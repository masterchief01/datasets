_width: expression(this.parentNode.offsetWidth -
parseInt("200px") -
parseInt("200px") + 'px');
}
.main-inner .fauxcolumn-left-outer {
width: 200px;
}
.main-inner .fauxcolumn-right-outer {
width: 200px;
}
.main-inner .column-left-outer {
width: 200px;
right: 100%;
margin-left: -200px;
}
.main-inner .column-right-outer {
width: 200px;
margin-right: -200px;
}
#layout {
min-width: 0;
}
#layout .content-outer {
min-width: 0;
width: 800px;
}
#layout .region-inner {
min-width: 0;
width: auto;
}
--></style>
