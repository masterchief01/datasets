	lockw = loc.indexOf('keywords=')
	if  (lockw != -1) {
	 kw = loc.substring(lockw + 9); kw = escape(kw.substring(0,kw.indexOf('&')));
	}
	else {
	 var kw = document.title.toLowerCase(); pipe = kw.lastIndexOf("| "); var kw = (pipe != -1)? kw.substring(pipe + 2) : "";
	}
