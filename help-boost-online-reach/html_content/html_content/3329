var TweetThisLink = {
    baseUrl : '',
    shorten : function(event) {
        
        event.preventDefault();
        var twitter_url = this.getAttribute('href'),
			   times_url = twitter_url.match(/http:\/\/www\.time.*\.html/);

        if(times_url){
          TweetThisLink.baseUrl = twitter_url;
          BitlyClient.shorten(times_url[0], 'TweetThisLink.response');
        }
    },
    response: function(data){
    
		var base_link = TweetThisLink.baseUrl;
    
		for (var r in data.results) {
		  
			base_link = base_link.replace(/http:\/\/www\.time.*\.html/,encodeURIComponent(data.results[r]['shortUrl']));
			break;
		}
		if(navigator.userAgent.indexOf('AppleWebKit') > -1 ) {

			window.location.href = base_link;
		} else {
			window.open(base_link);
	    	//window.open(base_link + encodeURIComponent(bitly_link),'','width=770,height=530,scrollbars=yes');
		}
	}
};
jQuery.fn.extend({

	tinyURL : function(){
		var $this = jQuery(this),
			cUrl = $this.attr('href');
			if(!$this.attr('href')){
				return false;
			}
		var urlBase = cUrl.slice(0,49),
			urlExt = cUrl.slice(50),
			bLink = null,
			bty = {
				min: function(e){
					e.preventDefault();
					BitlyClient.shorten(urlExt, 'bty.response');
				},
				response: function(data){
					var tinyUrl;
					for (var r in data.results) {
			            bLink = data.results[r]['shortUrl']; 
			            break;
        			}
					tinyUrl = urlBase+' '+bLink;
					if ($this.attr('href').indexOf('bit.ly') == -1) {
						$this.attr('href', tinyUrl);
					}	
				}
	        };
			bty.min();
    }
});

var openTimePopup = function(options){
	var initFn = null;
	var settings = jQuery.extend({
	trigger:jQuery(this),
	url	: null,
	name : '',
	height : 600,
	width : 600,
	popupOptions : 'toolbar=0,scrollbars=1,location=1,statusbar=1,menubar=0,resizable=1'
	}, options);
	if(settings.initFn){settings.initFn();}
    settings.url = settings.url ? settings.url : settings.trigger.attr('href');  
    settings.popupOptions +=',width='+settings.width+',height='+settings.height+',left = '+(screen.width - settings.width) / 2+',top ='+(screen.height - settings.height) / 2;
	newwindow = window.open(settings.url,settings.name,settings.popupOptions);
	if (window.focus) {newwindow.focus();}
};

var slideTimeDropDown = function(options,thisObj){
	var $this = jQuery(thisObj);
	var $thisParent = $this.parent();
	var showFn = function() {
			settings.content.slideDown(settings.animSpeed, function() {
			settings.triggerParent.addClass(settings.cssOpened);
			});
	};
	var hideFn = function() {
			settings.content.slideUp(settings.animSpeed, function(){
			settings.triggerParent.removeClass(settings.cssOpened);
		});
	};
	var initFn = null;
	var settings = jQuery.extend({
	trigger : $this,
	triggerParent : $this.parent(),
	content : $thisParent.find('ul:first'),
	hideFn : hideFn,
	showFn : showFn,
	initFn : initFn,
	animSpeed : 'fast',
	cssOpened : 'opened',
	hoverSpeed : 2000
	}, options);
	if(settings.initFn){settings.initFn();}
	
	if (settings.triggerParent.hasClass(settings.cssOpened)) {
		settings.hideFn();
	} 
	else {
		settings.showFn();
		var t;
		settings.triggerParent.hover(function(){clearTimeout(t);},function(){t = setTimeout(settings.hideFn,settings.hoverSpeed);});
		}
};

