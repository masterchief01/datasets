<script type="text/javascript">
(function($) {
    $('.mod-most-popular .more').live('click', function() {
        
        var button = $(this);
        var type = button.data('type');
        var offset = button.data('offset') + 5;
        var element =  $("ul[data-type='" + type + "']");
        $.ajax({
            url: '/ajax/right_rail/most_popular.php',
            data: {type: type, offset: offset},
            success: function(data) {
                element.fadeOut('slow', function() {
                    element.html(data);
                    element.fadeIn('slow');
                });
                button.data('offset', offset);
            }
        });
    });
})(jQuery);
</script>
<!-- /.mod-most-popular -->
