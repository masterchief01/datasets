	document.onkeyup=function(e) {
		var thelink;
		var thetoplink;
	 if (window.event) Ucode = window.event.keyCode;
	else if (e) Ucode = e.which;

		  if (Ucode==37) {
		  thelink='cPrev';
		  thetoplink='cPrevTop';
		  }
		  else if (Ucode==39) {
		  thelink='cNext';
		  thetoplink='cNextTop';
		  }
		  else return
		  var t = getTarget(e);
		  if ((t.type && (t.type == "text" || t.type=="textarea")) || t.nodeName === "textarea") {
			
		  }else{
		  	 if(document.getElementById(thelink)){document.location.href=document.getElementById(thelink).href}
			 else if(document.getElementById(thetoplink)){document.location.href=document.getElementById(thetoplink).href}
		  }
		 
	}
	
	function getTarget(e) {
		e = e || window.event;
		return e.target || e.srcElement;
	}
	
