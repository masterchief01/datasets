<!-- Some fun JavaScript to change the gravatar on entering an e-mail address -->
<script type="text/javascript" src="http://cdn.b5media.com/hubs-v3/js/md5.js"></script>
<script type="text/javascript">
	gravatar_found = false;
	jQuery('#email').blur(function() {
		_b5p_fetch_gravatar(jQuery(this).val());
	});

	function _b5p_fetch_gravatar(email) {
		var hash = hex_md5(email);
		gravatar_found = false;
		jQuery.ajax({
			url: 'http://www.gravatar.com/'+hash+'.json',
			dataType: 'jsonp',
			timeout: 2000,
			success: function(data) {
				if(data.entry[0].id) {
					jQuery('#gravatar-image').attr('src', data.entry[0].thumbnailUrl);
					jQuery('#add-avatar').css('display', 'none');
					gravatar_found = true;
				}
			},
		});
		setTimeout('_b5p_check_gravatar();', 2000);
	}

	function _b5p_check_gravatar() {
        	if(!gravatar_found) {
                        jQuery('#gravatar-image').attr('src', jQuery('#gravatar-image').attr('default'));
                        jQuery('#add-avatar').css('display', 'inline');
                }
	}
</script>


