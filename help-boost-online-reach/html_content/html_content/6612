<script type="text/javascript">
function preCheckOnion100821($){
    loc = window.location + '';
    if (loc.match(/\/(articles|video)\/.+/i)) {
        return 1;
    }
    return 0;
}
</script>


<script type="text/javascript">
    function ga_section_click() {
        try {
            var section = $(this).parents(".ga_section").attr("name");
            var url = this.href.replace(/(http:\/\/[a-z]+\.theonion\.com)/, "");
            if (this.href.match(/(http:\/\/[a-z]+\.theonion\.com)/)) {
                _gaq.push(['_trackEvent', 'click', section, url]);
                // console.log("CLICK section: " + section + " url: " + url);
            }
            else {
                _gaq.push(['_trackEvent', 'click-outbound', section, url]);
                // console.log("CLICK-OUTBOUND section: " + section + " url: " + url);
            }
        }
        catch (err) {}
        return true;
        // return false;
    }
    //only for onion, not sportsdome...(for now)
    if (window.location.href.match(/(http:\/\/[a-z]+\.theonion\.com)/)) {
        $("a").click(ga_section_click);
        /*
        FB.Event.subscribe('edge.create', function(targetUrl) { 
            console.log('like');
            _gaq.push(['_trackSocial', 'facebook', 'like', targetUrl]); 
        }); 
        
        FB.Event.subscribe('message.send', function(targetUrl) { 
            console.log('send');
            _gaq.push(['_trackSocial', 'facebook', 'send', targetUrl]); 
        });

        twttr.events.bind('tweet', function(event) { 
            if (event) { 
                var targetUrl; 
                if (event.target && event.target.nodeName == 'IFRAME') { 
                    targetUrl = extractParamFromUri(event.target.src, 'url'); 
                } 
                console.log('tweet');
                _gaq.push(['_trackSocial', 'twitter', 'tweet', targetUrl]); } 
        });
        */
    }

    function extractParamFromUri(uri, paramName) {
      if (!uri) {
        return;
      }
      var uri = uri.split('#')[0];  // Remove anchor.
      var parts = uri.split('?');  // Check for query params.
      if (parts.length == 1) {
        return;
      }
      var query = decodeURI(parts[1]);

      // Find url param.
      paramName += '=';
      var params = query.split('&');
      for (var i = 0, param; param = params[i]; ++i) {
        if (param.indexOf(paramName) === 0) {
          return unescape(param.split('=')[1]);
        }
      }
