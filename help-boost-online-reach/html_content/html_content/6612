try {
    var agent = navigator.userAgent;
    if(/mobile=false/.test(window.location.href) || /mobile=false/.test(document.cookie)) {
        var expire = new Date();
        expire.setTime(new Date().getTime() + 3600000*24*5);
        document.cookie = "mobile=false;expires="+expire.toGMTString()+";domain=.theonion.com;path=/;";
    }
    else {
        if(agent && agent !== undefined) {
            if(/blackberry|iemobile|symbian/i.test(agent) && !/mobile=/.test(window.location.href)) {
                var url = window.location.href.replace("www","mobile").replace("test","testmobile").split("?");
                window.location = url[0] + "?mobile=true";
            }
        }
    }
} catch(err) {
}
</script>






    
