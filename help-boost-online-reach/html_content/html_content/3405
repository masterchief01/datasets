  $('div.nextLink a').click(
    function() {
      $(this).parent('div.nextLink').parent('div.contentA').hide() ;
      $(this).parent('div.nextLink').parent('div.contentA').siblings('div.contentB').show() ;
      return false ;
    }
  ) ;
  $('div.prevLink a').click(
    function() {
      $(this).parent('div.prevLink').parent('div.contentB').hide() ;
      $(this).parent('div.prevLink').parent('div.contentB').siblings('div.contentA').show() ;
      return false ;
    }
  ) ;
});
