<script type="text/javascript">
  var pause = false ;
  var selectedSlideKey = 1 ;
  setInterval('rotateSlide()', 5000) ;
  $('a.slice').hover(
    function() {
      pause = true ;
      showSlide($(this).parent('li')) ;
      var id = $(this).parent('li').attr('id') ;
      selectedSlideKey = parseInt(id.replace(/slide_/, '')) ;
    },
    function() {
      pause = false ;
    }
  );
  $('div.slide').hover(
    function() { pause = true ; },
    function() { pause = false ; }
  ) ;
  function showSlide(li) {
    li.siblings('li').fadeTo(500, .3) ;
    li.siblings('li').children('div.slide').fadeTo(500, 0) ;
    li.fadeTo(500, 1) ;
    li.children('div.slide').fadeTo(500, 1) ;
  }
  function rotateSlide() {
    if (!pause) {
      selectedSlideKey = (selectedSlideKey == 3) ? 1 : selectedSlideKey+1 ;
      showSlide($('#slide_'+selectedSlideKey)) ;
    }
  }
</script>
