						        if(email.val() == "" || email.val() == "your.name@example.com"){
						            email.addClass('new_search_box_error')
									$('#newsletter-status-message').replaceWith("<p id='newsletter-status-message' style='margin-bottom:3px;color:#CC1F19;'>Drawing a blank here. What'd you say your address was?</p>");
