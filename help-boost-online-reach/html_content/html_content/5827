<script type="text/javascript">
    function doSearch() {
        var searchString = $('#ctl00_fragment_16cc57fd_f97f_4b4c_b8aa_e89c6a033739__132eb5fd6ee5_SearchText')[0].value;

        if (searchString != '') {
            var newLocation = 'http://www.peta.org/search/SearchResults.aspx?q=' + searchString;
            $(location).attr('href', newLocation);
        }
        return false;
    }

    function keyHandler(event) {
        if (event.keyCode == 13) {

            Page_ValidationActive = false;
            event.returnValue = false;
            event.cancel = true;

            return doSearch();
                    }
        return true;
    }

    $(document).ready(function() {
        $('#ctl00_fragment_16cc57fd_f97f_4b4c_b8aa_e89c6a033739__132eb5fd6ee5_SearchText').coolinput({ hint: 'SEARCH PETA.ORG' });
    });
