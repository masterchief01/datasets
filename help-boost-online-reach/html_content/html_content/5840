    <form>
     <p><input class="email_field" type="text" name="email" rel:gt_act="newsletter/email-address" id="sailthru_email_form-1" value="your email address" onfocus="if(this.value=='your email address')this.value='';" /></p>
     <input class="button2 email_signup bf_dom" rel:gt_act="newsletter/signup" rel:bf_bucket="event_handler" rel:actions="click:sailthru_submit_email" rel:data="{id:'1'}" type="submit" value="Sign Up!"  />
    </form>
   </div>
   <div id="sailthru_email_spinner-1" class="spinner-text hidden">Please Wait...</div>
