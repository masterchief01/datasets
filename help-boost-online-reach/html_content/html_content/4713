<script type="text/javascript">
	var loadCounter = 3;
	function nextVideo(){
		var autoplay = $("#nextvideo").attr('checked');
		if(autoplay){
			if(loadCounter <= 0){
				nextVideoGo();
			}
			else{
				$("#loadCounter").html('&nbsp;&nbsp; Loading in .. ' + loadCounter);
				loadCounter--;
				setTimeout(nextVideo, 1000);
			}
		}
	}
	function nextVideoGo(){
				window.location = "http://www.videobash.com/video_show/amazing-eminem-violin-cover-6104";
			}
	function setNextVideo(){
		var autoplay = $("#nextvideo").attr('checked') ? 1 : 0;
		$.post('http://www.videobash.com/video/autoplay_click',{'autoplay':autoplay},function(response) {});		
	}
	
	function rateItemPlayer(){
		rateItem(6110, 1);
	}
</script>	</div>
	
	
