			gigya_comments();
			function gigya_comments(){
				var meta_img = $('meta[property=og:image]').attr("content");
				var act = new gigya.services.socialize.UserAction();
				if(meta_img){
					act.addMediaItem({type: 'image', src: meta_img, href: 'http://www.videobash.com/video_show/3d-projection-on-a-3d-surface-6110'});
				}
				act.setTitle('3D projection on a 3D surface!');
