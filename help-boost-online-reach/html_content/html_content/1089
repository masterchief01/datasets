<script language="javascript" type="text/javascript">
//$(document).ready(function(){
//$("#custom-doc #ft span .column ul li:last-child span").hide();
    //});

 function setDynamicOmniture(linkTitle) {
        var linkVal = '';
        linkVal = linkTitle;
        if ((linkVal == "Readers Digest") || (linkVal == "Career Center")) {
            trackExitLink('Footer', linkVal, location.pathname.toLowerCase(), true);
        }
        else {
            trackLink('Footer', linkVal, location.pathname.toLowerCase(), true);
        }
    }

</script>




