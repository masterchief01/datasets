  <script language="javascript"> 
   var IsLogin = 'false';
   var only_close_nav= false;	
   if(IsLogin == 'true')
   {
	    only_close_nav= true;
	    var popupContact = document.getElementById("popupContact");        
    	showPopupWithReturn($("#popupContact"),true, "ClosePopUp", true);
        popupContact.style = "display:block";	
   }
   	
   </script>

 

    <script language="javascript">

            $("#backgroundPopup").click(function(){

            if(only_close_nav)

            window.location.href = $("#popupContact a.popupClose").attr("href");

            });

                $(".rec-Toolbox .bottomBG .icnRate, #RDNEW .recipeHeader .Recipe-Intro p.link a").click(function(){

                $("#backgroundPopup").show();         

                $(".yui-u.first").css("z-index","9999999");

                });

                $("#RDNEW .recipeHeader .Recipe-Intro p.link a[id$=lnkReadReview]").click(function(){

                $("#backgroundPopup").hide();

                });

                $("#popupContact .RecPopup-ContainerMid .popupClose, #backgroundPopup, .Rating-ContainerMid .popupClose").click(function(){

                        $("#popupContact, #backgroundPopup, #popupRating").hide();

                $(".yui-u.first").css("z-index","1");

                });

    </script>
    
    <script language="javascript">
    // Script for hiding rate box when login	
	    if(document.cookie.indexOf("immediate_login=immediate")>=0)
	    {
	  
	    document.cookie ='immediate_login=immediate;expires=Fri, 3 Aug 2001 20:47:11 UTC; path=/'; 
	    }
	    if($("#popupContact").css("display")=="block")
	    {
	    $(".yui-u.first").css("z-index","9999999");
	    $(".RecPopup-ContainerMid p").html("Please log in or join now to view our<br/>Member-Exclusive Recipes"); 
	    }
	   

	    $(document).ready(function(){
	    if($("#popupContact").css("display")=="block")
	    document.cookie ='immediate_login=immediate; path=/';
	    });

	    $(".icnRate a").click(function(){
	    $("#popupRating").css("visibility","visible");
	    });

               
        function Rating_Auto()
        {
            $("#backgroundPopup").show();         
            $(".yui-u.first").css("z-index","9999999");
        }  
            </script>

 
                          
                


