<script type="text/javascript">
var minHeight = 76;
var maxHeight = 211;
var currentHeight = 76;
function adjustHeight(isExpand) {
if ((isExpand) && ((Cookie.get('mb_pop_limit') != null) || (Cookie.get('mb_subscribed') != null))) {
return;
} 
var yardbar = document.getElementById("b");
var doAdjust = true;
if ((isExpand) && (currentHeight < maxHeight)) {
currentHeight += 10;
if (currentHeight > maxHeight) {
currentHeight = maxHeight;
}
} else if (currentHeight > minHeight) {
currentHeight -= 10;
if (currentHeight < minHeight) {
currentHeight = minHeight;
}
} else {
doAdjust = false;
}
var isLast = true;
if (doAdjust) {
var newMargin = parseInt((minHeight - currentHeight) * minHeight / (maxHeight - minHeight));
yardbar.style.marginTop = newMargin + "px";
yardbar.style.height = (currentHeight - newMargin) + "px";
document.body.style.paddingTop = (currentHeight + 2) + "px";
if ((currentHeight != minHeight) && (currentHeight != maxHeight)) {
setTimeout("adjustHeight(" + isExpand + ");", 16);
isLast = false;
}
}
if (isLast) {
Cookie.set('mb_pop_limit', 'true', 3600 * 24 * 42);  
}
}
</script>
