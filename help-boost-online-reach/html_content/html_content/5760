    <script type="text/javascript">
	if (window["FD"]) {
		/**
		 * Updates the white strip with the new time and number of users.
		 *
		 * @param data Date to extract new time and number of users from.
		 * @private
		 */
		function updateWhiteStrip(data) {
			var response = $.parseJSON(data);
			var whiteStripTimeElement = $("whiteStrip-time");
			if ((whiteStripTimeElement) && (whiteStripTimeElement.length > 0)) {
				whiteStripTimeElement.empty();
				whiteStripTimeElement.html(response.time);
			}
			var whiteStripPeopleElement = $("whiteStrip-users");
			if ((whiteStripPeopleElement) && (whiteStripPeopleElement.length > 0)) {
				whiteStripPeopleElement.empty();
				whiteStripPeopleElement.html(response.people + " online now");
			}
		}
		/**
		 * Initiates the client side asset view tracking component (self executing).
		 */
		(function() {
			FD.pingServerWithSuccessCallback("2608202", "Article", "Sport", updateWhiteStrip);
		})();
	}
</script>

        
          
