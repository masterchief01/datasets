	<a href="javascript:;" class="share-icon bf_dom" id="facebook" rel:bf_bucket_data="{'fb-share':{'id':'1788305_617487'}}" rel:sub_title='Reuse plastic bottles to close up your plastic bags.' rel:gt_act="share/facebook/list-item:23" rel:gt_label=""></a>
	
	<a href="javascript:;" class="share-icon bf_dom" id="pinterest" rel:bf_bucket_data="{'pin-share':{'id':'1788305_617487'}}"  rel:sub_title='Reuse plastic bottles to close up your plastic bags.' rel:gt_act="share/pinterest/list-item:23" rel:gt_label=""></a>
	
	<a href="javascript:;" class="share-icon bf_dom" id="twitter" rel:bf_bucket_data="{'tw-share':{'id':'1788305_617487'}}" rel:sub_title='Reuse plastic bottles to close up your plastic bags.' rel:gt_act="share/twitter/list-item:23" rel:gt_label=""></a>
