	<a href="javascript:;" class="share-icon bf_dom" id="pinterest" rel:bf_bucket_data="{'pin-share':{'id':'1788305_617437'}}"  rel:sub_title='How to store tomatoes:' rel:gt_act="share/pinterest/list-item:22" rel:gt_label=""></a>
	
	<a href="javascript:;" class="share-icon bf_dom" id="twitter" rel:bf_bucket_data="{'tw-share':{'id':'1788305_617437'}}" rel:sub_title='How to store tomatoes:' rel:gt_act="share/twitter/list-item:22" rel:gt_label=""></a>
	<a href="javascript:;" class="share-icon bf_dom" id="stumble" rel:bf_bucket_data="{'su-share':{'id':'1788305_617437'}}" rel:sub_title='How to store tomatoes:' rel:gt_act="share/stumble/list-item:22" rel:gt_label=""></a>
