	<article class="module open" id="moduleGlo">
		<header class="moduleHeader">
			<h1>the latest from glo.com</h1>
			<a class="btnOpen" title="Open/Close"><span>Open/Close</span></a>
			<div class="clear"></div>
		</header>
		<div class="moduleBody clearfix">
			<div id="dataGlo">
				<div id="xmlImg"></div>
				<ul id="xmlList"></ul>
			</div>
