        function letsfxad_scrl(){
            var i = parseInt(document.getElementById('divadfloat').style.bottom);
            if(i>=0) i = parseInt(eval('document.all.divadfloat'+'.style.bottom'));
            if(i>=0) document.getElementById('divadfloat').style.bottom=0;
