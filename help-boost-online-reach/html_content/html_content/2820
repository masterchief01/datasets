<script type="text/javascript">
// enable the click event handler on all the pin it buttons
$(function () {
    $(".pin-it-button").attr("href", "#").click(function () {
        handlePinItClickEvent(this);
    });
});

// Handle the pin it click event by building the url and calling the pinIt helper.
function handlePinItClickEvent(element) {

    // select recipe page photo - alt text used for description
    var media, description, mainelement = $("#r_mainPhoto");    

    // if we have a main photo now we are on a recipe page
    if (mainelement.length) {
        // pull the image and use the alt text for the description
        description = mainelement.attr("alt");
        media = $("meta[property='og:image']").attr("content");
    }
    else {
        // pull the header for the article page
        mainelement = $("#RecipeBody h1");
        if (mainelement.length) {
            // pull the title of the article and the image from the meta tag
            description = $("#RecipeBody h1").text();
            media = $("meta[property='og:image']").attr("content");

        } 
        // no other scenarios are supported at this time
    }

    var pinQueryString = '?url=';
    pinQueryString += document.URL.split("?")[0].replace("#", "");
    pinQueryString = pinQueryString + "&media=" + media;
    pinQueryString = pinQueryString + "&description=" + escape(description + " @ " + location.host);

    return pinItToPopupWindow(element, 'http://pinterest.com/pin/create/button/' + pinQueryString);
}

// Create a new window that directs to the pinit url.
function pinItToPopupWindow(pinItButton, pinUrl) {
    // open a new window with the pin url
    var modal = window.open(pinUrl, 'signin', 'width=670,height=300');

    // define a method that will wait for the dialog to be closed so we can update the button class
    var wait = function () {
        setTimeout(function () {
            if (modal == null) {
                //failure();
                return;
            }
            if (modal.closed)
                pinItButton.setAttribute('class', 'pinned');
            else
                wait();
        }, 25);
    };
    // run our wait method so the button will be updated if possible
    wait();

    return false;
} 
</script>
