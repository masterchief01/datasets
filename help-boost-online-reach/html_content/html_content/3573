<script>
function replaceLinkHashes()
{
	var divElems = document.getElementsByTagName('div');
	for(var i = 0; i < divElems.length; i++)
	{
		if (divElems[i].className.indexOf('RelatedTopicsContainer') != -1)
		{
			var aElems = divElems[i].getElementsByTagName('a');
			for(var j = 0; j < aElems.length; j++)
			{
				/*  a - article id, t - template, k - keyword
						b - block num, p - link position inside block*/
				aElems[j].hash = 'a=' + encodeURIComponent(37683) +  '&t=' + encodeURIComponent('default') +
												'&k=' + encodeURIComponent(aElems[j].innerHTML) + '&b=' + encodeURIComponent(divElems[i].getAttribute('blockNum')) +
												'&p=' + encodeURIComponent(j);
			}
		}
	}
}

window.addEventListener ? window.addEventListener('load', replaceLinkHashes, false) :
												 (window.attachEvent ? window.attachEvent("onload", replaceLinkHashes) : false);

</script>

