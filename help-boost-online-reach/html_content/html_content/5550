		_gaq.push(['_set', 'page', BF_STATIC.custom_ga_url]);
		_gaq.push(['_trackPageview', BF_STATIC.custom_ga_url]);
	} 
	else { _gaq.push(['_trackPageview']); }
} catch (err) {}
}


