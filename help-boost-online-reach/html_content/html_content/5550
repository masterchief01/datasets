	<a href="javascript:;" class="share-icon bf_dom" id="pinterest" rel:bf_bucket_data="{'pin-share':{'id':'1811555_640282'}}"  rel:sub_title='Bunker Truffles' rel:gt_act="share/pinterest/list-item:7" rel:gt_label="/static/enhanced/terminal05/2012/10/15/11/enhanced-buzz-18891-1350314526-22.jpg"></a>
	
	<a href="javascript:;" class="share-icon bf_dom" id="twitter" rel:bf_bucket_data="{'tw-share':{'id':'1811555_640282'}}" rel:sub_title='Bunker Truffles' rel:gt_act="share/twitter/list-item:7" rel:gt_label="/static/enhanced/terminal05/2012/10/15/11/enhanced-buzz-18891-1350314526-22.jpg"></a>
	<a href="javascript:;" class="share-icon bf_dom" id="stumble" rel:bf_bucket_data="{'su-share':{'id':'1811555_640282'}}" rel:sub_title='Bunker Truffles' rel:gt_act="share/stumble/list-item:7" rel:gt_label="/static/enhanced/terminal05/2012/10/15/11/enhanced-buzz-18891-1350314526-22.jpg"></a>
    <a href="javascript:;" class="share-icon bf_dom" id="link" rel:bf_bucket_data="{'link-share':{'id':'1811555_640282'}}" rel:gt_act="share/link/list-item:7" rel:gt_label="/static/enhanced/terminal05/2012/10/15/11/enhanced-buzz-18891-1350314526-22.jpg"></a>
