CN.features.render({
    features : (function() {
        var
            i,
            feature = null,
            render = [],
            featuresOnPage = "sandbox  top-nav  top-rail  related-features carousel  display-2 thumbnail-grid popular-galleries display-4  highlighted-panel2".replace(/\s{2,}/g, ' ').replace(/-/g,'').split(' '),
            len = featuresOnPage.length,
            cssClass;

        featuresOnPage = jQuery.grep(featuresOnPage,function(el,idx){
            return jQuery.inArray(el, featuresOnPage) === idx;
        });
        for(i = 0; i < len; i++){
            cssClass = featuresOnPage[i];
            if (CN.features.types.hasOwnProperty(cssClass)) {
                render.push(cssClass);
            }
        }
        jQuery(window).trigger('customSetStamp', ['CN.features.render(features)']);
        return render;
    }())
});

