  function wlGetMetaTag(tagname){	   
    var tmp = "null"; 
    if(document.getElementsByName(tagname)[0] != null){ 
      tmp = document.getElementsByName(tagname)[0].content; 
    } 
    return tmp.toLowerCase(); 
  } 
