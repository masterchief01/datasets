function createIFrame(src, width, height, ad_iframe_id) {     
      
    var ifrm = document.createElement("IFRAME");
    ifrm.setAttribute("src", src);
    ifrm.style.width = width +"px";
    ifrm.style.height = height +"px";
    ifrm.style.border = "none";
    ifrm.style.position = "absolute";
    ifrm.style.zIndex = "2147483640";
    ifrm.marginheight = 0;
    ifrm.marginwidth = 0;
  
    ifrm.width = width;
    ifrm.height = height;
    ifrm.scrolling = "no";
    ifrm.frameBorder = "0";
    ifrm.border = "0";
    ifrm.id = ad_iframe_id;  
  
    return ifrm;
}


function changeLike(url)
{
//refresh digg and gplus codes

          var s = document.createElement('SCRIPT'), s1 = document.getElementsByTagName('SCRIPT')[0];
          s.type = 'text/javascript';
          s.async = true;
          s.src = 'http://widgets.digg.com/buttons.js';
          s1.parentNode.insertBefore(s, s1);
          
jQuery("#like").html('');
jQuery("#fbcomments").html('');
jQuery("#tweet").html('');
//jQuery("#diggme").html('');
//jQuery("#gplus").html('');
jQuery("#like").append("<div class='fb-like' data-href='"+url+"' data-send='false' data-layout='button_count'data-width='60' data-show-faces='false'><\/div>");
jQuery("#fbcomments").append("<div class='fb-comments' data-href='"+url+"' data-num-posts='10' data-width='612'><\/div>");
var turl= "//platform.twitter.com/widgets/tweet_button.html?url="+url;
var durl ="http:\/\/digg.com\/submit?url="+url;
jQuery("#tweet").append("<iframe allowtransparency='true' frameborder='0' scrolling='no' src='"+turl+"' style='width:130px; height:20px;'><\/iframe>");
//jQuery("#diggme").append("<a class='DiggThisButton DiggCompact' href='"+durl+"'></a>");
//jQuery("#gplus").append("<g:plusone size='medium' href='"+url+"' ><\/g:plusone>");

//alert("//platform.twitter.com/widgets/tweet_button.html?counturl="+url);

        
FB.XFBML.parse(document.getElementById('like'));
FB.XFBML.parse(document.getElementById('fbcomments'));
}

function reloadBanners(url) {
	
	//_gaq.push(['_trackPageview', url]); //google analytics
  	//_qevents.push( {qacct:"p-a0gOdUACucKCE", event:"refresh" }); //quantcast
  	
  	var iframe = document.createElement('iframe'); 
	iframe.style.width = '0px';						 
	iframe.style.height = '0px'; 						 
	document.body.appendChild(iframe); 

	iframe.src = 'http://www.totalprosports.com/wp-content/themes/newsports/tracking.php?rand=' + Math.random();
	
	var count = 0;
	var ord = Math.random()*10000000000000000;
	var adlink = cmnunt_site + '/' + cmnunt_zone +'_norm;kw='+ cmnunt_silo + ',' + cmnunt_tier;
	
	// 728x90
	var my_ad_iframe_728 = createIFrame("http://ad.doubleclick.net/adi" + adlink + ";sz=728x90;tile=" + count + ";ord=" + ord +"", 728, 90, 'my_ad_iframe_id_728');
	jQuery("#ad728").html(my_ad_iframe_728);		
	document.getElementById('my_ad_iframe_id_728').src = "http://ad.doubleclick.net/adi/" + adlink + ";sz=728x90;tile=" + count + ";ord=" + ord +"";
	
	count++;
	
	// 300x250
	var my_ad_iframe_300 = createIFrame("http://ad.doubleclick.net/adi" + adlink + ";sz=300x250;tile=" + count + ";ord=" + ord +"", 300, 250, 'my_ad_iframe_id_300');
	jQuery("#ad300").html(my_ad_iframe_300);		
	document.getElementById('my_ad_iframe_id_300').src = "http://ad.doubleclick.net/adi/" + adlink + ";sz=300x250;tile=" + count + ";ord=" + ord +"";
		
	count++;
	
	// 160x600
	var my_ad_iframe_160 = createIFrame("http://ad.doubleclick.net/adi" + adlink + ";sz=160x600;tile=" + count + ";ord=" + ord +"", 160, 600, 'my_ad_iframe_id_160');
	jQuery("#ad160").html(my_ad_iframe_160);		
	document.getElementById('my_ad_iframe_id_160').src = "http://ad.doubleclick.net/adi/" + adlink + ";sz=160x600;tile=" + count + ";ord=" + ord +"";
	
	count++;
   

}    
	
jQuery(document).ready(function(){
	
	var hash = window.location.hash.substr(1);
	var count = 0;
	
	
	jQuery('a[id=next_page]').live('click', function(e){
		var page_id = jQuery(this).attr('name');
		var max_id = jQuery(this).attr('alt');
		
		if (page_id != max_id)
		{
			var next_id = parseInt(page_id) + 1;
			var prev_id = page_id;
			
			jQuery('li[id=page_' + page_id + ']').fadeOut(function() {
				jQuery('li[id=page_' + next_id + ']').fadeIn();
			});
			
			jQuery('a[id=next_page]').attr('name', next_id);
			jQuery('a[id=prev_page]').attr('name', next_id);
		}
		
		return false;
	});
	
	jQuery('a[id=prev_page]').live('click', function(e){
		var page_id = jQuery(this).attr('name');
		
		if (page_id > 1)
		{
			
			var next_id = page_id;
			var prev_id = parseInt(page_id) - 1;
			
			jQuery('li[id=page_' + page_id + ']').fadeOut(function() {
				jQuery('li[id=page_' + prev_id + ']').fadeIn();
			});
			
			jQuery('a[id=next_page]').attr('name', prev_id);
			jQuery('a[id=prev_page]').attr('name', prev_id);
		}
		
		return false;
	});
	

	jQuery('div[id=gallery-next] a').live('click', function(e){
		var image_id = jQuery(this).attr('id');
		var ids = jQuery('a[id=thumb_' + image_id + ']').attr('rel');
		var cur_loc = jQuery('div[id=slidecaption] span').html();
		
		ids = ids.split(":");
		var prev_id = ids[0];
		var cur_id = ids[1];
		var next_id = ids[2];
		
		var image_url = jQuery('a[id=thumb_' + next_id + ']').attr('name');
		var url = jQuery('a[id=thumb_' + next_id + ']').attr('href');
		jQuery('#fullimage').attr('src',image_url);
		

		if (next_id == '')
		{
			window.location = jQuery('#next-album').attr('href');
		}
		
		
		jQuery('div[id=gallery-next] a').attr('id',next_id);
		jQuery('div[id=gallery-prev] a').attr('id',next_id);
		
		jQuery('a[id=thumb_' + cur_id + ']').removeClass('active');
		jQuery('a[id=thumb_' + next_id + ']').addClass('active');
		
		jQuery('#galtitle').html(jQuery('a[id=thumb_' + next_id + ']').attr('title'));
		jQuery('div[id=slidecaption] span').html(parseInt(cur_loc)+1);
		
		
		reloadBanners(url);		
		  //changeLike(url);
		return false;
	});
	
	jQuery('div[id=gallery-prev] a').live('click', function(e){
		var image_id = jQuery(this).attr('id');
		
		var ids = jQuery('a[id=thumb_' + image_id + ']').attr('rel');
		var cur_loc = jQuery('div[id=slidecaption] span').html();

		ids = ids.split(":");
		var prev_id = ids[0];
		var cur_id = ids[1];
		var next_id = ids[2];
		
		if (prev_id == '')
		{
			window.location = jQuery('#prev-album').attr('href');
		}
		
		var image_url = jQuery('a[id=thumb_' + prev_id + ']').attr('name');
		var url = jQuery('a[id=thumb_' + prev_id + ']').attr('href');
		jQuery('#fullimage').attr('src',image_url);
		
		jQuery('div[id=gallery-next] a').attr('id',prev_id);
		jQuery('div[id=gallery-prev] a').attr('id',prev_id);
		
		jQuery('a[id=thumb_' + cur_id + ']').removeClass('active');
		jQuery('a[id=thumb_' + prev_id + ']').addClass('active');
		
		jQuery('#galtitle').html(jQuery('a[id=thumb_' + prev_id + ']').attr('title'));
		jQuery('div[id=slidecaption] span').html(parseInt(cur_loc)-1);
		
		reloadBanners(url);
		 // changeLike(url);

		
		
		return false;
	});

	jQuery('#gal-nav-previous a, #gal-nav-next a, a#gal-thumb').live('click', function(e){

		count++;
		
		/*if( count % 3 == 0 ) {

