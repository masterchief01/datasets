    <script type="text/javascript">

        var _gaq = _gaq || [];
        _gaq.push(['_setAccount', 'UA-1005942-26']);
        _gaq.push(['_trackPageview']);

        (function () {
            var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
            ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
            var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
        })();



        $(document).ready(function () {


            var testTextBox = $('#search-text');
            var code = null;

            if ($("#s_g").val() == 'mens') $('#mensradio').attr('checked', true); else $('#womensradio').attr('checked', true);

            testTextBox.keypress(function (e) {
                code = (e.keyCode ? e.keyCode : e.which);
                if (code == 13) {
                    doSearchTop();
                    return false;
                }

            });


        });

        doSearchTop = function () {

            var Gender, sSearch, sURL = '';

            Gender = str_replace($('input:radio[name=rdgender]:checked').val(), "'", "&apos;");
            sSearch = str_replace($("#search-text").val(), "'", "&apos;");

            //alert(Gender);

            $.ajax({
                type: "POST",
                url: strLocalHost + "services/wardrobe.asmx/GetShopSearch",
                data: "{Gender: '" + Gender + "', Keyword: '" + sSearch + "'}",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (obj) {
                    sURL = obj.d;
                    //$.cookie("GNDR", $('input:radio[name=rdgender]:checked').val(), { expires: 1 });
                    if (sURL != '') {
                        sURL = strLocalHost + sURL;
                        document.location = sURL;
                    }
                    else {
                        sURL = strLocalHost + "fashion/" + $('input:radio[name=rdgender]:checked').val() + "?query=" + $("#search-text").val();
                        document.location = sURL;
                    }

                },
                error: function (response) {
                    sURL = strLocalHost + "fashion/" + $('input:radio[name=rdgender]:checked').val() + "?query=" + $("#search-text").val();
                    //$.cookie("GNDR", $('input:radio[name=rdgender]:checked').val(), { expires: 1 });
                    document.location = sURL;
                }

            });




        };

        function str_replace(haystack, needle, replacement) {
            var temp = haystack.split(needle);
            return temp.join(replacement);
        }

        function runSave(e) {
            code = (e.keyCode ? e.keyCode : e.which);
            if (code == 13) {
                LoginModal();                
                return false;
            }
        }

 







    </script>
