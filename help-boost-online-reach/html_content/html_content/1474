<!-- key[default-9d479a14608df110VgnVCM1000003d370a0aRCRD-recipecontent-f70436ae59a71110VgnVCM1000003d370a0aRCRD];compId[5018aa638abba110VgnVCM1000003d370a0aRCRD];dir[/80/62/SwOT3UuayL-uBYt4i1FDk12Dzh4=.ser]-->


	    <script type="text/javascript">

 

      zl_host = 'www.ziplist.com';

	 

				

		function getEpoch(){

			return Math.floor((new Date()).getTime() / 1000 + 60 * 5);

		}

		

		var epoch = "";

		var partner_key = 'mslo'; 

		var partner_add_path = '/minibox/partner_add';

		

		function getblob(){

			epoch = getEpoch();

			return epoch + partner_key + userid + partner_add_path + getzl_request();

		}

			

				

		 function getzl_sha(){

			 var zl_sha_url = '/portalext/ajax/msozlsha';

			 var queryString = "BLOB="+getblob();

				new Ajax.Request(zl_sha_url, {

		  		method: 'post',

		  		parameters: queryString,

		

		 		onComplete: function(transport){

						zl_sha = transport.responseText;

						submitToZLInternal(zl_sha)

		 		},

		

				onException: function(req,exception) {

					//alert("Sorry, your request could not be performed. \n\n" + exception.message);

					//return true;

				},

		

		  		onFailure: function(transport){

		  			//alert('failure');

		  			//alert(transport.responseText);

		  		}

		

		  		});

		 }

		

		function submitToZLInternal(zl_sha){

		  var iframe = document.getElementById('zl_iframe');

		

		  submit_partner_add(iframe, zl_host, partner_add_path, epoch, partner_key, userid, getzl_request(), zl_sha);

		

		  document.getElementById('zl_div').style.display = 'inline';

		  return false;

		}

		function submitToZipList() {

			getzl_sha();		

		}

		function resetDemo() {

		    closeZl();

		    ziplist_reset_demo(zl_host);

		    return false;

		}

		

		function closeZl() {

			  document.getElementById('zl_div').style.display = 'none';

		  getIFrameDoc(document.getElementById('zl_iframe')).body.innerHTML = '';

		}

		

		var zlRequest ="";

		function getzl_request()

		{ 

			var t = new Template('<zl:envelope xmlns:zl="ziplist.com:rest-1-0"><partner_user_email>#{emails}</partner_user_email><publisher_recipe_id>#{recipeid}</publisher_recipe_id></zl:envelope>');

			var test ={emails: email, recipeid: contentId};

			zlRequest = t.evaluate(test);

			return zlRequest; 

		}



    </script><div id="ms-col2-article-content"><div class="ms-col2-article-content-inner"><div class="ms-col2-article-body">
<div class="ms-col2-article-body-inner">
<div class="ms-col2-recipe-ingredients">
<h2>Ingredients</h2>
<p>Serves 6</p>
<ul>
<li>6 rice-paper wrappers</li>
<li>2 cups radish sprouts ( 1/2 ounce)</li>
<li>1 red beet, trimmed and thinly sliced crosswise</li>
<li>1 medium carrot, peeled and julienned</li>
<li>1 cucumber, julienned</li>
<li>1 red bell pepper, stem and seeds removed, julienned</li>
<li> 3/4 cup coarsely grated daikon</li>
<li>3 medium carrots, peeled and coarsely chopped</li>
<li>1 small shallot, quartered</li>
<li>2 tablespoons coarsely grated peeled fresh ginger</li>
<li> 1/4 cup rice-wine vinegar (not seasoned)</li>
<li>2 tablespoons low-sodium soy sauce</li>
<li> 1/4 teaspoon toasted sesame oil</li>
<li>  Pinch each of coarse salt and freshly ground pepper</li>
<li> 1/4 cup vegetable oil</li>
<li> 1/4 cup water</li>
</ul>
</div>
<div class="ms-col2-recipe-directions">
<h2>Directions</h2>
<ol>
<li>
<span>Make the spring rolls: Soak one rice-paper wrapper in a large bowl of hot water until pliable. Transfer to a work surface. Place one-sixth of the sprouts, beet slices, carrot, cucumber, bell pepper, and daikon on the wrapper, towards the bottom. Fold ends in and roll tightly to enclose filling. Repeat with remaining ingredients to make 5 more rolls.</span>
</li>
<li>
<span>Make the dipping sauce: Puree carrots, shallot, ginger, vinegar, soy sauce, sesame oil, salt, and pepper in a food processor until smooth. With machine running, add vegetable oil and then water through the feed tube in a slow, steady stream. Serve sauce with spring rolls.</span>
</li>
</ol>
<div class="content_origin">

