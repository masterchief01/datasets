                <script type="text/javascript">
                    var numOfCards = 1;
                    var index = 0;

                    function SwapCards()
                    {
                        document.getElementById("Card" + index).style.display = "none";
                        index++;
                        if(index == numOfCards)
                        {
                            index = 0;
                        }
                        document.getElementById("Card" + index).style.display = "block";
                    }

                    function SetCards(newIndex)
                    {
                        document.getElementById("Card" + index).style.display = "none";
                        index = newIndex;
                        if(index == numOfCards)
                        {
                            index = 0;
                        }
                        document.getElementById("Card" + index).style.display = "block";
                    }
                </script> 
                
