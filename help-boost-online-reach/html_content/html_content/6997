	<script>

	
	
	function toggleFrame(tabId) {
		var tabId = "frame" + tabId;
		var centerCol = document.getElementById('channelcontent-framework');
		var divCollection = centerCol.getElementsByTagName('div');
		for (i=0; i<divCollection.length; i++) {
			var divLayer = divCollection.item(i);
			var tabLayer = document.getElementById("frametab"+i);
			if (divLayer.id == tabId) {
				divLayer.style.display = "block";
				tabLayer.className = "active";
			} else {
				if(divLayer.id.indexOf("frame")!=-1) {
					divLayer.style.display = "none";
					tabLayer.className = "";
				}
			}
		} 
	}
	
	
	

	</script>

