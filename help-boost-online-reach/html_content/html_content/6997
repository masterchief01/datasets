var displayLength = allRecipes.length - 1;
var rowLength = displayLength/3;
var divLength = parseInt(rowLength);
var remainderLength =  displayLength%3;

if(remainderLength == 1){
	leftEndPosition = divLength + 1;
	centerStartPosition = leftEndPosition + 1;
	centerEndPosition = (centerStartPosition + divLength) - 1;
} else if (remainderLength == 2){
	leftEndPosition = divLength +1;	
	centerStartPosition  = leftEndPosition +1;
		if(displayLength < 5){
		centerEndPosition = centerStartPosition;
		} else {
		centerEndPosition = centerStartPosition + divLength;
		}
} else {
	leftEndPosition = divLength;
	centerStartPosition = leftEndPosition +1;
	centerEndPosition = (centerStartPosition + divLength) - 1;
}


