    <h4 class="firstImpression">First Impression <span>This article makes me</span></h4>
    <script>
    var ttl = 86400;
    $(document).ready(function() {
        var selection = readCookie( "reaction[" + getMeta( "aid" ) + "]" );
        if( selection ) {
            hideButtons( selection );
        }
        initializeButtons();
        $("input[name*=reaction]").click(function(a){
            hideButtons( a.target.value );
            setCookie( "reaction[" + getMeta('aid') + "]", a.target.value );
            $.getJSON( '/includes/views/helpers/cache.php?c=reactions/put/' + getMeta( "aid" ) + '/' + a.target.value + '/t:1&cc=reactions/get/' + getMeta( "aid" ) + '&jsoncallback=?',
                function( data ) {
                    populateData( data );
                }
            )
            omniTrackLink('#',reactions[a.target.value],$(this).parents("div[omni_section]").attr("omni_section"));
        }
        )
    });

    function hideButtons( selection ) {
        $("#shareThis").slideDown();
        $("#makesMeBox > div > input").each(
        function( index ) {
            $(this).hide();
            if( index == selection ) {
                $("#p_"+selection).addClass( "checked" );
            }
        });
    }

    function initializeButtons( selection ) {
        $.getJSON( '/includes/views/helpers/cache.php?c=reactions/get/' + getMeta( "aid" ) + '&ttl=' + ttl + '&jsoncallback=?',
            function( data ) {
                populateData( data );
            }
        )
    }

    function populateData( data ) {
        $("#makesMeBox > div > p.score").each( function( index ) {
            this.innerHTML = data[index] + "<span>%</span><br /><strong>" + reactions[index] + "</strong>";
        }
        )
        $("#makesMeBox").css( "visibility", "visible" );
    }

</script>
