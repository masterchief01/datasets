<script type="text/javascript">
    var gAd = {}
    gAd.channel = null;
    
    if(typeof getMeta != 'undefined'){
        gAd.channel = getMeta('cRoot');
        gAd.channel = gAd.channel.replace(/\//gi,'');
        }
    if(gAd.channel == null){
        gAd.channel = 'dating';
        }
    
    
    
    
    gAd.ciPrefix = 'fim_ign-askmen_channel-index-';
    gAd.altPrefix = 'fim_ign-askmen_';
    gAd.Placements = {
        galleries   : 'fim_ign-askmen_picture-galleries',
        celebs      : gAd.ciPrefix+ '',
        dating      : gAd.ciPrefix+ 'dating-and-love',
        sports      : gAd.ciPrefix+ 'health-and-sports',
        money       : gAd.ciPrefix+ 'power-and-money',
        gaming      : gAd.ciPrefix+ 'gaming',
        cars        : gAd.ciPrefix+ 'cars',
        fine_living : gAd.ciPrefix+ 'fashion-and-lifestyle',
        fashion     : gAd.ciPrefix+ 'fashion-and-lifestyle',
        video       : gAd.altPrefix+'video-of-the-day-article'
        }
    function google_ad_request_done(google_ads){
        var i = 0;
        if (google_ads == null || google_ads.length == 0) return;
        var ctl = null;
        var str = '';
        str = '';
        /*document.write(gAd.style);*/
        str += '  <div id=ad-wrap> <DIV id=ad-hdr>Sponsored Links</div>';
        for (l = 0; l < 4; l++){
            if (google_ads[i] == null) break;
            str += ' <a onmouseover="window.status=\'go to ' +
                    google_ads[i].visible_url +
                    '\'; return true;" onmouseout="window.status=\'\';" href="' +
                    google_ads[i].url + '" target=_blank><strong>' +
                    google_ads[i].line1 + '</strong>  <p>' +
                    google_ads[i].line2 + '<br />' +
                    google_ads[i].line3 + '</p><span>' +
                    google_ads[i].visible_url + '</span></a> ';
            i++;
            }
        str += ' </div>';
        document.write(str);
        var gafc = document.getElementById("gafc");
        if (gafc == null) return;
        gafc.className = gafc.className.replace("adcount4", "adcount" + google_ads.length);
        }
    
      var google_ad_client = "fim_ign-askmen_js";
      var google_encoding = "utf8";
      var google_ad_channel = gAd.Placements[gAd.channel]+",united-states";
      var google_safe = "high";
      var google_max_num_ads = 4;
      var google_ad_type = "text";
      var google_adtest = "off";
      var google_ed = "";
      var google_ad_output = "js";
</script>
