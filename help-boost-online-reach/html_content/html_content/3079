<script type="text/javascript">

function startChat()
{
	if($("#chat_box").val()=="" || $("#chat_box").val()=='Enter your question here')
	{
		alert('please enter description');
		$("#chat_box").focus();
		return false;
	}
	else
	{
		var desc = $('#chat_box').val();
		$('#cdesc').val(desc);
		$('#slide1').css("display","");
		$('#slide2').css("display","none");
	}
}

function submitForm()
{
	var category = $('#category').val();
	var brand = $('#brand').val();
	var os = $('#os').val();
	var emailid = $('#email_id').val();
	var issue_desc = $('#cdesc').val();
	if(($('#category').val()=='') || ($('#category').val()=='0'))
	{
		alert('Select a category');
		return false;
	}
	else if(($('#brand').val()=='') || ($('#brand').val()=='0'))
	{
		alert('Select a brand');
		return false;
	}
	else if(($('#os').val()=='') || ($('#os').val()=='0'))
	{
		alert('Select an operating system');
		return false;
	}
	else if(($('#email_id').val()=='') || ($('#email_id').val()=='0'))
	{
		alert('Enter your email address');
		$('#email_id').focus();
		return false;
	}
	else
	{
		$('.mid_sec').html('<iframe src="https://services.iyogi.net/iyogichat/whosonchat.aspx?domain=www.seo.iyogi.com&source=windows7.iyogi.com&description='+issue_desc+'&email='+emailid+'&category='+category+'&brand='+brand+'&operatingsystem='+os+'" frameborder="0" width="100%" height="320" scrolling="no"></iframe>');
		//alert($('.mid_sec').html());
		$('.chat_pop').show('slow');	
	}

		
}

</script>
            
                        
                 
			
