		<script type="text/javascript">
		if ( navigator.userAgent.indexOf("Mobile") != -1 && navigator.userAgent.indexOf("Safari") != -1 )  {
			jQuery(document).ready(function()  {
				window.scrollTo(265, 100);
			});
		}
		
		jQuery(document).ready(function() {
			$('span#makeAtHome').click(function() {
				$('#homeMeUp').show('fast');
				return false;
			});
			$('span#goodByeHome').click(function() {
				$('#homeMeUp').hide('fast');
				return false;
			});
		});
		
		jQuery.azc.set_carousel_opts({
			dimensions: '60x65',
			formatter: function(item)  {
				var htmlentities = jQuery.azc.htmlentities;
				
				var html = '';
				
				html += '<a href="' + item['url'] + '" title="' + htmlentities(item['headline']) + '">';
				html += '<img src="' + item['imageurl'] + '" width="60" height="65" alt="' + htmlentities(item['headline']) + '" class="azcDexRotThumb" border="0" />';
				html += '</a>';
				html += '<p>';
				html += '<a href="' + item['url'] + '" title="' + htmlentities(item['headline']) + '">';
				html += item['headline'];
				html += '</a><br />\r\n';
				html += item['abstract'];
				html += '</p>';

				return html;
			}
		});
		
		jQuery.azc.afterItExists('#azcDexScanner', 30, function()  {
			jQuery('#azcDexScanner').commentscanner({
			  onplay : function(self)  {
			  	self.container.find('.azcLivePlayButton').html("<div style='width: 30px; height: 30px;'></div>").css({background: "url(/imgs/azcdexscan-pause.gif)"});
			  },
			  onpause : function(self)  {
			  	self.container.find('.azcLivePlayButton').html("<div style='width: 30px; height: 30px;'></div>").css({background: "url(/imgs/azcdexscan-play.gif)"});
			  }
			});
		});
		
		jQuery.azc.fasttabs('#also-heds-tabs ul', { fxFade: true, fxSpeed: 'fast' });
		jQuery.azc.fasttabs('#weatherTrafficTabs ul', { fxFade: true, fxSpeed: 'fast' });
		
		jQuery.azc.afterItExists('#azcCommentScanner', 30, function()  {
			jQuery('#azcCommentScanner').commentscanner();
		});
	</script>
		<script type="text/javascript">
		jQuery.azc.fasttabs('#aztalk-tabs ul', { fxFade: true, fxSpeed: 'normal' });
		jQuery.azc.fasttabs('#realestate-tabs-wrap ul', {
			show: function() {
				if(jQuery('#realestate-tabs-wrap ul').tabsSelected() == "1") {
					document.getElementById('realestate_searchbytype').value = "resale_mls,resale_class,resale,new_const,new_class,new_mls,new_ecom_agent,new_ecom_broker,new_ecom_builder,resale_ecom_agent,resale_ecom_broker,resale_ecom_builder,new_ecom_owner,resale_ecom_owner";
				} else if(jQuery('#realestate-tabs-wrap ul').tabsSelected() == "2") {
					document.getElementById('realestate_searchbytype').value = "new_const,new_mls,new_class,new_ecom_agent,new_ecom_broker,new_ecom_builder,new_ecom_owner";
				} else {
					document.getElementById('realestate_searchbytype').value = "resale_mls,resale,resale_class,resale_ecom_agent,resale_ecom_broker,resale_ecom_builder,resale_ecom_owner";
				};
			}
		});
