    <script type="text/javascript"> $("#global_top_content .widget_nav li a").click(function(e){ e.preventDefault(); $("#global_top_content .widget_nav li a").removeClass("current"); $(this).addClass("current"); widget_slide(); }); $("#global_top_content .widget_posts .widget_item_content .post_thumb a img").load(function(){ $(this).parent().parent().css('height', $(this).height()+"px"); }); $("#global_top_content .widget_list_holder ul").each(function(i, el){ var classname = $(el).attr("class"); var rem = false; var hei = $(el).find("li.readmore").height(); $(".widget_list_holder ul."+classname+" li.item").each(function(li_i, li_el){ var li_hei = $(li_el).outerHeight() + parseInt($(li_el).css("margin-bottom").replace('px', '')) + parseInt($(li_el).css("padding-bottom").replace('px', '')); var buff = (classname=="widget_posts")?150:0; if(rem || ((hei + li_hei) > $(".widget_list_holder ul.widget_posts").outerHeight()+buff)){ rem = true; $(li_el).remove(); } else hei += li_hei; }); }); function widget_slide(){ var leftPos = 8; var curPanel = "widget_posts"; var curLink = $("#global_top_content .widget_nav li a.current"); if(curLink.hasClass("posts")){ leftPos = 8; } else if(curLink.hasClass("ecards")){ curPanel = "widget_cards"; leftPos = -311; } else if(curLink.hasClass("tweets")){ curPanel = "widget_tweets"; leftPos = -631; } else if(curLink.hasClass("store")){ curPanel = "widget_store"; leftPos = -951; } $("#global_top_content .widget_list_holder").animate({'margin-left':leftPos+'px'}, 400); widget_lazyload(curPanel); } function widget_lazyload(panel_class){ $("."+panel_class+" li.item .post_thumb a img").each(function(){ if(!$(this).hasClass("loaded")){ $(this).attr("src", $(this).attr("title")).removeAttr('title').addClass("loaded"); } }); } var widget_interval = 0; function start_widget_rotate(){ widget_interval = setInterval(function(){ var currentInd = $("#global_top_content .widget_nav li a.current").parent().index(); if(currentInd >= $("#global_top_content .widget_nav li").length-1) { currentInd = 1; kill_widget_rotate(); } else currentInd++; $("#global_top_content .widget_nav li a").removeClass("current"); $("#global_top_content .widget_nav li").eq(currentInd).find("a").addClass("current"); widget_slide(); }, 7000); } function kill_widget_rotate(){ clearTimeout(widget_interval); } function scroll_widget(){ var holder_hei = $("#global_top_content .widget_content").outerHeight(); var holder_pos = $("#global_top_content .widget_content").offset(); var win_hei = $(window).scrollTop()+$(window).height(); if(win_hei > (holder_pos.top+100) && win_hei < (holder_pos.top+holder_hei+$(window).height()-150)){ if(widget_interval<=0) start_widget_rotate(); } else { clearInterval(widget_interval); widget_interval=0; } } $("#global_top_content").hover( function(){ kill_widget_rotate(); }, function(){} ); $(window).scroll(function(){ scroll_widget(); }); scroll_widget(); </script>
