<h3><strong>Filter</strong></h3>
<p>
<select name="style_format" id="style_format"><option value="both">All Formats</option><option value="OpenType">OpenType Only</option><option value="TrueType">TrueType Only</option></select><br />
	
	<input type="checkbox" name="style_fontface" id="style_fontface" class="style_changer" value="1"> <img src="/site_resources/themes/default_theme/images/flag_fontface.png" width="22" height="9" alt="Flag Fontface" /> @font-face <br />
</p>
<script type="text/javascript" charset="utf-8">
	$(document).ready(function() {
		// read cookie and set options above
		if ($.cookie('style_format') == null)
		{
			// no cookies yet, so set default cookie
			$.cookie('style_format','both', { path: '/' });
		} else {
			$('#style_format option[value='+$.cookie('style_format')+']').attr('selected','selected');
		}
		
		
		$('#style_format').change(function(){
			$.cookie('style_format',$('#style_format option:selected').val(), { path: '/' });
			update_font_menu();
		});
		
		
		if ($.cookie('style_fontface') != null)
		{
			$("#style_fontface").attr("checked","checked");
		}
		
		
		$(".style_changer").click(function(){
			
			if($(this).attr('checked') === true)
			{
				$.cookie($(this).attr('id'), $(this).val(), { path: '/' }); // set cookie
					
			} else {
				$.cookie($(this).attr('id'), null, { path: '/' }); // delete cookie
			}
			update_font_menu();
		});
		
		
		
		update_font_menu();
	});
	
	function update_font_menu()
	{
		$.post('/fontmenu',{style_format: $.cookie('style_format'), font_face: $.cookie('style_fontface'), families: $.cookie('style_famsonly')},function(data){
			$("#font_menu").html(data);
		});
	}
</script>
	
