            jQuery('#carousel_1349136388947287e051').animate({
                height: jQuery('#carousel_1349136388947287e051 .jcarousel-item-1').outerHeight(true)
            });
        }
    }
	jQuery('#jcarousel-next-horizontal_1349136388947287e051').live('click', function(){
		var slide = parseInt(jQuery('#slide_1349136388947287e051').html());
		if(++slide > 1) slide = 1;
		jQuery('#slide_1349136388947287e051').html(slide);
		if(jQuery('#carousel_1349136388947287e051').outerHeight(true) < jQuery('#carousel_1349136388947287e051 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_1349136388947287e051').animate({
				height: jQuery('#carousel_1349136388947287e051 .jcarousel-item-' + slide).outerHeight(true)
			});
		jQuery('#carousel_1349136388947287e051').animate({
			marginLeft: 0-300
		}, 300, function(){
			var a = jQuery(this).children('li:first').detach();
			jQuery(this).css({
				marginLeft: 0
			}).append(a);
		});
		jQuery('#carousel_1349136388947287e051').animate({
			height: jQuery('#carousel_1349136388947287e051 .jcarousel-item-' + slide).outerHeight(true)
		});
	});
	jQuery('#jcarousel-prev-horizontal_1349136388947287e051').live('click', function(){
		var slide = parseInt(jQuery('#slide_1349136388947287e051').html());
		if(--slide < 1) slide = 1;
		jQuery('#slide_1349136388947287e051').html(slide);
		if(jQuery('#carousel_1349136388947287e051').outerHeight(true) < jQuery('#carousel_1349136388947287e051 .jcarousel-item-' + slide).outerHeight(true))
			jQuery('#carousel_1349136388947287e051').animate({
				height: jQuery('#carousel_1349136388947287e051 .jcarousel-item-' + slide).outerHeight(true)
			});
		var a = jQuery('#carousel_1349136388947287e051').children('li:last').detach();
