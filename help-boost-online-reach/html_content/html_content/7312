<script>
function submit_regliteComment(){
	document.commentform.submit();
}

function doPostForBlogComments(){
        var opt = {
            method: 'post',
            postBody: Form.serialize($('comment_form')),
            onSuccess:
            function(response) {
                var jsonContent = eval("(" + response.responseText + ")");
                if (jsonContent.loginFailed) {
					alert('there was an error');
                } else if (jsonContent.attemptLimitReached || jsonContent.accountDisabled) {
                    window.location = '/user/login/retrieve?returnto=' + escape(window.location.pathname + window.location.search);
                } else if (jsonContent.loginSuccess) {
                    comment_module.hide();
                }
            },
            onFailure:
                function(response) {
                    // TODO:  This needs to be reported. For now, let's keep.
                    alert("An internal server error has prevented the processing of your request.  Please try again later.");
                    window.location = "";
                }
        }
        /* hand off to you Horace :) */
        //new Ajax.Request('/user/login/light', opt);
		alert('ajax comments handoff to Horace from Matt');
    }
</script>

