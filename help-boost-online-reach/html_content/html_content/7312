    if (jQuery('#featured_stories_h').length != 0) {
        var featuredStoriesModuleH = new Slider({
            containerid: 'featured_stories_h',
            groupsize: 6,
            speed: 15,
            hasgroupnav: false,
            hasloop: false,
            effect: 3,
            direction: 'horizontal',
            hastabs: true,
