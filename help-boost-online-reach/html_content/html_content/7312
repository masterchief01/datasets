<!-- Signin or Registration Module ------------------------------------------------------------

	Author: Matthew Mah
	Date: 7/17/2007
	Function: ajax login, ajax register, ajax commenting for style blogs

---------------------------------------------------------------------------------------------->
<link rel="stylesheet" href="/stylesheets/registration.css" type="text/css" media="screen" />
<script src="/scripts/prototype/prototype.js" type="text/javascript"></script>
<script type="text/javascript" language="javascript1.2">
runOnLoad(init_reglite);
var signin_module = null;
var reglite_module = null;
var comment_module = null;

function init_reglite(){
	signin_module = getSignInModule();
	reglite_module = getRegliteModule();
	comment_module = getCommentModule();
	YAHOO.util.Event.addListener('logInBtn', 'click', doLoginPost);
	YAHOO.util.Event.addListener('joinBtn', 'click', showRegliteModule);
	YAHOO.util.Event.addListener('cancelLogIn', 'click', closeSignInModule);
    YAHOO.util.Event.addListener('logInBtn', 'keypress',
        function (e) {
            if(13 == e.keyCode) {
                doLoginPost();
            }
        }
    );
    YAHOO.util.Event.addListener('cancelLogIn', 'keypress',
        function (e) {
            if(13 == e.keyCode) {
                closeSignInModule();
            }
        }
    );
	YAHOO.util.Event.addListener("email2", 'blur', checkEmailAvailabilityCallback);
	YAHOO.util.Event.addListener("userNameRate", 'blur', checkUsernameAvailabilityCallback);
	YAHOO.util.Event.addListener("reglite_close", 'click', hideRegliteModule);
	YAHOO.util.Event.addListener("reglite_submit_button_link", 'keypress',
		function (e) {
			if(13 == e.keyCode) {
				doLiteRegistration();
			}
		}
	);
	YAHOO.util.Event.addListener("comment_close", 'click', hideCommentModule);
}
function showMouseOverPointer(e) {
    $(this.id).style.cursor = 'pointer';
}
function getSignInModule() {
    if (signin_module == null) {          
        signin_module = new YAHOO.widget.Overlay("signInMod", {
            visible:false,
            width:"253px",
            fixedcenter:true
            } );
        signin_module.render();
    }
    return signin_module;
}
function closeSignInModule() {
    if(getSignInModule().cfg.getProperty("visible")){
        signin_module.hide();
    }
    $('progressMsg').style.display = 'none';
}
function clearErrorMessage() {
    $('signInErrorMsgDiv').style.display = 'none';
}
function clearFormFields() {
    $('user').value = '';
    $('pass').value = '';
}
function doLoginPost(){
    $('progressMsg').style.display = 'block';
    var opt = {
        method: 'post',
        postBody: Form.serialize($('signInForm')),
        onSuccess:
            function(response) {
                $('progressMsg').style.display = 'none';
                var jsonContent = eval("(" + response.responseText + ")");
                if (jsonContent.loginFailed) {
                    $('signInErrorMsgDiv').style.display = 'block';
                    $('user').focus();
                } else if (jsonContent.attemptLimitReached || jsonContent.accountDisabled) {
                    $('progressMsg').style.display = 'block';
                    window.location = '/user/login/retrieve?returnto=' + escape(window.location.pathname + window.location.search);
                } else if (jsonContent.loginSuccess) {
                    closeSignInModule();
                    doShowSignedInNavbar();
					postComment(current_post_id);
                }
            },
        onFailure:
            function(response) {
                // TODO:  This needs to be reported. For now, let's keep.
                $('progressMsg').style.display = 'none';
                window.location = "";
            }
    }
    new Ajax.Request('/user/login/light', opt);
}
var current_post_id = null;			//global variable...prolly not the best idea
function postComment(post_id){
   if (new UserInfo().isLoggedIn()){
        comment_module.show();
		var userName = new UserInfo().getUserName();
		document.commentform.comment_post_ID.value = post_id;
		document.commentform.email.value = new UserInfo().getUserName()+'@xyz.com';
		document.commentform.author.value = new UserInfo().getUserName();
		
   }else{
        current_post_id = post_id;
        signin_module.show();
   }
}
function getCommentModule(){
	if (comment_module == null) {
            
        comment_module = new YAHOO.widget.Overlay("commentModule", {
            visible:false,
            fixedcenter:true
            } );
        comment_module.render();
    }
    return comment_module;
}
function hideCommentModule(e) {
	comment_module.hide();
}
</script>
