                new Tab({tabId: 'shows_content_tab', contentId: 'shows_content'})
    
    
            ]
        });
    }
    /*From Social bookmarking*/
    var articleTitle = document.title;

    articleTitle = articleTitle.replace(/\s+/g,'+');

    jQuery(document).ready(function(){
        jQuery('#social_bkmrks').hide();
        jQuery('a#social_bookmarking').click(function () {
            jQuery('#social_bkmrks').toggle();
            jQuery(this).toggleClass('active'); return false;
        });
    });

    function gotoEmailAFriend( ) {
        window.location = "/emailafriend?referringPage=" + escape(location.href);
    }

    function sbm_Window(docurl, doctitle){

        var docurl = docurl + doctitle;

        var Newsbm=window.open(docurl,'sbmWindow',"width=1000,height=500,directories=no,menubar=no,toolbar=no,scrollbars=yes,left=0,top=0,screenX=0,screenY=0");
    }
