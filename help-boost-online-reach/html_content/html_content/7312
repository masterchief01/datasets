<script type='text/javascript' src='/dwr/interface/amgAccountManager.js'></script>
<script type='text/javascript' src='/dwr/engine.js'></script>
<script type='text/javascript' src='/dwr/util.js'></script>
<script language="javascript">

    // Map form errors codes to form div errors ids.
    var regLiteErrorsCodes = new Array();
    regLiteErrorsCodes['form.errors.username.required']  = 'reglite.username';
    regLiteErrorsCodes['form.errors.email.required']     = 'reglite.email';
    regLiteErrorsCodes['form.errors.password1.required'] = 'reglite.password1';
    regLiteErrorsCodes['form.errors.password2.required'] = 'reglite.password2';
    regLiteErrorsCodes['error.optin.required']           = 'reglite.optin';
    regLiteErrorsCodes['errors.user.invalid.username']   = 'reglite.username';
    regLiteErrorsCodes['errors.user.invalid.password']   = 'reglite.password1';
    regLiteErrorsCodes['error.email.taken']              = 'reglite.email';
    regLiteErrorsCodes['error.username.taken']           = 'reglite.username';
    regLiteErrorsCodes['form.errors.user.required']      = 'reglite.username';
    regLiteErrorsCodes['error.email.invalid']            = 'reglite.email';
    regLiteErrorsCodes['form.error.passwords.mismatch']  = 'reglite.password1';
	
    function getRegliteModule() {
        if (reglite_module == null) {
            reglite_module = new YAHOO.widget.Overlay("reglite_module", {
                width:"403px",
				fixedcenter:true,
                visible:false
            } );
            reglite_module.render();
        }
        return reglite_module;
    }
    function showRegliteModule(e) {
        closeSignInModule();
        reglite_module.show();
    }
    function hideRegliteModule(e) {
        reglite_module.hide();
    }
    function doLiteRegistration() {

        var opt = {
            method: 'post',
            postBody: Form.serialize($('reglite_form')),
            onSuccess:
                function(response) {
                    var jsonContent = eval("(" + response.responseText + ")");
                    var formName = "org.springframework.validation.BindingResult.form";
                    hideErrorMessages($('reglite_errors'), regLiteErrorsCodes);
                    if (jsonContent[formName] != null && jsonContent[formName].errorCount > 0) {
                        $('reglite_errors').style.display = 'none';
                        displayBindExceptionErrorsMessages(jsonContent[formName], $('reglite_errors'), 'regLiteMsgId,', regLiteErrorsCodes);
                        $('reglite_errors').style.display = 'block';
                    } else {
                        reglite_module.hide();
                        doShowSignedInNavbar();
						postComment(current_post_id);
                    }
                },
            onFailure:
                function(response) {
                    // TODO:  This needs to be reported. For now, let's keep.
                    alert("An internal server error has prevented the processing of your request.  Please try again later.");
                    // window.location = '';

                }
        }
        new Ajax.Request('/user/registration/light', opt);
    }
	
function hideErrorMessages (errGroupElem, errorCodes) {
// Clear error messages but leave the first node which is present
		for (var i=errGroupElem.childNodes.length-1; i > 0; i--) {
            errGroupElem.removeChild(errGroupElem.childNodes[i]);
        }
        for (anId in errorCodes) {
            var theFlag = errorCodes[anId] + ".warning";
            if ($(theFlag) != null) {
                $(theFlag).style.display = 'none';
            }
        }
    }
    function displayBindExceptionErrorsMessages(formErrors, errGroupElem, classId, errorCodes) {
        for (i=0; i < formErrors.errorCount; i++) {

            var currErr = formErrors.allErrors[i];
            var errorElem = document.createElement('div');
            errorElem.setAttribute("class", classId);
            var errorMsgId = errorCodes[currErr.code];
            // Turn on the warning flags
            if (errorMsgId != null && errorMsgId != '') {
                var theFlag = errorMsgId + ".warning";
                if ($(theFlag) != null) {
                    $(theFlag).style.display = 'inline';
                }
            }
            var aLi = document.createElement('li');
            aLi.appendChild(errorElem);
            errorElem.innerHTML = currErr.defaultMessage;
            errGroupElem.appendChild(aLi);
        }
    }
    function displayAvailabilityErrorMessage(e, isTaken, prefix) {
        if (e != null) {
            e.classname = "unavailable";
            e.style.display = 'inline';
            if (isTaken) {
                e.innerHTML = prefix + " is not available!";
            } else {
                e.innerHTML = prefix + " is available!";
            }
        }
    }
    function checkUsernameAvailabilityCallback() {
        if ($('userNameRate').value != null && $('userNameRate').value != '') {
            DWREngine._execute(amgAccountManager._path, 'amgAccountManager', 'isUsernameTaken', $('userNameRate').value,

                function (isTaken) {
//                    displayAvailabilityErrorMessage($('username_error_msg'),isTaken,'username');
                    if (isTaken) {
                        $('username_error_msg').style.display = 'block';
						$('userNameRate').value = '';
						$('userNameRate').focus();
                    } else {
                        $('username_error_msg').style.display = 'none';
                    }
                }
            );
        } else {
            $('username_error_msg').style.display = 'none';
        }
    }
    function checkEmailAvailabilityCallback() {
        if ($('email2').value != null && $('email2').value != '') {
            DWREngine._execute(amgAccountManager._path, 'amgAccountManager', 'isEmailTaken', $('email2').value,
                function (isTaken) {
//                    displayAvailabilityErrorMessage($('emailMsg2'), isTaken, 'email');
                    if (isTaken) {
                        $('email_taken_msg').style.display = 'block';
						$('email2').value = '';
						$('email2').focus();
                    } else {
                        $('email_taken_msg').style.display = 'none';
                    }
                }
            );
        } else {
            $('email_taken_msg').style.display = 'none';
        }
    }
</script>
