			var ph_col = {
				ra: function () {}, 
				go: function (o, ns) {
					if(o == null) o = 1;
					jQuery.ajax({
						type: 'GET', cache: false, 
						url: '/story/photos/UPI-35521304221808/'+o+'/', 
						dataType: 'json', 
						success: function(d) {
							jQuery('#story_photos .photo_cont').html(d.photo);
							jQuery('#story_photos .caption').html(d.caption);
							ph_col.next = d.n;
							ph_col.prev = d.p;
							upi_pt('/story/photos/UPI-35521304221808/'+o+'/', '');
						}
					});
					ph_col.ra();
					return false;
				},
				next: 1, 
				prev: 1, 
				wait: null
			}

			jQuery(document).keydown(function(e) {
				if(e.keyCode == 18) {
					if(ph_col.wait) clearTimeout(ph_col.wait);
					ph_col.wait = setTimeout(function() {}, 1000);
				
				}
				if(!ph_col.wait) {
					if(e.keyCode == 37) {ph_col.go(ph_col.prev);}
					else if(e.keyCode == 39) {ph_col.go(ph_col.next);}
				}
			});
