		function trackRSSClick(obj)
		{
			s.events = 'event15';
			s.linkTrackEvents = 'event15';
			s.tl(obj, 'o');
		}
		function getDomain(url) {
			return (url.match(/:\/\/(.[^/]+)/)[1]).replace('www.','');
		}
