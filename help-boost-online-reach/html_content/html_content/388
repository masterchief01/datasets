		function trackDownload(obj, linkname)
		{
			s.linkTrackVars='None';
			s.linkTrackEvents='None';
			s.tl(obj,'d',linkname);
		}
		function trackHeadlineClicks(obj)
		{
			s.prop8 = $(obj).attr("data-position");
			s.prop9 = $(obj).attr("data-category");
			s.prop10 = "Headline Hover Link Clicked " + s.prop8;
			s.eVar6 = $(obj).parent().parent().find('h2 a').text();
