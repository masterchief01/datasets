			s.linkTrackVars = 'prop8,prop9,prop10,eVar6';
			s.linkTrackEvents = 'event11';
			s.linkInternalFilters=getDomain(obj.href)
			s.tl(obj, 'o');
		}
		function trackHeadlineStackClicks(obj)
