    $(document).ready(
    function() {
        var isMetric = ($('#ctl00_CenterColumnPlaceHolder_recipe_hdnIsMetric').val() == '1');
        $('#ctl00_CenterColumnPlaceHolder_recipe_rblMetric').attr('checked', isMetric);
        $('#ctl00_CenterColumnPlaceHolder_recipe_rblUS').attr('checked', !isMetric);       
    });    
</script>
 
</div>

