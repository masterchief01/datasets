<script type="text/javascript">
$(document).ready(function(){

	/* Hides the print options slickbox */
	$('#print').hide();

	/* Toggles the print options slickbox on clicking the noted link */
	$('a#toggle').click(function() {
		$('#print').toggle(400);
		return false;
	});

	/* Switching the print style sheets */
	$('#fullPage').click(function() {
		$('link#printStylesheet').attr('href', '/css/wfm-print.css');
		$('#print').hide();
		window.print();
	});

	$('#fullNoImage').click(function() {
		$('link#printStylesheet').attr('href', '/css/recipeNoImage.css');
		$('#print').hide();
		window.print();
	});

	$('#card').click(function() {
		$('link#printStylesheet').attr('href', '/css/recipeCard.css');
		$('#print').hide();
		window.print();
	});


	$("#report_abuse").click(function(){
		var user = "";
		var photoKey = $("a.ad-active img.gallery-image").attr("key");
		var preview = $("a.ad-active img.gallery-image").attr("src");
		if(user == "ok"){
			if(photoKey == "wfm"){
				$("#wfm_preview").attr('src', preview);
				$("#dialog-wfm").dialog('open');
			}else{
				$("#report_preview").attr('src', preview);
				//alert("Photo Key: " + photoKey);
				$("#abuseComment").val("");
				$("#abuse_form").show();
				$("#abuse-result").hide();
				$("#dialog-report").dialog('open');
			}
		}else{
			window.location = "http://www.wholefoodsmarket.com/users/login.php?url=/recipes/2937";
		}
	});

	$("#recommend").click(function(){
		var user = "";
		var photoKey = $("a.ad-active img.gallery-image").attr("key");
		var preview = $("a.ad-active img.gallery-image").attr("src");
		if(user == "ok"){
			if(photoKey == "wfm"){
				$("#wfm_preview").attr('src', preview);
				$("#dialog-wfm").dialog('open');
			}else{
				$("#recommend_preview").attr('src', preview);
				$("#recommend-text").html('Sending ...');
				$("#dialog-recommend").dialog('open');
				pluck.sitelife.photos.report.recommend(photoKey);
			}
		}else{
			window.location = "http://www.wholefoodsmarket.com/users/login.php?url=/recipes/2937";
		}
	});

	$("#dialog-recommend").dialog({
		modal: true,
		width: 350,
		height: 200,
		draggable: false,
		autoOpen: false,
		buttons: {
			OK: function(){
				$(this).dialog('close');
			}
		}
	});

	$("#dialog-wfm").dialog({
		modal: true,
		width: 350,
		height: 200,
		draggable: false,
		autoOpen: false,
		buttons: {
			OK: function(){
				$(this).dialog('close');
			}
		}
	});

	$("#dialog-report").dialog({
		modal: true,
		width: 350,
		height: 330,
		draggable: false,
		autoOpen: false,
		buttons: {
			Yes: function() {
				// Do Stuff
				pluck.sitelife.photos.report.abuse($("a.ad-active img.gallery-image").attr("key"), $("#abuseReason").val(), $("#abuseComment").val());
				// Update Status
				$("#abuse-result").html("Sending ...");
				$("#abuse_form").hide();
				$("#abuse-result").show();
			},
			Cancel: function(){
				$(this).dialog('close');
			}
		}
	});
});
</script>

