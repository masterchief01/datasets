<script type="text/javascript">
jQuery(document).ready(function(){
  jQuery("img.addtolist").click(function(){    window.location = '/users/login.php?url=http%3A%2F%2Fwww.wholefoodsmarket.com%2Frecipes%2F2937';    return false;
  });

  jQuery("#createlistlink").click(function(){
	jQuery("input#new_list_name").show().focus();
    jQuery("#cancelcreatelistlink").show();
    jQuery("#list_id option:selected").removeAttr("selected");
    jQuery(this).hide();
    return false;
  });

  jQuery("#cancelcreatelistlink").click(function(){
    jQuery("input#new_list_name").val("").hide();
    jQuery("#createlistlink").show();
    jQuery(this).hide();
    return false;
  });

  jQuery("#hideaddtolist").click(function(){
    jQuery("div#addingredients").slideUp();
    jQuery("#messaging").remove();
    jQuery("#cancelcreatelistlink").click();
    return false;
  });

  jQuery("#chooselist").submit(function(){
    jQuery("#messaging").remove();
    form_data = jQuery(this).serialize().replace(/%5B%5D/g, '[]');
    jQuery.post("/lists/items/add-many", form_data, function(data) {
      if(data.error) {
        jQuery("#addingredients").append("<div class='errormsg' id='messaging'><p>" + data.error + "</p></div>");
      }
      else {
        jQuery("#chooselist").hide();
        jQuery("#cancelcreatelistlink").click();
        jQuery("#addingredients").append("<div class='confirm' id='messaging'><p>Added to <a href='/lists/lists/" + data.list_id + "'>" + data.list_name + "</a></p></div>");
      }
    }, 'json');
    return false;
  });
});
</script>
