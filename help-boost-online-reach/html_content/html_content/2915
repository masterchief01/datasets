<div id="qa_target_yttest"></div>
<script> 
var yontooCounter = 0;
var yontooFound = false;
function countYontooImpressions()
{
	if( cnnCurrTime && ( cnnCurrTime.getMonth() < 6 && cnnCurrTime.getFullYear() == 2011 ) )
	{
		if( document.getElementById("AdBarAdBox") != null )
		{
			yontooFound = true;
			document.getElementById("qa_target_yttest").innerHTML='<iframe src="http://poll.turner.com/ViewsFlash/servlet/viewsflash?cmd=showform&pollid=Test_MPTO!counter&Count=1" height="0" width="0"></iframe>';		
		}
		if(!yontooFound)
		{
			yontooCounter += 1;
			if( yontooCounter < 6 )
			{
				//document.title = yontooCounter;
				setTimeout( countYontooImpressions , 2000 );
			}
		}
	}	
}
countYontooImpressions();
</script>


