		var ad_tag = 'http://ad.doubleclick.net/N6556/adj/' + BF_DFP_SITE + BF_DFP_ZONE + ';wid=10;sz=3x3;pos=header;tile=10;' + BF_DFP_KVS + ';' + BF_TAG + 'ord=' + BF_DFP_ORD + '?';
	
	
