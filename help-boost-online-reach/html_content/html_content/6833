      if (!window.registeredBloggerCallbacks) {
        window.registeredBloggerCallbacks = true;

        

        
        gadgets.rpc.register('requestReload', function() {
          document.location.reload();
        });

        
        gadgets.rpc.register('requestSignOut', function(siteId) {
          
          google.friendconnect.container.openSocialSiteId = siteId;
          google.friendconnect.requestSignOut();
        });
      }
    </script>
<script type="text/javascript">
    
    function registerGetBlogUrls() {
      gadgets.rpc.register('getBlogUrls', function() {
        var holder = {};
        
          
            
            
              holder.currentPost = "http://www.blogger.com/feeds/1338074902542181897/posts/default/5960024977993093106";
            
            
            
              holder.currentComments = "http://www.blogger.com/feeds/1338074902542181897/5960024977993093106/comments/default";
            
            holder.currentPostUrl = "";
            holder.currentPostId = 5960024977993093106
          
          
          
            holder.postFeed = "http://www.blogger.com/feeds/1338074902542181897/posts/default";
          
          
          
            holder.commentFeed = "http://www.blogger.com/feeds/1338074902542181897/comments/default";
          
          holder.currentBlogUrl = "http://pass-forward.blogspot.com/";
          holder.currentBlogId = "1338074902542181897";
        
        return holder;
      });
    }
  </script>
