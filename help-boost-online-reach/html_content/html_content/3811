	<script type="text/javascript">
	$('#footer').waypoint({
		offset: "100%",
		triggerOnce: true,
		handler: function(event, direction){
			if ($(document).width() >= 768) {
				$('#partner-feeds .rss-partner-feed').each(function(){
					var feedDiv = $(this);
					var rssFeedUrl = $(this).data('rss-feed-url');
					var rssImageUrl = $(this).data('rss-image-url');
					var rssImageLink = $(this).data('rss-image-link');
					$.ajax({
						type: 'GET',
						url: 'http://www.nickmom.com/wp-content/themes/nickmom-blog-responsive/inc/partner-rss-feeds.php?rss-feed-url=' + rssFeedUrl + '&rss-image-url=' + rssImageUrl + '&rss-image-link=' + rssImageLink,
						dataType: 'html',
						success: function(html){
							$(feedDiv).html(html);
							$(feedDiv).find('.partner-block:last-child').css('background-image', 'none');
							// send tracking event when going to partner site
							$(feedDiv).find('.partner-block a').click(function(){
								var imageLink = $(feedDiv).find('.partner-block a img').parent().attr('href');
								KIDS.reporting.omnifunctions.sendAnalyticsEvent('','Click to ' + imageLink);
							});
						}
					});
				});
			}
		}
	});
	</script>

