    <div class="mod mod-ingredients">
      <h2 class="mod-title title-ingredients pngfix">
        Ingredients
      </h2>
      <div class="media-wrap">
        <div class="top">
          <div class="btm content">
            <table class="ingredient-tbl">
              <tr>
                <td class="col-one ">
                    <span>2 teaspoons</span>
                    vegetable oil
                </td>
                <td class="col-two "></td>
                <td class="col-three ">
                      <span>2 teaspoons</span>
                      coarse salt
                </td>
              </tr>
              <tr>
                <td class="col-one ">
                    <span>20</span>
                    chicken wings
                </td>
                <td class="col-two "></td>
                <td class="col-three ">
                      <span>1/4 teaspoon</span>
                      cayenne pepper
                </td>
              </tr>
              <tr>
                <td class="col-one ">
                    <span>3</span>
                    large eggs, beaten
                </td>
                <td class="col-two "></td>
                <td class="col-three ">
                      <span>1/2 cup</span>
                      fresh breadcrumbs
                </td>
              </tr>
              <tr>
                <td class="col-one ">
                    <span>1/2 cup</span>
                    sesame seeds
                </td>
                <td class="col-two "></td>
                <td class="col-three ">
                      <span>2 to 3</span>
                      garlic cloves, minced
                </td>
              </tr>
              <tr>
                <td class="col-one last">
                    <span>1/2 cup</span>
                    all-purpose flour
                </td>
                <td class="col-two last"></td>
                <td class="col-three last">
                      <span></span>
                      Hot sauce, for serving
                </td>
              </tr>
            </table>
            <div class="ingredient-footer">
              <div class="yield">
                <span>
                  Yield:
                </span>
                20
                Servings
              </div>
              <div class="shopping_list_add_item">
