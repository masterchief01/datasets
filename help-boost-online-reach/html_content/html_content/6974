    var optimizations = PageParams.amGetPageInventory().optimizations;
    urlParams.push('amtestserver=amtestserver');
    for (var i = 0; i < optimizations.length; i++) {
        var optimization = optimizations[i];
        for (var j = 0; j < optimization.weblets.length; j++) {
