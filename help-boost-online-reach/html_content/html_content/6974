<script language="javascript" type="text/javascript">
    jQuery(function() {
        jQuery('.email_recipe_modal').jqm({ modal: true, trigger: '.emailRecipeTrigger', closeClass: 'closebutton', overlayClass: 'emailModalMask', zIndex: 3, setTop: 20 });
        jQuery('.closebutton').click(function() {
            ResetEmailModal();
        });
    });
    jQuery('.email_frame').load(function() {
        jQuery('.email_frame').contents().find('.closebutton').click(function() {
            jQuery('.email_recipe_modal').jqmHide();
            ResetEmailModal();
        });
    });

    function ResetEmailModal() {
        jQuery('.email_frame').contents().find('.jqEmailPanel').show();
        jQuery('.email_frame').contents().find('.jqThankYouPanel').hide();
    }
</script>

