<div id="RecipeGroceryListAdd" class="modal groceryList-modal" data-bind='template: { name: "RecipeAddTemplate" }'></div>
<script id='RecipeAddTemplate' type='text/html'>
<h3>Add Recipe to Grocery List</h3>
<div id="recipeGroceryListEnter">
    <a href="#" data-bind="click: Close" class="close-x">x</a>
    <p class="subheader">Add to Current Grocery List</p>
    <p>Choose List:</p>
    <select data-bind="value: selectedList, options: grocerylists, optionsCaption: 'Create New or Select', optionsText:'ListName', optionsValue:'ListId'" class="listChange" onchange="return GrocerylistChange(this)"></select>
    <p class="subheader">Create a New Grocery List:</p>
    <p>Name of New List:</p> 
    <input type="text" data-bind="value: newListName" id="NewListName" />
    <p data-bind="visible: ShowError()" class="errorMessage">Please make a choice to either Add to Current Grocery List or Create a New Grocery List.</p>
    <div class="buttons clearfix">
        <p class="left"><a href="#" data-bind="click: Close">Cancel</a></p>
        <div class="sd_btn left"><a id="AddGroceryListClick" href="#" data-bind="click: AddRecipe"><span>Add to List</span></a></div>
    </div>
</div>
<div id="recipeGroceryListConfirmation" style="display: none;">
    <p><b>${addedRecipeName}</b> has been added to the <b>${addedListName }</b> grocery list.</p>
    <div class="confirmed clearfix">
        <p><a id="ListConfirmationLink" href="/mypillsbury/mygrocerylist/#${addedListId}">Go To ${addedListName}</a></p>
        <div class="sd_btn left"><a href="#" data-bind="click: Close"><span>Close</span></a></div>        
    </div>
</div>
</script>
<script language="javascript" type="text/javascript">
//<![CDATA[

    var addGoceryListOpen = function (hash) {
        LoadAddView();
        hash.w.show();
        return false;
    };

    jQuery(function () {
        if (isKnown) {
            jQuery('#RecipeGroceryListAdd').jqm(
            {
                modal: true,
                trigger: '.rtGroceryList, .addRecipeGroceryListTrigger',
                onShow: addGoceryListOpen,
                closeClass: '',
                overlayClass: 'modalMask',
                zIndex: 3,
                setTop: 20
            });
        }
        MarinTagging('UTM:I||revie595||||');        
    });

    function LoadAddView() {
        GetGroceryLists(ApplyBindings);        
    }

    var viewModel;
    function ApplyBindings(data) {
        viewModel = {
            selectedList: ko.observable(''),
            newListName: ko.observable(''),
            addedListName: ko.observable(''),
            addedRecipeName: ko.observable(''),
            addedListId: ko.observable(''),
            ShowError: ko.observable(false),
            AddRecipe: function () {
                MarinTagging('UTM:I||addli054||||');
               
                if (jQuery.trim(this.newListName()).length > 0) {
                    AddRecipeToNewGroceryList(this.newListName(), recipeId, ReadAddResponse);
                   

                    dcsMultiTrack('DCS.dcsuri', '/Recipes/GroceryListItemAdd', 'WT.ti', 'Item Added to Grocery List');
                    if (!isMemberGenerated) {
                        dcsMultiTrack('DCS.dcsuri', '/recipes/AddToGroceryList.aspx', 'DCS.dcsqry', 'recipeIds=' + recipeId, 'WT.ti', 'Recipe Added to Grocery List');
                    } else {
                        dcsMultiTrack('DCS.dcsuri', '/userrecipes/AddToGroceryList.aspx', 'DCS.dcsqry', 'recipeIds=' + recipeId, 'WT.ti', 'Member Generated Recipe Added to Grocery List');
                    }

                } else if (jQuery.trim(this.selectedList()).length > 0) {
                    AddRecipeToGroceryList(this.selectedList(), recipeId, ReadAddResponse);
                   
                    dcsMultiTrack('DCS.dcsuri', '/Recipes/GroceryListItemAdd', 'WT.ti', 'Item Added to Grocery List');
                    if (!isMemberGenerated) {
                        dcsMultiTrack('DCS.dcsuri', '/recipes/AddToGroceryList.aspx', 'DCS.dcsqry', 'recipeIds=' + recipeId, 'WT.ti', 'Recipe Added to Grocery List');
                    } else {
                        dcsMultiTrack('DCS.dcsuri', '/userrecipes/AddToGroceryList.aspx', 'DCS.dcsqry', 'recipeIds=' + recipeId, 'WT.ti', 'Member Generated Recipe Added to Grocery List');
                    }
                   
                } else {
                    this.ShowError(true);
                }
            },
            Close: function () {
                jQuery('#RecipeGroceryListAdd').jqmHide();
            },
            ReinitializeForm: function () {
                this.ShowError(false);
                this.selectedList('');
                this.addedListName('');
            },
            grocerylists: ko.observableArray(data.GroceryLists)
        }
        ko.applyBindings(viewModel, document.getElementById('RecipeGroceryListAdd'));

        jQuery('#recipeGroceryListEnter').keydown(function (event) {
            if (event.keyCode == '13') {
                event.preventDefault();
            }
        });

        jQuery('#recipeGroceryListConfirmation').keydown(function (event) {
            if (event.keyCode == '13') {
                event.preventDefault();
            }
        });        
    }

    function ReadAddResponse(data) {
        viewModel.addedRecipeName(data.Recipe.RecipeName);
        viewModel.addedListName(data.List.ListName);
        viewModel.addedListId(data.List.ListId);
        jQuery("#recipeGroceryListEnter").hide();
        jQuery("#recipeGroceryListConfirmation").show();
    }

    jQuery(".emailRecipeTrigger").click(function () { MarinTagging('UTM:I||email358||||'); });
    
//]]>
</script>

</div>

