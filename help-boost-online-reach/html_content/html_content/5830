<script type="text/javascript">
function rotateAds(cm8cat)
{
	try {
    	if (!is_empty(cm8cat)) {
    		CM8Cat = cm8cat;

    		// reset rotation timeout
    		clearTimeout(rotateAds.timeoutId);
    		rotateAds.timeoutId = setInterval(rotateAds, 1000*150,CM8Cat);
    	}

    	if (!is_empty(rotateAds.requestId)) {
    		CM8AjaxAbort(rotateAds.requestId);
    	}

    	rotateAds.requestId = CM8AjaxRefresh(CM8OnLoadFormats);
	}
	catch (e) {}
}
rotateAds.requestId = null;
rotateAds.timeoutId = setInterval(rotateAds, 1000*150,CM8Cat);
</script>
