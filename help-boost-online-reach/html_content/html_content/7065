	function openSubscribePopUp(src){

            if (src.value == 'enter email') {
		alert('Please enter your email address');
            } else {
		window.open("/newsletter/newslettersignup?email=" + src.value,"newslettersignup","status=yes,scrollbars=yes,resizable=yes,width=420,height=250");
            }
	}

