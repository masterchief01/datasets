        if ( file && file.down("a") && file.down("a").down("img") ) {
            new HelpBubble({
        	clickEl : file.down("a"),
        	width: 200,
        	popUp: true,
        	contents : makeHelpBubbleFileText(),
        	additionalActions: focusOnHelpBubbleLogin
            });
        }
        //
    });
});

    </script></div><table class="nextprev"><tr><td><a id="prevstepbutton" class="big-button disabled" href="#" onclick="return false;">&laquo; Previous Step</a></td><td width="100%" class="upsell">
