    jQuery(function() {

        var oHoverConfig = {
            sensitivity: 3, // number = sensitivity threshold (must be 1 or higher)    
            interval: 0, // number = milliseconds for onMouseOver polling interval    
            over: over, // function = onMouseOver callback (REQUIRED)    
            timeout: 100, // number = milliseconds delay before onMouseOut    
            out: out // function = onMouseOut callback (REQUIRED)    
        };

        jQuery('li:has(a[title="Segments"])').append(jQuery('div#aots-subnav-dropdown'));
        jQuery('div#aots-subnav-dropdown').hide();
        jQuery('li:has(a[title="Segments"])').hoverIntent(oHoverConfig);
        
        function over(e) {
            jQuery('div#aots-subnav-dropdown').show();
        }
        function out(e) {
            jQuery('div#aots-subnav-dropdown').hide();
        }

    });
</script> 
