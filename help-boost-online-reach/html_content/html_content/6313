 <script type="text/javascript">
        //<![CDATA[
        G4TV.Dom.ready(function() {

        if (jQuery(".fbCommentCount60468").length != 0) {
                var url = "http://www.g4tv.com/videos/60468/candace-bailey-climbs-a-wall-like-spider-man/";
                jQuery.getJSON("http://graph.facebook.com/?ids=" + url + "&callback=?", function(json) {


                    var comments = Number(json["http://www.g4tv.com/videos/60468/candace-bailey-climbs-a-wall-like-spider-man/"].comments);
                    // console.log('number of comments is ' + comments);
                    if (isNaN(comments)) {
                        comments = 0;
                        var html = ''
