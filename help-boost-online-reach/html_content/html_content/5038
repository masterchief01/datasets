	if(cur > 1) {
		cur--;

		$("#current-page").html(cur);

		$('#kitchendaily-tv .jcarousel-item-' + cur + ' img').each(function(i){

			$(this).attr('src', $(this).attr('data-img'));
		});
	}
});

$(".jcarousel-next").click(function(){

	if(cur < cnt) {
		cur++;

		$("#current-page").html(cur);

		$('#kitchendaily-tv .jcarousel-item-' + cur + ' img').each(function(i){

			$(this).attr('src', $(this).attr('data-img'));
		});
	}
});

