						<script type="text/javascript"><!--
function clearText(thefield){
  if (thefield.defaultValue==thefield.value){
    thefield.value = "";
  }
} 
function checkEmpty(thefield){
  if (thefield.value=="") {
    thefield.value = thefield.defaultValue;
  }
} 
function isValidEmail(str) {
   return (str.indexOf(".") > 2) && (str.indexOf("@") > 0);
}
function isValidPost(post){
  var valid_email = isValidEmail(post.email.value)
  if (!valid_email){
    post.email.style.border = '2px solid #C00';
    if (post.hasError) {
      post.removeChild(post.hasError);
    }
    var error_paragraph = document.createElement('p');
    error_paragraph.style.color = '#C00';
    error_paragraph.name = 'invalid_email';
    error_paragraph.appendChild(document.createTextNode("We're sorry, but this does not appear to be a valid e-mail address. Please re-enter."));
    post.appendChild(error_paragraph);
    post.hasError = error_paragraph;
  }
  return valid_email;
}
--></script>

