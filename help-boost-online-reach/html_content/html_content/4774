	function replaceSmallLike(el) {
		var element = $(el);
		if( ! element.hasClassName('real_btn') ) {
			element.addClassName('real_btn');
   	 	var like_xfbml = new Element('fb:like', { 'class': 'like_xfbml', 'send': 'false', 'rel:page_type': 'Buzz', 'rel:buzz_id': '1515490', 'href': 'http://www.buzzfeed.com/whitneyjefferson/12-ways-to-get-the-best-glamour-shot', 'layout': 'button_count', 'show_faces': 'false', 'width' : '110' });
			element.update('');
			element.appendChild(like_xfbml);
			FB.XFBML.parse(element);			//make sure this is last
		}
  }
</script>

