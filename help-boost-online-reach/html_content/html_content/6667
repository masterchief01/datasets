<script type="text/javascript"><!--//--><![CDATA[//><!--
sfHover = function() {
	if (!document.getElementsByTagName) return false;
	var sfEls = document.getElementById("nav").getElementsByTagName("li");

	for (var i=0; i<sfEls.length; i++) {
		sfEls[i].onmouseover=function() {
			this.className+=" sfhover";
		}
		sfEls[i].onmouseout=function() {
			this.className=this.className.replace(new RegExp(" sfhover\\b"), "");
		}
	}
}
if (window.attachEvent) window.attachEvent("onload", sfHover);
//--><!]]></script>





<script type="text/javascript" src="http://www.totalprosports.com/wp-content/themes/newsports/jquery.carousel.lite.js"></script>


<script type="text/javascript">
jQuery(document).ready(function() {

	if (window.location.hash) {
		var current = window.location.hash;
		current = parseInt(current.substring(1))-1;
	}
	else
	{
		var current = 0;
	}

	
	var first = 0;
   jQuery('#slide_list').cycle({
    fx:     'scrollLeft',
    prev:    'a[id=next]', 
    next:    'a[id=prev]',
    speed:    500,
    startingSlide: 0,
    timeout: 	0,
    after:     onAfter,
    startingSlide: current,
    before:	   onBefore
   });
   
   function onAfter(curr,next,opts) {
		var caption = '' + (opts.currSlide + 1) + ' of ' + opts.slideCount;
		var title = jQuery('#slidetitle_' + (opts.currSlide + 1)).attr('alt');
		jQuery('div[id=slidecaption]').html(caption);
		jQuery('#slidetitle').html(title);
		
		//get the height of the current slide
		var $ht = jQuery(this).height();
		//set the container's height to that of the current slide
		jQuery(this).parent().css("height", $ht);
		
	}
	
	function onBefore(curr,next,opts) {
		
		if (first == 0)
		{
			first = 1;		
		}
		else
		{
			//alert('CUR: ' + opts.currSlide + ' --- NEXT: ' + opts.nextSlide + ' --- TOTAL: ' + opts.slideCount);
			if (opts.nextSlide == 0 && opts.currSlide+1 == opts.slideCount)
			{
				window.location = jQuery('#prev').attr('href');
				return false;
			}
			
			if (opts.currSlide == 0 && opts.nextSlide == opts.slideCount-1)
			{
				window.location = jQuery('#next').attr('href');
				return false;
			}
			
			var slide = parseInt(opts.nextSlide);
			window.location = '#' + (slide + 1);
			
			reloadBanners(window.location + '/page/' + opts.currSlide+1);
		}
		
		
		
		
	}
});
</script>
