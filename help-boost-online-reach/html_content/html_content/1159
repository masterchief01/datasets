  document.write('<span id="title">1 comments so far...</span>');
else
  document.write('<span id="title">Displaying only the last 100 comments. You can read all 1 comments <a href="http://www.boston.com/sports/blogs/bigshots/2010/06/comments_big_shots_for_june_4_2010.html#comments">over here</a>.</span>');
};
displayCommNum();
</script>

