function validateComments()
	{
		var name = $('#user_name');
		var email = $('#user_mail');
		var comment = $('#comment');
		var captcha = $('#captcha_response');
			
		if ( name.val() == 'Name' || name.val() == '')
		{
			alert('Please write your name.');
			name.css('color','red');
			return  false;
		}	
		if ( email.val() == 'Email (will not be published)' || email.val() == '')
		{	
			alert('Please write your e-mail.');
			email.css('color','red');
			return  false;
		}	
		
		if ( comment.val() == '')
		{
			alert('Please write a comment.');	
			return  false;
		}	
		validateCaptcha(captcha.val());
		
		
		return false;
	}
</script>


