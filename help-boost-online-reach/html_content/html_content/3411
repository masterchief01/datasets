<script>
$(document).ready( function() {
		
	$('#saveForgot').click(function(){
		
		var datastring = 'email='+$('#forgot-email').val();
			
			$.ajax({
					type: 'POST',
					url: '/member/forgot',
					dataType: "json",
					data: datastring,
					success: function(response) {
						if (response.status) {
							
							// update ok - change state
							$('#forgot-thanks').show();
							$('#forgot-form').hide();
							
						} else {
							alert(response.message);
						}
						
					}
				});
			
	});
	
});

</script></div>

