<script>

	$(document).ready( function(){
		
		$('#newsletter-btn').click( function() {
			
			var email = $('#PageNl-email').val();
			var firstname = $('#PageNl-firstname').val();
			var lastname = $('#PageNl-lastname').val();
			var country = $('#countryselect').val();
			
			var newsletter = 0;
			var marketing = 0;
			
			if ( $('#PageT3-marketing').attr('checked') ) {
				var marketing = 1;
			};
			if ( $('#PageT3-newsletter').attr('checked') ) {
				var newsletter = 1;
			};
			

			
			var url = "/member/newsletter";
			var data = 'email='+email+'&firstname='+firstname+'&lastname='+lastname+'&country='+country+'&newsletter='+newsletter+'&marketing='+marketing;
			
			// do json request to ajax login sys.
			$.ajax({
				type: 'post',
				url: url,
				data: data,
				dataType: 'json',
				success: function(response) {
					
					if (response.status == true) {
						$('#loginform').hide();
						$('.regBtn').hide();
						$('#thanksNL').show();
					} else {
					
						alert('Sorry, there seems to have been an error. Please try again.');
						
					}
					
					
				}
			});
			
			return false;
			
		});		

	});

</script><section class="overlayBox login box" id="loginForm">
	
	<a href="" class="closeOverlay"><span class="fIR"></span>Close</a>

