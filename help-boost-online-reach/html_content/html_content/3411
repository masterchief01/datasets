<script>
$(document).ready( function() {

	//var activeFlash = 0;
	$('#savePrefs').click(function(){
		
		var newsletter = 0;
		var password = '';
		var network = 0;
		var error = false;
		
		// set up data
		if ( $('#pref-email').attr('checked') ) {
			newsletter = 1;
		}
		if ( $('#pref-network').attr('checked') ) {
			network = 1;
		}
		
		var datastring = 'newsletter='+newsletter+'&network='+network;
		
		if ( $('#pref-pass').val() != '' ) {	
			if ( $('#pref-pass').val() == $('#pref-pass2').val() ){
				password = $('#pref-pass').val();
				var datastring = datastring + '&password='+password;
			} else {
				error = true;
				alert('These passwords to not match!');
			}	
		}
	
		
		// send ajax request
		if (error != true) {
			
			$.ajax({
					type: 'POST',
					url: '/member/saveUserPrefs',
					dataType: "json",
					data: datastring,
					success: function(response) {
						if (response.status) {
							
							// update ok - change state
							$('#pref-thanks').show();
							$('#pref-form').hide();
							
						} else {
							alert(response.message);
						}
						
					}
				});
			
		}

		// return message
		
	});
	
});

</script></div>

