			<script type="text/javascript">
				var storePromo = false;
				var promoOffsetTop = $('#container').offset().top + $('#store-promo').offset().top - 15;
				var promoOffsetLeft = $('#store-promo').offset().left;
				
				$(document).ready(function() {
					
					$(window).scroll(function () {
						if (storePromo == false && $(window).scrollTop() >= promoOffsetTop) {
					        $('#store-promo').css('position', 'fixed');
					        $('#store-promo').css('left', promoOffsetLeft + 'px');
					        $('#store-promo').css('top', '0px');
							setTimeout("storePromo = true", 500);
						}
						
						if (storePromo == true && $(window).scrollTop() <= promoOffsetTop) {
					        $('#store-promo').css('position', 'relative');
					        $('#store-promo').css('left', '0px');
							setTimeout("storePromo = false", 500);
						}
					});
				});
			</script>
			
