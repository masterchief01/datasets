<script>

       $('.slb-rrw-mps-menu-link-ms').click(function() {
             $(".slb-rrw-shared").css("display","block");
             $(".slb-rrw-read").css("display","none");
	         $("#tabchange").removeClass("slb-rrw-mps-mode-read");
             $("#tabchange").addClass("slb-rrw-mps-mode-shared");
       });
       $('.slb-rrw-mps-menu-link-mr').click(function() {
             $(".slb-rrw-shared").css("display","none");
             $(".slb-rrw-read").css("display","block");
			 $("#tabchange").removeClass("slb-rrw-mps-mode-shared");
             $("#tabchange").addClass("slb-rrw-mps-mode-read");
       });
</script>
