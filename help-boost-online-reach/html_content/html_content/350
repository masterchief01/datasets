  <script type="text/javascript">      

	
	$('#li-cartoon').bind('click', function() {
  
		$('#tm_img').css("display","block");
		$('#tm_video').css("display","none");
		$('#tm_caption').css("display","block");
        var currBlockId = $('#tm_caption').attr("value");
        //Today's Cartoon
        $('.sl-rww-tm-head').css("background-position","-1px -343px");
		$('#tm_caption').html("Cartoonists&rsquo; Take on Sports");
        $('#tm_image').attr('src', '/content/dam/slate/Homepage/todaysmedia/2012/02/tc.gif');
		$('#img_link').attr('href', 'http://cartoonbox.slate.com/hottopic/?image=0&amp;topicid=61');
		
        $("#"+currBlockId).css("display","block");
		$('#tm_caption').attr("value","li-cartoon");
        $(this).css("display","none");
		
        
    });     
	$('#li-photo').bind('click', function() {	
		$('#tm_img').css("display","block");
		$('#tm_video').css("display","none");
		$('#tm_caption').css("display","block");
        var currBlockId = $('#tm_caption').attr("value");
        //Today's Photos
        $('.sl-rww-tm-head').css("background-position","-1px -408px");
        $('#tm_caption').html("Great Stadiums");
        $('#tm_image').attr('src', '/content/dam/slate/Homepage/todaysmedia/2012/02/tp.gif');
		$('#img_link').attr('href', 'http://todayspictures.slate.com/20120203');
        $("#"+currBlockId).css("display","block");
		$('#tm_caption').attr("value","li-photo");
        $(this).css("display","none");
        
    }); 
	
	
	$('#li-doonesbury').bind('click', function() {		
		$('#tm_img').css("display","block");
		$('#tm_video').css("display","none");
		$('#tm_caption').css("display","block");
        var currBlockId = $('#tm_caption').attr("value");
        //Today's Doonesbury
        $('.sl-rww-tm-head').css("background-position","-1px -278px");
        $('#tm_caption').html("An Age-Old Problem");
        $('#tm_image').attr('src', '/content/dam/slate/Homepage/special features/weekend-TodaysDoonesbury.jpg');
		$('#img_link').attr('href', 'http://www.doonesbury.com/');
        $("#"+currBlockId).css("display","block");
		$('#tm_caption').attr("value","li-doonesbury");
        $(this).css("display","none");
        
    }); 

	$('#li-video').bind('click', function() {		
		$('#tm_img').css("display","none");
		$('#tm_video').css("display","block");
		$('#tm_caption').css("display","none");
        var currBlockId = $('#tm_caption').attr("value");
        //Today's Videos
        $('.sl-rww-tm-head').css("background-position","-1px -473px");
        $("#"+currBlockId).css("display","block");
		$('#tm_caption').attr("value","li-video");
        $(this).css("display","none");
        
    });
    var scrID = Math.floor(Math.random()*5)
	if(scrID ==1) {
		$('#li-cartoon').trigger('click');
	} else if(scrID ==2){
		$('#li-photo').trigger('click');
	} else if(scrID ==3){
		$('#li-doonesbury').trigger('click');
	}else if((scrID ==4) ||(scrID==0) || (scrID==5)){
		$('#li-video').trigger('click');
	}
	
    </script>
	<div id="todays_media_player" style="clear:both"></div>
	</div>
