<script language="JavaScript">
adjustTileSize();

function adjustTileSize() {
mWidth = $(window).width()/7;   // Set mHeight to the larger of the two heights
var gridTile = $('.gridtile');
var single_gridTile = $('.single_gridtile');
var navTile = $('.gridnav_item');
var navTile = $('.gridnav_item');
var nameTileSub = $('#nameTastyPhotos');
var nameTileName = $('#nameLaraFerroni');
var gridThumb = $('.gridthumb');
var searchtile = $('#searchform');
var searchInput = $('#home_search_input')
var sizeNavFont = mWidth/13;
var sizeNameFont = mWidth/6;
var sizeSubFont = mWidth/8;
gridTile.css({ height: mWidth, width: mWidth-10 });
single_gridTile.css({ height: mWidth, width: mWidth-10 });
gridThumb.css({ height: mWidth, width: mWidth-10 });
searchInput.css({width: sizeNavFont*8 });
navTile.css('font-size',sizeNavFont);
nameTileName.css('font-size',sizeNameFont);
nameTileSub.css('font-size',sizeSubFont);
searchtile.css('font-size',sizeNavFont);
}


$(window).resize(function() {
	adjustTileSize();  // Set the heights once more
});



function toggleVisibility(me){
	if (me.style.visibility=="hidden"){
		me.style.visibility="visible";
		}
	else {
		me.style.visibility="hidden";
		}
}
</script>


