  var val;
  
  if (r29cookie.indexOf("r29comstore=") != -1)
   {
      i = r29cookie.indexOf("r29comstore=");
      s = r29cookie.substring(i + 12);
      j = s.indexOf(";");
      if (j == -1)
      {
          val = s;
      }
      else {
          val = s.substring(0, j);
      }
  } else {
