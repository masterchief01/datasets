	<script type="text/javascript">
    $(document).ready(function(){
		$('.search-type label').click(function(){
            if ($(this).find('input').length) {
                $('.search-type label').each(function(){
					$(this).removeClass('checked');
				});
                $(this).addClass('checked');
			};			
        });
		
        $("form#cse-search-box").submit(function() {
            if ($(".search-type label.superSearch").hasClass('checked')) {
                var q = $('form#cse-search-box input[name=q]').val();              
                window.location.href = "/recipe-search/#/" + q;
                return false;
            }            
		});
        
    });
    </script>

