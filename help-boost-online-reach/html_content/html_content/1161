<script type="text/javascript" src="/assets/js/jquery/jquery-ui.js"></script>
<script type="text/javascript">
	$(document).ready(function() {
		var x = readCookie('hideconnectbar')
		var mobile = (/iphone|ipod|android|blackberry|mini|windows\sce|palm/i.test(navigator.userAgent.toLowerCase()));  

		//Hide the Connect bar on mobile
    	if (!mobile) {
			if (x) {
                $("#connect-bar-container-mini").show();
			} else {
                $("#connect-bar-container-full").show();    
			}
		}

		//Create a cookie that hides the main bar, hide the full bar, and show the mini bar
		$("#connect-bar-close").click(function () {
			$("#connect-bar-container-full").hide("slide", { direction: "left" }, 1000, function() {
				//Drop a cookie
				createCookie('hideconnectbar','y')

				$("#connect-bar-container-mini").show("slide", { direction: "left" }, 500);				
			});			
		});

		//Destroy the hide cookie, hide the mini bar, and show the full bar
		$("#connect-bar-container-mini").click(function() {
			$("#connect-bar-container-mini").hide("slide", { direction: "left" }, 500, function () {
				//Destroy the cookie
				eraseCookie('hideconnectbar');

				$("#connect-bar-container-full").show("slide", { direction: "left" }, 1000);
			});
		})
	});
</script>
