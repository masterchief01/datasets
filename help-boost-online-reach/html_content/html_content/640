          var check_and_submit = function() {
            if ($('#login').val() === "" && $('#password').val() === "") {
              $('#inline_login').show();
              $('#inline_login_form, #forgot_password').hide();
            } else if ($('#login').val() === "") {
              $('#login').focus();
            } else if ($('#password').val() === "") {
              $('#password').focus();
            } else {
              $('#inline_login_form form').submit();
            }
          }
