<script>
$(document).ready(function() {
  mixpanel.track_links('#share-using-social-media', 'RC FTUX - Share using social media');
  
	$('#favorite_37104').hover(
		function() {
			$('#star_tooltip_37104').show();
		},
		function() {
			$('#star_tooltip_37104').hide();
		}
	);

	$('#favorite_37104').on('click', function() {
		$.ajax({
			type: "post",
			url: "/rewards/37104-Reward-for-the-recipe-for-key-lime-pie-from-bricktops-restaurant/favorite",
			beforeSend: function() {
				// Set new state before POST to make it feel faster
				if ($('.favorite_text').text() == 'watch this reward') {
					$('.favorite_text').text('stop watching this reward');
				} else {
					$('.favorite_text').text('watch this reward');
				}
			},
			success: function(response) {
				// Double check that we have the right state showing
				if (response === "false") {
					$('.favorite_text').text('watch this reward');
				} else if (response === "true") {
					$('.favorite_text').text('stop watching this reward');
				}
			}
		});
		return false;
	});
});
</script>



