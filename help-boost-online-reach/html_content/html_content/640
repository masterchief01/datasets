<script>
$(document).ready(function() {
  $('#copy_link_text').show();
  
  var top = $('#sharing_column_content').offset().top - parseFloat($('#sharing_column_content').css('marginTop').replace(/auto/, 0)) - 15;
  $(window).scroll(function (event) {
    // what the y position of the scroll is
    var y = $(this).scrollTop();

    // whether that's below the form
    if (y >= top) {
      // if so, ad the fixed class
      $('#sharing_column_content').addClass('fixed');
    } else {
      // otherwise remove it
      $('#sharing_column_content').removeClass('fixed');
    }
  });
});
</script>

