    if (getURLParam('notify') === "login") {
      var flash_notify = document.createElement('span');
      flash_notify.className = "flash-notify";
      $(flash_notify).html("<span class='green-check-sprite sprite'></span>You just earned <span class='points'>3 points</span> simply by logging in today!");
      $(".notify_center").append(flash_notify);

      if (history && history.replaceState) {
        window.history.replaceState(null, document.title, location.pathname);
      }
    }
  }, 1000);
  
  setTimeout(function() {
    $('.flash-notify').animate( { top: "-35" }, 'slow', function() {
      $(".flash-notify").remove();
    } );
  }, 7500);
});
</script>

<script type="text/javascript">
$(document).ready(function() {
  function createNotification( m ) {
    var flash_notify = document.createElement('span');
    flash_notify.className = "flash-notify";
    $(flash_notify).html(m);
    $(".notify-center").append(flash_notify);

    $('.flash-notify').animate( { top: "0" }, 'slow' );
    setTimeout(function() {
      $('.flash-notify').animate( { top: "-35" }, 'slow', function() {
        $(".flash-notify").remove();
      } );
    }, 7500);
  }

  $(function() {
    var pusher = new Pusher('f7300819469acb7215f0');
    var notify_channel = pusher.subscribe('');
    
    notify_channel.bind('reward_created', function( data ) {
      createNotification(data.html_content);
    });
    notify_channel.bind('won_reward', function( data ) {
      createNotification(data.html_content);
    });
    notify_channel.bind('submission_recieved_reply', function( data ) {
      createNotification(data.html_content);
    });
    notify_channel.bind('friend_joined', function( data ) {
      createNotification(data.html_content);
    });
    notify_channel.bind('reputation_level_upgrade', function( data ) {
          createNotification(data.html_content);
    });
  });
  
});
</script>

