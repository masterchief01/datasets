<script type="text/javascript">
//<![CDATA[
    var timeout = null;
    function checkHover(obj, check)
    {
        if (!check.hasClass('hover')) {
            obj.hide();
        }
    }
    
	$j(document).ready(function() {
		$j('.recipe-social #clear-rating').live('click', function() { clearMyRating(158140); });
		$j('.recipe-sections .supplement').hover(function() {
		    $j(this).addClass('hover');
		}, function() {
		    $j(this).removeClass('hover');
		    var id = '#' + $j(this).attr('id').replace('cnt', 'btn');
		    var it = $j(this);
		    clearTimeout(timeout);
		    timeout = setTimeout(function(){checkHover(it, $j(id))}, 150);
	    });
		$j('.recipe-sections .section').not(':first').each(function(i) {
		    var p = $j(this);
		    var c = p.attr('id').replace('btn', 'cnt');
		    p.attr('rel', c);
		    var l = (i * 141) + 143 + 92;
	        var s = $j('.recipe-sections .supplement.' + c);
	        var h = s.outerHeight()/2;
	        s.css({'top': 0 - h + 23, 'left': l});
	        s.children('.arrow').css('top', h - 30);
	        
	        $j(this).hover(function() {
	            $j(this).addClass('hover');
	            $j('.recipe-sections .supplement').not('.' + $j(this).attr('rel')).hide();
	            $j('.recipe-sections .supplement.' + $j(this).attr('rel')).show();
	        }, function() {
	            $j(this).removeClass('hover');
	            var h = $j('.recipe-sections .supplement.' + $j(this).attr('rel'));
	            clearTimeout(timeout);
	            timeout = setTimeout(function(){checkHover(h, h)}, 150);
	        });
	    });
		$j(".share-bar-small a").live ("click", function() {
		    setGoogleEvent("recipe", "share", self.location.href);
		    setGoogleEvent("share-" + $j(this).text(), "recipe", self.location.href);
		    setGoogleEvent("recipe-share", $j(this).text(), self.location.href);
		});
		$j("#shareForm .modalRightCol a").live("click", function() {
		    setGoogleEvent("share-" + $j(this).text().toLowerCase(), "recipe", self.location.href);
		    setGoogleEvent("recipe-share", $j(this).text().toLowerCase(), self.location.href);
		});
		$j("#shareForm .send-email").live("click", function() {
		    setGoogleEvent("share-email", "recipe", self.location.href);
		    setGoogleEvent("recipe-share", "email", self.location.href);
		});
	    $j("#recipe-actions .save-button").live("click", function() {  if (typeof recordIt != "undefined") recordIt('Recipe Save','1',''); });
	    
	    
	    $j('#nutrition-list > li').not(':has(h6)').filter(':odd').addClass('colorRow');
	    $j('#nutrition-list > li:last').addClass('lst');
	});
	
	function printRecipe()
	{
	    window.open('http://www.tablespoon.com/recipes/panda-bread-recipe/1/print/','printPreview','toolbar=yes,location=yes,directories=yes,status=yes,menubar=yes,scrollbars=yes,resizable=yes');
	    if (typeof recordIt != "undefined") recordIt('Recipe Print 1','1','');
	    setGoogleEvent("print", "recipe", self.location.href);
	    setGoogleEvent("recipe", "print", self.location.href);
	    setGooglePageView('/recipes/print/');
	}
