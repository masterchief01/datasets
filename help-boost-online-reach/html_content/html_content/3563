        linear-gradient(bottom, #f16900 0%, #f06800 100%);

    background-image: 
        -webkit-gradient(linear, left bottom, left top, color-stop(0%,#f16900), color-stop( 100%, #f06800));

    }

.aol-most-popular-social span.pane-nav:after {
    border-right: #adadad solid 20px;
}

.aol-most-popular-social .nav .back span, .aol-most-popular-social .nav .forward span {
    border-bottom: #f06800 solid 6px;
    border-left: #f06800 solid 6px;
    border-right: #f06800 solid 6px;
    border-top: #f06800 solid 6px;
}

.aol-most-popular-social .nav .back span {
    border-left-width: 0;
    border-right-color: #fff;
}

.aol-most-popular-social .nav .forward span {
    border-left-color: #fff;
    border-right-width: 0;
}

.aol-most-popular-social .nav .back:hover span {
    border-right-color: #ffcca5;
}

.aol-most-popular-social .nav .forward:hover span {
    border-left-color: #ffcca5;
}



