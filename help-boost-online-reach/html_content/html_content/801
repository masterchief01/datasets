//<![CDATA[
jQuery(document).ready(function(){
	// detect FF2								
	jQuery.each(jQuery.browser, function(i, val) {
		if(i=="mozilla" && jQuery.browser.version.substr(0,3)=="1.8"){
			var ff2Styles = 'global/resources/styles/ff2.css'; 
			$('<link rel="stylesheet" type="text/css" href="'+ff2Styles+'" >').appendTo("head");
		}
	});								
