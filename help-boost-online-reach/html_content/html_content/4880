{
	var articleurlpath=urlpath+'?print=true';
	window.open(articleurlpath, 'win_article_print','toolbar=no,directories=no,resize=yes,menubar=no,location=no,scrollbars=yes,width=600,height=500,maximize=null,top=70,left=80'); 
}

