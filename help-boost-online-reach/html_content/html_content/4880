function movePrev()
{
	sldcounter-- ;
	if(sldcounter >= 0 )
	{
		for(i=0; i<maxslide; i++ )
		{
			document.getElementById('slide_'+i).style.display ='none' ;
			document.getElementById('slideTitle_'+i).style.display ='none' ;
		}
		document.getElementById('slideTitle_'+sldcounter).style.display ='block' ;
		document.getElementById('div_dispalyslide').innerHTML = document.getElementById('slide_'+sldcounter).innerHTML;	
		document.getElementById('sstemp_div').innerHTML = "";
		document.getElementById('sldthumb2').style.width = '138px';
