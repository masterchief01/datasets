    visible: 1,
    scroll: 1,
    vertical: false,
    wrap: 'both',
    auto: 9,
    animation: 'fast',
    start: 1,
    buttonNextHTML: '<div></div>',
    buttonPrevHTML: '<div></div>',
    itemFirstInCallback: trigger_js_scroll_2,
