        FB.Event.subscribe('edge.create', function (href, widget) {
            /* Trigger custom event on the FacebookWrapper object */
            $(AR.FacebookWrapper).trigger("FacebookLikeButton_onClick", href);
        });
        FB.Event.subscribe('edge.remove', function (href, widget) {
            /* Trigger custom event on the FacebookWrapper object */
            $(AR.FacebookWrapper).trigger("FacebookUnLikeButton_onClick", href);
        });
    };
    $(function () {
        if (document.getElementById('facebook-jssdk')) { return; }
