            jQuery("#AMS_VYF_GLOBAL_ZRAIL").html(pageAds.AMS_VYF_GLOBAL_ZRAIL.replace(/document.write\(.*\)/gi,"/*filtered out because of document.write(...)*/"));
        }
    }
    if ('AMS_VYF_GLOBAL_ZRAIL' === 'AMS_VYF_MAGAZINE_SUBNAV') {
                if (jQuery('#AMS_VYF_GLOBAL_ZRAIL').length > 0) {
                    jQuery('#mag_subnav_subscribe').parent().html(pageAds.AMS_VYF_GLOBAL_ZRAIL);
                    jQuery('#AMS_VYF_GLOBAL_ZRAIL').hide();
                }
    }
})(window.pageAds);
(function(pageAds){
    var
        div = null;

    if ((typeof pageAds !== 'undefined') && (typeof pageAds.AMS_VYF_SITE_FOOTER_SUBSCRIBE !== 'undefined')) {
        div = document.getElementById("AMS_VYF_SITE_FOOTER_SUBSCRIBE");

        if (div !== null) {
            div.innerHTML = "";

            jQuery("#AMS_VYF_SITE_FOOTER_SUBSCRIBE").html(pageAds.AMS_VYF_SITE_FOOTER_SUBSCRIBE.replace(/document.write\(.*\)/gi,"/*filtered out because of document.write(...)*/"));
        }
    }
    if ('AMS_VYF_SITE_FOOTER_SUBSCRIBE' === '') {
                if (jQuery('#AMS_VYF_SITE_FOOTER_SUBSCRIBE').length > 0) {
                    jQuery('#mag_subnav_subscribe').parent().html(pageAds.AMS_VYF_SITE_FOOTER_SUBSCRIBE);
                    jQuery('#AMS_VYF_SITE_FOOTER_SUBSCRIBE').hide();
                }
    }
})(window.pageAds);
                }
                CN.ecom.show();
            });
            setTimeout(CN.ecom.show, 10 * 1000);
