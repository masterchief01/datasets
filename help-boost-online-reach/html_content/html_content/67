<script type="text/javascript">
    CN.labjsEventTracker.wait(function(){
        var
            i   = 0,
            ns  = CN,
            len = 0,
            absoluteUrl    = false,
            trackingUrl    = "/services/lookup/2000000001071242",
            allowedDomains = [],
            domain         = location.protocol + "//" + location.host;

        if (CN.site && CN.site.allowedDomains) {
            allowedDomains = CN.site.allowedDomains.split(",");
            len            = allowedDomains.length
        }

        for(; i < len; i++) {
            if (domain.indexOf(allowedDomains[i]) >= 0) {
                absoluteUrl = true;
            }
        }

        if (absoluteUrl) {

            trackingUrl = domain + trackingUrl;
            ns.site     = ns.site || {};

            ns.site.eventTrackingList   = new EventList("eventListHolder");
            ns.site.eventTrackingList.setBaseUrl("http://event.vanityfair.com/");
            ns.site.eventTrackingEvent  = new EventObject("VYF", "most_viewed");
            ns.site.eventTrackingEvent.setEnvironment("PROD");
            ns.site.eventTrackingEvent.setSection(CN.utils.getSection());

            if (CN.site.pageId && CN.site.pageId !== "null") {
                ns.site.eventTrackingEvent.setContentId(CN.site.pageId);
            }

            ns.site.eventTrackingEvent.setFullUrl(trackingUrl);
            ns.site.eventTrackingEvent.setContentTitle(CN.utils.trim("&lt;i&gt;WWD&lt;/i&gt;&amp;#8217;s Fabulous 100th Feted With Bellinis and Beauties"));

            
                ns.site.eventTrackingEvent.setProperty("pd","2010/11/03 00:00:00");
            

            ns.site.eventTrackingList.addEvent(ns.site.eventTrackingEvent);
            ns.site.eventTrackingList.writeImageTags();
        }
    });
</script>
