<script type="text/javascript">
function openBurstVideoPage()
{
  var w = window.open('','popUpWindow','height=600,width=1025,left=10,top=10,resizable=es,scrollbars=yes,toolbar=yes,menubar=no,location=no,directories=no,status=yes');
  w.document.write("<html><head><title>Watch TV</title></head>");
  w.document.write("<script type='text/javascript' src='http://cplayer.blinkx.com/?i=eecd47032949acf1fe7454770f19e30a486c4210&adc_pub_id=ad23215b'></sc"+"ript>");
  w.document.write("</head></html>");
  w.document.close();
}
</script>
