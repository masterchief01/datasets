	var template_featured_1 = _.template('<!-- Start of Funny or Die Funny or Die Featured Videos Widget -->\n<noscript><div><a href=\"http://www.funnyordie.com/?utm_campaign=featured&amp;utm_medium=widget&amp;utm_source=featured\">Funny or Die Featured Videos<\/a> from <a href=\"http://www.funnyordie.com/?utm_campaign=featured&amp;utm_medium=widget&amp;utm_source=featured\">Funny or Die<\/a><\/div><\/noscript>\n<iframe id=\"odn_featured_1\" frameborder=\"0\" marginheight=\"0\" marginwidth=\"0\" src=\"http://www.funnyordie.com/widget/featured/1#{querystring}\" width=\"#{width}\" height=\"#{height}\"><\/iframe>\n<div><a href=\"http://www.funnyordie.com/?utm_campaign=featured&amp;utm_medium=widget&amp;utm_source=featured\">Funny or Die Featured Videos<\/a> from <a href=\"http://www.funnyordie.com/?utm_campaign=featured&amp;utm_medium=widget&amp;utm_source=featured\">Funny or Die<\/a><\/div>\n<!-- End of Funny or Die Funny or Die Featured Videos Widget -->');

  function querystring_featured_1() {
    var querystring = '';
    var pairs = {};
    var defaults = {theme:'dark',layout:'horizontal',height:'300',width:'300',font_size:'17',header:'22'};
    _(defaults).each(function(value,key){
      var field_id = "#" + key+'_featured_1';
      if(value != $(field_id).val()) {
        pairs[key] = $(field_id).val();
      }
    });
    if(_(pairs).size() > 0) {
      querystring = '?' + $.param(pairs);
    }
    var title_field_id = '#title_featured_1';
    if($(title_field_id).val() != '' && $(title_field_id).val() != 'Funny or Die Featured Videos') {
      querystring += '#'+escape($(title_field_id).val());
    }
    return querystring;
  }

	function update_embed_widget_code_featured_1() {
	  $('#embed_widget_featured_1').val(template_featured_1({
	    width:$('#width_featured_1').val(), height:$('#height_featured_1').val(), querystring:querystring_featured_1()
	  }));
	  $('#embed_widget_featured_1_dialog_box').css({width:(parseInt($('#width_featured_1').value)+240+'px')});
		$('#embed_widget_preview_featured_1').html($('#embed_widget_featured_1').val());
	}

	function get_embed_code_featured_1() {
	  GA.event('sharing','embed_widget_featured_copy','#{widget_object.id}')
	  return $('#embed_widget_featured_1').val();
	}

  $(function($) {
