  <script type="text/javascript">
//<![CDATA[

    var currentUser = null;
    var user_key = 'global';

    User.setUser();
    if (User.isLoggedIn()) {
      Subscriptions.check();
      display_not_confirmed_message();
		}
    jQuery(function($) {
      Flash.init();
      Funbar.init();
      if ((!User.isLoggedIn()) && (Cookie.get('auth_token'))) $.post('/sessions/check_cookie');
      
  var slider_images_loaded = 0;
  $('#slider > li > img').load(function() {
    slider_images_loaded += 1;
    if(slider_images_loaded == $('#slider > li > img').length) {
      startPanel = (window.location.hash) ? parseInt(window.location.hash.replace('#slide','')) : 1;
      if(startPanel > $('#slider > li > img').length) startPanel = 1;
      $('#slider').anythingSlider({
        width: 600,
        delay: 6000,
        startPanel: startPanel,
        hashTags: false,
        resizeContents: false,
        startStopped: true,
        buildNavigation: false,
        buildStartStop: false,
    
        onSlideBegin: function(slider) {
          $('#slides .active').removeClass('active').hide();
        },
        onSlideComplete: function(slider) {
          $('#slideshow h4 span').text($('#title_'+slider.currentPage).text());
          $('#caption_'+slider.currentPage).addClass('active').fadeIn();
          $('#slide_count').text(slider.currentPage);
          window.location.hash = '#slide'+slider.currentPage;
          if(!slider.playing) {
            $('#ss_play').show();
            $('#ss_pause').hide();
          }
        }
      });
    
      $('#slide_counter').prependTo('.anythingControls');
      $('#slide_count').text(startPanel);
  
      $('#view_all').click(function() {
        $('#slideshow').hide();
        $('#slides hr, #slides img, #slides .title:not(:empty), #slides .caption:not(:empty)').show();
        return false;
      }).prependTo('.anythingControls');
    
      $('#slideshow h4').show();
  	  $('#slides img, #slides hr, #slides .title').hide();
  	  $('#slides .caption').hide().eq(startPanel-1).addClass('active').show();
  	  $('#slideshow h4 span').text($('#title_'+startPanel).text());
    }
  }).each(function(){
  	// this hack found in the comments of http://api.jquery.com/load-event/
  	// it ensures that the load event fires even when the images are loaded from cache (FF and IE don't trigger load() in those cases)
  	if(this.complete || (jQuery.browser.msie && parseInt(jQuery.browser.version) == 6)) 
  	$(this).trigger("load");
  });
  
  $('#ss_previous').click(function() { $('#slider').data('AnythingSlider').goBack(); });
  $('#ss_next').click(function() { $('#slider').data('AnythingSlider').goForward(); });
  $('#ss_play').click(function() {
    $('#slider').data('AnythingSlider').goForward();
    $('#slider').data('AnythingSlider').startStop(true);
    $(this).hide();
    $('#ss_pause').show();
  });
  $('#ss_pause').click(function() {
    $('#slider').data('AnythingSlider').startStop(false);
    $(this).hide();
    $('#ss_play').show();
  });
    

  $('#favorite_tab, #report_tab').click(function() {
    if ($('#add_to_playlist').length + $('#report_medium').length < 2) {
      $.get('/media/ajax_tabs/7aaa18ed5f');
    }
    return false;
  });
  $('.expander_head').live('click', function() {
    $(this).parents('.expandable').toggleClass('collapsed');
    if ($.data(document.body, $(this).attr('data-remote')) != true)
      $.get($(this).attr('data-remote'));
    $.data(document.body, $(this).attr('data-remote'), true);
    return false;
  });
  var interstitial = false;
  
  if(User.isLoggedIn() && currentUser.newsletter) {
    store_action('opt-in');
  }
  if(!Cookie.get("_nwsl")) {
    var pv = Cookie.get("_pv", function(val) { return parseFloat(val); }) || 0
    pv += 1;
    Cookie.set("_pv", pv, {path: '/'});
    if(pv >= 3) {
      interstitial = true;
      var noautostart = true;
    }
  }
  
  function validate_newsletter_signup() {
    if($('#newsfeed_dialog_email').length) {
      return Validation.validate('newsfeed_dialog_email');
    } else {
      return true;
    }
  }
  
  function temp_dismiss_dialog() {
    Cookie.set("_pv", 0, {path: '/'})
    dismiss_dialog('opt-in', '#newsletter_dialog');
  }

  if(interstitial)
    show_dialog('#newsletter_dialog');
    
  $('#newsletter_dialog_form').submit(function() {
    $('#newsletter_subscription_entry_dialog').slideUp();
    $('#newsletter_subscription_waiting_dialog').show();
    GA.event('subscribing','newsletter','interstitial/media/show/7aaa18ed5f');
  });

    });
  
    

