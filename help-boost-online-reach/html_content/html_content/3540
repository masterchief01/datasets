<script type="text/javascript">
    
	function RateReviewClick() {
		if (RatingRequired() == true) {
			var customLinkName = document.getElementById("ctl00_ctl00_MainContent_MainContent_RecipeUtility1_rateReview_hidSetFocus").value

			var commentsSubmitted = false;
			var comments = document.getElementById("ctl00_ctl00_MainContent_MainContent_RecipeUtility1_rateReview_txtComment");
			if (comments !=null && comments.value.length > 0) {
				commentsSubmitted = true;
			}

			var ratingSubmitted = false;
			if (document.getElementById("ctl00_ctl00_MainContent_MainContent_RecipeUtility1_rateReview_hidRating").value > 0 &&
			 document.getElementById("ctl00_ctl00_MainContent_MainContent_RecipeUtility1_rateReview_hidPreviouslyRated").value == "false") {
				ratingSubmitted = true;
			}

			RateReviewRecipeTrackLink("Rate Recipe", customLinkName, commentsSubmitted, ratingSubmitted);
		}
		else {
			return false;
		}
	}

	function RatingRequired() {
//		if (document.getElementById('ctl00_ctl00_MainContent_MainContent_RecipeUtility1_rateReview_hidRating').value == "0") {
//			document.getElementById('ctl00_ctl00_MainContent_MainContent_RecipeUtility1_rateReview_lblErrorMsg').innerHTML = "Recipe must be rated";
//			return false;
//		}
//		else {
			return true;
//		}
	}
	function SetFocusRating() {
		var SetTop = document.getElementById("ctl00_ctl00_ctl01_ctlSearchHeader_ctl00_txtSearchPhrase");
		SetTop.focus();
		
		var SetTop1 = document.getElementById("ctl00_ctl00_MainContent_MainContent_RecipeUtility1_rateReview_txtComment");
		SetTop1.focus();
	}

	function SetStatusFromComments(status) {
		document.getElementById("ctl00_ctl00_MainContent_MainContent_RecipeUtility1_rateReview_hidSetFocus").value = status;
	}

	function RateReviewRecipeTrackLink(linkLocation, linkName, userComments, ratedRecipe) {
		var s = s_gi("rditasteofhomeglobal");
		s.linkTrackVars = 'prop33,prop11,prop12,prop34,prop5,eVar12,eVar13,eVar14,eVar23,eVar24,events';

		if (userComments && ratedRecipe) {
			s.linkTrackEvents = 'event20,event31';
			s.events = 'event20,event31';
		}
		else if (ratedRecipe) {
			s.linkTrackEvents = 'event20';
			s.events = 'event20';
		}
		else if (userComments) {
			s.linkTrackEvents = 'event31';
			s.events = 'event31';
		}
		else {
			s.linkTrackEvents = '';
			s.events = '';
		}

		s.prop34 = linkName;
		s.prop11 = linkLocation;
		s.prop12 = "/erms/recipe.aspx";
		s.eVar12 = linkName;
		s.eVar13 = linkLocation;
		s.eVar14 = "/erms/recipe.aspx";

		//only track if they actually rated or commented
		if (userComments || ratedRecipe) {
			s.tl(this, 'o', linkName);
		}
	}

	function ToggleControls(show) {
	    var ctlRatingQuestion = $('#ctlRatingQuestion');
	    var ctlReviewComment = $('#ctlReviewComment');
	    if (show) {
	        ctlRatingQuestion.show();
	        ctlReviewComment.show();
	    }
	    else {
	        ctlRatingQuestion.hide();
	        ctlReviewComment.hide();
	    }
	}
</script>

