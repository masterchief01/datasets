	<script language="javascript">

      $(document).ready(function() {
	  	
          $('#rate2').rating('/erms/recipe.aspx', { maxvalue: 5, curvalue: 0 });
      });

   var IsLogin = 'false';

   var only_close_nav= false;     

   if(IsLogin == 'true')

   {

                only_close_nav= true;

                var popupContact = document.getElementById("popupContact");        

            //showPopupWithReturn($("#popupContact"),true, "ClosePopUp", true);

       // popupContact.style = "display:block";        

   }


   function AddRecipeToFolderTrackLink(propList, boolCreate) {
       var s = s_gi("rditasteofhomeglobal");
       var linkLocation = 'Add to Recipe Folder';
       var linkName = "";
       s.linkTrackVars = 'prop11,prop12,prop34,eVar12,eVar13,eVar14,prop24,events';

       if (boolCreate) {
           linkName = 'Create Folder and Save';
           s.linkTrackEvents = 'event35';
           s.events = 'event35';
       }
       else {
           linkName = 'Save';
           s.linkTrackEvents = 'event29';
           s.events = 'event29';
       }

       s.prop24 = propList
       s.prop34 = linkName;
       s.prop11 = linkLocation;
       s.prop12 = location.pathname.toLowerCase();
       s.eVar12 = linkName;
       s.eVar13 = linkLocation;
       s.eVar14 = location.pathname.toLowerCase();

       s.tl(true, 'o', linkName);

   } 

   </script>

