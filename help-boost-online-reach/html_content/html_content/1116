                        <script type="text/javascript">
                        //<![CDATA[
                            var FSLanding = {
                                filters: CN.$("menuFilters").getElementsByTagName("li"),

                                filterToListMap: {
                                    "new_today_filter": "new_today_list",
                                    "all_shows_filter": "all_shows_list",
                                    "details_filter": "details_list",
                                    "front_row_filter": "front_row_list",
                                    "beauty_filter": "beauty_list",
                                    "backstage_filter": "backstage_list",
                                    "moveit_filter": "moveit_list"
                                },
                                activateTab: function() {
                                    for (var i = 0; i < FSLanding.filters.length; i++) {
                                        var filter = FSLanding.filters[i];
                                        filter.className = "";
                                        CN.$(FSLanding.filterToListMap[filter.id]).style.display = "none";
                                    }
                                    this.className = "active";
                                    if(this.id  == "moveit_filter") {
                                         document.getElementById('list_hed').innerHTML ='see every look in motion';
                                         document.getElementById('moveit_filter').innerHTML = '<p><img src="/images/moveit_selected.jpg" style="margin-top: -10px;"/></p>';
                                    }
                                    else {
                                        document.getElementById('list_hed').innerHTML ='select a collection';
                                        document.getElementById('moveit_filter').innerHTML = '<p><img src="/images/moveit.jpg" style="margin-top: -10px;"/></p>';
                                    }

                                    CN.$(FSLanding.filterToListMap[this.id]).style.display = "block";
                                }
                            };
                            FSLanding.activateTab.call(FSLanding.filters[0]);

                            for (var i = 0; i < FSLanding.filters.length; ++i) {
                                Handler.add(FSLanding.filters[i], "click", FSLanding.activateTab);
                            }
                        //]]>
                        </script>
