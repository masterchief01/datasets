<script type="text/javascript">
    document.observe('dom:loaded', function(){
        $('frm_signup_newsletter').observe('keypress', function(event){
            if (event.keyCode == 13)
            {
                signupNewsletter();
            }
        });

        $('frm_signup_newsletter').observe('keydown', function(event){
            HideErrorMessage();
        });
    });
    function signupNewsletterWomen()
    {
        $('newsletter_type').value = '21';
        signupNewsletter();
    }
    function signupNewsletterMen()
    {
        $('newsletter_type').value = '2';
        signupNewsletter();
    }
    function checkSignupNewsletter()
    {
        activeEle = 'frm_signup_newsletter';
        if(!isEmpty($('newsletter[email]')))
        {
            ShowNoticeMessage("Please enter your email", '');
            return false;
        }
        else if(!validateEmailByElement($('newsletter[email]')))
        {
            ShowNoticeMessage("Invalid email", '');
            return false;
        }
        return true;
    }
    function signupNewsletter()
    {
        if(checkSignupNewsletter('frm_signup_newsletter'))
        {
            $('frm_signup_newsletter').submit();
        }
        return false;
    }
</script> </td>
