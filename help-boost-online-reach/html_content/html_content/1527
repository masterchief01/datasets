function selectTab2(tabIndex) {
	for (i = 1; i <= 5; i++) {
		if ($('tab' + i) != null) {
			$('tab' + i).className = 'tab-off tab';
			$('content' + i).className = 'invisible';
		}
	}
	$('tab' + tabIndex).className = 'tab-on tab';
	$('content' + tabIndex).className = 'visible';
}
</script>

