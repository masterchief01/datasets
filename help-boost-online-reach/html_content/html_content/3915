  <script type="text/javascript">
  var current = 1;
  var max = 1;
  var gITWLiveCarousel = null;
  var shouldScroll = true;
  function mycarousel_initCallback(carousel) {
      // Disable autoscrolling if the user clicks the prev or next button.
      carousel.buttonNext.bind('click', function() {
          carousel.startAuto(0);
          shouldScroll = false;
      });

      carousel.buttonPrev.bind('click', function() {
          carousel.startAuto(0);
          shouldScroll = false;
     });

      // Pause autoscrolling if the user moves with the cursor over the clip.
      carousel.clip.hover(function() {
          carousel.stopAuto();
          shouldScroll = false;
      }, function() {
          carousel.startAuto();
          shouldScroll = true;
      });
      
      gITWLiveCarousel = carousel;
      
      setTimeout("autoNext();", 5000);
  };

function autoNext() {
	if (shouldScroll) {
		gITWLiveCarousel.prev();
	}
	setTimeout("autoNext();", 5000);
}

  function mycarousel_itemLoadCallback(carousel,item,idx, state){

    if (current == -1){
      current = 1;
    }

    if (item == "next" || item == "init" ){

      $("#activity"+current).attr("src", "/sites/all/themes/itworld/images/icons/pager-btn.jpg");
      current += 1;
      $("#activity"+current).attr("src", "/sites/all/themes/itworld/images/icons/pager-active.jpg");
    }else{
      $("#activity"+current).attr("src", "/sites/all/themes/itworld/images/icons/pager-btn.jpg");
      current =current - 1;
      $("#activity"+current).attr("src", "/sites/all/themes/itworld/images/icons/pager-active.jpg");
    }

    max++;
  }

var totItems = jQuery('#activitycarousel > li').size();

    jQuery('#activitycarousel').jcarousel({
          vertical: true,
          scroll:1,
          start: totItems,
          animation: 'slow',
          initCallback: mycarousel_initCallback,
          itemLoadCallback: mycarousel_itemLoadCallback
          });
          
     
  </script>
</div> <!-- /#itworldlive -->
  </div>

  
