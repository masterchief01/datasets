<script type="text/javascript">
//<![CDATA[
function getObject(objectId) {	
  if(document.getElementById && document.getElementById(objectId)) {return document.getElementById(objectId);} 
  else if (document.all && document.all(objectId)) {return document.all(objectId); } 
  else if (document.layers && document.layers[objectId]) {return document.layers[objectId]; } 
  else {return false;}	 
}
function doZoom(size){
	var o=document.getElementById("newsContent").className="newsContent"+size;
}	
function Addme(title){
      var url = document.location; 
      window.external.AddFavorite(url,title);
}
function  goToSendErrorPage(channel){
      var urlTo="http://61.135.180.143/Correction/send-error.sohu?method=init&title=";
      urlTo+=escape(document.title); 
      urlTo+="&url="+escape(window.location);     
      urlTo+="&sourceid="+channel;      
      window.open(urlTo);          
}
function printArticle()
{
	var articleprint_url = 'http://www.beijing2008.cn/upload/cms_owrp2/column_en/printArticle.js';
	var print_js = document.createElement("script"); 
	window.printCallBack = function()
	{
		if(typeof printer == "object" && printer.print)	printer.print();
	}
	print_js.setAttribute( "type", "text/javascript" );
	print_js.setAttribute( "src", articleprint_url);
	if (navigator.userAgent.indexOf("MSIE") != -1)
	{
		document.body.insertBefore(print_js, null);
		print_js.onreadystatechange = printCallBack;
	}
	else
	{
		print_js.setAttribute( "onload", "printCallBack()");
		document.body.insertBefore( print_js, null);					
	}
}//]]>
</script>
