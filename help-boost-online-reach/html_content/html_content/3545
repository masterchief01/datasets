	//<![CDATA[
	
	function seel(t) {
		un = document.getElementsByClassName('inSelected');
		for(i=0;i<un.length;i++) {
			un[i].className='';
		}
		t.className='inSelected';
	}
	function oopen(t,id) {
		t.blur();
		ob = document.getElementById(id);
		if(ob.style.display=='none') {
			t.className='isSel';
			Effect.BlindDown(id);
		} else {
			t.className='';
			Effect.BlindUp(id);
		}
	}
	SORTDIFF=false;
	function toggleSort()
	{
		SORTDIFF = ! SORTDIFF;
	}
	function doCat(t,cat) {
		ic = document.getElementById('inLoading');
		ic.innerHTML = 'Loading...';
		if(SORTDIFF)
			sendReq(t,'q=inc&inc=blogIndex&sort=diff&cat='+cat,'inContent');
		else
			sendReq(t,'q=inc&inc=blogIndex&cat='+cat,'inContent');
	}


	//]]>
	</script>

