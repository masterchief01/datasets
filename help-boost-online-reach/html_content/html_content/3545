		function fixLinkTargets()
		{
			domain = "startcooking.com";
			lnks = document.getElementsByTagName("a");
			al="link addresses"
			for(i=0;i<lnks.length;i++)
			{
				address = lnks[i].href;
				//al+=address
				if(address.indexOf(domain)==-1 && address.indexOf("javascript")==-1)
				{
					lnks[i].target="_blank";
					//al+=" is external";
				}
				//al+="\n";
			}
			//alert(al)
		}
