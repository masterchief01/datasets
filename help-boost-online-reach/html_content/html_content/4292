                <script type="text/javascript">
                    $(function() {

                        $("#button-share756667").click(function(){
                            $("#menu-share756667").fadeIn("fast");
                            $(this).data("in", true);
                            $("#menu-share756667").data("hidden", false);
                        });
                        $("#menu-share756667").mouseenter(function(){
                            $("#menu-share756667").fadeIn("fast");
                            $(this).data("in", true);
                            $("#menu-share756667").data("hidden", false);
                        });
                        $("#button-share756667, #menu-share756667").mouseleave(function(){
                            $(this).data("in", false);
                            setTimeout(hideMenu, delay);
                        })

                        var delay = 400;
                        var hideMenu = function(){
                            if (!$("#button-share756667").data("in") && !$("#menu-share756667").data("in") && !$("#menu-share756667").data("hidden")){
                                $("#menu-share756667").fadeOut("fast");
                                $("#menu-share756667").data("hidden", true);
                            }
                        }

                        $("#menu-share756667 a").click(function(){
                            $("#menu-share756667").hide();
                        });
                    });
                </script>
                
