			var key = $(this).attr('key');
			var thiscontainer = $(this);
			$.ajax({
				url:		'/utilities/photodefault/format/json/type/recipe/id/7181/photo_id/'+key,
				beforeSend: function() {thiscontainer.html('Updating...')},
				success: 	function(data) {
					if(data.success){
						$('.default_image').removeClass('default_image');
						$('.photo_'+key).each(function(){$(this).addClass('default_image')});
						$('.update_default').remove();
					} else {
						alert ('Error: Could not convert to main photo.');
						$('.update_default').remove();
					}
				}
			});
			return false;
		});
	});

