        <div id="fb-root"></div>
        <script>
        window.fbAsyncInit = function() {
            FB.init({
            appId : "133109973370101",
            session : null,
            status : true, // check login status
            cookie : true, // enable cookies to allow the server to access the session
            xfbml : true, // parse XFBML
            channelUrl: 'http://www.justapinch.com/xd_receiver.html'
            });
        };

        var fb_login = function(){
            FB.login(function(response) {
                if (response.session) {
                    parent.window.location = "/member/login/rpx/";
                    $('#fb-container').hide();
                    $('#fb-loader').show();
                }
            },{
                perms: "user_about_me,email,user_hometown,user_location,publish_stream,offline_access"
            });
        };

        var fb_logout = function(next){
            FB.logout(function(response) {
                if (next !== undefined) {
                    document.location = next;
                } else {
                    document.location = "https://www.facebook.com/logout.php?api_key=133109973370101&next=http%3A%2F%2Fwww.justapinch.com%2Frecipe%2Frobdupree%2Fextreme-banana-nut-bread%2Fsweet-bread";
                }
            });
        };
        
        var fb_dialog = function(options) {
            FB.ui(
                {
                    method: "stream.publish",
                    message: options.message,
                    link: options.url,
                    picture: options.picture
                }, function(response) {
                    if (response && response.post_id) {
                        $("#fb-ui-flash").hide();
                    }
                }
            );
        };
        
        var fb_isAuth = function() {
            FB.ui({
                method:"auth.status",
                display:"hidden"
            }, function(obj) {
                if (obj.perms) {
                    return true;
                } else {
                    return false;
                }
            });
        }

        $(".fb_a").click(function(){
            $('#fb-container').hide();
            $('#fb-loader').show();
        });

        (function() {
            var e = document.createElement("script");
            e.src = document.location.protocol + "//connect.facebook.net/en_US/all.js";
            e.async = true;
            document.getElementById("fb-root").appendChild(e);
        }());
        </script>
        
