                <script type="text/javascript">
                    $(function() {

                        $("#button-share305292").click(function(){
                            $("#menu-share305292").fadeIn("fast");
                            $(this).data("in", true);
                            $("#menu-share305292").data("hidden", false);
                        });
                        $("#menu-share305292").mouseenter(function(){
                            $("#menu-share305292").fadeIn("fast");
                            $(this).data("in", true);
                            $("#menu-share305292").data("hidden", false);
                        });
                        $("#button-share305292, #menu-share305292").mouseleave(function(){
                            $(this).data("in", false);
                            setTimeout(hideMenu, delay);
                        })

                        var delay = 400;
                        var hideMenu = function(){
                            if (!$("#button-share305292").data("in") && !$("#menu-share305292").data("in") && !$("#menu-share305292").data("hidden")){
                                $("#menu-share305292").fadeOut("fast");
                                $("#menu-share305292").data("hidden", true);
                            }
                        }

                        $("#menu-share305292 a").click(function(){
                            $("#menu-share305292").hide();
                        });
                    });
                </script>
                
