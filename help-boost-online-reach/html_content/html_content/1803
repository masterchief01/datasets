function google_radlink_request_done(radlinks) {
    var apostrophe1 = /'/ig;
    var apostrophe2 = /&#39;/ig;
    var s = '';
    var rad_keywords = '';

    if (radlinks.length == 0) {
        return;
    }

    try {
        if (typeof(google_page_url) == 'undefined') {
            var google_page_url = window.location.toString();
        }
        else {
            google_page_url = window.location.toString();
        }
        google_page_url = window.location.protocol + "//www." + google_page_url.substring(google_page_url.indexOf('.')+1);
    }
    catch (e) {}

    for(i = 0; i < radlinks.length; i++) {
        try {
            rad_keywords = rad_keywords + radlinks[i].term;
            if (i < (radlinks.length - 1)) {
                rad_keywords = rad_keywords + "|";
            }
        }
        catch (e) {continue;}
    }
   
    var rad_rts = "";
    for(i = 0; i < radlinks.length; i++) {
        try {
            rad_rts = rad_rts + radlinks[i].radlink_token.replace(apostrophe2,"");
            if (i < (radlinks.length - 1)) {
                rad_rts = rad_rts + "|";
            }
        }
        catch (e) {continue;}
    }
    for (i = 0; i < radlinks.length; i++) {
        try {
            s +=    '<li>';
            s +=    '<a  rel="nofollow" href=\"/ehow_radlinks_ads.html?term=';
            s +=    encodeURIComponent(radlinks[i].term.replace(apostrophe2,""));
            s +=    '&channel=spor_fitness,algorithm3,text';
            s +=    '&amp;google_kw_type=radlinks';           
            s +=    '&amp;google_rt=';
            s +=    encodeURIComponent(radlinks[i].radlink_token.replace(apostrophe2,""));
            s +=    '&amp;google_rts=' + rad_rts;
            s +=    '&amp;google_page_url=' + encodeURIComponent(google_page_url);
            s +=    '&amp;radkws=' + encodeURIComponent(rad_keywords.replace(apostrophe2,""));
            s +=    '&amp;contentid=5298026';
            s +=    '&amp;studioid=8bc653cf-1339-43f4-9b21-9a72a0f2cc31';
            s +=    '&amp;category=Sports+%26+Fitness';
            s +=    '&amp;subcategory=Fitness';
            s +=    '\">\n\n';
            s +=    radlinks[i].term.replace(apostrophe2,"");
            s +=    '\n\n\n';
            s +=    '</a>';
            s +=    '</li>';
        }
        catch (e) {continue;}
    }
    document.write('<ul>\n' + s + '</ul>\n');
}

google_page_url         = 'http://www.ehow.com/how_5298026_flatten-firm-belly.html';
google_max_num_ads      = '0';
google_num_radlinks     = '6';
google_max_radlink_len  = '27';
google_ad_client        = 'ca-ehow-radlinks_js';
google_ad_channel       = 'spor_fitness,algorithm3,text';
google_ad_output        = 'js';
google_safe             = 'high';
google_adtest           = 'off';
google_language         = 'en';


try {
    if (google_page_url == null || google_page_url == '') {
        google_page_url = window.location.toString();
    }
}
catch (e) {}

try {
    window.google_page_channel = google_ad_channel;
}
catch (e) {}

// -->
</script>

