						FB.init({appId: '63203377906', status: true, cookie: true, xfbml: false,
							channelUrl : (window.location.protocol + '//' + window.location.host + '/xd_receiver.htm')});
						$('.jsFacebookConnect').css('visibility','visible');
						FB.XFBML.parse();
						FB.getLoginStatus(function(response) {
						if (response.session && dlabs.user.isLoggedIn) {
							$('.jsFacebookLogout').click(function(){
								FB.logout(function(response){
									window.location.reload();
									return false;
								});
							});
							for (var i = 0; i < onFBInit.length; i++) {
