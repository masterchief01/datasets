		    $.get("http://gotsmile.net/a/vote?access_token=c0eb0e3a314f5d05b447310d47b19585&type=image&mediaId=158336&vote=" + direction, {}, function(data, textStatus) {
	        	if(direction == 'up'){
	        	    $(".upvoteimg").css("background-image", "url(http://static.gotsmile.net/i/vote_up_normal.gif)"); 
                	    $(".upvoteimg").css("background-position", "0 0");
                    	}
	        	if(direction == 'down'){
	        	    $(".downvoteimg").css("background-image", "url(http://static.gotsmile.net/i/vote_down_normal.gif)"); 
            		    $(".downvoteimg").css("background-position", "0 0");
                    	}
			var errorOccured = data["err"];
			if(errorOccured){
			    $("#votestatus").html('<span style=\'color:#dd3939;background:#ffc7c7;font-size:0.8em;padding:1px;margin-right:0;\'>'+data["err"]+'</span>');
			    setTimeout(function() {   
                                $('#votestatus').hide();
                            }, 2000);
			}else{
			    $("#votestatus").html('<span style=\'font-size:0.8em;color:#fff;background:#3198d3;padding:1px;margin-right:0;\'>'+data["msg"]+'</span>');
                            $.get("http://gotsmile.net/a/getRating?access_token=c0eb0e3a314f5d05b447310d47b19585&type=image&mediaId=158336", function(ratingData){
                        	$("#ratingtxt").text(ratingData);
                        	$("#rtxt").text(ratingData);
                        	$("#rcss").animate({width:''+ratingData+'%'});
                            });
                            if(direction == 'up'){                                                                                           
                                var current_vote = $("#ucount").text();                                                                 
                                $("#ucount").text(parseInt(current_vote)+1);                                                            
                    	    }
                            if(direction == 'down'){                                                                                           
                                var current_vote = $("#dcount").text();                                                                 
                                $("#dcount").text(parseInt(current_vote)+1);                                                            
                    	    }
			    setTimeout(function() {
				$('#votestatus').hide();
			    }, 2000);
			}
		    }, "json");
		}
		$("#upvote").click(function() { vote('up'); return false; });
		$("#downvote").click(function() { vote('down'); return false; });
		-->
	    </script>
	    <p>
	    <b>Author:</b> This image was added on 19 December, 2011 in 23:13pm by <a href="http://gotsmile.net/user/sharo"><span class="prlink">Milen Mihalev (sharo)</span></a>.
	    </p>
