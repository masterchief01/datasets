    $(document).keyup(function(e){
	var inputHasFocus = false;
	var focusedInputs = $(".inplace_form input:focus");
	if (focusedInputs != null && focusedInputs.length > 0) { inputHasFocus = true; }
	if(!inputHasFocus){
	    switch (e.keyCode){
		case 39:
		if( $(".imgnav-next").attr("href")){
	    	    window.location = $(".imgnav-next").attr("href");
		}
		break;
		case 37:
		if( $(".imgnav-prev").attr("href")){
	    	    window.location = $(".imgnav-prev").attr("href");
		}
		break;
	    }
	}
    });
