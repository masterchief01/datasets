	$("[id^='toggle_']").click(function () {

		var myId=this.id

		var myList=myId+'_list';

		var up_display=$("#"+myList).css('display');

		if (up_display=='none') {

			$("#"+myList).css('display', 'block');

			$("#"+myId).attr('class', 'control_toggle_on')

		} else {

			$("#"+myList).css('display', 'none');

			$("#"+myId).attr('class', 'control_toggle_off')

		}

	});

        

