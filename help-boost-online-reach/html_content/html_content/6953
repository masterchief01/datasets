	<script type="text/javascript">
$(function() {
var active_searchform = "radio-search-nym";

$('#radio-search-nym').attr("checked","checked")

$("#formswitcher input").click(function(i){
		$(this).parent().addClass("active");
		$(this).parent().siblings().removeClass("active");
		$("#txtCategoryOrItem").hide().attr("value","search for food");
		if(this.id !== active_searchform){
			active_searchform = this.id;
			if(this.id === "radio-search-mp"){
				$("#search-mp").show().addClass("active-form");
				$("#search-nym").hide().removeClass("active-form");	
			}
			else{
				$("#search-nym").show().addClass("active-form");
				$("#search-mp").hide().removeClass("active-form");
			}
		}
	});

});

</script>


