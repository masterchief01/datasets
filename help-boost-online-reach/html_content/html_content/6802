<!-- This style for the #mobileAd ID needs to be in whichever stylesheet the page is using. -->
<style type="text/css">
      #mobileAd
      {
            display: block;
            text-align: center;
      }
</style>

<!-- This is the image tag which is defaulted to not show. The width can be changed here. --> 
<!-- When testing, we had to put the script below the image, so that it would run after the image was loaded into the browser. -->
<!--  top: 0 -->

<div id="mobileAd" style="height: 147px;"> 
    <a id="mobileAppUrl" name="mobileAppUrl" href="#">
        <img id="mobileImage" name="mobileImage" src="http://www.mediapeta.com/peta/Images/Banners/640x96-mobilebanner.jpg" alt="peta2 mobile" style="width: 980px"/> 
    </a>
</div>

<!-- CODE TO DETECT MOBILE DEVICE --> 
<script type="text/javascript">
var devicesArray = new Array();

// These are the different mobile platforms to be detected. More can be added if needed.
devicesArray[0] = "iphone";
devicesArray[1] = "ipod";
devicesArray[2] = "android";  
devicesArray[3] = "series60";
devicesArray[4] = "symbian";  
devicesArray[5] = "windows ce";
devicesArray[6] = "blackberry";
devicesArray[7] = "palm";
devicesArray[8] = "iemobile";
devicesArray[9] = "windows phone os 7.0";
devicesArray[10] = "ipad";
devicesArray[11] = "windows phone os 7.5";

//Initialize our user agent string to lower case.
var uagent = navigator.userAgent.toLowerCase();

// Detects if the current device is a mobile device.
function DetectMobile()
{     
    for(var i = 0; i<devicesArray.length; i++)
    {
       if (uagent.search(devicesArray[i]) > -1)
       {
            return true;
       }
   }

    return false;
}
function IsMobileApple() {
    return IsIPhone() || IsIPod();
}
function IsIPhone() {
    return uagent.search(devicesArray[0]) > -1;
}
function IsIPod(){
    return uagent.search(devicesArray[1]) > -1;
}
function IsIPad(){
	return uagent.search(devicesArray[10]) > -1;
}
function IsAndroid() {
    return uagent.search(devicesArray[2]) > -1;
}
function IsSeries60() {
    return uagent.search(devicesArray[3]) > -1;
}
function IsSymbian() {
    return uagent.search(devicesArray[4]) > -1;
}
function IsWindowsCE() {
    return uagent.search(devicesArray[5]) > -1;
}
function IsBlackberry() {
    return uagent.search(devicesArray[6]) > -1;
}
function IsPalm() {
    return uagent.search(devicesArray[7]) > -1;
}
function IsMobileWindows7x() {
    return uagent.search(devicesArray[8]) > -1 || uagent.search(devicesArray[9]) > -1 || uagent.search(devicesArray[11]) > -1;
}
function IsMobileWindows() {
    return IsWindowsCE() || IsMobileWindows7x();
}

// If the page is being viewed on a mobile device, change the image's display property. 
if (DetectMobile()) {
	var mobileImg = "http://www.mediapeta.com/peta/images/banners/";	//This is the base location.
	var mobileImgUrl = "";

    document.getElementById('mobileAd').style.display = 'block';

	//See if the user is from Canada.
	if (geoip_country_code() == 'CA') {
		mobileImg += "640x96-petaMobileBanner-CN.jpg";
		mobileImgUrl = "http://www.peta.org/about/learn-about-peta/subscribe-to-mobile-alerts.aspx";
	} else if (IsIPhone() || IsIPod()) {
		mobileImg += "640x96-mobileAlerts-iPhone.jpg";
		mobileImgUrl = "http://www.peta.org/iphone?c=pfip11";
	
	
	
	//} else if (IsIPad()) {
	//	mobileImg += "640x96-mobileAlerts-iPad.jpg";
	//	mobileImgUrl = "http://www.petatest.org?IPad";
	} else if (IsAndroid()) {
		mobileImg += "640x96-mobileAlerts-DroidPhone.jpg";
		mobileImgUrl = "https://play.google.com/store/apps/details?id=com.peta.android";
	//} else if (IsBlackberry()) {
	//	mobileImg += "640x96-mobileAlerts-BlackBerry.jpg";
	//	mobileImgUrl = "http://www.petatest.org?Blackberry";
	//} else if (IsMobileWindows7x()) {
	//	mobileImg += "640x96-mobileAlerts-Windows7.jpg";
	//	mobileImgUrl = "http://www.petatest.org?Windows7x";
	
	
	
	} else {
		mobileImg += "640x96-mobileAlerts-Default.jpg";
		//mobileImgUrl = "http://www.petatest.org?DefaultBanner";
	}
	
	//Now, set the correct value for the banner image and url.
	document.getElementById('mobileImage').src = mobileImg;
	document.getElementById('mobileAppUrl').href = mobileImgUrl;
} else {
	document.getElementById('mobileAd').style.display = 'none';
}
</script> 
<!-- END MOBILE DEVICE CODE -->
