<div class="search-box">
    <input name="ctl00$fragment_43b89c32_292d_4187_999d_058d54ae3ead$_a236a547082$SearchText" type="text" maxlength="64" size="45" id="ctl00_fragment_43b89c32_292d_4187_999d_058d54ae3ead__a236a547082_SearchText" ButtonId="SearchButton" class="text" onkeydown="return keyHandler(event);" onfocus="if(this.defaultValue==this.value) this.value='';" onblur="if(this.value=='') this.value=this.defaultValue;" />
    <span></span>
    <input type="submit" value="search" onclick="return doSearch();" />
</div>

<script type="text/javascript">
    function doSearch() {
        var searchString = $('#ctl00_fragment_43b89c32_292d_4187_999d_058d54ae3ead__a236a547082_SearchText')[0].value;

        if (searchString != '') {
            var newLocation = 'http://www.peta.org/search/SearchResults.aspx?q=' + searchString;
            $(location).attr('href', newLocation);
        }
        return false;
    }

    function keyHandler(event) {
        if (event.keyCode == 13) {

            Page_ValidationActive = false;
            event.returnValue = false;
            event.cancel = true;

            return doSearch();
                    }
        return true;
    }

    $(document).ready(function() {
        $('#ctl00_fragment_43b89c32_292d_4187_999d_058d54ae3ead__a236a547082_SearchText').coolinput({ hint: 'SEARCH PETA.ORG' });
    });
</script>






