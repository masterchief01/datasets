<script type="text/javascript">
    function CheckAll() {
        setItems(true);
        return false;
    }

    function UnCheckAll() {
        setItems(false);
        return false;
    }

    function setItems(checked) {
        var listbox = $find("ctl00_PlaceHolderMain_suppressShoppingListLinkInEditMode_AddToShoppingListFromRecipeArticle_AddToShoppingListModalControl_C_shoppingListCheckList");
        var items = listbox.get_items();
        items.forEach(function (itm) { itm.set_checked(checked); });
    }
</script>

