    function SetLargeImage() {
        if (document.getElementById('ctl00_PlaceHolderMain_LargeImageUC_hdnLink') != null && document.getElementById('ctl00_PlaceHolderMain_LargeImageUC_hdnLink').value != '') {
            //document.getElementById('detailimage').style.display = 'block';
            document.getElementById('lnkDetailImage').setAttribute('href', document.getElementById('ctl00_PlaceHolderMain_LargeImageUC_hdnLink').value);
            document.getElementById('lnkDetailImageExpand').setAttribute('href', document.getElementById('ctl00_PlaceHolderMain_LargeImageUC_hdnLink').value);
        }
        else {
            //document.getElementById('detailimage').style.display = 'none';
            document.getElementById('lnkDetailImage').style.display = 'none';
            document.getElementById('lnkDetailImageExpand').style.display = 'none';
        }
    }
