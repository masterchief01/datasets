<script language="Javascript">
function CountLeft() 
{
    var field = this.document.forms[0].ctl00$PlaceHolderMain$EditorialCommentsControl$tbxBody;
    var count = this.document.forms[0].ctl00$PlaceHolderMain$EditorialCommentsControl$txtRemainingLength;
    max = 1000;

    if (field.value.length > max)
        field.value = field.value.substring(0, max);
    else
        count.value = max - field.value.length;
}
$(document).ready(function() {
  Sys.WebForms.PageRequestManager.getInstance().add_endRequest(AddButtons);
   AddButtons();
});

function AddButtons()
{
 $('.RadDataPager_Default .rdpWrap:nth-child(2n)').addClass("prev");
    $('.RadDataPager_Default .rdpWrap:nth-child(2n)').find('a').attr('rel', 'prev');
    $('.RadDataPager_Default .rdpWrap:nth-child(4n)').addClass("next");
    $('.RadDataPager_Default .rdpWrap:nth-child(4n)').find('a').attr('rel', 'next');

    if (location.search.indexOf("Page") != -1) {
               location.hash = "CommentsPage";
               }
   
    //Hide Prev button on First page
    if($("div.rdpWrap.prev a").attr("onclick") != null) {
        var nextValue = $("div.rdpWrap.prev.next").html();
        $("div.rdpWrap.prev").html(" ");
        $("div.rdpWrap.prev.next").html(nextValue);
    }
    //Hide Next button on Last page
    if($("div.rdpWrap.prev.next a").attr("onclick") != null) {
        $("div.rdpWrap.prev.next").html(" ");
    }

}
</script>

