function sl_get_cookie ( cookie_name )
{
  var results = document.cookie.match ( '(^|;) ?' + cookie_name + '=([^;]*)(;|$)' );

  if ( results )
    return ( unescape ( results[2] ) );
  else
    return null;
}
//alert('geoip says '+sl_get_cookie('geo_country'));
switch(sl_get_cookie('geo_country'))
{
case 'GB':
		//alert('GB Script');

default:
		//alert('Default Script');
		break;
}
</script>
