		<script>
			var t = 3000;
			
			function fade(slide) {
				if (slide > 0) {
					$(".slide.z" + slide).fadeOut();
					var t1 = setTimeout(function() {
						slide--;
						fade(slide);
					},t);
				} else {
					$(".slide.z4").fadeIn(function() {
						$(".slide").show();
						var t2 = setTimeout(function() {
							fade(4);
						},t);
					});
				}
			}
			
			var t0 = setTimeout(function() {
				fade(4);
			},t);

		</script>
		
