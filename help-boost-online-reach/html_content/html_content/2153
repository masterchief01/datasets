		function getQueryStringVal(key) {
			var queryString = document.location.search.substring(1);
			var keys = queryString.split("&");
			var value = "";
			for (var i=0; i < keys.length && value.length==0;i++) {
				var pos = keys[i].indexOf('=');
				var keyName = "";
				if (pos >= 0) {
					keyName = keys[i].substring(0, pos);
					if (keyName==key)
					value = keys[i].substring(pos+1);
				}
			}
			return value;
		}
	
		var adConfig = new TiiAdConfig("3475.mre2");
		adConfig.setCmSitename("cm.mre");
		adConfig.setRevSciTracking(true);			

		var zoneOverride = getQueryStringVal("ao");
		if(zoneOverride != null && zoneOverride!=""){
			var zone = zoneOverride;
		} else{
			var zone = "recipe-finder";
		}
		
		var adFactory = new TiiAdFactory(adConfig, zone);
		if (location.search.indexOf("xid=google") >= 0 || location.search.indexOf("xid=gl") >= 0) {
			adConfig.setPopups(false);
		}
