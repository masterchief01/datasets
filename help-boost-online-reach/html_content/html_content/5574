<script type="text/javascript">
/* <![CDATA[ */
function writeImage(method, data, action, current) {
    $(document.createElement("img")).attr({
        id: 'hswOmniImage',
        src: data
    }).load(function () {
        if (method !== 'none') {
            clearTimeout(t);
            doAction(method, action, current);
        }
    });
    if (typeof method === 'string' && method !== 'none') {
        var t = setTimeout(function () {
            doAction(method, action, current);
        }, 500);
    }
}

function doAction(method, action, current) {
    if (method == 'form') {
        current.submit();
    } else {
        document.location.href = action;
    }
}

function isPurchaseEvent(tag, lookupArray) {
    var found = false;
    for (key in lookupArray) {
        if (tag.indexOf(lookupArray[key]) >= 0) {
            found = lookupArray[key];
            break;
        }
    }
    return found;
}

function inArray(tag, lookupArray) {
    var found = false;
    for (key in lookupArray) {
        if (tag.indexOf(lookupArray[key]) >= 0) {
            found = true;
            break;
        }
    }
    return found;
}

function sendTags(tag, vars, current, eventObj, noRedirect) {
    if (tag != '') {
        var hswOmniImage = document.getElementById('hswOmniImage');
        var omniImageData = "http://metrics.howstuffworks.com/b/ss/" + s_account + "/1/H.17/s45086097826030?";
        var gotoAction = ''
        var tagName = '';
        var timestamp = new Date();
        var year = timestamp.getFullYear();
        var month = timestamp.getMonth() + 1;
        var day = timestamp.getDate();
        var hours = timestamp.getHours();
        var mins = timestamp.getMinutes();
        var secs = timestamp.getSeconds();
        var weekday = timestamp.getDay();
        var timezoneOffset = timestamp.getTimezoneOffset();
        var gValue = location.protocol + '//' + location.hostname;
        var omniPrefix = 'Parentables : ';
        if (location.pathname) {
            gValue += location.pathname;
        }
        omniImageData += '&AQB=1';
        omniImageData += '&t=' + month + '/' + day + '/' + year + ' ' + hours + ':' + mins + ':' + secs + ' ' + weekday + ' ' + timezoneOffset;
        omniImageData += '&vmt=' + '4467DB86';
        omniImageData += '&ns=' + 'hswmedia';
        omniImageData += '&g=' + encodeURIComponent(gValue);
        var subDomain = location.href;
        subDomain = subDomain.substring(7, subDomain.indexOf('.', 7));
        if (subDomain == 'recipes') {
            if (vars) {
                vars = vars.replace(/=/gi, '=TLC : ');
                vars = vars.replace(/events=TLC :/gi, 'events=');
                vars = vars.replace(/products=TLC :/gi, 'products=');
            }
        }

        if (vars) {
            omniImageData += vars;
        }

        var _href = $(current).attr('href');
        if (_href) {
            if (omniImageData.indexOf('c25=') >= 0 && omniImageData.indexOf('c7=') < 0) {
                var endPos = _href.indexOf('?');
                if (endPos < 0) {
                    endPos = _href.length;
                }
                var startPos = _href.lastIndexOf('/', endPos);
                var shortHref = _href.substr(startPos + 1, endPos - startPos - 1);
                if (shortHref == '') {
                    shortHref = _href;
                }
                omniImageData += '&c7=' + shortHref;
                omniImageData += '&v9=' + shortHref;
            }
        }
        omniImageData += '&pe=' + 'lnk_o';
        omniImageData += '&pev2=' + tag.replace(/&/g, ' and ');
        omniImageData += '&pid=' + encodeURIComponent(location.href);
        omniImageData += '&AQE=1';
        if (noRedirect == undefined) {
            if (current.action) {
                gotoAction = current.action;
                tagName = current.nodeName || '';
                if (omniImageData.indexOf('pev1=') < 0) {
                    omniImageData += '&pev1=' + encodeURIComponent(gotoAction);
                    omniImageData += '&oid=' + encodeURIComponent(gotoAction);
                }
                omniImageData += '&ot=' + tagName;
                if ((current.method.toLowerCase() == 'get') && (current.name.substr(0, 15) == 'frmSelMakeModel')) {
                    writeImage('url', omniImageData, current.action, current);
                } else {
                    writeImage('form', omniImageData, current.action, current);
                }
            } else if (_href) {
                gotoAction = _href;
                gotoTarget = current.target;
                tagName = current.nodeName || '';
                if (omniImageData.indexOf('pev1=') < 0) {
                    omniImageData += '&pev1=' + encodeURIComponent(gotoAction);
                    omniImageData += '&oid=' + encodeURIComponent(gotoAction);
                }
                omniImageData += '&ot=' + tagName;
                writeImage('url', omniImageData, gotoAction, current);
            } else if (eventObj.target.href) {
                gotoAction = eventObj.target.href;
                tagName = current.nodeName || '';
                if (omniImageData.indexOf('pev1=') < 0) {
                    omniImageData += '&pev1=' + encodeURIComponent(gotoAction);
                    omniImageData += '&oid=' + encodeURIComponent(gotoAction);
                }
                omniImageData += '&ot=' + tagName;
                writeImage('url', omniImageData, gotoAction, current);
            } else {
                gotoAction = document.location.href;
                tagName = current.nodeName || '';
                if (omniImageData.indexOf('pev1=') < 0) {
                    omniImageData += '&pev1=' + encodeURIComponent(gotoAction);
                    omniImageData += '&oid=' + encodeURIComponent(gotoAction);
                }
                omniImageData += '&ot=' + tagName;
                writeImage('none', omniImageData, gotoAction, current);
            }
        } else {
            gotoAction = document.location.href;
            tagName = current.nodeName || '';
            if (omniImageData.indexOf('pev1=') < 0) {
                omniImageData += '&pev1=' + encodeURIComponent(gotoAction);
                omniImageData += '&oid=' + encodeURIComponent(gotoAction);
            }
            omniImageData += '&ot=' + tagName;
            writeImage('none', omniImageData, gotoAction, current);
        }
    }
}

