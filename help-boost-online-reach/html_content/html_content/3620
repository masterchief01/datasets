                                                <script type="text/javascript">

                                                    function toggle(target)
                                                    {
                                                        obj=(document.all) ? document.all[target] : document.getElementById(target);
                                                        obj.style.display=(obj.style.display=='none') ? 'inline' : 'none';
                                                    }



                                                    function validate()
                                                    {
                                                        function validateEmail(str)
                                                        {
                                                            var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
                                                            if(reg.test(str) == false)
                                                            {
                                                                alert("Invalid Email Address");
                                                                return false;
                                                            }
                                                        }

                                                        var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;

                                                        if(jQuery('input[name=therapists]').is(":checked") || $('input[name=forum]').is(":checked"))
                                                        {
                                                            if($('input[name=agreement]').attr('checked') !== true)
                                                            {
                                                                alert('You should accept the agreement to submit your message.');
                                                                return false;
                                                            }
                                                                else if($('input[name=author]').val() == '')
                                                                {
                                                                    alert('You should enter your name.');
                                                                    return false;
                                                                }
                                                                else if($('input[name=email]').val() == '')
                                                                {
                                                                    alert('You should enter your email.');
                                                                    return false;
                                                                }
                                                                else if($('input[name=email]').val() != '' && reg.test($('input[name=email]').val()) == false)
                                                                {
                                                                    alert("Invalid Email Address");
                                                                    return false;
                                                                }
                                                            else if($('textarea[name=comment]').val() == '')
                                                            {
                                                                alert('You should enter a message.');
                                                                return false;
                                                            }
                                                            else if($('input[name=therapists]').is(":checked") && $('input[name=forum]').is(":checked"))
                                                            {
                                                                if($('#therapists-title').val() == '')
                                                                {
                                                                    alert('You should enter a title for your message.');
                                                                    return false;
                                                                }
                                                                else if($('input[name=zip]').val() == '')
                                                                {
                                                                    alert('You should enter your zip code.');
                                                                    return false;
                                                                }
                                                                else if($('#service_requested').val() == '0')
                                                                {
                                                                    alert('You should choose the service you request.');
                                                                    return false;
                                                                }
                                                              }
                                                              else if($('input[name=forum]').is(":checked"))
                                                              {
                                                                  if($('#forum-title').val() == '')
                                                                  {
                                                                      alert('You should enter a title for your message.');
                                                                      return false;
                                                                  }
              }
              else
              {
                  return true;
              }
          }
          else
          {
              return true;
          }
      }
	
      $('input[name=forum]').click(function()
      {
          if($('#therapists-email').val() != '')
          {
              $('#forum-email').val($('#therapists-email').val());
          }
		
          if($('#therapists-title').val() != '')
          {
              $('#forum-title').val($('#therapists-title').val());
          }
		
          showHide();
      });
	
      $('input[name=therapists]').click(function()
      {
          if($('#forum-email').val() != '')
          {
              $('#therapists-email').val($('#forum-email').val());
          }
		
          if($('#forum-title').val() != '')
          {
              $('#therapists-title').val($('#forum-title').val());
          }
		
          showHide();
      });
	
      function showHide()
      {
          if($('input[name=therapists]').is(":checked"))
          {
              $('.title').show();
              $('.therapists').show();
              $('.forum').hide();
              $('.forum input').attr('disabled', 'disabled');
              $('.therapists input, .therapists select').removeAttr('disabled');
          }
          else if($('input[name=forum]').is(":checked") && $('input[name=therapists]').not(":checked"))
          {
              $('.title').show();
              $('.forum').show();
              $('.therapists').hide();
              $('.therapists input, .therapists select').attr('disabled', 'disabled');
              $('.forum input').removeAttr('disabled');
          }
          else
          {
              $('.title').hide();
              $('.therapists').hide();
              $('.forum').hide();
              $('.therapists input, .therapists select').attr('disabled', 'disabled');
              $('.forum input').attr('disabled', 'disabled');
          }
      }
	
      showHide();
</script>
<style type="text/css">
    .progress{
        width: 1px;
        height: 20px;
        font-family:Arial, Helvetica, sans-serif;
        line-height:20px;
        color: white;
        font-size: 12px;
        overflow: hidden;
        background-color: navy;
        padding-left: 5px;
        margin-left:110px
    }
</style>

<script type="text/JavaScript">
    /***********************************************
     * Form Field Progress Bar- By Ron Jonk- http://www.euronet.nl/~jonkr/
     * Modified by Dynamic Drive for minor changes
     * Script featured/ available at Dynamic Drive- http://www.dynamicdrive.com
     * Please keep this notice intact
     ***********************************************/

    function textCounter(field,counter,maxlimit,linecounter) {
        // text width//
        var fieldWidth =  parseInt(field.offsetWidth);
        var charcnt = field.value.length;

        // trim the extra text
        if (charcnt > maxlimit) {
            field.value = field.value.substring(0, maxlimit);
        }

        else {
            // progress bar percentage
            var percentage = parseInt(100 - (( maxlimit - charcnt) * 100)/maxlimit) ;
	
            var rest       = maxlimit - charcnt;
            if(rest == '')
            {
                rest = '0';
            }
            else
            {
                rest = rest;
            }
	
            $('#charleft, #charleft2').html(rest);
	
            if(parseInt((fieldWidth*percentage)/100) < 30)
            {
                document.getElementById(counter).style.width =  "30px";
            }
            else
            {
                document.getElementById(counter).style.width =  parseInt((fieldWidth*percentage)/100)+"px";
            }
            document.getElementById(counter).innerHTML=""+percentage+"%"
            // color correction on style from CCFFF -> CC0000
            setcolor(document.getElementById(counter),percentage,"background-color");
        }
    }

    function setcolor(obj,percentage,prop){
        obj.style[prop] = "rgb(80%,"+(100-percentage)+"%,"+(100-percentage)+"%)";
    }
</script>                </div>
            
