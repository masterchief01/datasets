<script type="text/javascript">
    // array used in the mini features in the drop downs
    var miniFeatureDone = new Array(false,false,false,false,false,false,false,false,false);
    // the url
    var theurl = '/feeds/featured/dropdown?section=';
    $(".nav").hover(
        function () {
            $('.sublinks').stop(true, true);
            $('.sublinks').each(function() {
                $(this).hide();
            });
            var index = $(this).index();
            //console.log('index NEW ' + index);
            var submenu = $(this).find('.sublinks');
            if(submenu) {
                //var position = $(this).position();
                $(submenu).css({
                    position:'absolute',
                    top: Math.round($(this).find('.mainmenu').position().top + 24) + 'px',
                    left: $(this).find('.mainmenu').position().left +'px',
                    zIndex:32700
                });
                $(submenu).show();

                var miniFeature = $(this).find('.right_stuff');
                var id = $(this).attr('id').split('_');
                id = id[1];
                //console.log('id ' + id);
                //console.log('miniFeatureDone[index] ' + miniFeatureDone[index]);
                // check to see if already got miniFeature
                if(miniFeatureDone[index] == false) {
                    $.ajax({
                        url: theurl+id,
                        dataType: 'json',
                        cache: true,
                        success: function(data) {
                            $(miniFeature).find('a').attr('href', data.itemHref);
                            $(miniFeature).find('img').attr('src', data.imgSrc);
                            $(miniFeature).find('.imHeading').html(data.Headline);
                            $(miniFeature).show();
                        }
                    });

                    miniFeatureDone[index] = true;
                }
            }

        },
        function () {

            $('.sublinks').each(function() {
                $(this).hide();
            });
        }
    );
    $('.topmenu').mouseleave(function(event) {
        $('.sublinks').each(function() {
            $(this).hide();
        });
    });


</script>
