//<!--
	/*global document, ga, _gaq*/
	
	function ipcCreateCookie(name, value, days) {
		var date,
			expires = "";
	
		if (days) {
			date = new Date();
			date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
			expires = "; expires=" + date.toGMTString();
		}
		document.cookie = name + "=" + value + expires + "; path=/";
	}
	
	function ipcReadCookie(name) {
		var nameEQ = name + "=",
			ca = document.cookie.split(';'),
			i,
			caLength = ca.length,
			c;
		for (i = 0; i < caLength; i += 1) {
			c = ca[i];
			while (c.charAt(0) === ' ') {
				c = c.substring(1, c.length);
			}
			if (c.indexOf(nameEQ) === 0) {
				return c.substring(nameEQ.length, c.length);
			}
		}
		return null;
	}

	// Start Month on Month tracking
	var	visitedLastMonth = false;
	var currentDate		= new Date();
	var currentMonth	= currentDate.getMonth();
	var monthCookie		= parseInt(ipcReadCookie("monthOnMonth"));
	var ipcUpdateMonthCookie = function () {
		ipcCreateCookie("monthOnMonth", currentMonth, 365);
	};
	
	if (isNaN(monthCookie)) {
		// Has never been to the site before
		ipcUpdateMonthCookie();
	} else if (monthCookie === (currentMonth - 1) || (currentMonth === 0 && monthCookie === 11)) {
		// Has been in the last month
		visitedLastMonth = true;
		ipcUpdateMonthCookie();
	} else if (currentMonth !== monthCookie) {
		// Has been to the site but not in the last month
		ipcUpdateMonthCookie();
	}
	// End Month on Month tracking


	var today = (new Date()).getTime();
	var daysSinceLastVisit = 0;
	
	lastVisitDate = ipcReadCookie("lastVisit");

	if (lastVisitDate !== null) {
		timeSinceLastVisit = today - parseInt(lastVisitDate);
		daysSinceLastVisit = Math.floor(timeSinceLastVisit/(1000*60*60*24));
	}

	ipcCreateCookie('lastVisit', today, 365);

var _gaq =  _gaq || [];
