<script type="text/javascript" charset="utf-8">
  (function() {
    var i, length, placement;
    adprobe.content_unit_ids = adprobe.content_unit_ids.split(',');
    // function to process and push params
    function parse_key(unit_ids) {
    var key = '', i = 0, content_unit_id_name = '';
      for (i=0; i < unit_ids.length; i+=1) {
        content_unit_id_name = 'wl' + unit_ids[i] + 'camp';
      if ('' === window[content_unit_id_name]) {
          key += unit_ids[i] + '-no';
      } else {
          key += unit_ids[i] + '-yes';
      }
      // don't add a + if this is the last of the key to be processed
        (i !== (unit_ids.length - 1)) ? key += '+': '';
    }
      return key;
    }
    /* if specific placments, push specific placements, else push the generic placement */
    if (adprobe.placement_unit_ids.length > 0) {
      length = adprobe.placement_unit_ids.length;
      for (i=0; i < length; i++) {
        placement = adprobe.placement_unit_ids[i];
        window['ipc.adprobe_' + placement.size] = parse_key([placement.contentid]);
        if (typeof(ipc) !== 'undefined' && ipc && ipc.adtech) {
          ipc.adtech.pushParams({
            'name': 'ipc.adprobe_' + placement.size,
              'placement': 'before',
              'adunit': placement.size,
              'order': 5, 'fn': function(val) {
                if (!val) {
                  val = '';
                }
                return val;
              }});
        }
      }
    } else {
      window.ipc_adprobe_ids = parse_key(adprobe.content_unit_ids);
      if (typeof(ipc) !== 'undefined' && ipc && ipc.adtech) {
        ipc.adtech.pushParams({
          'name': 'ipc_adprobe_ids',
            'placement': 'before',
            'order': 5, 'fn': function(val) {
              if (!val) {
                val = '';
              }
              return val;
            }});

      };
    }
    for (i=0; i < adprobe.content_unit_ids.length; i+=1) {
      content_unit_id_name = 'wl' + adprobe.content_unit_ids[i] + 'camp';
      ipc.adtech.pushTransferVariables({
        'name': content_unit_id_name,
          'value': window[content_unit_id_name]
      });
    }
  }());
</script>

