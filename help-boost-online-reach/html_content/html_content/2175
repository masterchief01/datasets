        /****POST*****/

		$('body.slug-portfolio #content .post') 
		.css( {backgroundColor: "white"} )
		.mouseover(function(){
			$(this).stop().animate({backgroundColor:"black"}, {duration:200})
			
		})
		.mouseout(function(){
			$(this).stop().animate({backgroundColor:"white"}, {duration:800})
			
		})
