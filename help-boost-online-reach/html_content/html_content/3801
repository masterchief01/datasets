        var playerVersion = swfobject.getFlashPlayerVersion();
        var home_main_div = document.getElementById("home_main_div");
        function EmbedHome()
        {
            var flashvars = {};
            var params = { wmode: "transparent" };
            var attributes = {};
            if (swfobject.hasFlashPlayerVersion("9.0.0"))
                swfobject.embedSWF("flash/swf/HomeTout.swf", "home_tout", "615", "380", "9.0.0", "", flashvars, params, attributes);
            else
                document.getElementById('home_tout').innerHTML = '<img src="flash/alt_images/home.jpg" alt="" />';
        }
        function CylToutLinkMouseOver() {
            document.getElementById('cyl_tout_link').style.display = 'none';
            document.getElementById('cyl_tout_hover_link').style.display = '';
        }
        function CylToutLinkMouseOut() {
            document.getElementById('cyl_tout_link').style.display = '';
            document.getElementById('cyl_tout_hover_link').style.display = 'none';
        }
        function EmbedHome1()
        {
            home_main_div.style.marginLeft = "0px";
            home_main_div.style.marginTop = "0px";
            var flashvars = { linkURL: "fwi/fwi.aspx" };
            var params = { wmode: "transparent" };
            var attributes = {};
            if (swfobject.hasFlashPlayerVersion("9.0.0") && readCookie('HomeToutsDisplayed') != "1")
                swfobject.embedSWF("flash/swf/HomeCylinder_en.swf", "home_tout", "607", "420", "9.0.0", "", flashvars, params, attributes);
            else
                document.getElementById('home_tout').innerHTML = '<div style="width: 607px; height: 322px;"><img src="flash/alt_images/homeCylinder_en.jpg" alt="" /></div>' +
                                        '<img id="cyl_tout_link" src="flash/alt_images/homeCylinder_en_low.jpg" alt="" style="border-style: none" />' +
                                        '<img id="cyl_tout_hover_link" src="flash/alt_images/homeCylinder_en_low_hover.jpg" alt="" style="display: none; border-style: none" /></div>' +
                                        '<div onmouseover="javascript:CylToutLinkMouseOver();" onmouseout="javascript:CylToutLinkMouseOut();"' + 
                                        ' onclick="location = \'fwi/fwi.aspx\'" style="cursor: pointer; margin-left: 410px; top: 320px; position: absolute; width: 170px; height: 80px"><img' +
                                        ' src="assets/images/general/transparent.gif" height="80" width="170" /></div>';
        }
