    var menu_items = new Array();
    var overrideBehavior = false;
    function FadeMenu() { if (!overrideBehavior) { for (var i = 0; i < menu_items.length; i++) { var elm = document.getElementById(menu_items[i]); elm.style.opacity = 0.5; } } }
    function UnFadeMenu() { if (!overrideBehavior) { for (var i = 0; i < menu_items.length; i++) { var elm = document.getElementById(menu_items[i]); elm.style.opacity = 1; } } }
    function UnFadeMenuById(id) { if (!overrideBehavior) { var elm = document.getElementById(id); elm.style.opacity = 1; } }
</script>
