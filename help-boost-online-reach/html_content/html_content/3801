        function EmbedBookExplorer()
        {
            var flashvars = { modulesDirectory: "flash/modules/" };
            var params = { wmode: "transparent" };
            var attributes = {};
            if (swfobject.hasFlashPlayerVersion("9.0.0"))
                swfobject.embedSWF("flash/swf/BookExplorer.swf", "book_explorer", "600", "380", "9.0.0", "", flashvars, params, attributes);
            else
                document.getElementById('book_explorer').innerHTML = '<img src="flash/alt_images/bookExplorer.jpg" alt="" />';
        }
        function EmbedMap()
        {
            var flashvars = { xmlPath: "flash/data/en/fwiMap/" };
            var params = { wmode: "transparent" };
            var attributes = {};
            if (swfobject.hasFlashPlayerVersion("9.0.0"))
                swfobject.embedSWF("flash/swf/FwiMap.swf", "fwiMap", "980", "650", "9.0.0", "", flashvars, params, attributes);
            else
