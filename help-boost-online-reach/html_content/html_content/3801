                $('ctl00_ContentPlaceHolderMain_ImageFlyingPig').setStyle('opacity', 0);
                fadePig = new Fx.Tween('ctl00_ContentPlaceHolderMain_ImageFlyingPig', {
                    property: 'opacity',
                    duration: 1000,
