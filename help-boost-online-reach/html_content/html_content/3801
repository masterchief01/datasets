                    this.piggy = new FlyingPiggy();
                    setTimeout("this.piggy.Callbacks.interrupt.run('bottom', this.piggy)", 12500);
                    setTimeout("fadePig.start(0)", 13500);
                    setTimeout("$('flying_pig').setStyle('display', 'none')", 14500);
                }
                function ToggleClick() {
                    document.getElementById('flying_pig').style.display = 'none';
                    if (readCookie('home_pig') == null) createCookie('home_pig', 'true', '200');
                }
            </script>
