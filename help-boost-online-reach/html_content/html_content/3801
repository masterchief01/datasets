        }
        var UtfFlashEmbedded = false;
        function EmbedUtfFlash()
        {
            if (!UtfFlashEmbedded)
            {
                UtfFlashEmbedded = true;
                EmbedBookExplorer();
                EmbedMap();
            }
        }
        function StartPorkVideo()
        {
            doAboveFoil();
            document.getElementById("ctl00_ContentPlaceHolderMain_ImagePorkVidFpo").style.display = "none";
            var flashvars = { 'file': '/en-US/../Media/PorkQuest_Website.flv',
                'autostart': 'true', 'stretching': 'exactfit', 'allowscriptaccess': 'always' };
            var params = { wmode: "transparent", allowfullscreen: "false" };
            var attributes = {};
            swfobject.embedSWF("scripts/videobox/swf/player.swf", "pork_video", "640", "360", "9.0.0", "", flashvars, params, attributes);
            return false;
        }
