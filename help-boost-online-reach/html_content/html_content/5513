    <script type="text/javascript">
        $(document).ready(function () {
            //dostuff
            IncrementViews();
            GetLinks();
            if($.cookie('ExternalSort') == null) {
                $.cookie("ExternalSort", "Recent", { expires: 10, path: '/' });
            }
        });

        function IncrementViews() {
            var url = "/i/ajax/IncrementContentViews/2/14329";
            $.post(url);
        }

        function GetLinks() {
            var geturl = "/i/ajax/GetPrevNext/2/14329?sort=" + $.cookie('ExternalSort');

            $.ajax({
                type: "GET",
                url: geturl,
                data: "",
                success: function (msg) {
                    $("#prevbutton").attr('href', msg.prevpage);
                    $("#nextbutton").attr('href', msg.nextpage);
                }
            });
        }

    </script>

    
    <script type="text/javascript">

        

        $.fn.preload = function () {
            this.each(function () {
                $('<img/>')[0].src = this;
            });
        }

        //preload images
        jQuery(document).ready(function () {
            var geturl = "/i/ajax/GetPrevNextImages/2/14329?sort=" + $.cookie('ExternalSort');

            $.ajax({
                type: "GET",
                url: geturl,
                data: "",
                success: function (msg) {
                    $([msg.previmage, msg.nextimage]).preload();
                }
            });

        });


        $("ul.subnav > li").each(function () {
            var text = $(this).text();
            if (text.toLowerCase() == $.cookie('ExternalSort').toLowerCase()) {
                $(this).addClass("active");
                //$.cookie("ExternalSort", text, { expires: 10, path: '/' });
            }

        });

        $("ul.subnav > li").click(function () {
            var text = $(this).text();
            if (text != "Home")
            {
                $.cookie("ExternalSort", text, { expires: 10, path: '/' });
            }
            
        });



    </script>
    
    
