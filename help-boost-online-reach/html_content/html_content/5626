	        <li class="r4"><a onclick="OnRate('ratings', '1.1063203', '4', 'http://www.iol.co.za:80/news/world/bin-laden-s-bloodied-face-broadcast-on-tv-1.1063203')" title="4 Stars">4</a></li>
	        <li class="r5"><a onclick="OnRate('ratings', '1.1063203', '5', 'http://www.iol.co.za:80/news/world/bin-laden-s-bloodied-face-broadcast-on-tv-1.1063203')" title="5 Stars">5</a></li>
        <!--<![endif]-->        
      </ul> <!-- .rating -->
    </li>
        <li class="avRating"><ins>Average reader rating</ins> <em>(0 votes)</em> <span class="stars0">0 Stars</span></li>
  </ul>
</div>

<script type="text/javascript">
    
	function OnRate(ratingsid, id, level, redirect) {	
		
		jQuery.getJSON('/ajax?action=rate&cookieSet=true&id='+id+'&level='+level, testVote);
	}
	
	function testVote(json) {
	
	    var level = parseInt(json);
        var totalvotes = parseInt(0 + 1)
        var ratingaverage = Math.round((0 + level)/totalvotes)
        
        var HTML = "<li class=avRating>"
        HTML += "<ins>Average reader rating</ins>"
        HTML += "<em>(" + totalvotes + " votes)</em>"
        HTML += "<span class=stars" + ratingaverage + ">" + ratingaverage + " Stars</span>"
        HTML += "</li>"
        
        document.getElementById("ratings").innerHTML = HTML;
	   
	}

</script>         
