<script type="text/javascript">

document.write(' <div class="forma_email"> <div class="email_alerts_head"> <div class="email_alerts_head_img"> <img src="/images/sidebar/email.gif" style="margin:0px; padding:0px;" /> </div> <div class="email_alerts_head_text" style="color:#E49400;"> Get Breaking News Alerts </div> </div> <div class="email_alerts_subsForm"> <form id="media_form"> <input type="text" class="input_edit" id="subscribe_user_email" /> <input type="button" onClick="QuickSubscribeUser.pop(\'media_form\', \'f29\'); return false;" style="background-color:#E49400;" class="email_alert_button" value="SIGN UP" /> </form> <div class="email_alert_neverSpam" style="color:#E49400;">never spam</div> </div> <div class="clear"></div> ');
