        var a = arguments, o = null, i, j, d;
        for (i = 0; i < a.length; ++i) {
          d = a[i].split(".");
          o = SM;
          // SM is implied, so it is ignored if it is included
          for (j = (d[0] === "SM") ? 1 : 0; j < d.length; ++j) {
            o[d[j]] = o[d[j]] || {};
            o = o[d[j]];
          }
        }
        return o;
