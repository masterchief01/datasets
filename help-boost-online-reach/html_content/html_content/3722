if(maxslide >1)
{
	document.getElementById('sldthumb2').src=slides[sldcounter+1][0] ;
}
function moveNext()
{
	sldcounter++ ;
	if(sldcounter < maxslide)
	{
		for(i=0; i < maxslide; i++ )
		{
			document.getElementById('slide_'+i).style.display ='none' ;
			document.getElementById('slideTitle_'+i).style.display ='none' ;
		}
		document.getElementById('slideTitle_'+sldcounter).style.display ='block' ;
		//document.getElementById('div_dispalyslide').innerHTML = document.getElementById('slide_'+sldcounter).innerHTML ;
		document.getElementById('slide_'+sldcounter).style.display ='block';
		
		document.getElementById('sldthumb1').src=slides[sldcounter-1][0] ;
		document.getElementById('sldthumb1').className='curhand' ;		
		if(sldcounter < maxslide-1 )
		{
			document.getElementById('sldthumb2').src=slides[sldcounter+1][0] ;
		}
		else
		{
			if((msgid == "movies" || msgid == "getahead") && (slides[0][0].indexOf("trans.gif") == -1))
			{
				document.getElementById('sldthumb2').src="http://imworld.rediff.com/worldrediff/pix/blank.gif"; 		
				document.getElementById('sldthumb2').style.width = '1px'	;
				document.getElementById('sldthumb1').className = 'curhand'	;
				if(document.getElementById('sstemp_div'))
				{
					document.getElementById('sstemp_div').innerHTML = "<div class='alignC'><a href=\""+org_domain+"/slidesongs/article/aHR0cDovL3d3dy5yZWRpZmYuY29tL3Nwb3J0cy9zbGlkZS1zaG93L3NsaWRlLXNob3ctMS1uYmEtc2thdGluZy1jaGVlcmxlYWRlcnMtYWMtbWlsYW4temxhdGFuLWlicmFoaW1vdmljLXNlcmJpYS1qZWxlbmEtamFua292aWMvMjAxMjA0MjMuaHRt\"><div class='f14 blue bold'>Watch this<br />with music &gt;</div></a><div style='width:85px;height:65px;overflow:hidden;margin:0 auto;'><a href=\""+org_domain+"/slidesongs/article/aHR0cDovL3d3dy5yZWRpZmYuY29tL3Nwb3J0cy9zbGlkZS1zaG93L3NsaWRlLXNob3ctMS1uYmEtc2thdGluZy1jaGVlcmxlYWRlcnMtYWMtbWlsYW4temxhdGFuLWlicmFoaW1vdmljLXNlcmJpYS1qZWxlbmEtamFua292aWMvMjAxMjA0MjMuaHRt\"><img src='"+slides[0][0]+"' width=85 /></a></div></div>" ;	
				}
			}
			else
			{
				document.getElementById('sldthumb2').src="http://imworld.rediff.com/worldrediff/pix/blank.gif";
				document.getElementById('sldthumb2').className = '';
			}
			
			document.getElementById('sldthumb1').className = 'curhand';
			
		}
	    reloadAd(sldcounter);
	}
	else
	{
		sldcounter =  maxslide-1 ;
	}
	window.scroll(0,0);
}
function movePrev()
{
	sldcounter-- ;
	if(sldcounter >= 0 )
	{
		for(i=0; i<maxslide; i++ )
		{
			document.getElementById('slide_'+i).style.display ='none' ;
			document.getElementById('slideTitle_'+i).style.display ='none' ;
		}
		document.getElementById('slideTitle_'+sldcounter).style.display ='block';
		//document.getElementById('div_dispalyslide').innerHTML = document.getElementById('slide_'+sldcounter).innerHTML;
		document.getElementById('slide_'+sldcounter).style.display ='block';
		document.getElementById('sstemp_div').innerHTML = "";
		document.getElementById('sldthumb2').style.width = '138px';
		if(sldcounter > 0 )
		{
			document.getElementById('sldthumb1').src=slides[sldcounter-1][0] ;
			document.getElementById('sldthumb2').className = 'curhand'	;
		}
		else
		{
			document.getElementById('sldthumb1').src="http://imworld.rediff.com/worldrediff/pix/blank.gif"; 		
			document.getElementById('sldthumb1').className = ''	;
		}
		document.getElementById('sldthumb2').src=slides[sldcounter+1][0] ;
		reloadAd(sldcounter);
	}
	else
	{
		sldcounter =  0 ;
	}
	window.scroll(0,0);
}
function getIEVersionNumber() {
    var ua = navigator.userAgent;
    var MSIEOffset = ua.indexOf("MSIE ");
    if (MSIEOffset == -1) {
        return 0;
    } else {
        return parseFloat(ua.substring(MSIEOffset + 5, ua.indexOf(";", MSIEOffset)));
    }
}
function reloadAd(sldcounter)
{
	if((getIEVersionNumber() < 6) && (navigator.userAgent.indexOf("MSIE") != -1))
	{
		metriccall_img(sldcounter);
		return false;
	}
	if(document.getElementById("frame_ad_BOX02"))
	{
		document.getElementById("frame_ad_BOX02").src = document.getElementById("frame_ad_BOX02").src;
	}
	if(document.getElementById("frame_ad_BOX01"))
	{
		if(msgid == "movies")
		{
			document.getElementById("frame_ad_BOX01").src = "http://imworld.rediff.com/worldrediff/pix/rightAdmovies.html?http%3A%2F%2Fwww.rediff.com%2Fsports%2Fslide-show%2Fslide-show-1-nba-skating-cheerleaders-ac-milan-zlatan-ibrahimovic-serbia-jelena-jankovic%2F20120423.htm";
		}
		else if(msgid == "sports" || msgid == "cricket")
		{
			document.getElementById("frame_ad_BOX01").src = "http://imworld.rediff.com/worldrediff/pix/rightAdsports.html?http%3A%2F%2Fwww.rediff.com%2Fsports%2Fslide-show%2Fslide-show-1-nba-skating-cheerleaders-ac-milan-zlatan-ibrahimovic-serbia-jelena-jankovic%2F20120423.htm";
		}
		else if(msgid == "money")
		{
			document.getElementById("frame_ad_BOX01").src = "http://imworld.rediff.com/worldrediff/pix/rightAdmoney.html?http%3A%2F%2Fwww.rediff.com%2Fsports%2Fslide-show%2Fslide-show-1-nba-skating-cheerleaders-ac-milan-zlatan-ibrahimovic-serbia-jelena-jankovic%2F20120423.htm";
		}
		else if(msgid == "getahead")
		{
			document.getElementById("frame_ad_BOX01").src = "http://imworld.rediff.com/worldrediff/pix/rightAdgetahead.html?http%3A%2F%2Fwww.rediff.com%2Fsports%2Fslide-show%2Fslide-show-1-nba-skating-cheerleaders-ac-milan-zlatan-ibrahimovic-serbia-jelena-jankovic%2F20120423.htm";
		}
