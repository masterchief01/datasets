			document.write("<TABLE CELLSPACING=0 CELLPADDING=0 BORDER=0><TR><TD><IMG SRC=\"http://loc.rediff.com/cgi-bin/loc/setup/blank.gif\" BORDER=0 HSPACE=0 VSPACE=0></TD></TR></TABLE>");
		}
		if(showAds('world_top'))
		{
			OAS_AD('LB01');
		}
	}
	</script>
	
</div>
