		{
			document.getElementById("frame_ad_BOX01").src = "http://imworld.rediff.com/worldrediff/pix/rightAdnews.html?http%3A%2F%2Fwww.rediff.com%2Fsports%2Fslide-show%2Fslide-show-1-nba-skating-cheerleaders-ac-milan-zlatan-ibrahimovic-serbia-jelena-jankovic%2F20120423.htm";
		}
