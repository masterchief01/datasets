	}
	if(document.getElementById("frame_ad_LB01"))
	{
		if(msgid == "movies")
		{
			document.getElementById("frame_ad_LB01").src = "http://imworld.rediff.com/worldrediff/pix/topAdmovies.html?http%3A%2F%2Fwww.rediff.com%2Fsports%2Fslide-show%2Fslide-show-1-nba-skating-cheerleaders-ac-milan-zlatan-ibrahimovic-serbia-jelena-jankovic%2F20120423.htm";
		}
		else if(msgid == "sports"  || msgid == "cricket" )
		{
			document.getElementById("frame_ad_LB01").src = "http://imworld.rediff.com/worldrediff/pix/topAdsports.html?http%3A%2F%2Fwww.rediff.com%2Fsports%2Fslide-show%2Fslide-show-1-nba-skating-cheerleaders-ac-milan-zlatan-ibrahimovic-serbia-jelena-jankovic%2F20120423.htm";
		}
		else if(msgid == "money")
		{
			document.getElementById("frame_ad_LB01").src = "http://imworld.rediff.com/worldrediff/pix/topAdmoney.html?http%3A%2F%2Fwww.rediff.com%2Fsports%2Fslide-show%2Fslide-show-1-nba-skating-cheerleaders-ac-milan-zlatan-ibrahimovic-serbia-jelena-jankovic%2F20120423.htm";
		}
		else if(msgid == "getahead")
		{
			document.getElementById("frame_ad_LB01").src = "http://imworld.rediff.com/worldrediff/pix/topAdgetahead.html?http%3A%2F%2Fwww.rediff.com%2Fsports%2Fslide-show%2Fslide-show-1-nba-skating-cheerleaders-ac-milan-zlatan-ibrahimovic-serbia-jelena-jankovic%2F20120423.htm";
		}
