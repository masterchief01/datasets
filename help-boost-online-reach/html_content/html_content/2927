<script type='text/javascript'>
$(document).ready(function() {
  var sidebar = $('#shares_for_post_44840'),
      sidebar_top = sidebar.offset().top - parseInt(sidebar.css('margin-top'), 10) - parseInt(sidebar.css('margin-bottom'), 10),
      sidebar_height = sidebar.outerHeight(true),
      post = $('div.content'),
      post_bottom = post.offset().top + post.outerHeight(),
      position = 'default';
  
  function setSharesPosition() {
    var current_y = $(this).scrollTop();
    var post = $('div.content');
    var post_bottom = post.offset().top + post.outerHeight();
    var positionCSS = {
        'default': {},
        'fixed': {position: 'fixed', top: 0},
        'bottom': {position: 'absolute', top: post.outerHeight() + $('#prev-next-container').outerHeight(true)  + $('.categories-tags').outerHeight(true) + $('.category-footer-image').outerHeight(true)}
    };
    if (current_y < sidebar_top) 
      position = 'default';
    else if (current_y + sidebar_height < post_bottom) 
      position = 'fixed';
    else 
      position = 'bottom';
    // Clear all styles, then set appropriately.
    sidebar.removeAttr('style');
    sidebar.css(positionCSS[position]);
  }
  // Only slide the sidebar on articles that are long enough.
  if (post_bottom + $('#prev-next-container').outerHeight(true)  + $('.categories-tags').outerHeight(true) + $('.category-footer-image').outerHeight(true) - sidebar_height > sidebar_top) {
    $(window).scroll(setSharesPosition);
    setSharesPosition();
  }
})
</script>
