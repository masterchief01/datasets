<script type="text/javascript">
	$(function(){
		var url = "http://www.good.is/post_service/neighbors/44840";
		var linkParams = "?utm_content=prev-next&utm_medium=post-page-bottom";
		if ( url ) {
			$.ajax({
					url: url,
					dataType: "json",
					cache: "false",
					success: function(data) {
						var prevHolder, nextHolder;
						var prev = data['previous'];
						var next = data['next'];
						var item = $('<a class="item"></a>');
						var label = $('<p class="label"></p>');
						var title = $('<span class="title"></span>');
						var thumb = $('<img class="thumbnail" />');
						
						$("#prev-next-container").empty();
						
						if ( prev['title'] && prev['href'] ) {
							prevHolder = item.clone();
							prevHolder.attr("href", data['previous']['href'] + linkParams );
							prevHolder.append( 
												label.clone()
												.append("Previous") 
											);
							if ( prev['thumbnail_url'] ) {
								prevHolder.append(
													thumb.clone()
													.attr("src", data['previous']['thumbnail_url'])
												);
							}
							prevHolder.append(
												title.clone()
												.append(data['previous']['title'])
											);
							$("#prev-next-container").append( prevHolder );
						}
						
						if ( next['title'] && next['href'] ) {
							nextHolder = item.clone();
							nextHolder.attr("href", data['next']['href'] + linkParams );
							nextHolder.append( 
												label.clone()
												.append("Next") 
											);
							if ( next['thumbnail_url'] ) {
								nextHolder.append(
													thumb.clone()
													.attr("src", data['next']['thumbnail_url'])
												);
							}
							nextHolder.append(
												title.clone()
												.append(data['next']['title'])
											);
							$("#prev-next-container").append( nextHolder );
						}
					}
				});
		}
	});
</script></div><div class="category-footer-image"><a href="/series/no-more-dirty-looks"><img src="http://cms0.good.is/images/series/no-more-dirty-looksFooterSm.jpg" alt="Read more" /></a></div>		<div class="categories-tags">
