<script type="text/javascript">
     $(document).ready(function(){
		$("#Left, #Right").click(function(){
     		window.location=$(this).find("a").attr("href");
     		return false;
		});
     
	 		$(function(){			
				$(".swipe").swipe({
						fingers:1,
						threshold:400,
						swipeLeft: function() { 
									$(this).animate({ marginRight: "1060px", opacity: "0"}, "fast");
     								window.location=$("#Right").find("a").attr("href");
									},
						swipeRight: function() { 
									$(this).animate({ marginLeft: "1060px", opacity: "0"}, "fast");
     								window.location=$("#Left").find("a").attr("href");
									},
				});				
			});
	 });

    $(document).keydown(function (e) {
      var keyCode = e.keyCode || e.which,
          arrow = {left: 37, right: 39 };
      switch (keyCode) {
        case arrow.left:
		$(".swipe").animate({ paddingLeft: '1060px', opacity: '0'}, "fast");
          window.location = "/post/2125/bangin-bangs";
		  
        break;
        case arrow.right:
			$(".swipe").animate({ marginRight: '1060px', opacity: '0'}, "fast");
          window.location = "/post/2123/the-real-story-about-success";
        break;
      }
    });
</script>
   
