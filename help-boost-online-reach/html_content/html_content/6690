     <script type="text/javascript" charset="utf-8">
        jQuery(function($) {
            function initializeLocaleOverlay(trigger) {
                Etsy.LocaleOverlay.init({
                    locale: {"language":{"code":"en-US","name":"English (US)","translation":"English (US)","is_detected":true,"is_default":true,"is_EU_lang":false},"currency":{"currency_id":840,"code":"USD","name":"United States Dollar","number_precision":2,"symbol":"$","listing_enabled":true,"browsing_enabled":true,"buyer_location_restricted":false,"rate_updates_enabled":true,"is_detected":true,"is_default":true},"region":{"code":"US","name":"United States","translation":"United States","is_detected":true,"is_default":true,"is_EU_region":false}},
                    trigger: trigger
                });
                $("#locale-select-overlay").overlay({load:true});
                $('#footer-spinner').hide();
            }

            function loadAndInitializeLocaleOverlay(trigger) {
                if (Etsy.LocaleOverlay) {
                    // Locale js logic is ready
                    initializeLocaleOverlay(trigger);
                } else {
                    // Load locale js logic
                    Etsy.loader.require('overlays/locale');
                    Etsy.loader.complete(function() {
                        initializeLocaleOverlay(trigger);
                    });
                }
            }

            function getOverlaySourceURL() {
                return window.location.protocol == 'https:' ? "/locale/overlay_secure" : "/locale/overlay";
            }

            function showLocaleOverlay(e) {
                if ($("#locale_overlay_container").is(':empty')) {
                    $('#footer-spinner').css('display', 'inline-block');
                    $("#locale_overlay_container").load(getOverlaySourceURL(), null, function() {
                        loadAndInitializeLocaleOverlay(e.target);
                    });
                } else {
                    $("#locale-select-overlay").overlay({load:true});
                }
                try {
                    Etsy.GA.track(['_trackEvent', 'Locale selector', 'footer']);
                } catch (e) {
                    // Etsy.GA not working or available, pass for now so we don't break functionality.
                }
            }

            $("a[href=#language-select],a[href=#region-select],a[href=#currency-select]").live('click', function(e) {
                e.preventDefault();
                showLocaleOverlay(e);
            });

        });
    </script>

</div>

        

