                element: i,
                type: h,
                value: k
            }
        }
        function g(k) {
            var j = document.getElementsByTagName("A");
            for (var h = 0; h < j.length; h++) {
                if (j[h].href.indexOf("#comments") >= 0) {
                    k(j[h])
                }
            }
        }
        d.domain = "disqus.com";
        d.forum = "";
        d.getCount = function () {
            var m, h, k, j;
            var l = [],
                o = 0,
