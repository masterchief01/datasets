					var title=jQuery(this).attr('data-title');
					var link=jQuery(this).attr('data-link');
					
					if(jQuery(this).hasClass('large_entry')){
						var thumbUrl = jQuery(this).find('.widget_thumb').attr('longdesc');
						var _h = 190;
						var _w = 260;
						var _pattern = /^([^_]+)(_[0-9ct]+)(_[0-9]+_[0-9]+)?(\.)?(\w{3})$/;
						var _matches = _pattern.exec(thumbUrl);
						var _newStr = _matches[1] + _matches[2] + '_'+_w +'_'+ _h + _matches[4] + _matches[5];
						jQuery(this).find('.widget_thumb').attr('longdesc', _newStr).attr('src', _newStr);
					}
					
					jQuery('a', this).click(function(e){ndn_lightbox_launch({ 'title': title, 'PageURL': link, 'ReferrerURL': link },link); e.preventDefault(); e.stopPropagation(); return false;});
					
