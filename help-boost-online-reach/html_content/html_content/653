					success: function(MAILCHIMP)
						{
						if(MAILCHIMP == '1')
							{
							$('#emi-status-1c0cbf6dc0-0').html('<div class="emi-success"><p>Thank You for subscribing! Check your email for the confirmation message.</p></div>');
							$('#emi-mailchimpFormContainerInner_1c0cbf6dc0-0').slideUp('fast', function(){
								$('#emi-status-1c0cbf6dc0-0').slideDown('fast');
							});
							}
						else
							{
							$('#emi-submit_1c0cbf6dc0-0').removeAttr('disabled');
							$('#emi-status-1c0cbf6dc0-0').html('<div class="emi-error"><p>There was an error submiting the form -- please try again!</p></div>');
							$('#emi-status-1c0cbf6dc0-0').slideDown('fast');
							}
						}
				});
				}
			return false;
		});
		$('.emi-field-type-date').datepicker({
			changeMonth:	true,
			changeYear:		true,
			yearRange:		((new Date).getFullYear()-100)+':'+((new Date).getFullYear()),
			dateFormat: 	'yy-mm-dd'
		});
		$('#emi-submit_1c0cbf6dc0-0').click(function(e){
			e.preventDefault();
			$('#emi-mailchimp-form_1c0cbf6dc0-0').submit();
			return false;
		});
		$('#ui-datepicker-div').addClass('emi-mailchimpFormDatepickerContainer');
	});
</script>

