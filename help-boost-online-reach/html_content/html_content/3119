function popup(pagina, windowname) {
	
	wleft = (screen.width - 715) / 2;
    wtop = (screen.height - 500) / 2;
	
	 if (wleft < 0) {
		w = screen.width;
		wleft = 0;
	  }
	  if (wtop < 0) {
		h = screen.height;
		wtop = 0;
	  }
	
	window.open(pagina, windowname, 'left=' + wleft + ', top=' + wtop + ', ' +
    ' location=0, directories=0, status=0, resizable=0, menubar=no,width=715,height=500,scrollbars=yes');
}

