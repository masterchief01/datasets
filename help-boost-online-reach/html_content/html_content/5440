<script>
listTotal = 0;
function numReplace(toReplace){
var numGrphx = new Array(	"assets/dailies/daily_dish/0_magnet.gif",
"assets/dailies/daily_dish/1_magnet.gif",
"assets/dailies/daily_dish/2_magnet.gif",
"assets/dailies/daily_dish/3_magnet.gif",
"assets/dailies/daily_dish/4_magnet.gif",
"assets/dailies/daily_dish/5_magnet.gif",
"assets/dailies/daily_dish/6_magnet.gif",
"assets/dailies/daily_dish/7_magnet.gif",
"assets/dailies/daily_dish/8_magnet.gif",
"assets/dailies/daily_dish/9_magnet.gif");

for(i = 1; i <= toReplace; i++){
if(i > 9){
var strNum = i.toString();
var fNum = strNum.substring(0,1);
var sNum = strNum.substring(1,2);
document.getElementById("dirNum_"+i).innerHTML = "<img src='/" + numGrphx[parseInt(fNum)] + "'>" + "<img src='/" + numGrphx[parseInt(sNum)] + "'>";
}else{
document.getElementById("dirNum_"+i).innerHTML = "<img src='/" + numGrphx[i] + "'>";
}
}
}
</script>
