$(".videos-menu-item").append('<div style="position:relative;"><div class="new" style="background: url(http://www.parentsconnect.com/assets/nav/PC-NEW-div.png) no-repeat; width:26px; heigh: 15px; text-indent:-999em; position: absolute; top:-35px; margin-left: 50%;">new</div></div>');
