window.calUrl = '/templates/archives/archive.php';
window.calParamNames = 'thingId,date';
window.calParamValues = '139941248,dateSelected';

window.calLinks = "1111111";

window.startDay = "0";
window.startMonth = "1";
window.startYear = "1993";

window.selDay = 15;
window.selMonth = 11;
window.selYear = "2011";

window.tableCellSpacing = "2";

document.frmblogCalendar.myMonth.options.selectedIndex = window.selMonth;
for (myy_i=0;myy_i<document.frmblogCalendar.myYear.options.length;myy_i++)
{
    if (document.frmblogCalendar.myYear.options[myy_i].value==window.selYear)
    {
        document.frmblogCalendar.myYear.options.selectedIndex = myy_i;
    }
}

setCalendarOnLoadValues();
doCalendarLoad();

function changeYear(sel)
{
    window.selDay = '';
    window.selYear = sel.options[sel.selectedIndex].value;
    setCalendarOnLoadValues();
    doCalendarLoad();
}

function changeMonth(sel)
{
    selDay = '';
    window.selMonth = sel.options[sel.selectedIndex].value;
    setCalendarOnLoadValues();
    doCalendarLoad();
}
