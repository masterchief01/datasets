<script>
	var mobileweb = function () {
		/* Read cookie if jquery cookie plugin is supported this may not be required */
		function readCookie (cookieName) {
			var cookieMatchResult = document.cookie.match ( '(^|;) ?' + cookieName + '=([^;]*)(;|$)' );

			if (cookieMatchResult) {
				return unescape(cookieMatchResult[2]);
			} else {
				return null;
			}
		}

		return {
			showViewMobile: function showViewMobileButton () {
				var mobileView=readCookie('mobileview');
				if (mobileView!=null && mobileView=='desktop') {
					var mobileViewDiv = $('#mobileweb');
					if (mobileViewDiv!=null) {
						mobileViewDiv.css('display','block');
						mobileViewDiv.css('display','visibile');
					}
				}
			}
		};
	}();

	$(document).ready(mobileweb.showViewMobile());
</script>
