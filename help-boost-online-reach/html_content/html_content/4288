                    function querySt(ji) {
    hu = window.location.search.substring(1).toLowerCase();
    ji = ji.toLowerCase();
    gy = hu.split("&");
    rt = '';
    cm = '';
    for (i=0;i<gy.length;i++) {
        ft = gy[i].split("=");
        if (ft[0] == ji) {
            rt += cm + ft[1];
            cm = ',';
        }
    }
    return rt;
}

