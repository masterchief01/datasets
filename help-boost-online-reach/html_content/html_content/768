    InstructJS.load(["prototype"], function () {
      var id = "EJMOXBAFTDBMYS4";
      var keywords = ["potato","kale","spinach","vegan","nutritional yeast","fat free"];
      var categories = ["food vegetarian-and-vegan"];

      //

      // Set up View All link
      // We do this in script because we need to do the output of keywords
