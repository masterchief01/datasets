    InstructJS.load(["tabs"], function () {
    var id = "EJMOXBAFTDBMYS4";
	  var entryType = "INSTRUCTABLE";

    // Set up tabs
    var tabs = $A(
      $('meta-info-sidebar-box').down("ul").getElementsByTagName("li")
    );

    

    new Tabs({
      box : $("meta-info-sidebar-box"),
