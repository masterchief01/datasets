    <div class="mod mod-ingredients">
      <h2 class="mod-title title-ingredients pngfix">
        Ingredients
      </h2>
      <div class="media-wrap">
        <div class="top">
          <div class="btm content">
            <table class="ingredient-tbl">
              <tr>
                <td class="col-one ">
                    <span>1 cup (2 sticks)</span>
                    sweet butter, softened
                </td>
                <td class="col-two "></td>
                <td class="col-three ">
                      <span>1 cup</span>
                      cream cheese, softened (for frosting)
                </td>
              </tr>
              <tr>
                <td class="col-one ">
                    <span>1 cup</span>
                    superfine sugar
                </td>
                <td class="col-two "></td>
                <td class="col-three ">
                      <span>1.5 cups</span>
                      cpnfectioner's sugar, sifted (for frosting)
                </td>
              </tr>
              <tr>
                <td class="col-one ">
                    <span>2 cups</span>
                    self-rising flour
                </td>
                <td class="col-two "></td>
                <td class="col-three ">
                      <span>1/3 cup</span>
                      honey (for frosting)
                </td>
              </tr>
              <tr>
                <td class="col-one ">
                    <span>4</span>
                    eggs
                </td>
                <td class="col-two "></td>
                <td class="col-three ">
                      <span></span>
                      blue food coloring (for frosting)
                </td>
              </tr>
              <tr>
                <td class="col-one last">
                    <span>1 tsp</span>
                    vanilla extract
                </td>
                <td class="col-two last"></td>
                <td class="col-three last">
                      <span>2 tbsp</span>
                      dried lavender flowers (for frosting)
                </td>
              </tr>
            </table>
            <div class="ingredient-footer">
              <div class="yield">
                <span>
                  Yield:
                </span>
                18
                Servings
              </div>
              <div class="shopping_list_add_item">
