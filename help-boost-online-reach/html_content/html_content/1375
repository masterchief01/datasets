<script type="text/javascript">
function page_popular(page, section ) {
  var url = "http://www.oprah.com/modules/mod_most_pop_pager.inc";
  new Ajax.Updater('quick_popular_content', url, {
    method: 'get',
    parameters: {
            page:        page,
            section:     section
    },
    evalScripts: true
  });
}
</script>
