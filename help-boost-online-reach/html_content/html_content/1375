      <div id="article_contents" class="clearfix" style="margin-top:5px; ">
        <script type='text/javascript' src='http://myown.oprah.com/js/2012020101/min/carousel.js'></script>
<style type="text/css">
.slide_thumb {width:90px; height:62px; float:left; padding-left:10px; padding-right:10px;}
  .slide_thumb .box {width:90px; height:62px; position:relative; overflow:hidden; background-color:#ffffff; text-align:center; font-size: 0}
  .selected_slide .border {position:absolute; top:0; left:0; width:84px; height:56px; border:3px solid #A59EC8; }
