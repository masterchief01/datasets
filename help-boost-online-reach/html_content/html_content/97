<script language="javascript" type="text/javascript">

var storySectionId = 1;
var storyShowPopular = 'N';
var storyPopularType = 'None';
var storySiteId = 0;
var storyTagId = 0;
var storyUserId = 0;
var storyTagGroupId = 0;
var storyCategoryId = 0;
var storyChannelId = 0;
var storyType = '';
var storyLinkStoryType = '';
var storyDisplayAdSection = 'media';
var storyLastRefreshDate = '4/6/2011 2:17:42 AM';
var storyFirstStoryPubDate = '4/6/2011 1:57:54 AM';
var storyRows = 3;
var storyCols = 3;
var storySquareWidth = 215;
var storySquareHeight = 143;
var storyHeight = 0;
var storyRowsToGetInitial = 3;
var storyRowsToGetScroll = 3;
var storyRowsTotal = storyRowsToGetInitial;
var storyAutorefreshTime = 300000;
var storyClickToScroll = true;
var storyHardsoft = 5;
var storyShowGridLeaderboardAds = false;
var storyStoryView = 'G';
var isUserGrid = false;
var storyShowNBUFrontPage = false;
var storyShowStats = false;
var storyStartDate = '';
var storyEndDate = '';
var storyPickADate = '';
var storyStoryId = '63869';

StoryGetRowsBuffer();
StoryStartAutorefresh();

if (jQuery('#storyCalendarContainer') != null && jQuery('#storyCalendarContainer') != undefined) {
    var c = new Calendar({outputObj: "storyCalendarContainer",clickListener:GetCalendarDate});
    c.render();
}

if (storyClickToScroll) {
    //Start capturing the scroll event for the main window
    jQuery(window).bind('scroll', function () { if (loaded) StoryCheckAutorefreshScroll(); });
}
else {
    //Start capturing the scroll event for the grid div
    jQuery('#storyGrid').bind('scroll', function () { if (loaded) StoryHandleGridScroll('media'); });
}
</script>

<!-- END: StoryGrid -->

