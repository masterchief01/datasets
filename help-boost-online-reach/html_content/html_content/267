            <script type="text/javascript" src="http://w.sharethis.com/button/buttons.js"></script>
            <script type="text/javascript">
                stLight.options({
                    publisher:'8e528737-1b85-4ce3-859f-8b5a2246c67d'
                });
                
                jQuery(document).ready(function(){
                    var shareUrl = document.URL;
                    var fbUrl = document.URL;
                    var twitterUrl = document.URL;
                    var fbLikeMarkup = '';
                    var emailMarkup = '';
                    var twitterMarkup = '';
                    var fbMarkup = '';
                    var pvOptions = '';
                    var pvOptionsFb = 'ei=79';
                    var pvOptionsTwitter = 'ei=80';
                    if(getQueryVariable('ds')) {
                        var newDs = Number(getQueryVariable('ds')) + 1;
                        var newStFb = 'fb';
                        var newStTwitter = 'twitter';
                        var ai = getQueryVariable('ai');
                        var zi = getQueryVariable('zi');
                        var st = getQueryVariable('st');
                        var pageView = new BrkPageView();
                        pvOptions = 'ai='+ai+'&zi='+zi+'&ds='+newDs+'&ci='+sGlobalContentID;
                        pvOptionsFb = 'ai='+ai+'&zi='+zi+'&ds='+newDs+'&st='+newStFb+'&ci='+sGlobalContentID+'&ei=79';
                        pvOptionsTwitter = 'ai='+ai+'&zi='+zi+'&ds='+newDs+'&st='+newStTwitter+'&ci='+sGlobalContentID+'&ei=80';
                        shareUrl = document.URL.replace(/(ds=)[^\&]+/, '$1' + newDs);
                        fbUrl = escape(shareUrl.replace(/(st=)[^\&]+/, '$1' + newStFb));
                        twitterUrl = escape(shareUrl.replace(/(st=)[^\&]+/, '$1' + newStTwitter));
                        shareUrl = escape(shareUrl);
                    }
                    
                    // Create FB Like                
                    if (typeof useDarkScheme == "undefined") {
                        // Use light scheme
                        fbLikeMarkup += '<fb:like href="'+document.URL+'" send="false" width="307" show_faces="false" font="Arial"></fb:like>';
                    } else {
                        // Use dark scheme
                        fbLikeMarkup += '<fb:like href="'+document.URL+'" send="false" width="307" show_faces="false" font="Arial" colorscheme="dark"></fb:like>';
                    }
                    jQuery('#newShare-fbLike').append(fbLikeMarkup);
                    
                    // Create E-mail Share
                    emailMarkup += '<span rel="notip" class="st_email_custom" title="E-mail to a friend">';
                    emailMarkup += '<img src="http://media1.break.com/break/img/site/sharebar-btn-email24.png" />';
                    emailMarkup += '</span>';
                    jQuery('#newShare-email').append(emailMarkup);
                    
                    // Create Twitter Share
                    twitterMarkup += '<a onClick="pageView.addEvent(\''+pvOptionsTwitter+'\');_gaq.push([\'_trackEvent\', \'click\', \'twittercdp_new\']);window.open (this.href, \'child\', \'height=450,width=550\'); return false;" target="_blank" rel="notip" title="Share on Twitter" href="http://twitter.com/share?url='+twitterUrl+'">';
                    twitterMarkup += '<img src="http://media1.break.com/break/img/site/sharebar-btn-twitter24.png" />';
                    twitterMarkup += '</a>';
                    jQuery('#newShare-twitter').append(twitterMarkup);
                    
                    // Create FB Share
                    fbMarkup += '<a onClick="pageView.addEvent(\''+pvOptionsFb+'\');_gaq.push([\'_trackEvent\', \'click\', \'facebookcdp_new\']);window.open (this.href, \'child\', \'height=600,width=800\'); return false;" rel="notip" title="Share on Facebook" target="_blank" href="http://www.facebook.com/share.php?u='+fbUrl+'">';
                    fbMarkup += '<img src="http://media1.break.com/break/img/site/sharebar-btn-fb24.png" />';
                    fbMarkup += '</a>';
                    jQuery('#newShare-fbShare').append(fbMarkup);
                    
                    //ShareThis GA tracking
                    jQuery('#newShare-shareThis').click(function() {
                        _gaq.push(['_trackEvent', 'click', 'sharethiscdp_new']);
                    });
                    
                    //E-mail GA tracking
                    jQuery('.st_email_custom').click(function() {
                        _gaq.push(['_trackEvent', 'click', 'emailcdp_new']);
                    });
                    
                    FB.XFBML.parse (jQuery('#newShare-fbLike').get(0));
                });
            </script>               
