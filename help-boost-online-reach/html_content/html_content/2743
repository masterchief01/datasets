<script type="text/javascript" charset="utf-8">
OA_show('zone_49');
document.write('<br/>');
OA_show('zone_11');
if(jQuery('#followingAd').length){
	jQuery(function($){
	    var top = $('#followingAd').offset().top;
		var follow = false;
		if($.cookie){
			if($.cookie('stopScrollingAd') != 'true'){
				$(window).bind('scroll', adScrollListener);
				follow = true;
			}
		}
		function adScrollListener(event) {
			var y = $(this).scrollTop();
			//console.log('image at: '+top+ ' scroll at: '+y);
			if (y >= top) {
				var diff = y-top;
				scrollAd(diff+10);
			} else {
				scrollAd(0);
			}
	    }
		if(follow){
			$('#followingAd a:eq(0)').click(function(){
				$(window).unbind('scroll', adScrollListener);
				$(this).hide();
				$.cookie('stopScrollingAd', 'true', { path: '/', expires: 14 });
				return false;
			});
		}else{
			$('#followingAd a:eq(0)').remove();
		}
	    function scrollAd(pos){
	            $('#followingAd').clearQueue().stop(true).animate({"margin-top": pos + "px"}, 500);    
	    }
	});
}
</script>
