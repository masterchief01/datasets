	<form action="/customweb/ajax/subscribe_to_list.php" id="newsletter_signup">
	  <input id="nl-email" type="text" class="email_tb" name="email" />
	  <input id="nl-sub" type="submit" class="go_button" name="Submit Email" disabled="disabled" value="Sign Me Up!" />
	</form>
	<div class="clear"></div>
	<div class="note">Note: We will not share your email address with anyone.</div>
