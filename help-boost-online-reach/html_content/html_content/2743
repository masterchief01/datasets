<script type="text/javascript" >
jQuery(function($) {
	$('#nl-sub').attr('disabled', '');
	$('#newsletter_signup').submit(function(){
		var email_input = $('#nl-email').val();
		if(email_input != ''){
			$.get($(this).attr('action'), {email:email_input}, function(data){
				if(data == 'joined'){
					$('div.newsletter .note').fadeOut('slow', function(){
						$(this).html("Thanks for signing up for SMP's newsletter!").css({
							background : '#F0F8FB',
							margin : '5px',
							'margin-top' : '-10px'
						}).fadeIn()
					});
					$('#nl-email, #nl-sub').fadeOut('slow');
				}else{
					console.log('failed?');
				}
			});
		}
		return false;
	});
});
</script>
