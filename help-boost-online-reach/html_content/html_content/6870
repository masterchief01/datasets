<script>
YAHOO.Sports.Window.set("stories", "nfl-video-player");

YAHOO.Sports.storiesTabsInit = function() {
  YAHOO.Sports.tabs.init("stories");

  var domTabs = YAHOO.util.Dom.getElementsByClassName("tab", "li", "stories");
  var defaultTab = YAHOO.Sports.SessionCookie.get('oly_stories_tab');
  for(var i in domTabs) {
    if (i == defaultTab)
    {
      YAHOO.Sports.tabs.selectPane(null, {"tabsbase": "stories", "showPane": i});
    }
    YAHOO.util.Event.addListener(domTabs[i], "click", YAHOO.Sports.updateStoriesCookie, {"league": "oly", "index": i});
    YAHOO.util.Event.addListener(domTabs[i], "click", YAHOO.Sports.ultTrackClick, {"sec": "topstories_inpage", "slk": "comingsoon", "pos": (i +1)});
  }
}
YAHOO.util.Event.onContentReady("stories", YAHOO.Sports.storiesTabsInit);
</script>
