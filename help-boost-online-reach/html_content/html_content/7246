$(setWindowDimensions);

function setWindowDimensions () {
	if ($("div").hasClass("slideshow-content")) {		
			if(window.location.href.match(".com/news/anniversary/slideshow/")) {
				var pWidth = $(".slideshow-content").width() + 6;
				var pHeight = $(".slideshow-content").height() + 158;
				window.resizeTo(pWidth,pHeight);
			}
			
			else {
				var pWidth = $(".slideshow-content").width() + 6;
				var pHeight = $(".slideshow-content").height() + 48;	
				window.resizeTo(pWidth,pHeight);
			}
	}
}
</script>





