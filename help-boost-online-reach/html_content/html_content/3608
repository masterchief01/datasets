	$(".iconHolder").append("<div class='shadow'></div>");
	$(".iconHolder").hover(function() {
    	var that = this;
		var desc = $(that).find("p").html();		
        $(that).find("a").stop().animate({ marginTop: "-12px", marginBottom: "12px" }, 250, function() {
        	$(that).find("a").animate({ marginTop: "-8px", marginBottom: "8px"}, 200);
        });
        $(that).find(".shadow").stop().animate({ width: "80%", opacity: 0.25 }, 250);
		$(".textHolder").html(desc);
    },function(){
    	var that = this;
        $(that).find("a").stop().animate({ marginTop: "4px", marginBottom: "-4px" }, 250, function() {
        	$(that).find("a").animate({ marginTop: "0px", marginBottom: "0px" }, 200);
        });
       $(that).find(".shadow").stop().animate({ width: "100%", opacity: 1 }, 150);	   
	}); 
