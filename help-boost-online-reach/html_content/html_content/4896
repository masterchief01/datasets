<script type="text/javascript">
  var r29cookie = document.cookie;
  var val;
  
  if (r29cookie.indexOf("r29comstore=") != -1)
   {
      i = r29cookie.indexOf("r29comstore=");
      s = r29cookie.substring(i + 12);
      j = s.indexOf(";");
      if (j == -1)
      {
          val = s;
      }
      else {
          val = s.substring(0, j);
      }
  } else {
      val = Math.floor(Math.random() * 12345678);
      var date = new Date();
      date.setTime(date.getTime() + (365 * 24 * 60 * 60 * 1000));
      document.cookie = 'r29comstore=' + val + '; expires=' + date.toGMTString() + '; path=/';
  }
  
  var currentUrl = self.location.url;
  document.write(unescape("%3Cscript src='" + (document.location.protocol == "https:" ? "https://sb": "http://b") + ".scorecardresearch.com/beacon.js' %3E%3C/script%3E"));

