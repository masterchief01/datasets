		<script type="text/javascript" src="http://static1.cloudfront.goodinc.com/aa2a3b8eb3c5234056b2d41c558f5a5e2731f2f6/js/bottom.js"></script><script type="text/javascript"></script><script type="text/javascript">
//<![CDATA[

			$(function () {
				$('#shares_for_post_51094').load('/thin/index.php?action=shares&post_id=51094', function () {
					var shares = $(this),
						content = $('div.content'),
						sharesHeight = shares.outerHeight(true),
						sharesLeft = shares.offset().left,
						yMin = shares.offset().top - parseInt(shares.css('margin-top'), 10),
						yMax = content.offset().top + content.outerHeight(),
						position = 'default',
						positionCSS = {
							'default': {},
							'fixed': {position: 'fixed', top: 0},
							'bottom': {position: 'absolute', top: yMax - sharesHeight}
						};
					function setSharesPosition() {
						var w = $(this), x = w.scrollLeft(), y = w.scrollTop();
						if (y > yMin) {
							if (y + sharesHeight < yMax) {
								position = 'fixed';
							}
							else {
								position = 'bottom';
							}
						}
						else {
							position = 'default';
						}
						// Clear all styles, then set appropriately.
						shares.removeAttr('style');
						shares.css(positionCSS[position]);
						// Adjust for horizontal scroll.
						if (position === 'fixed' && x > 0) {
							shares.css({left: sharesLeft - x});
						}
					}
					// Only slide the shares box on articles that are long enough.
					if (yMax - sharesHeight > yMin) {
						$(window).scroll(setSharesPosition);
						setSharesPosition();
					}
				});
			});
//]]>
</script>
<script type="text/javascript">
//<![CDATA[

// Submit only valid newsletter forms via AJAX
$('#newsletter_form_157807').submit(function (e) {
	e.preventDefault();
	var $form = $(this),
		$email = this['data[NewsletterSubscriber][email]'],
		$error = $('#newsletterError'),
		message = GOOD.form.validate($email, 'email');

	if (message !== 'valid') {
		$error.html(message);
	} else {
		$.ajax({
			type: this.method,
			url: this.action,
			data: $form.serialize(),
			dataType: 'html',
			success: function (confirmation) {

				$form
					.add($error)
					.animate({ opacity:0 }, function () {
						$form
							.html('<div class="confirmation">' + confirmation + '</div>')
							.animate({ opacity:1 });
					});
			}
		});
	}

});
//]]>
</script>
