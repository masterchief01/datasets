				/* <![CDATA[ */

				function displayLocation(data) {
		          if(data.loc.city) {
		            var locHtml = 'your location: ' + data.loc.city.replace(/\+/g, ' ') + ', ' + data.loc.state.replace(/\+/g, ' ') + ' <span id="titleLocationZip">' + data.loc.zip + '</span>';
		            if(!data.isAuth) {
		              locHtml += ' (<a href="http://www.mycricket.com/cart/setCommunityZipForm" class="change-shop-zip">Change</a>)';
		            }
		            $('#customer-location').html(locHtml);
		          }

