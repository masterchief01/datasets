function submituserform(newuser)
 {
   var errors='',sendform=true;
   
   // check teh fields completition
   if ((document.reguser.usFirstName.value=="")||(document.reguser.usLastName.value=="")||(document.reguser.usUserName.value=="")||((document.reguser.usPassword.value=="")&&(newuser))
      ||((document.reguser.usPasswordre.value=="")&&(newuser))||(document.reguser.usEMail.value=="")||(document.reguser.usEMailre.value=="")
      ||(document.reguser.usCountry.value=="")||(document.reguser.usNewsletter.value==""))
     {
      errors+='- not completed all the fields\\n';
      sendform=false;
     }
   
   // check the password length
   if (document.reguser.usPassword.value!="") {
      erteke=document.reguser.usPassword.value;
      if (erteke.length<6) {
	     errors+='- the password is short\\n';
         sendform=false;
      }
   }
   
   // check the password confirmation
   if ((newuser)||(document.reguser.usPassword.value!="")) {
     pw=document.reguser.usPassword.value;
     pwre=document.reguser.usPasswordre.value;
     pnc=false;
     if (pw.length==pwre.length) {
	   h1=pw.length;
	   h2=pwre.length;
	   for (j=0;j<=(h1-1);j++) {
		 if (pw.charAt(j)!=pwre.charAt(j)) {
	        pnc=true;
		 }
	   }
	  
     } else {
	   pnc=true;
     }	 			 
     if (pnc==true) {
	   errors+='- the password is not confirmed correctly\\n';
	   sendform=false;
     }
   } 	 
   
   // check the e-mail
   if (document.reguser.usEMail.value!="") {
      emailvalue=document.reguser.usEMail.value;
      searched1=emailvalue.indexOf("@");searched2=emailvalue.indexOf(".");
      if ((searched1==-1)||(searched2==-1)) {
         az=1;
		 errors+='- the E-mail field is not contain a valid e-mail address\\n';
         searched1=0;searched2=0;
         sendform=false;
      }
   }
   
   // check the e-mail confirmation
   if (document.reguser.usEMail.value!="") {
     em=document.reguser.usEMail.value;
     emre=document.reguser.usEMailre.value;
     emnc=false;
