<!-- Multi-level Navigation Plugin v2.2.4 by PixoPoint Web Development ... http://pixopoint.com/multi-level-navigation/ -->
<link rel="stylesheet" type="text/css" href="http://cocktailswithmom.com/wp-content/plugins/multi-level-navigation-plugin/style.php" />
<script type="text/javascript">
var blenza_ifr = null;
var blenza_whoName = null;
var blenza_whoURL = null;

function blenza_setwho(n, e, u)
{
  blenza_whoName = n;
  blenza_whoEmail = e;
  blenza_whoURL = u;
}

function blenza_mylink(postid, entrid)
{
  try {
    var e = document.getElementById("mylink-" + postid + "-" + entrid);
    if (e == null) return;
    e.style.display = "inline";
  }
  catch (e) {
    //alert("trouble with blenza_mylink(" + postid + ", " + entrid + ")");
  }
}

function blenza_submitdata( data, xurl )
{
  if ( data == null ) return;
  var url = xurl;
  for ( i = 0; i < data.length; i++ ) {
    if ( i == 0 ) {
      url += "?";
    }
    else {
      url += "&";
    }
    var name = data[i].name;
    if ( name == "" ) {
      name = "xyzzy";
    }
    var val = data[i].value;
    url += name;
    url += "=";
    url += escape( val );
  }
  var ifr = document.createElement( "iframe" );
  blenza_ifr = ifr;
  // comment out next for debugging
  ifr.style.visibility = "hidden";
  ifr.style.position = "absolute";
  ifr.style.top = "100px";
  ifr.style.left = "100px";
  ifr.style.width = "400px";
  ifr.style.height = "400px";
  ifr.style.background = "#00aa00";
  ifr.style.color = "#000000";
  document.body.appendChild( ifr );
  var nf = frames[frames.length - 1];
  var nd = nf.document;
  nd.write( "<html>\n<head><title>Submit Data</title>\n</head>\n<body><span onclick=\"javascript:parent.blenza_ifr.style.visibility=\'hidden\';\">Hide This Window</span>.\n<iframe src=\"" + url + "\"/></iframe>\n<p>" + url.replace(/&/g,"&amp;") + "</p></body></html>\n" );
  nd.close();
}

function blenza_doSubmit( form, op, action )
{
  var xurl;
  if ( form.ml_xurl ) {
    xurl = form.ml_xurl.value + "/" + action + ".php";
  }
  else if ( form.xurl ) {
    xurl = form.xurl.value + "/" + action + ".php";
  }
  else {
    xurl = "http://{$blenza_host}{$blenza_root}/" + action + ".php";
  }
  var data = new Array();
  for ( i = 0; i < form.elements.length; i++ ) {
    var name = form.elements[i].name;
    if ( name == "" ) {
      name = "xyzzy";
    }
    if ( name.substring(0,3) == "ml_" ) {
      name = name.substring(3);
    }
    var val;
    if ( form.elements[i].type == "checkbox" ) {
      if ( form.elements[i].checked ) {
        val = form.elements[i].value;
      }
      else {
        val = "";
      }
    }
    else {
      val = form.elements[i].value;
    }

    data.push( { "name": name, "value": val } );
  }
  blenza_submitdata( data, xurl );
}

function blenza_switcheroo( off, on )
{
  var ele1 = document.getElementById( off );
  ele1.style.display = "none";
  var ele2 = document.getElementById( on );
  ele2.style.display = "block";
}

function blenza_setNextone( pi, name, url )
{
  var nx = document.getElementById( "nextone-" + pi );
  if ( nx != null ) {
    nx.innerHTML = '<a class="blenza-link" href="' + url + '">' + name + '</a>';
  }
}

function blenza_doEnter( form )
{
  var name;
  if ( form.ml_name ) {
    name = form.ml_name.value;
  }
  else {
    name = form.name.value;
  }
  var url;
  if ( form.ml_url ) {
    url = form.ml_url.value;
  }
  else {
    url = form.url.value;
  }
  if ( ! blenza_checkName( name ) ) {
    return false;
  }
  if ( ! blenza_checkUrl( url ) ) {
    return false;
  }
  blenza_doSubmit( form, "Entry", "entry_do" );
  var pi;
  if ( form.ml_postid ) {
    pi = form.ml_postid.value;
  }
  else {
    pi = form.postid.value;
  }
  blenza_setNextone( pi, name, url );
  blenza_switcheroo( "theform-" + pi, "theform2-" + pi );
  return false;
}

function blenza_checkName( name )
{
  if ( name.length == 0 ) {
    alert( "Please enter your name." );
    return false;
  }

  return true;
}

function blenza_checkUrl( url )
{
  if ( url.length == 0 ) {
    alert( "Please enter your link URL." );
    return false;
    return true;
  }
  var test = "http://"
  if ( url.substring( 0, test.length ) != test ) {
    alert( "Link URL must begin with 'http://'" );
    return false;
  }
  if ( url == "http://www." ) {
    alert( "Please enter your link URL." );
    return false;
  }
  return true;
}
</script>

