	<!-- Start WSS-->
	<script language="JavaScript1.2" type="text/javascript">
	function getDisplayNumber(iNumber) {
	   var sNumber = (iNumber <= 9) ? "0" + iNumber : "" + iNumber;
	   return sNumber;
	}
	function doBIScript() {
	  var s = '';
	  var date = new Date();
	  s += getDisplayNumber(date.getFullYear());
	  s += getDisplayNumber( date.getMonth()+1 );
	  s += getDisplayNumber(date.getDate());
