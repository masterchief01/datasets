<input name="ctl00$Header2$ctlSearch$TopNavSearchText" type="text" maxlength="200" id="ctl00_Header2_ctlSearch_TopNavSearchText" type="text" class="TopNavSearchText hintText watermark searchbox noBorder" name="recipeSearch" autocomplete="Off" value="Search" />
 
</div>
<div class="search-button">
    <input type="image" name="ctl00$Header2$ctlSearch$Search" id="ctl00_Header2_ctlSearch_Search" class="SearchExecute" alt="all" src="/Images/Experience/global/go_btn.png" border="0" />
</div>
</div>

    <script type="text/javascript">
        $(document).ready(function() {
            $(".TopNavSearchText").autocomplete('/Services/PredictiveSearch/SearchSuggestionService.svc/json/suggestions',
            { width: 320,
                max: 10,
                selectFirst: false,
                scroll: false,
                scrollHeight: 250,
                minChars: 3,
                dataType: "json",
                cacheLength: 0,
                extraParams: { terms: function() { return $(".TopNavSearchText").val() + "*"; } },
                parse: function(data) {
                    var items = data[1];
                    var parsed = [];
                    for (var i = 0; i < items.length; i++)
                        parsed[i] = {
                            data: [items[i]],
                            value: items[i],
                            result: [items[i]]
                        };
                    return parsed;
                }
            });
        });

</script>


 
<script language="JavaScript" type="text/javascript">
    //<![CDATA[

    $(function() {
        $('.TopNavSearchText').keypress(function(e) {
            if (e.keyCode == '13') {
                e.preventDefault();
                TopNavSearch();
            }
        });

        $('.SearchExecute').click(function (e) {
            e.preventDefault();
            TopNavSearch();
        });
    });

    function TopNavSearch() {
        if (TopNavSearchHasErrors())
            alert('Enter Search Term');
        else {
            dcsMultiTrack('DCS.dcsuri', '/site_search', 'WT.ti', 'User Submitted Site Search');
            PerformSearch($('.TopNavSearchText').val());
        }
    }

    function TopNavSearchHasErrors() {
        var result = false;
        var searchTerm = $('.TopNavSearchText').val();
        searchTerm = jQuery.trim(searchTerm);
        result = (searchTerm == '' || searchTerm == 'Search' || searchTerm == 'search');
        return result;
    }
    //]]>
</script>


		<!--[if IE 6]>
        <script type="text/javascript">
            // HOVER FOR IE 6
            tnHover = function() {
                var topNav = document.getElementById("main-nav");
                if (topNav == null) return;

                var tnEls = topNav.getElementsByTagName("LI");
                if (tnEls == null) return;

                for (var i = 0; i < tnEls.length; i++) {
                    tnEls[i].onmouseover = function() {
                        this.className += " tnhover";
                    }
                    tnEls[i].onmouseout = function() {
                        this.className = this.className.replace(new RegExp(" tnhover\\b"), "");
                    }
                }
            }
            if (window.attachEvent) window.attachEvent("onload", tnHover);
        </script>
        <![endif]-->
