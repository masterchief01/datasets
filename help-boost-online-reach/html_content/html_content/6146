    <script type="text/javascript" src="/Scripts/Recipes/RecipeBar.js"></script>
    <script type="text/javascript" src="/Scripts/Recipes/jquery.tooltip.min.js"></script>

    <script type="text/javascript">
        var recipeId = 46915;
        
        function saveRecipe() {
            
            var dataStr = "{'saveForm':'{\"recipeId\":\"" + recipeId + "\"}'}"

            function success(res) {
                if (res.d.ErrorMessage == null || res.d.ErrorMessage.length == 0) {
                    $("#save_modal").find("div.saveFormSection").hide();
                    $("#save_modal").find("div.saveResultSection").show();
                    dcsMultiTrack('DCS.dcsuri', '/Recipes/AddtoRecipebox.aspx', 'WT.ti', 'Add Recipe to mySpot', 'DCS.dcsqry', 'rid=' + recipeId);
                    
                    // Activity Name for this tag is: save_eba
                    fireSpotlight('save_eba');
                }
                else {
                    $("#save_modal").find("div.saveFormSection").hide();
                    $("#save_modal").find("div.saveResultSection").show();
                    $("#save_modal").find("div.saveResultSection").append(res.d.ErrorMessage);

                }
            }
            function failure() {
            }
            submitAJAX(dataStr, "/AJAX/Recipes/AddToMySpot.asmx/AddToMySpot", success, failure);
            return false;
        }
        function populateEmailTopBar() {
            var emailData = "{'recipeId':" + recipeId + ",'activityType':'EmailCount'}";
            
            function emailSuccess(res) {
                $('#email_topbar').html(res.d);
                dcsMultiTrack('DCS.dcsuri', '/Recipes/RecipeEmail.aspx', 'WT.ti', 'Email recipe', 'DCS.dcsqry', 'rid=' + recipeId);
            }
            function emailFailure() {
            }
            var emailData = "{'recipeId':" + recipeId + ",'activityType':'EmailCount'}";
            submitAJAX(emailData, "/AJAX/Recipes/RecipeActivity.asmx/ProcessActivityRequest", emailSuccess, emailFailure);
        }

        $(document).ready(function() {
            
            //Control turning off the next and previous buttons
            if ((document.location.pathname.toLowerCase()).search(/healthified/) != -1 || (document.location.pathname.toLowerCase()).search(/slideshows/) != -1) {
                $('.recipeBarNextButton').hide();
                $('.recipeBarPreviousButton').hide();
              };
              
            $("#screenName").tooltip();
            $('.rating').stars({
                inputType: 'select',
                oneVoteOnly: false,
                cancelShow: false,
                callback: function(ui, type, value) {
                    $("#rating").val(value);
                }
            })
            $("div.buttons ul li a").mouseover(function() { highlightButton(this) });
            $("div.buttons ul li a").mouseout(function() { fadeButton(this) });
            $("a.biggerText").click(function() { changeFonts(); toggleBigSmall(this) });
            
            $("a.printLink").click(function() {
                var printData = "{'recipeId':" + recipeId + ",'activityType':'PrintCount'}";
                submitAJAX(printData, "/AJAX/Recipes/RecipeActivity.asmx/ProcessActivityRequest", incrementSuccess, incrementFailure);
                var imgsrc = $("a.biggerText").children("img").attr("src");
                if (imgsrc.match(/bigger/)) {
                    OpenPrintWindow(recipeId,"Smalltext");   
                }
                else {
                    OpenPrintWindow(recipeId,"Biggertext"); 
                }
                return false;
                function incrementSuccess(res) {
                    //topBar.html(res.d);
                }
                function incrementFailure() {
                }
                
            });

            $("a.modalLink").click(function() {
                if ($(this).attr("disabled") != "disabled") {
                    var modal;
                    if($(this).attr("rel")=="rate")
                    {
                        modal = $("#review_modal");
                        var rateCookie = "returnAction=" + escape('rate');
                        document.cookie = rateCookie;
                    }
                    else
                    {
                        modal = $("#" + $(this).attr("rel") + "_modal");
                    }
                    var topBar = $("#" + $(this).attr("rel") + "_topbar");
                    var modalLink = $(this);
                    if (!$(modal).is(":visible")) {
                        modalClickType($(this).attr("rel"));
                        showWindows(modal, topBar, modalLink);
                    }
                }
                function modalClickType(whichType) { 
                    // joinNowModal
                }
                function incrementSuccess(res) {
                    topBar.html(res.d);
                }
                function incrementFailure() {
                }
                

                switch (modal.selector) {
                    case "#print_modal":
                        // this code has been moved to a.printLink function
                        break;
                    case "#save_modal":
                        $("#activitysave_topbar").show();
                        $("div","#activitysave_topbar").show();

                        var saveData = "{'recipeId':" + recipeId + ",'activityType':'SaveCount'}";
                        submitAJAX(saveData, "/AJAX/Recipes/RecipeActivity.asmx/ProcessActivityRequest", incrementSuccess, incrementFailure);                      
 
                        break;
                    case "#share_modal":
                        $("#activityshare_topbar").show();
                        $("div","#activityshare_topbar").show();

                        //fire spotlight for share_eba
                        fireSpotlight('share_eba');
                        
                        var shareData = "{'recipeId':" + recipeId + ",'activityType':'ShareCount'}";
                        submitAJAX(shareData, "/AJAX/Recipes/RecipeActivity.asmx/ProcessActivityRequest", incrementSuccess, incrementFailure);
                        dcsMultiTrack('DCS.dcsuri', '/Recipes/ShareRecipe.aspx', 'WT.ti', 'Share recipe', 'DCS.dcsqry', 'rid=' + recipeId);
                        break;
                }

                return false;
                
            });

            $("a#saveRecipeLink").click(function() { saveRecipe(); });
            $("a.modalLink[rel=save]").click(function() { saveRecipe(); });
            $("input.submit").click(function() {
               
                var data = $("#review_modal > div.reviewFormSection").find("input").add("#review_modal > div.reviewFormSection > textarea").serializeArray();
                var dotNETjson = dotNETConverter(data);
                var dataStr = "{'reviewForm':'" + dotNETjson + "'}";
                function success(res) {
                    if (res.d.ErrorMessage == null || res.d.ErrorMessage.length == 0) {
                        $("#review_modal div.reviewFormSection").hide();
                        //var imgsrc = $("img.writereview").attr("src");
                        //$("img.writereview").attr("src", imgsrc.replace(/writeareview/ig, "edityourreview"));
                        $(".writereview").parent().click(function() {
                            $("#review_modal div.reviewFormSection").show();
                            $("#review_modal div.reviewResultSection").show();
                            $("#review_modal div.editResultSection").hide();
                        });
                        if (res.d.Action == "edit") {
                            $("#review_modal div.editResultSection").show();
                        }
                        else {
                            $("#review_modal div.reviewResultSection").show();
                            //Fire spotlight for new review
                            fireSpotlight('review_recipe_eba');
                        }
                        var rateCountText
                        if (res.d.RateCount == 1) { rateCountText = ' rating'; } else { rateCountText = ' ratings'; }
                        $('#ratingCount').html(res.d.RateCount + rateCountText + ' - You gave this recipe a ' + res.d.UserRating + '/5.')
                        //Fire web trends both for rating and reviewing
                        dcsMultiTrack('DCS.dcsuri','/Myspot/rateRecipe.aspx','WT.ti','Rate a Recipe', 'DCS.dcsqry','rid=' + recipeId);
                        dcsMultiTrack('DCS.dcsuri','/Myspot/reviewRecipe.aspx','WT.ti','Review a Recipe', 'DCS.dcsqry','rid=' + recipeId);
                    }
                    else {
                        $("#review_modal div.reviewFormSection .errorBox").empty().html(res.d.ErrorMessage);
                        $("#review_modal div.reviewFormSection .errorBox").show();
                        $("#review_modal div.reviewFormSection .errorBox").focus();
                    }
                }
                function failure(res) {
                    $("#review_modal div.reviewFormSection .errorBox").empty().html("There has been an error.  Please try again later.");
                    $("#review_modal div.reviewFormSection .errorBox").show();
                    $("#review_modal div.reviewFormSection .errorBox").focus();
                }
                submitAJAX(dataStr, "/AJAX/Recipes/RecipeReview.asmx/Review", success, failure);
                return false;
            });
            
            $('.stars-oneVoteOnly').stars({
                inputType: 'select',
                oneVoteOnly: true,
                callback: function(ui, type, value) {
                    $("#rating1").val(value);
                    //Fire web trends
                    dcsMultiTrack('DCS.dcsuri','/Myspot/rateRecipe.aspx','WT.ti','Rate a Recipe', 'DCS.dcsqry','rid=' + recipeId);
                    
                    var data = $("#rating_data").find("input").serializeArray();
                    var dotNETjson = dotNETConverter(data);
                    var dataStr = "{'rateForm':'" + dotNETjson + "'}";
                    function success(res) {
                        var rateCountText
                        if (res.d.RateCount == 1) { rateCountText = ' rating'; } else { rateCountText = ' ratings'; }
                        $('#ratingCount').html(res.d.RateCount + rateCountText);
                        
                        $('#RateCountSpan').html(res.d.UserRating);
                        //Fire Spotlight
                        if (res.d.Action != "edit")
                            fireSpotlight('rate_recipe_eba');

                        var modal = $("#rate_modal");
                        var topBar = $("#" + $(this).attr("rel") + "_topbar");
                        var modalLink = $("#rateHiddenLink")
                        if (!$(modal).is(":visible")) {
                            showWindows(modal, topBar, modalLink);
                        }
                        
                    }
                    function failure(res) {
                    }
                    submitAJAX(dataStr,  "/AJAX/Recipes/RecipeReview.asmx/Rate", success, failure);
                }
            })
            
            $(".ui-stars-star a").mouseover(function() { 
                    switch(this.innerHTML)
                    {
                        case '1': this.title = 'No thanks'; break;
                        case '2': this.title = 'Not my favorite'; break;
                        case '3': this.title = 'It was OK'; break;
                        case '4': this.title = 'Tasty'; break;
                        case '5': this.title = 'MMMMMM'; break;
                    }
             });


            $('.loginModalLink').click(function() {
                var modal = $("#review_modal");
                var rateCookie = "returnAction=" + escape('rate');
                document.cookie = rateCookie;
                var topBar = $("#" + $(this).attr("rel") + "_topbar");
                var modalLink = $(this);
                if (!$(modal).is(":visible")) {
                    modalClickType($(this).attr("rel"));
                    showWindows(modal, topBar, modalLink);
                }
            });
            
            $("#confirmDelete").click(function() {
                var deleteData = "{'recipeId':" + recipeId + "}";
                function success(res) {
                    if (res.d.ErrorMessage == null || res.d.ErrorMessage.length == 0) {
                        location.reload(true);
                    }
                    else {
                        $("#delete_modal").hide();
                    }
                }
                function failure(res) {
                }
                submitAJAX(deleteData, "/AJAX/Recipes/RecipeReview.asmx/DeleteReview", success, failure);
                return false;
            });
            $("#noDelete").click(function() {
                closeModal($(this).parent().parent());
                return false;
            });
        });
    </script>


