<script type="text/javascript">
    $(function () {
        $("li.opener").mouseover(function () {
            $("select").blur(); 0
        });
        $('.register_popup').click(function () {
            $("#loginIFrame").hide();
            var SourceCodeID = $('.register_popup').attr("rel");

            iframeOpen('register', SourceCodeID);
            $('.registerButton').hide();
        });
    });

    function iframeOpen(action, optionalsourcecode) {
        $("#iframe1").attr('src', '/LoginRegModal/' + action + '.aspx?ESRC=' + optionalsourcecode);
        $(".white-mask").show();
        $("#divRegistrationModal").hide();
        $("#divWhiteMaskModal2").hide();
        (action == 'register') ? $("#loginIFrame").addClass('registerModal') : $("#loginIFrame").addClass('login-modal-land');

        //(action == 'register') ? $("#iframe1").height(500) : $("#iframe1").height(408);
        $("#iframe1").height(408);
        //sizeIFrame(295);
        showIFrame();
        (action == 'register') ? $("#loginIFrame").hide('register_popup') : $("#loginIFrame").show('register_popup');

        return false;
    }
    $("a.close-popup").click(function () {
        //$("#iframe1").attr('src', '/LoginRegModal/RedirectCloseModal.aspx');
        closeIFrame();
    });


    function refreshPage() {
        window.location = window.location; return false;

    }
    function closeIFrame() {
        window.location = window.location; return false;
    }

    function openNewsletterSample() {
        // window.location = window.location; return false;
        // window.open('http://www.eatbetteramericacommunity.com/newsletter_archive/eba_example.html');
        //ev.preventDefault();
        // return false;
    }


    function showIFrame() {
        $("#loginIFrame").show();

    }

    function sizeIFrame(innerDocHeight) {
        var helpFrame = jQuery("#iframe1");
        // var innerDoc = (helpFrame.get(0).contentDocument) ? helpFrame.get(0).contentDocument : helpFrame.get(0).contentWindow.document;
        // helpFrame.height(innerDoc.body.scrollHeight + 35);
        helpFrame.height(innerDocHeight);
    }

	
</script>


