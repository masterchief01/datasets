YAHOO.util.Event.onContentReady('ysp-videos', function() {
  var a = YAHOO.util.Dom.getElementsByClassName('scrollbody', 'div', 'videos-carousel')[0].getElementsByTagName('a'),
      i = j = 0;

  for (i = 0; i < a.length; i = i + 2) {
    YAHOO.util.Event.addListener([a[i], a[i + 1]], 'click', function(e, i) { YAHOO.util.Event.preventDefault(e); YAHOO.Sports.Video.play(i, true); }, j);
    j++;
  }
});


