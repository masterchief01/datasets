            <input name="ctl00$CenterColumnPlaceHolder$recipe$txtConversion" type="text" value="4" id="ctl00_CenterColumnPlaceHolder_recipe_txtConversion" class="servings-amount" style="float: left;" /><input name="ctl00$CenterColumnPlaceHolder$recipe$hdnScaledServings" type="hidden" id="ctl00_CenterColumnPlaceHolder_recipe_hdnScaledServings" class="servings-hdnservings" value="4" /><input name="ctl00$CenterColumnPlaceHolder$recipe$hdnIsMetric" type="hidden" id="ctl00_CenterColumnPlaceHolder_recipe_hdnIsMetric" value="0" class="servings-hdnIsMetric" />
