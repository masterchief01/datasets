<script type="text/javascript" >
                                   
                                            $(document).ready(function() {

                                            $('#facebookshare,#fshare_btn').socialbutton('facebook_share', {
                                                button: 'button_count',
                                                url: document.location.href,
                                                encoded: true,
                                                text: 'Share'
                                            });
                                            $('#twittershare,#retweet_btn').socialbutton('twitter', {
                                                button: 'horizontal',
                                                url: document.location.href,
                                                encoded: true,
                                                text: 'Check out this Picc at Piccsy.com',
                                                lang: 'en',
                                                via: 'ilovepiccsy',
                                                related: 'twitter'
                                            });

  
                                            $( "#socialshare" ).dialog({
                                                        autoOpen: false,
                                                        show: "fade",
                                                        hide: "fade"
                                                });

                                          $("#new_tags").autocomplete("/tags/search", {
                                                    width: 150,
                                                    dataType: 'json',
                                                    highlight: false,
                                                    autoFill: false,
                                                    delay: 400,
                                                    scroll: true,
                                                    scrollHeight: 300,
                                                    multiple: true,
                                                    matchContains: true,
                                                    selectFirst: false,

                                                    parse: function(data) {
                                                            var array = new Array();
                                                            for(var i=0;i<data.tags.length;i++)
                                                            {
                                                                    array[array.length] = { data: data.tags[i], value: data.tags[i].id, result: data.tags[i].tag };
                                                            }
                                                            return array;
                                                    },

                                                    formatItem: function(row) {
                                                            return row.tag;
                                                    }
                                                });

		// "share this page" link on right click
                /*
		jQuery('#indivPost-leftColumn').qtip({
			content: {
				text: jQuery('div#share'),
				title: {
					text: 'Link to this page'
				}
			},
			position: {
				target: 'mouse',			// position it where the click was...
				adjust: { mouse: false }	// ...but don't follow the mouse
			},
			show: 'mousedown',
			hide: 'unfocus',
			style: {
				width: 437
			}
		});
		jQuery('#indivPost-leftColumn').bind('contextmenu', function(e){ return false; });
                */


                                            });

                                            function picc_it(post_id,type) {
								jQuery.ajaxSetup({ error:function(x, e){ alert('Error piccing post ['+post_id+']'); } });
								picc_url = 'http://piccsy.com/post/unpicc/'+post_id+'/';
                                                                if(type=='picc') { picc_url = 'http://piccsy.com/post/picc/'+post_id+'/'; }
                                                                jQuery.post(picc_url, function(data, s){                                                                 
                                                                        if (data.redirect) {
										document.location = 'http://piccsy.com/'+data.redirect;
										return;
									}
									if (data.success == false) {
										alert('Error piccing post ['+post_id+']');
									}
                                                                        if(type=='picc') {

                                                                        
                                                                        $( "#socialshare" ).dialog({ autoOpen: false, draggable: false , resizable:false, position: ['right','bottom'] });
                                                                        $('.ui-dialog').css({position:"fixed",margin:"20px"});
                                                                        $( "#socialshare" ).dialog('open');
                                                                        
                                                                        $(window).resize(function() {
                                                                          $( "#socialshare" ).dialog({ position: ['right','bottom'] });
                                                                        });

                                                                            $('#picc_it').hide();
                                                                            $('#unpicc_it').show();
                                                                        } else {
                                                                            $('#unpicc_it').hide();
                                                                            $('#picc_it').show();
                                                                        }
                                                                }, "json");
							}



                                                       
                                        </script>
