var boxname = ".sharebox, .author-meta";
var navname = "section#single-article .navigation div a, section.ten-facts .navigation div a";
var menuYloc = null;
var menuZloc = null;

$(document).ready(function(){
	menuYloc = parseInt($(boxname).css("top").substring(0,$(boxname).css("top").indexOf("px")))
	menuZloc = parseInt($(navname).css("bottom").substring(0,$(navname).css("bottom").indexOf("px")))
	$(window).scroll(function () {
		var offset = menuYloc+$(document).scrollTop()+"px";
		var offsetZ = menuZloc+140+$(document).scrollTop()+"px";
		$(boxname).animate({top:offset},{duration:350,queue:false});
		$(navname).animate({top:offsetZ},{duration:350,queue:false});
	});
});
</script>

