			_load_user_comment_form();
		}
	});

function _load_user_comment_form() {
	jQuery.ajax({
		url: '/ajax/fetch.php?section=comment-form&post_id=54242',
		success: function(data) {
			if(data.length)
				jQuery('#comment-form').html(data);
		}
	});
}
</script>

