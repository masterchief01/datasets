			textarealength = $(this).val().length;
			if(textarealength == 0){
				$("#bouton").fadeOut("fast");
			}else{
				$("#bouton").fadeIn("fast");
			}
		});
		
		$(".menuDropDown li").each(function(){
			$(this).children("ul").css({"display":"none","width":$(this).width()-21});
			$(this).hover(function(){
				if($(this).children("ul").is(":hidden")) {
					$(this).children("ul").fadeIn("fast");
				}else{
					$(this).children("ul").css("display", "none");
				}
			});
			
			$(this).mouseleave(function(){
				$(this).children("ul").css("display", "none");
			});
			
			$(this).click(function(){
				$(this).children("ul").css("display", "none");
			});
		});
		
		//jQuery Fancybox
		$("a.fancybox").fancybox();
