<!------ OAS SETUP end ------>
<script type="text/javascript">OAS_AD('AdController');</script>    	
<script>
	function movead(position) {
		var tolocation = $('.oasad[data-ad='+position+']');
		var fromlocaition = $('.lowad[data-ad='+position+']');
	
		if(tolocation.length != 1) return;
		if(fromlocaition.length != 1) return;
	
		// remove scripts so they don't rerun
		fromlocaition.find('script').remove();
	
		// move the nodes to the proper locations
		tolocation.append( fromlocaition.detach() );
	}
</script>

<div style="display:none">
   	
	<div class="lowad" data-ad="BigBanner">
		<script type="text/javascript">OAS_AD('BigBanner');</script>
	</div>
	<script>
		movead('BigBanner');
	</script>
	
	<div class="lowad" data-ad="Block">
		<script type="text/javascript">OAS_AD('Block');</script>
	</div>
