<script type ="text/javascript">
	$(document).ready(function(){

		$('#search_txt').each(function(){

		this.value = this.value ==''? $(this).attr('title'):this.value;

		$(this).focus(function(){
			if(this.value == $(this).attr('title')) {
				this.value = '';
			}
		});
		$(this).blur(function(){
			if(this.value == '') {
				this.value = $(this).attr('title');
			}
		});
	});
	});
</script>
		
