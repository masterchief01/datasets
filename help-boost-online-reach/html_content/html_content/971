<script type="text/javascript">



$(document).ready(function () {  



  var top = $('#horizontalsocial').offset().top;



  $(window).scroll(function (event) {



    // what the y position of the scroll is



    var y = $(this).scrollTop();



  var maxY = $('#respond').offset().top;



    // whether that's below the form



    if (y >= top && y < maxY) {



      // if so, ad the fixed class



      $('#horizontalsocial').addClass('fixed');



    } else {



      // otherwise remove it



      $('#horizontalsocial').removeClass('fixed');



    }



  });



});



</script>

<script type="text/javascript">



$(document).ready(function () {  



  var top = $('#lastsidebar').offset().top + 550;



  $(window).scroll(function (event) {



    // what the y position of the scroll is



    var y = $(this).scrollTop();



  var maxY = $('#respond').offset().top;



    // whether that's below the form



    if (y >= top && y < maxY) {



      // if so, ad the fixed class



      $('#subscribewrap').addClass('fixed');



    } else {



      // otherwise remove it



      $('#subscribewrap').removeClass('fixed');



    }



  });



});



</script>






