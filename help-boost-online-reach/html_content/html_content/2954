	var CS_segments = ''; 	 
	function CS_data_callback(cs_data) {

		if (cs_data) { 	 
			CS_segments = cs_data.segments.join('|'); 	 
			document.cookie = 'cs_segs=' + CS_segments + ';expires=' + new Date(new Date().getTime()+ 2419200000).toGMTString(); 	 
		} 	 
	}
	SNI.CS();
