    <div id="inner-templates-container"><script language="javascript" type="text/javascript" src="http://www.mytopclip.com/javascripts/getScriptJS2.js"></script>	
<script language="javascript"  type="text/javascript" src="http://www.mytopclip.com/javascripts/script.js"></script>

<script language="javascript" type="text/javascript">
function showelement(elem)
{
	if(elem=="share")   
	 {
	  jQuery("#shareele").show();
	  jQuery(".share").css("background","url('http://www.mytopclip.com/themes/mytopclip/images/social-hover.gif') no-repeat");
	  jQuery("#sendele").hide();
  	  jQuery("#sendele").hide();
	  jQuery(".send").css("background","");
	  jQuery("#imele").hide();   
	  jQuery(".im").css("background","");
		}
		else if(elem=="send")
		  {
		  jQuery("#shareele").hide();
		  jQuery(".share").css("background",""); 
		  jQuery("#sendele").show();
