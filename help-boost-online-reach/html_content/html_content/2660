        InstructJS.load(["helpbubble"]);
        function replyLinks () {
            var replylinks = $$(".replylink");
            $(replylinks).each(function(el){
                var elid = el.id;
                var postid = elid.substring(elid.indexOf('_') + 1);

                new HelpBubble({
                    clickEl : el,
                    width: 200,
                    popLeft: true,
                    contents : makeHelpBubbleTextForComments("reply" + postid),
                    additionalActions: focusOnHelpBubbleLogin
                });
            });
        }
        replyLinks();

    </script><div class="comments_div_4"><div class="comments-pagination clearfix"><table cellpadding="3"><tr><td><span class="label">1-40 of 89</span></td><td></td><td><a rel="nofollow" href="http://www.instructables.com/id/Cake/?&amp;sort=ACTIVE&amp;limit=40&amp;offset=40" class="med-button" onmouseout="this.className = 'button';" onmouseover="this.className = 'button_over';">next &raquo;</a></td></tr></table></div></div><a name="POST" id="POST"></a><table class="comments_table_5"><tr valign="top"><td><img src="/static/defaultIMG/comment.TINY.gif" alt="" /></td><td class="comments_td_6"><div id="NewCommentLINK"><a class="stepLabel" href="#" onclick="return false;">
