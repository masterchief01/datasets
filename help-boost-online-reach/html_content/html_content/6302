    <script type="text/javascript">

    $(function () {
        $('#askBeckyForm #submit').click(doCreateAskBeckyForm);
    });

    function doCreateAskBeckyForm() {
        clearFormErrors($('#askBeckyForm'));
        requestData = {
            ageVerificationCheckbox: $('#askBeckyForm input[id*=ageVerificationCheckbox]:checked').val(),
            firstName: $('#askBeckyForm #firstName').val(),
            lastName: $('#askBeckyForm #lastName').val(),
            emailAddress: $('#askBeckyForm #emailAddress').val(),
            comment: $('#askBeckyForm #comment').val(),
            termVerificationCheckbox: $('#askBeckyForm input[id*=termVerificationCheckbox]:checked').val()
            
        };

        onSuccess = partial(CreateAskBeckyFormSuccess);
        CreateAskBeckyForm(requestData);

        return false;
    }

    /* function clearErrors() {
       // $('#askBeckyForm input[type=text]').each(function () { $(this).val(''); });
        //$('#askBeckyForm input[type=checkbox]').each(function () { $(this).removeAttr('checked'); });
        $('#askBeckyForm .error_message').css('display', 'none');
        $('#askBeckyForm *').removeClass('formError');
    }*/

    function CreateAskBeckyForm(requestData) {
        AjaxCall(requestData);
    }

    function CreateAskBeckyFormReply(reply) {
        if (reply.Success != "true")
            displayErrors(reply.Errors, $('#askBeckyForm'));
        else
            CreateAskBeckyFormSuccess();
    }

    function CreateAskBeckyFormSuccess() {
        confirmationPopup('askBeckyFormConfirmationPopup', 400, 200);
    }

    </script>


