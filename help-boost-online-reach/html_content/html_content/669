	<script language="javascript">
	var ad_site = 'espn.us.com.espn';
	var ad_zone = 'espnfrontpage';
	var ad_kvps = 'pgtyp=espnfrontpage;sp=espn;';
	var ad_swid = '';
	var ad_counter = 1;
	var ad_ord = Math.floor(Math.random()*9999999999);
	var ad_seg = '';
	var ad_ref = 'other';
	var ref = document.referrer;

	if (ref.match('facebook.com')) {
		ad_ref = 'facebook';
	} else if (ref.match('twitter.com|t.co')) {
		ad_ref = 'twitter';
	} else if (ref.match('plus.url.google.com|plus.google.com')) {
		ad_ref = 'googleplus';
	} else if (ref.match('google.com')) {
		ad_ref = 'google';
	} else if (ref.match('bing.com')) {
		ad_ref = 'bing';
	} else if (ref.match('yahoo.com')) {
		ad_ref = 'yahoo';
	} else if (ref.match('espn.go.com')) {
		ad_ref = 'espn';
	}

	ad_kvps = ad_kvps + 'ref=' + ad_ref + ';';

	
		ad_swid = jQuery.cookie('SWID');
		if(typeof espn.core.ad_segments == 'function') {
			ad_seg = espn.core.ad_segments();
			if (ad_seg != null && ad_seg != '') {
				ad_kvps = ad_kvps + ad_seg
			}
		}
	

	// the u
	var ad_u = 'u=pgtyp=espnfrontpage|sp=espn|ref=' + ad_ref + '|';
	var ad_seg_u = ad_seg;
	if (ad_swid != null && ad_swid != '') {
		if (ad_swid.length > 54) {
			ad_swid = ad_swid.substring(0,54);
		}
		ad_newu = 'u=swid='+ad_swid+'|';
		ad_u = ad_u.replace('u=',ad_newu);
	}
	if (ad_seg_u != null && ad_seg_u != '') {
		while(ad_seg_u.indexOf(';')!=-1) {
			ad_seg_u = ad_seg_u.replace(';','|');
		}
		ad_u = ad_u + '|' + ad_seg_u;
	}
	ad_u = ad_u + ';';
	if (ad_u.length > 255) {
		ad_u = ad_u.substring(0,254);
	}
	</script>
