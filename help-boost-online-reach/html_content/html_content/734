<script src="/scripts/lookin/lookin-1.1.0.js" type="text/javascript" charset="utf-8"></script>
<script src="/scripts/lookin/lookinSponsorship-1.0.1.js" type="text/javascript" charset="utf-8"></script>
<script src="/shared/scripts/periodicalExecuter.js" type="text/javascript" charset="utf-8"></script>
<style type="text/css">
	#drawer_container.lookin          {text-align:center; line-height:20px;}
	#drawer_container.lookin a {color:#f90; font-weight:normal;}

	#drawer_container .lookin          {border-right:1px solid #333;border-left:1px solid #333;}
	#drawer_container.lookin           {z-index:0; position:relative; width:984px; background:#585858 url(/images/drawer/bg_drawer.png) no-repeat 0 -25px; display:block; margin:0px 3px; overflow-y:hidden;}
	#drawer_container.lookin.presizing {height:0; overflow:hidden;}
	* html #drawer_container.lookin    {display:none;}

</style>

<script type="text/javascript">
$(function () {
  
	var prevContainer = (club==="mlb") ? "#homepage_container" : "#home_container";
	
	var LiveLookIn = bam.widget.LiveLookIn;

	LiveLookIn.init({
		grep: function (medium) {
			return medium.away_team_id=="133" || medium.home_team_id=="133";
		}
	});
	LiveLookIn.zPlayer.shouldShowRelatedContent = function(){ return false; }; // disable related content for LLI	
	LiveLookIn.zPlayer.bind("startPlayback", function(){
		bam.clubhome.playerObj.execute("pausePlayback");
	});
	LiveLookIn.start();
	LiveLookIn
		.onSuccess(function(e, media){
			if ($("#drawer_container.lookin").length===0){
				var drawerHtml = '' + 
						'<div id="drawer_container" class="presizing lookin">' +
							'<a href="#" class="liveLookIn" data-calendar-event-id="'+media[0].calendar_event_id+'" data-link-origin="OAK Homepage Drawer">' +
								'<strong>LIVE LOOK-IN:</strong> '+media[0].headline+' &nbsp;&nbsp; Watch Now &raquo;' +
							'</a>' +
						'</div>';
				$(drawerHtml).insertBefore(prevContainer).animate({height:"20px"}).find("a");
			}
		})
		.onEmpty(function(){
			$("#drawer_container.lookin")
			  .animate({height:"0"}, function(){ $(this).remove() });
		});
		
	// may need to use something like: eventProxy(ScoreboardTickerApp.displayLiveLookIn, ScoreboardTickerApp)
});
</script>


