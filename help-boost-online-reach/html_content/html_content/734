		<div class="row">
			



 






<script type="text/javascript">
$(document).ready(function () {
	bam.loadSync(bam.homePath + 'bam.datetime.js');
	$.ajax({
		type: "GET",
      	//url: "/lookup/json/named.schedule_next_x_series.bam?season=2012&team_id=133&game_date=%272012/02/02%27&results=3&game_type=%27R%27",
		//HARDCODED URL VAL - @TODO: Make dynamic before 2012 season
		url: "/lookup/json/named.schedule_next_x_series.bam?season=2012&team_id=133&game_date=%272012/04/01%27&results=3&game_type=%27R%27",
		dataType: "json",
		success: function(json) {
			var results = bam.util.getQueryResults(json, "schedule_next_x_series");
			$.each(results, function(i,item){
				var series_nbr = item.series_nbr;				
				var hs_start_time = item.series_start;	// Ex: 2011-03-31T00:00:00
				var hs_end_time = item.series_end;		// Ex: 2011-04-03T00:00:00				
				var hs_start_month = hs_start_time.slice('6','7');
				var hs_end_month = hs_end_time.slice('6','7');				
				var hs_start_formatDate = bam.datetime.parseISODate(hs_start_time);
				hs_start_time = hs_start_formatDate.formatDate("MMMM d");				
				var hs_end_formatDate = bam.datetime.parseISODate(hs_end_time);					
				var time_display = "";						
				if(hs_start_month === hs_end_month){
					hs_end_time = hs_end_formatDate.formatDate("MMMM d");
					if(hs_start_time === hs_end_time){
						time_display = hs_start_time;
					}else{
					hs_end_time = hs_end_formatDate.formatDate("d");
					time_display = hs_start_time+'-'+hs_end_time;	
					}
				}else{					
					hs_end_time = hs_end_formatDate.formatDate("MMMM d");
					time_display = hs_start_time+' - '+hs_end_time;
				}				
				var opponent = item.opponent.toLowerCase();
				var opponent_name = item.opponent_brief;
				if (i<3) {			
					$('<li></li>')
					.html('<img height="56" width="66" src="/images/homepage/team/y2011/logos/'+opponent+'.png" class="png seriesLogo" /><div><h4>vs '+opponent_name+' <br />'+time_display+', 2012</h4></div>')
					.appendTo('#homestand_items');
				}
			});
			$("#homestand_items").find("li:even").addClass("odd");
    	}
   	});
});
</script>
<!-- CHANGE LABEL OF MODULE FROM "NEXT HOMESTAND" TO "2012 SCHEDULE" -->
<div id="homestand_container" class="module">
	<h3><span style="font-size:14px">2012</span> Schedule - Upcoming Homestand</h3>
	<div id="homestand_content">
		<ul id="homestand_items"></ul>
		<ul>
			<li class="more seriesloge"><a href="/schedule/index.jsp?c_id=oak#y=2012&m=4">View Full Schedule</a></li>
			<li class="more seriesloge"><a href="/schedule/sortable.jsp?c_id=oak&year=2012">View Sortable Schedule</a></li>
			<li class="more seriesloge"><a href="/schedule/downloadable.jsp?c_id=oak&year=2012">View Downloadable Schedule</a></li>
		</ul>
	</div>
</div>





		</div>
	</div><!--Main Column 2 End-->
