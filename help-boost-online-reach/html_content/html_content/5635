<script type="text/javascript">
var jQ = jQuery;
jQ(document).ready(function(e) {
var button = jQ("#shareit"); // Share It Button
var code = jQ("#sharecode"); // Buttons Code DIV
var container = jQ("#share-container"); // Social Media Container DIV
var res = 1; // To check if the code DIV is open or close
/* Click Function */
button.click(function(e) {
code.slideToggle("slow");
container.stop();
if( res == 0 ){
container.animate({
top: '254px'
} , 500);
return res = 1;
}else{
container.animate({
top: '20px'
} , 500);
return res=0;
}
});
/* Scroll Function */
jQ(window).scroll(function(e) {
var scrl = jQ(this).scrollTop(); // Getting Vertical Scroll bar position
container.stop();
if(scrl == 0 && (res != 0)){
container.animate({
top: '254px'
} , 500);
}else if(scrl==0 && res==0){
container.animate({
top:'20px'
} , 500);
}else if(scrl > 0){
container.animate({
top: '20px'
} , 500);
}
});
});
</script>







