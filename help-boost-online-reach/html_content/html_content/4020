  <div class="widget_embed_code_item margin widget" id="widget_embed_code_item_224647">
    <script>
var slideWidth = 320;
var currentPosition = 0;
var numberOfSlides = 0;
var count = 0;
var slideshow = null;

function galeriaInterval(numberOfSlides) {
    currentPosition = counter(currentPosition, numberOfSlides);
    nextSlide(currentPosition, numberOfSlides);
}

function nextSlide(currentPosition, numberOfSlides){

    manageControls(currentPosition, numberOfSlides);
    $('.fcbotigaSlider .images').animate({
        'marginLeft' : slideWidth*(-currentPosition)
    });

    $('.navigation ul li a').each(function(ev){
        if($(this).attr('rel') == currentPosition){
            $(this).addClass('active');
        }else {
            $(this).removeClass('active');
        }
    }
    );
}

function counter(count, numberOfSlides){
    count++;
    if(count >= numberOfSlides){
        count = 0;
    }
    return count;
} 
function manageControls(position, numberOfSlides){
    if(position<=0){ $('#leftControl').hide(); $('#leftControlArrow').show(); } else{ $('#leftControl').show();$('#leftControlArrow').hide(); }
    if(position==numberOfSlides-1){ $('#rightControl').hide(); $('#rightControlArrow').show(); } else{ $('#rightControl').show();$('#rightControlArrow').hide();}
}
$(document).ready(function(){

    var tmp = 0;

    tmp = $('.fcbotigaSlider .navigation ul li').length;

    $('.fcbotigaSlider .navigation ul').css({
        'width' : (tmp*19), 
        'right' : '30px'
    });

    $('.fcbotigaSlider .navigation #leftControl').css({
        'right' : (20) + (tmp*19)
    });
    $('.fcbotigaSlider .navigation #leftControlArrow').css({
        'right' : (20) + (tmp*19)
    });
  var slides = $('.fcbotigaSlider .images img');
  numberOfSlides = slides.length;
  $('.fcbotigaSlider .images').css('overflow', 'visible');

  $('.fcbotigaSlider .images').css('width', slideWidth * numberOfSlides);

  slideshow = window.setInterval('galeriaInterval('+numberOfSlides+')', 5000);

  $('.control')
    .bind('click', function(){
        if($(this).attr('id') =='leftControlArrow'){return;}
        if($(this).attr('id') =='rightControlArrow'){return;}

        window.clearInterval(slideshow);

if(($(this).attr('id')=='rightControl')){
            currentPosition = parseInt(currentPosition)+1;
        }else{
            currentPosition = parseInt(currentPosition)-1;
            if(currentPosition < 0){
                currentPosition = 0;
            }
        }

      nextSlide(currentPosition, numberOfSlides);

      slideshow = window.setInterval('galeriaInterval('+numberOfSlides+')', 5000);
  });

  $('.fcbotigaSlider .navigation ul li a').click(function(){

      window.clearInterval(slideshow);

    currentPosition = $(this).attr('rel');
    nextSlide(currentPosition, numberOfSlides);

    slideshow = window.setInterval('galeriaInterval('+numberOfSlides+')', 5000); 
  });

}); 

</script>
