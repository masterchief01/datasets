<script type="text/javascript" src="http://cdn1.static.videobash.com/js/swfobject.js" language="javascript"></script>
<script type="text/javascript">
	var playCheck = false;

    function displayPlayer(autoplay) {
     	var autoplay = typeof(autoplay) != 'undefined' ? autoplay : true;
        var flashvars = {};
        flashvars.autoplay = autoplay;
        flashvars.autoreplay = "true";
        flashvars.video_url = 'http://' + 'media1.ord.videobash.com/mp4/000/013/278/480_13278.mp4';
        flashvars.options = 'http://' + 'www.videobash.com/';
        flashvars.related_url = 'http://' + 'www.videobash.com/video/player_related' + '?id=13278';
        flashvars.link_url = 'http://' + 'www.videobash.com/video_show/amazing-trick-shots-13278';
        flashvars.video_title = "Amazing Trick Shots!"
        flashvars.isFavorite = '0';
        flashvars.favorite_js = 'addFavorite(13278)';
        flashvars.embed_js = "showEmbed()";
        flashvars.tj_zone_url = "http://delivery.trafficjunky.net/deliver2.php?zone_id=918&site_id=26";
                        		playCheck = true;
        var params = {};
        params.allowfullscreen = "true";
        params.allowScriptAccess = "always";
        params.wmode = "transparent";
        swfobject.embedSWF("http://resources.videobash.com/flash/player.swf", "player", "636", "425", "9.0.115", "", flashvars, params);
    }
</script>
