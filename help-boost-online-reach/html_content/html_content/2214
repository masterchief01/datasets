<script type="text/javascript">
	function flagVideo() {
		var option = $('input:radio[name=flag_option]:checked').val();
		var details = $("#flag_details").val();
		
		$.post(
			'http://www.videobash.com/flagitem',
			{'type':'video', 'id':13278 , 'option':option, 'details':details},
			function(response) {
				$("#flaggedMsg").html(response);
			}
		);			
		showHide('#flaggedMsg');
		showHide('div#formFlag');
	}

	var AddThisAPI = {
		endPoint_path		: "http://api.addthis.com/oexchange/0.8",
		endPoint_forward	: "/forward/",
		endPoint_url		: "/offer?url=",
		endPoint_title		: "&title=",
		//endPoint_description: "&description=",
		endPoint_username	: "&username=",
		endPoint_template	: "&template=",

		shareMenu: function(url,title,username) {
			return	this.endPoint_path +
					this.endPoint_url + url +
					this.endPoint_title + title +
					this.endPoint_username + username;
		},

		shareForward: function(forward,url,title,username,template) {
			return	this.endPoint_path +
					this.endPoint_forward + forward +
					this.endPoint_url + url +
					this.endPoint_title + title +
					this.endPoint_username + username +
					(template ? this.endPoint_template + template : '');
		}
	};

	function shareWindow(theURL,winName,features) {
	  window.open(theURL,winName,features);
	  return false;
	}

	function valueWidth(){
		if($("div.green-bar").width() <= ($("div.bar").width() / 2)){
			$("div#thumbs_percent").width($("div.bar").width() - $("div.green-bar").width());
		}else{
			$("div#thumbs_percent").width($("div.green-bar").width());
		}
	}

	function globeRollover(e){
		var globe_top = $("div.globe-text").parent().offset().top+$("div.globe-text").outerHeight();
		var globe_left = $("div.globe-text").parent().offset().left + ($("div.globe-text").outerWidth()/2);
		var btn_top = $(e.currentTarget).offset().top;
		var btn_left = $(e.currentTarget).offset().left + ($(e.currentTarget).outerWidth()/2);
		$("div.globe-text").css("left", Math.round(btn_left-globe_left - 1) );
		$("div.globe-text").css("top", Math.round(btn_top-globe_top + 14) );
		$("div.globe-text p").text( $(e.currentTarget).attr("title") );
		$("div.globe-text").show();
		$("div.globe-text p").css("padding-top", $("div.globe-text p").height() <= 14 ? "6px" : "0" );
	}

	function globeRollout(e){
		$("div.globe-text").hide();
	}

	$(document).ready(function(){
		valueWidth();

		$("div.up a").mouseenter(globeRollover);
		$("div.down a").mouseenter(globeRollover);
		$("li.facebook a").mouseenter(globeRollover);
		$("li.twitter a").mouseenter(globeRollover);
		$("li.stumble a").mouseenter(globeRollover);
		$("li.buzz a").mouseenter(globeRollover);
		$("li.messenger a").mouseenter(globeRollover);
		$("li.digg a").mouseenter(globeRollover);
		$("li.email a").mouseenter(globeRollover);
		$("li.more a").mouseenter(globeRollover);
		$("div.favorite a").mouseenter(globeRollover);
		$("div.favorite a").mouseenter(globeRollover);
		$("li.more a").mouseenter(function()		{ moveBoxApi(); });

		$("div.up a").mouseleave(globeRollout);
		$("div.down a").mouseleave(globeRollout);
		$("li.facebook a").mouseleave(globeRollout);
		$("li.twitter a").mouseleave(globeRollout);
		$("li.stumble a").mouseleave(globeRollout);
		$("li.buzz a").mouseleave(globeRollout);
		$("li.messenger a").mouseleave(globeRollout);
		$("li.digg a").mouseleave(globeRollout);
		$("li.email a").mouseleave(globeRollout);
		$("li.more a").mouseleave(globeRollout);
		$("div.favorite a").mouseleave(globeRollout);
		$("div.favorite a").mouseleave(globeRollout);
		$("li.more a").mouseleave(function()		{ moveBoxApi(); });

		$("li.facebook a").click(function()		{ return shareWindow( AddThisAPI.shareForward("facebook","http://www.videobash.com/video_show/amazing-trick-shots-13278","Amazing Trick Shots!",'videobash'),"facebookWindow", "width=634,height=531"); });
		$("li.twitter a").click(function()		{ return shareWindow( AddThisAPI.shareForward("twitter","http://www.videobash.com/video_show/amazing-trick-shots-13278","Amazing Trick Shots!",'videobash','{{title}}: {{url}} via @Videobash'),"twitterWindow", "width=634,height=531"); });
		$("li.stumble a").click(function()		{ return shareWindow( AddThisAPI.shareForward("stumbleupon","http://www.videobash.com/video_show/amazing-trick-shots-13278","Amazing Trick Shots!",'videobash'),"stumbleUponWindow", "width=850,height=631"); });
		$("li.buzz a").click(function()			{ return shareWindow( AddThisAPI.shareForward("googlebuzz","http://www.videobash.com/video_show/amazing-trick-shots-13278","Amazing Trick Shots!",'videobash'),"buzzWindow","width=823,height=529"); });
		$("li.digg a").click(function()			{ return shareWindow( AddThisAPI.shareForward("digg","http://www.videobash.com/video_show/amazing-trick-shots-13278","Amazing Trick Shots!",'videobash'),"diggWindow","width=823,height=520"); });


					$("div#favorite a").click(function(){
							});
			});

		function addFavorite(id){
		showHide('div.login-box-wrapper');
	}
		var t_up = 1296;
	var t_down = 97;
	function rateItem(id, type)
	{
		$.post(
			'http://www.videobash.com/video/rateVideo',
			{'id':id, 'type':type},
			function(response) {
				if(response){
					if(type){
						t_up = parseInt(response);
						var t_percent = (t_up * 100) / (t_up + t_down);
						$("#thumbs_up").text(response);
						$("#thumbs_width").css("width", t_percent + "%");
					}
					else{
						t_down = parseInt(response);
						var t_percent = (t_up * 100) / (t_up + t_down);
						$("#thumbs_down").text(response);
						$("#thumbs_width").css("width", t_percent + "%");
					}
					$("#thumbs_percent").html('THANKS!<br />' + Math.ceil(t_percent) + '%');
					$("#thumbs_percent").css("padding", "17px 0");
					$("#thumbs_percent").css("width", "100%");
				}
				else{
					$("#thumbs_percent").html('ALREADY<br />RATED');
					$("#thumbs_percent").css("padding", "17px 0");
					$("#thumbs_percent").css("width", "100%");
				}
			}
		);
	}
	
	function rateItemF(id, type)
	{
		if(type){
			t_up = parseInt($("#thumbs_up").text()) + 1;
			var t_percent = (t_up * 100) / (t_up + t_down);
			$("#thumbs_up").text(t_up);
			$("#thumbs_width").css("width", t_percent + "%");
		}
		else{
			t_down = parseInt($("#thumbs_down").text()) + 1;
			var t_percent = (t_up * 100) / (t_up + t_down);
			$("#thumbs_down").text(t_down);
			$("#thumbs_width").css("width", t_percent + "%");
		}
		$("#thumbs_percent").html('THANKS!<br />' + Math.ceil(t_percent) + '%');
		$("#thumbs_percent").css("padding", "17px 0");
		$("#thumbs_percent").css("width", "100%");
	}    
</script>	</div>
	
