			function facebookLogin(response) {
			    FBC.init({appId: '154739397877434',
						 status: true,
						 cookie: true,
                 		 xfbml: true});

				FBC.getLoginStatus(function(response) {
					if(response.session) {
						//facebookLogin(response);
					}
				});
				
				if(response == null) {
					FBC.login(function(response) {
						if(response.session) {
							$.post("http://www.videobash.com/user/facebookLogin/", '', onFBSubmitLogin);
						}
					});
				} else {
					$.post("http://www.videobash.com/user/facebookLogin/", '', onFBSubmitLogin);
				}
			}

			function onFBSubmitLogin(data, status){
				if(data == 1){
					document.location.reload();
				}else{
					return false;
				}
			}
