      function cleanSearchInput()
      {
          var text = document.getElementById('query-field').value;
          text = text.replace(/^\s+|\s+$/g,"");
          text = text.replace(/^\s+/,"");
          text = text.replace(/\s+$/,"");
          document.getElementById('query-field').value = text;
          return true;
