    <script type="text/javascript">

        $(document).ready(function(){

			$("#weatherAlert ul li").each(function () {

				if ($(this).text() == ""){

					$("#weatherAlert").css('display', 'none');

				} else {

					$("#player").css('marginTop', '100px');

					$("#weatherAlert").css('display', 'block');

					$('#weatherAlert ul').quote_rotator();

				}

			})

			

            $('#newsletterForm').html5form(); 

			

			$("#headerNews").click(function(){

            

                $( "#newsletterForm" ).dialog({

                    height: 320,

                    width: 500,

                    modal: true,

					resizable: false,

                    closeOnEscape: true,

                    autoOpen: true

                });

            });

			$("#headerNews2").click(function(){

            

                $( "#newsletterForm" ).dialog({

                    height: 320,

                    width: 500,

                    modal: true,

					resizable: false,

                    closeOnEscape: true,

                    autoOpen: true

                });

            });

			

			$(".join").click(function(){

				$.post("process/wd_newsletter.php", { email: $("#emailField").val(), state: $("#state").val(), country: $("#country").val() },

				function(html){

					$(".newsletteremail").html(html);

				});

            });	

			

        });

    </script>

