	<script type="text/javascript" charset="utf-8">
		$(document).ready(function() {
			if($('.artcomments').length > 0) {
				$('.globalcomments').hide();
			} else {
				$('.listcomments a').addClass('active');	
			}

			$('.commenttabs a').click(function() {
				$('.commenttabs a').removeClass('active');
				$(this).addClass('active');
			});

			$('.listcomments').click(function() {
				$('#comments div').hide();
				$('.globalcomments').show();
				return false;
			});

			$('.articlecomments').click(function() {
				$('#comments div').hide();
				$('.artcomments').show();
				return false;
			});
			
			$('.articlecomments').click(function() {
				$('#comments div').hide();
				$('.artcomments').show();
				return false;
			});
			
			if($('.content').length > 1) {
				$('.content').hide();
				$('#content1').show();
				
				$('.showcontent').click(function() {
					$('.showcontent').css('color', '#333');
					$(this).css('color', '#ac0101');
					var content = $(this).attr('title');
					$('.content').hide();
					$('#'+ content).show();
					return false;
				});
			}
			
			$('#entirelist ul').hide();
			
			
			$('#entirelist').find('ul:first').slideDown();
			i = 3;
			$('.showmore a').click(function() {
				if($('#entirelist ul:nth-child(' + i + ')').length) {
					$('#entirelist').find('ul:nth-child(' + i + ')').slideDown();
					i++;
					if($('#entirelist ul:nth-child(' + i + ')').length == 0) {
						$(this).find('img').attr('src', '/mhlists/2/images/more-disabled.gif');
					}
				}
				return false;
			});
		});
	</script>
