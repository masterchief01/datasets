<script type="text/javascript">
	function toggleDisplay(gameId, type)
	{
		thisGameButton = jQuery('#detail-display-button-' + gameId)

		buttontext = thisGameButton.html()

		if (type == 'null')
		{
			if (buttontext.indexOf('+') == -1)
			{
				type = 'collapse'
			}
			else
			{
				type = 'expand'
			}
		}

		if (type == 'collapse')
		{
			jQuery('#detail-display-' + gameId).css('display', 'none');
			jQuery('#' + gameId + '-gameLinks').css('display', 'none');
			if (jQuery('#'+gameId+'-gameNote').html().length > 6)
			{
				jQuery('#' + gameId + '-gameLinks-expand-container').css('border-bottom', 'none');
				jQuery('#'  + gameId + '-gameNote').css('border-top', '1px solid #C2C2C2');

			}
			thisGameButton.html('+');
		}
		else
		{
			jQuery('#detail-display-' + gameId).css('display', 'block');
			jQuery('#' + gameId + '-gameLinks').css('display', 'block');
			//jQuery('#' + gameId + '-gameLinks-expand-container').css('border-bottom', '1px solid #C2C2C2')
			jQuery('#'  + gameId + '-gameNote').css('border-top', 'none');
			var noteLength = jQuery('#'+gameId+'-gameNote').html().length;
			if (noteLength > 6)
			{
				jQuery('#' + gameId + '-gameLinks-expand-container').css('border-bottom', '1px solid #C2C2C2');
			}

			if (jQuery('#' + gameId + '-lastPlay').html().length > 6)
			{
				if (noteLength > 6)
				{
					jQuery('#' + gameId + '-lastPlay').removeClass('border-top').addClass('border-bottom');
				}
				else
				{
					jQuery('#' + gameId + '-lastPlay').addClass('border-top').removeClass('border-bottom');
				}
			}

			thisGameButton.html('-');
		}
	}

	function toggleAll()
	{
		type = 'expand'
		thisGameButton = jQuery('#expando-button')

		buttontext = thisGameButton.html()

		if (buttontext.indexOf('Expand') == -1)
		{
			thisGameButton.html('Expand All Games [+]')
			type = 'collapse'
		}
		else
		{
			thisGameButton.html('Collapse All Games [-]')
			type = 'expand'
		}

		theseGames = jQuery('.detail-display')

		for(var z=0; z<theseGames.length; z++)
		{
			thisGameId = jQuery(theseGames[z]).attr("id").substring(15)
			toggleDisplay(thisGameId, type)
		}
	}

	function gameObj(gameId, homeTeamId, awayTeamId)
	{
		this.gameId = gameId
		this.homeTeamId = homeTeamId
		this.awayTeamId = awayTeamId
	}
	var gameList = new Array()

	function espn360GameObj(gameId, gameTime, replay)
	{
		this.gameId = gameId
		this.gameTime = gameTime
		this.replay = replay
	}
	var espn360GameList = new Array()
</script>
