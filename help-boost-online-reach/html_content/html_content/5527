<script type="text/javascript">
/* Page JS */

/* Module JS */
// The loc parameter specifies the instance (number) of the menu you'd like to
// show after hiding all others. Specifying a value of 0 or not giving a value
// at all simply hides all instances of the menu.
function addmenu_toggleDrop(loc)
{
   if (typeof(loc) == "undefined")
      loc = 0;

   if (!document.getElementById)
      return;

   var i;

   // Hide all other instances of the menu. This way, clicking on one causes
   // all others to be hidden.
   for (i = 1; true; i++)
   {
      instance = addmenu_getInstance(i);

      if (!document.getElementById(instance))
         break;

      var drop;

      drop = document.getElementById(instance);

      if (drop && drop.style.visibility == "visible")
         addmenu_hideDrop(drop);
      else
      {
         // Only show the instance of the one that we were really trying to
         // toggle.
         if (i == loc)
            addmenu_showDrop(drop);
      }
   }
}

function addmenu_showDrop(drop)
{
   drop.style.visibility = "visible";

   if (typeof(addmenu_showCallback) != "undefined")
      addmenu_showCallback(drop);
}
function addmenu_hideDrop(drop)
{
   drop.style.visibility = "hidden";

   if (typeof(addmenu_hideCallback) != "undefined")
      addmenu_hideCallback(drop);
}
function addmenu_mouseOut(e)
{
   var id = this.getAttribute("id");
   var drop = document.getElementById(id);

   var dropRegion = YAHOO.util.Dom.getRegion(drop);
   var mousePoint = new YAHOO.util.Point(YAHOO.util.Event.getPageX(e), YAHOO.util.Event.getPageY(e));

   if (!dropRegion.contains(mousePoint))
      addmenu_hideDrop(drop);
}

function addmenu_getInstance(loc)
{
   if (loc == 1)
      return "addmenudrop";
   else
      return "addmenudrop" + loc.toString();
}

YAHOO.util.Event.addListener("addmenudrop", "mouseout", addmenu_mouseOut);
YAHOO.util.Event.addListener("addmenudrop2", "mouseout", addmenu_mouseOut);(function(){
   new YAHOO.Media.Buzz(
      "buzz_form",
      {
         "countPosition":"after",
         "loggedIn":false,
         "buzzUp":false,
         "showCount":true
      }
   );
}());      (function(){
   new YAHOO.Media.Buzz(
      "buzz_form_below",
      {
         "countPosition":"after",
         "loggedIn":false,
         "buzzUp":false,
         "showCount":true
      }
   );
}());      
</script>

