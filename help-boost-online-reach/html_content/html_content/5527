(function() {
var searchBar, text;
searchBar = document.getElementById("yat-search-bar");
searchBar.className = searchBar.className.replace("yat-search-bar-nojs", "", "g");
var text = document.getElementById("yat-search-bar-text");
var checkText = function()
{
   if (text.value !== "")
   {
     text.parentNode.className += " active";
   }
};
checkText();
text.onfocus = function(e)
{
   if (this.parentNode)
   {
      this.parentNode.className += " active";
   }
};
text.onblur = function(e)
{
   if (this.value == "")
   {
      this.parentNode.className = this.parentNode.parentNode.className.replace("active", "", "g");
   }
};
if (window.attachEvent) { window.attachEvent("onload", checkText); }
})();
</script>
