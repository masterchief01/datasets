/* push gaq to bottom of seccol */
var l = document.getElementById("pricol_art");
var r = document.getElementById("seccol_art");
var g = document.getElementById("yatgaqsim");
if (l && r) {   
   if (l.offsetHeight > r.offsetHeight) {      
      if (g) {
         g.style.marginTop = (l.offsetHeight - r.offsetHeight) + "px";
      }
   }
}
