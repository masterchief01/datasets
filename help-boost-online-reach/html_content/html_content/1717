			//<![CDATA[
					function checktopsearch(frm) {
						if (frm.q.value.replace(/^[\s\t]*/, "").replace(/[\s\t]*$/, "") == '') {
							alert("Please enter some search text");
							frm.q.focus();
							return false;
						}
						return true;
					}
