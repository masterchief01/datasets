			);
			
			var doScroll = false;
			
			function updateProfileLinks(commentsTable) {
				
				if(doScroll){
					document.location.href=document.location.href.replace(/#.*/, '') + "#sitelife-comments-bottom";
				}
				//get comment rows
				var commentsForm = $('.Comments_From')
				
				for (var cr = 0; cr < commentsForm.length; cr++ ) {
					try {
						//get username link on current comment row
						var profileUserNameLink = commentsForm[cr]
								.getElementsByTagName('A')[0];
												
						//update username link
						profileUserNameLink.href = profileUserNameLink.href.replace(
								/community\/profile.*/, 
								'community/profile/' + profileUserNameLink.innerHTML + '/'
							);
							
						//update avatar link
						$('.Comments_UserImage')[cr]
								.getElementsByTagName('A')[1].href = profileUserNameLink.href;
													
						//log update
						fcwLogger('info','updateProfileLinks(): updated ' + profileUserNameLink.innerHTML);
						
					} catch (err) { 
						fcwLogger('error','updateProfileLinks(): error updating ' + profileUserNameLink.innerHTML, err);
					}
					
				}
				//now update pagination to re-triger this update
				var paginator = document.getElementById('Paginator');
				if (paginator) {
					var paginatorLinks = paginator.getElementsByTagName('a');
					for (var p = 0; p < paginatorLinks.length; p++ ) {
						
						/*
						Using an anonymous function ensures that the var 'onclickEvent'
						 is bound to the old onclick event within the function scope of the
						 new onclick event
						*/ 
						(function(){
							var onclickEvent = paginatorLinks[p].onclick;
							paginatorLinks[p].onclick = function() {
							$('.Comments_Table')[0].className = '_Comments_Table';
							onclickEvent()
							waitForSLContent();
							 return false;
							}
						})();
					}
				}
				
				//If comments are loaded again we want to scroll top of comments
				doScroll = true;
			}
			
			function waitForSLContent() {				
				//check for Comments_Table class
				var commentsTable = $('.Comments_Table');
				if (commentsTable.length == 1) {
					updateProfileLinks(commentsTable[0]);
				} else { //check again in 1 second
					setTimeout('waitForSLContent()',1000);
					
					fcwLogger('info','waitForSLContent(): waiting for comments');
				}
			}
			
			var rewriteProfileURLS = 0;
			if (rewriteProfileURLS) {
				$(document).ready(function () {
					/* need to update profile page urls to include username
						This needs to happen after the comments widget has finshed loading
						so we will check back every second to see if it has finished loading */
					setTimeout('waitForSLContent()',1000);
				});
			}
			
		//]]>
		</script>
		
