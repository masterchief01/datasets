	// for image gallery kyte shows
	if (typeof(statsPageProps.galleryName) != 'undefined') {
		s.prop35=statsPageProps.galleryName;
		s.eVar12=statsPageProps.galleryName;
	}
	
	// for offers flyout
	if (typeof(statsPageProps.flyout) != 'undefined') {
		s.prop36 = statsPageProps.flyout;
	}
}





s.events=eventsList
s.eVar1=eVar1Val
s.eVar2=eVar2Val
s.eVar3=eVar3Val
s.eVar4=eVar4Val
s.eVar5=""


s.pageName = setOmniturePageName(pageName);
if (typeof(statsPageProps)!='undefined' && typeof(statsPageProps.pageNameSuffix)!='undefined') {
	s.pageName += statsPageProps.pageNameSuffix; // adds a suffix if present on page.
}

if (typeof(statsClassifiedCategory) != "undefined") {
	
	s.prop4 = statsClassifiedCategory;
	
	s.prop18 = s.prop18 || statsClassifiedCategory;
}
if (typeof(statsContentCategory) != "undefined") {
	s.prop18 = statsContentCategory;
}

if (s.prop4 != "" && s.prop5 != "") s.prop12 = s.prop5 + "_" + s.prop4;
if (s.prop4 != "" && s.prop16 != "") s.prop17 = s.prop16 + "_" + s.prop4;
var s_code=s.t();if(s_code)document.write(s_code);


var puffLinks = $('*[type=puff] a');
//Add click event
puffLinks.click(function(){
	//get object id
	var parentEl = $(this);
	var objectId = "";
	var containerName = "";
	
	//We want to find the content object on the page.
	do {
		parentEl = parentEl.parent();
	} while (parentEl && 
			!parentEl.attr('tagName').toLowerCase().match(/(tm:)?contentobject/));

	if (!parentEl) {
		tm.logger.error(['Puff Tracking:', 'No parent tm:contentobject tag'], OMNI_DEBUG_LEVEL);
		return !tm.logger.enabled; //if debug mode don't follow link
	} else {
		objectId = parentEl.attr('objectid');
	}
	
	// So we now have the content object we are looking for - we continue on up the tree
	// to get the name of the container holding it.
	do {
		parentEl = parentEl.parent();
	} while (parentEl && 
			!parentEl.attr('tagName').toLowerCase().match(/(tm:)?container/) );

	if (!parentEl) {
		tm.logger.error(['Puff Tracking:', 'no parent tm:container tag'], OMNI_DEBUG_LEVEL);
		return !tm.logger.enabled; //if debug mode don't follow link
	} else {
		containerName = parentEl.attr('name');
	}

	tm.logger.debug(['Puff Tracking:', 'objectId:', objectId, 
			'containerName:', containerName, parentEl ], OMNI_DEBUG_LEVEL);

	s.eVar13 = s.pageName + '-' + containerName + '-' + objectId;
	s.events = 'event17';
	s.t();
	
	tm.logger.info(['Puff Tracking:', 'Omniture event sent' ], OMNI_DEBUG_LEVEL);

	return tm.logger.debugLevel != OMNI_DEBUG_LEVEL; //if debug mode don't follow link
});
//Log to console if required.
tm.logger.debug( [ 'Puff Tracking: Links', puffLinks ], OMNI_DEBUG_LEVEL );




//--></script>
