				tm.cookieMasthead = function(){
					var siteName = "sunday-mail",
						mastheadRE = new RegExp(siteName);

					var mastheadcookie = tm.getCookieVal(siteName);
					
					if(document.URL.match(mastheadRE)){
						tm.setCookieVal(siteName,"true","","/");
						tm.setCookieVal(siteName + "title","Weird News","","/");
					}

					mastheadcookie = tm.getCookieVal(siteName);
					mastheadcookietitle = tm.getCookieVal(siteName + "title");
					
