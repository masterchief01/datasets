  scroll: 1,
  wrap: 'both',
  auto: 6,
  itemFallbackDimension: 608,
  initCallback: 
  function (carousel) {
    $('.control div.slide-btn', carousel.container).bind('click', function() {
      carousel.scroll(jQuery.jcarousel.intval($(this).attr('image_id')));
      $('.control div',carousel.container).removeClass('selected');
      $(this).addClass('selected');
      return false;
    });
    $('.control div.next', carousel.container).bind('click', function() {
      carousel.next();
      $('.control div',carousel.container).removeClass('selected');
      $('.control div[image_id='+carousel.first+']',carousel.container).addClass('selected');
