  (function () {
    var p = new Image(1, 1);
    p.onerror = p.onload = function () {
      p.onerror = p.onload = null;
    };
    p.src = ["//p.staticworld.net/pixel.gif?aid=249729&d=", (new Date()).getTime()].join('');
  })();
</script>


</body>
</html>
