	PCWSocial.loadRegoSignInPop();
	logUserRegistration("social bar");
});
$('.social-bar-logo > a').click(function() {
    PCWSocial.loadSignInPop();
});
SocialBadgePath = {};
SocialBadgePath.facebook20 = "http://zapp3.staticworld.net/images/common/social_badge/facebook_20.png";
SocialBadgePath.twitter20 = "http://zapp2.staticworld.net/images/common/social_badge/twitter_20.png";
SocialBadgePath.yahoo20 = "http://zapp0.staticworld.net/images/common/social_badge/yahoo_20.png";
SocialBadgePath.google20 = "http://zapp1.staticworld.net/images/common/social_badge/google_20.png";
SocialBadgePath.linkedin20 = "http://zapp3.staticworld.net/images/common/social_badge/linkedin_20.png";
PCWSocial.initSocialBar();
SocialConf.socialBarConnectParams['onClose'] = PCWSocial.checkGigyaLoginState;  
gigya.services.socialize.showAddConnectionsUI(SocialConf.main, SocialConf.socialBarConnectParams);
gigya.services.socialize.showLoginUI(SocialConf.main, SocialConf.socialBarLoginParams);
gigya.services.socialize.addEventHandlers(SocialConf.main, {
    context : {
        str : ''
    },
    onLogin : PCWSocial.gigyaGetUserInfoHandler,
    onConnectionAdded : PCWSocial.gigyaGetUserInfoHandler
});
function doGigyaAjax(res) {
    var gUid = new String(res.user.UID);
    var nickname = new String(res.user.nickname);
    var firstName = new String(res.user.firstName);
    var lastName = new String(res.user.lastName);
    var email = new String(res.user.email);
    var proxiedEmail = new String(res.user.proxiedEmail);
    var provider = new String(res.user.loginProvider);
    var signatureTimestamp = new String(res.user.signatureTimestamp);
    var signature = new String(res.user.UIDSignature);
    var comment = "";
    try {comment = $('textarea[name=comment_text_reg]').val();}catch(e){comment = "";};
    var threadId = 0;
    try {threadId=ArticleComments.threadId;}catch(e){threadId = 0;};
    $('#rego_signin_pop').css({"top": $(window).scrollTop() + 200});
    $.post("/pcworldconnect/ajax.html", {
        gUid : gUid,
        nickname : nickname,
        firstName : firstName,
        lastName : lastName,
        email : email,
        proxiedEmail : proxiedEmail,
        provider : provider,
        signatureTimestamp : signatureTimestamp,
        signature : signature,
        comment : comment,
        threadId : threadId
    }, function(data) {
        try {
    	   var resObj = eval('(' + data + ')');
        } catch (e) {
            var badResponse = "Oops something went wrong connecting you to your social service, try again later";
            $('#rego_signin_pop').fadeIn(1000);
            $('#rego_signin_pop').html('<div class="popContent radius_5 clearfix" id="social-bar-only-popContnet">'+ badResponse +'</div>');
            $('#social-bar-only-popContnet').before('<div class="close"><a href="javascript:PCWSocial.closeRegoSignInPop();"><img src="http://zapp0.staticworld.net/images/common/close-icon-clear.gif"/></a></div>');
            return false;
        }
        if (!resObj.sigVerification) {
            /*
            var badSigMessage = "Oops something went wrong connecting you to your social service, try again later";
            $('#rego_signin_pop').fadeIn(1000);
            $('#rego_signin_pop').html('<div class="popContent radius_5 clearfix" id="social-bar-only-popContnet">'+ badSigMessage +'</div>');
            $('#social-bar-only-popContnet').before('<div class="close"><a href="javascript:PCWSocial.closeRegoSignInPop();"><img src="http://zapp0.staticworld.net/images/common/close-icon-clear.gif"/></a></div>');
            */
            return false;
        }
        if (resObj.isNewReg) {
            $('#regForm').hide();
            $.post('/pcworldconnect/mergeStart', {
                gUid : resObj.gUid,
                pid : resObj.pid   
            }, function(data) {
                if ($('input[name=from-social-create]').val() == "true") {
                    $('#regContentContainer').html(data);
                }  else {
                    $('#rego_signin_pop').fadeIn(1000);
                    $('#rego_signin_pop').html('<div class="popContent radius_5 clearfix" id="social-bar-only-popContnet">'+ data +'</div>');
                    $('#social-bar-only-popContnet').before('<div class="close"><a href="javascript:PCWSocial.closeRegoSignInPop();"><img src="http://zapp0.staticworld.net/images/common/close-icon-clear.gif"/></a></div>');
                }
                $('input[name=merge_email]').focus();
            });
        } else {
            reload();
        }
   });
}
</script>



	
		
	

	
	
	
	
	
	
	
	

    
