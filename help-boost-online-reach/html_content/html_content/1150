	jQuery(window).scroll(function() {
		var b = jQuery(window).scrollTop();
		var d = jQuery(".redbluefloaterwidget-3-anchor").offset().top;
		var p = jQuery(".redbluefloaterwidget-3-placeholder");
		var c = jQuery(".redbluefloaterwidget-3");
		var w = c.width();
		var h = c.height();
		
		p.css({width: c.width()+"px", height: (c.height()+10)+"px"});
		
