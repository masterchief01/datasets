<script>
(function ($, NCA) {
	NCA.page = NCA.page || {};
	NCA.page.story = function(){
		var saveid, $storyheader = $('#story div.story-header');
		if ($('#story').length) {
			saveid = location.href;
	        if (saveid) {
	            saveid = saveid.match(/(story|gallery)+-+(\w{8})+-(\d{13})/);
				if (saveid) {
				    $('#story div.story-header').attr('id', saveid[0]);
				}
	        }
			$storyheader.saveicon({ 'appendto': '.story-headline', 'flavourtext': ['<strong>Save this story</strong> to read later', '<strong>This story is saved</strong> to read later'] });
			$storyheader.append($('#story div.story-summary-list'));
			$storyheader.append($('div.plmnt-alsoin')); // may use IDs for speed
			$('#story div.story-intro').before($('#story div.story-promo-middle'));
			$('#story div.story-intro').before($('#story div.story-sidebar'));
			$storyheader.append($('#story-social'));
			$storyheader.breadcrumbs();
		}
	};
}(ndm.jQuery, ndm.newscomau));
</script>
