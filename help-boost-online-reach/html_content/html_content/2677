<h1 class="changer">All Videos</h1>    
<div style="float:left;">
<div id="year">
			<span style="float:left; display:block; width:60px;">Filter</span>
            <ul>
            <li id="all" class="selected">All Videos</li>
            <li id="2011">2011</li>
            <li id="2010">2010</li>
            <li id="2009">2009</li>
            </ul>
        </div>
 <div id="sort">
 			<span style="float:left; display:block; width:60px;">Sort by</span>
            <ul>
            <li id="updated" class="selected">Most Recent</li>
            <li id="viewCount">Most Popular</li>
            <li id="videoName">A-Z</li>
            </ul>
        </div> 
<div style="padding-bottom:20px; width:100%; float:left;">
    <div id="big-box" style="width:100%; float:left;">
    </div>
</div>
     <script type="text/javascript">
	 	var str='updated';
		var year='all';
		
		$("#year li").click(function () {
		$(this).siblings().removeClass('selected');
		$(this).addClass('selected');
		year= $(this).attr('id');
		$(".changer").html(year+' Videos');
		showRSS(str, year);
		});
		
		$("#sort li").click(function () {
		$(this).siblings().removeClass('selected');
		$(this).addClass('selected');
		str= $(this).attr('id');
		showRSS(str, year);
		});
		

		
	function showRSS(sorter,filter,morevids)
	{
	if (str.length==0)
	  {
	  document.getElementById("big-box").innerHTML="";
	  return;
	  }
	else {}
	if (window.XMLHttpRequest)
	  {// code for IE7+, Firefox, Chrome, Opera, Safari
	  xmlhttp=new XMLHttpRequest();
	  }
	else
	  {// code for IE6, IE5
	  xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
	  }
	xmlhttp.onreadystatechange=function()
	  {
	  if (xmlhttp.readyState==4 && xmlhttp.status==200)
		{
			document.getElementById("big-box").innerHTML=xmlhttp.responseText;
			jQuery("div.clickable").click(
			function(e)
			{
				e.preventDefault();
				window.location = $(this).attr("url");
			});
			jQuery('div.holder-box').hover(
			  function () {
				jQuery(this).children('.rollover').removeClass('hidden')
			  }, 
			  function () {
				jQuery(this).children('.rollover').addClass('hidden');
			  }
			);
		}
	  }
	xmlhttp.open("GET","Content/getrss-2.php?q="+sorter+"&year="+filter,true);
	xmlhttp.send();
	}
	showRSS(str, year);



	</script> 


<!-- javascript coding -->



