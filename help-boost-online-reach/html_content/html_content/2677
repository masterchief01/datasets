		<script language="JavaScript">
	function getFlashMovie(movieName)
	{
		var isIE = navigator.appName.indexOf("Microsoft") != -1;
		return (isIE) ? window[movieName] : document[movieName];
	}
	
</script>
<script type="text/javascript">
$(function(){
		
$("#mainNav li a.conference").hover( 
function(){
$("#mainNav li.selected ul.x").hide();
$("#mainNav li.selected ul.z").hide();
},
function(){
$("#mainNav li.selected ul.x").show();
$("#mainNav li.selected ul.z").show();
}
);

$("#mainNav li ul.y li a").hover( 
function(){
$("#mainNav li.selected ul.x").hide();
$("#mainNav li.selected ul.z").hide();
},
function(){
