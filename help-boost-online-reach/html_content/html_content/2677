			jQuery(function() {
				var speakerNomination=false;				if(speakerNomination=='success')
				{
					$(".nominate .jqmConfirmWindow").html('<p style="margin-top:0"><a class="jqmClose submit">X</a></p><h2>Thank You!</h2><p>Your nomination has been submitted for review.</p>');
					$(".nominate").jqmShow();
					
				}
				else if(speakerNomination=='failure')
				{
					$(".nominate .jqmConfirmWindow").html('<p style="margin-top:0"><a class="jqmClose submit">X</a></p><h2>Sorry!</h2><p>Your request cannot be processed at this time. Please try again later.</p>');
					$(".nominate").jqmShow();
				}

				
				$("#signUp2").initEmailForm({"target":"/mcjoin","baseurl":"/"});
				$("#signUp5").initEmailForm({"target":"/mcjoin","baseurl":"/"});
				$("#contactForm").initContactForm({"target":"/mail","baseurl":"/"});
				// Menu Rollovers
				$().initRollovers("/");
				// Animated Quotes
				/*$("#quotes").iRottedInit("/",
					[
						"Images/Quotes/quote1.gif",
						"Images/Quotes/quote2.gif",
						"Images/Quotes/quote3.gif",
						"Images/Quotes/quote4.gif",
						"Images/Quotes/quote5.gif",
						"Images/Quotes/quote6.gif"
					]
				);*/
			});
		});
		
		
	
