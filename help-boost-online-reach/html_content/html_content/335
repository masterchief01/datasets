			function validateReviewForm() {
				if ((jQuery('input[name="post_rating"]').val() == 0) ||
					(jQuery.trim(jQuery('#reviewform #comment').val()) == '')) {
					jQuery('#required_fields_warning').css({'font-weight':'bold'});
