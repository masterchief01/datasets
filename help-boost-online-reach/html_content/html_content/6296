//<![CDATA[
$(document).ready(function() {
 // For mobile devices
 function toggle_menu(menu){
     $("div.ddmenu").children("ul").children("li").not(menu).find("ul,table").hide(0);
     var vis=0; // Cannot use toggle() to ensure desktop synch
        if(menu.find("ul,table").is(":visible")){menu.find("ul,table").hide(0);}else{vis=1;menu.find("ul,table").show(0);}
        if($("#html5_screen").length){if(vis){$("#html5_screen")[0].pause();$("#html5_screen").hide(0);}else{$("#html5_screen").show(0);}}
    }
    $("div.ddmenu").children("ul").children("li").click(function(){toggle_menu($(this));});
});
//]]>
</script>
