		<script type="text/javascript">


function checkscript() 

{

	strEmail = document.Form1._ctl0_Uc_sidefunnel_txtEmail.value;
	strWeight = document.Form1._ctl0_Uc_sidefunnel_txtWeight.value;
	strTargetWeight = document.Form1._ctl0_Uc_sidefunnel_txtTargetWeight.value;
	strAge = document.Form1._ctl0_Uc_sidefunnel_txtAge.value;
	
	if (isNaN(strWeight) == true)
	{
		alert("Please enter a numeric value for weight.");
		return false;
	}
	else if  (isNaN(strTargetWeight) == true)
	{
		alert("Please enter a numeric value for target weight.");
		return false;
	}
	else if  (isNaN(strAge) == true)
	{
		alert("Please enter a numeric value for your age.");
		return false;
	}
	
	else if  (strAge !=="" && strAge  < 18)
	{
		alert("You must be over 18 to sign up.");
		return false;
	}
	
	else if  (strAge  > 150)
	{
		alert("Please correct your age.");
		return false;
	}
	
	else if(isValidEmail(strEmail)==false)
	{
		alert("Please enter a valid e-mail address.");
		return false;
	}
	else
	{	
	

	
		
		return true;
		
	}
}
 

function isValidEmail(strEmail)
{
	validRegExp = /^[^@]+@[^@]+.[a-z]{2,}$/i;
	
	if (strEmail.search(validRegExp) == -1) 
	{
	  return false;
	} 
	else
	{
		return true; 
	}
}


