<!-- key[default-78740eb74ce5f010VgnVCM1000003d370a0aRCRD-recipecontent-a7f64558cf592110VgnVCM1000003d370a0aRCRD];compId[d507cf2a673ff010VgnVCM1000003d370a0aRCRD];dir[/467/151/v3Gw5oaB4dkz32KW5VNfo+FeU8o=.ser]-->


	    <script type="text/javascript">

 

      zl_host = 'www.ziplist.com';

	 

				

		function getEpoch(){

			return Math.floor((new Date()).getTime() / 1000 + 60 * 5);

		}

		

		var epoch = "";

		var partner_key = 'mslo'; 

		var partner_add_path = '/minibox/partner_add';

		

		function getblob(){

			epoch = getEpoch();

			return epoch + partner_key + userid + partner_add_path + getzl_request();

		}

			

				

		 function getzl_sha(){

			 var zl_sha_url = '/portalext/ajax/msozlsha';

			 var queryString = "BLOB="+getblob();

				new Ajax.Request(zl_sha_url, {

		  		method: 'post',

		  		parameters: queryString,

		

		 		onComplete: function(transport){

						zl_sha = transport.responseText;

						submitToZLInternal(zl_sha)

		 		},

		

				onException: function(req,exception) {

					//alert("Sorry, your request could not be performed. \n\n" + exception.message);

					//return true;

				},

		

		  		onFailure: function(transport){

		  			//alert('failure');

		  			//alert(transport.responseText);

		  		}

		

		  		});

		 }

		

		function submitToZLInternal(zl_sha){

		  var iframe = document.getElementById('zl_iframe');

		

		  submit_partner_add(iframe, zl_host, partner_add_path, epoch, partner_key, userid, getzl_request(), zl_sha);

		

		  document.getElementById('zl_div').style.display = 'inline';

		  return false;

		}

		function submitToZipList() {

			getzl_sha();		

		}

		function resetDemo() {

		    closeZl();

		    ziplist_reset_demo(zl_host);

		    return false;

		}

		

		function closeZl() {

			  document.getElementById('zl_div').style.display = 'none';

		  getIFrameDoc(document.getElementById('zl_iframe')).body.innerHTML = '';

		}

		

		var zlRequest ="";

		function getzl_request()

		{ 

			var t = new Template('<zl:envelope xmlns:zl="ziplist.com:rest-1-0"><partner_user_email>#{emails}</partner_user_email><publisher_recipe_id>#{recipeid}</publisher_recipe_id></zl:envelope>');

			var test ={emails: email, recipeid: contentId};

			zlRequest = t.evaluate(test);

			return zlRequest; 

		}



    </script><div id="ms-col2-article-content"><div class="ms-col2-article-content-inner"><div class="ms-col2-article-body">
<div class="ms-col2-article-body-inner">
<div class="ms-col2-recipe-ingredients">
<h2>Ingredients</h2>
<p>Makes 9 large or 16 small squares</p>
<ul>
<li>9 tablespoons unsalted butter, softened, plus more for pan</li>
<li>1 2/3  cups all-purpose flour</li>
<li>1  teaspoon baking powder</li>
<li>3/4  teaspoon coarse salt</li>
<li>1 cup packed light-brown sugar</li>
<li>2 large eggs</li>
<li>1 teaspoon pure vanilla extract</li>
<li>1/3 cup butterscotch chips</li>
<li>1/2 cup unsalted cashews, coarsely chopped (about 3 ounces)</li>
<li>1/4 cup toffee bits</li>
</ul>
</div>
<div class="ms-col2-recipe-directions">
<h2>Directions</h2>
<ol>
<li>
<span>Preheat oven to 350 degrees. Line a buttered 8-inch square baking pan with foil or parchment paper, allowing a 2-inch overhang. Butter lining (excluding overhang); set pan aside.</span>
</li>
<li>
<span>Whisk together flour, baking powder, and salt in a medium bowl; set aside.</span>
</li>
<li>
<span>Put butter and sugar in the bowl of an electric mixer fitted with the paddle attachment; mix on medium speed until pale and fluffy, about 3 minutes. Add eggs and vanilla; mix until combined. Reduce speed to low. Add flour mixture, and mix, scraping down sides of bowl, until well combined. Mix in butterscotch, cashews, and toffee.</span>
</li>
<li>
<span>Pour batter into prepared pan; spread evenly with a rubber spatula. Bake until golden brown and a cake tester inserted into blondies (avoid center and edges) comes out with a few crumbs but is not wet, 42 to 45 minutes. Let cool slightly in pan, about 15 minutes. Lift out; let cool completely on a wire rack before cutting into squares. Blondies can be stored in an airtight container at room temperature up to 3 days.</span>
</li>
</ol>
<div class="content_origin">

								From <span class="pub_source">Holiday Cookies 2005</span><span class="pub_date">,



												December&nbsp;2005</span><span class="sub_offer"><a href="#"><script type="text/javascript" language="JavaScript">

