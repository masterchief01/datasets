<script type="text/javascript">
$(document).ready(function(){

   	$('#header_hoverlist li.ptspecial').addClass("pholi");

	$('.special_offer').html($('#header_hoverlist li').contents('.headerhover_hidden').html() );

	$('#mhheader_topnav li').hover(
		function () {
			$('#header_hoverlist li.ptspecial').removeClass("pholi");
		},
		function () {
		}
     );

	$('#header_hoverlist li').hover(
		function () {
			var headerhover_data = $(this).contents('div').html();
			$('.special_offer').html(headerhover_data);
			$(this).addClass("holi");
		},
		function () {
			$(this).removeClass("holi");
		}
     );
});
