                      $(".header-button").each(function() {
                          (function(that) {
                              var buttonElem = $(that);
                              var menuElem = $("#"+that.id+"-menu");
                              var onButton = false;
                              var onMenu = false;
                              var loaded = that.id == "create";
                              var delay = 500;
                              $(that).hover(
