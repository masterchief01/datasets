<script type="text/javascript">
   $.preLoadImages('http://im.haberturk.com/2012/11/16/794766_manset.jpg?1353079101','http://im.haberturk.com/2012/11/16/794732_manset.jpg?1353070782','http://im.haberturk.com/2012/11/16/794735_manset.jpg?1353072057','http://im.haberturk.com/2012/11/16/794726_manset.jpg?1353069626','http://im.haberturk.com/2012/11/16/794657_manset.jpg?1353059416','http://im.haberturk.com/2012/11/16/794635_manset.jpg?1353057312','http://im.haberturk.com/2012/11/16/794611_manset.jpg?1353055552','http://im.haberturk.com/2012/11/16/794589_manset.jpg?1353054309','http://im.haberturk.com/2012/11/16/794561_manset.jpg?1353052684','http://im.haberturk.com/2012/11/16/794546_manset.jpg?1353050349');
	var switchesMagazin  = $('#magazin-control .controlMagazin a');
	var slidesMagazin	 = $('#magazin-container .header');
	var cycleItemMagazin = 0;
   
	 switchesMagazin.each(function(idx) {
		 $(this).bind('mouseover',
			 function() {
			    nextSlideMagazin(idx);
          $('.magazin-wrap').show();
		 });
	 });

   $('.magazin-close').bind('click',
     function(){
      $(this).parent().slideUp();
    });

   $('.item-next a').bind('click',
     function() {
		   cycleItemMagazin++;
		   if ( cycleItemMagazin > 9 )
		   {
			   cycleItemMagazin = 0;

		   }
       nextSlideMagazin(cycleItemMagazin);
       $('.magazin-wrap').show();
    });
    
   $('.item-prev a').bind('click',
     function() {
		   cycleItemMagazin--;
		   if ( cycleItemMagazin < 0 )
		   {
			   cycleItemMagazin = 9;

		   }
       nextSlideMagazin(cycleItemMagazin);
       $('.magazin-wrap').show();
    });
     
	 function nextSlideMagazin(idx, cycle)
	 {
		if ( cycle )
		{
		   cycleItemMagazin++;
		   if ( cycleItemMagazin > 9 )
		   {
			   cycleItemMagazin = 0;

		   }
		   idx = cycleItemMagazin;
		}

		var obj = $('#magazin-control a:eq(' + idx + ')');
		$(obj).data('slide', slidesMagazin.eq(idx));
		$(obj).data('idx', idx);

		switchesMagazin.removeClass('over');
		slidesMagazin.removeClass('active');
		$(obj).addClass('over');
		$(obj).data('slide').addClass('active');
	 }
/*
   var intervalId;

	 intervalId = setInterval("nextSlideMagazin(0, true)", 12000);
	 $('#magazin-container').bind('mouseout', function() {
		intervalId = setInterval("nextSlideMagazin(0, true)", 12000);
	 });

	 $('#magazin-container').bind('mouseover', function() {
		clearInterval(intervalId);
	 });
*/
</script>
