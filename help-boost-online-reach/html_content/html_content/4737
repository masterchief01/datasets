<script type="text/javascript">
	jQuery('#authors-carousel').jcarousel({
	  auto: 0,
	  //auto: 7,
	  scroll: 6,
	  visible: 6,
	  vertical: true,
	  wrap: 'both',
	  buttonNextHTML: null,
	  buttonPrevHTML: null,
	  initCallback: function(carousel, state) {
			jQuery('.ht-gazeteyazar .prev').bind('click', function() {
				carousel.prev();
				return false;
			});
			jQuery('.ht-gazeteyazar .next').bind('click', function() {
				carousel.next();
				return false;
			});
			jQuery('.ht-gazeteyazar').bind('mouseover', function() {
				carousel.stopAuto();
			});
			jQuery('.ht-gazeteyazar').bind('mouseout', function() {
				carousel.startAuto();
			});
	  }
  });



</script>
