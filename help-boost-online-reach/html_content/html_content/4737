<script type="text/javascript"> 
	(function(){
		$('.twitdivler').hide();
		$('.twit_bugun').show();
		$('.sub_headert [rel-list="bugun"]').addClass('selected');
		$('.sub_headert li').click(function(){
			var anahtar = $(this).attr('rel-list');
			$('.sub_headert li').removeClass('selected');
			$(this).addClass('selected');
			$('.twitdivler').hide();	
			$('.twit_' + anahtar).show();
		});
	})();
</script>
