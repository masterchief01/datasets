<script type="text/javascript"> 
	(function(){
		$('.facedivler').hide();
		$('.face_bugun').show();
		$('.sub_headerf [rel-list="bugun"]').addClass('selected');
		$('.sub_headerf li').click(function(){
			var anahtar = $(this).attr('rel-list');
			$('.sub_headerf li').removeClass('selected');
			$(this).addClass('selected');
			$('.facedivler').hide();	
			$('.face_' + anahtar).show();
		});
	})();
</script>
