(function (moduleID) {
	$('#' + moduleID + ' .carousel').jcarousel({
		scroll: 1,
		auto: 0,
		buttonnextHTML: null,
		buttonprevHTML: null,
		vertical: true,

		wrap: 'both',
		initCallback: function(carousel, state){
			if (state == 'init') {
				var container = carousel.container.parents('#' + moduleID + ' .context');
				$('#' + moduleID + ' .prev').click(function() {
					carousel.prev();
					return false;
				});
				$('#' + moduleID + ' .next').click(function() {
					carousel.next();
					return false;
				});
				container.mouseover(function() {
					carousel.stopAuto();
				});
				container.mouseout(function() {
					carousel.startAuto();
				});
			}
		}
    });
})('TvdeSuandaNeVarX');
