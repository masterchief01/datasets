     function() {
		   cycleItem++;
		   if ( cycleItem > 9 )
		   {
			   cycleItem = 0;

		   }
       nextSlide(cycleItem);
       $('.spor-wrap').show();
   });

   $('.item-prev a').bind('click',
     function() {
		   cycleItem--;
		   if ( cycleItem < 0 )
		   {
			   cycleItem = 9;

		   }
       nextSlide(cycleItem);
       $('.spor-wrap').show();
    });

	 function nextSlide(idx, cycle)
	 {
		if ( cycle )
		{
		   cycleItem++;
		   if ( cycleItem > 9 )
		   {
			   cycleItem = 0;

		   }
		   idx = cycleItem;
		}

		var obj = $('#spor-control a:eq(' + idx + ')');
		$(obj).data('slide', slides.eq(idx));
