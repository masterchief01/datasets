<script type="text/javascript">
	jQuery('#astroloji-carousel').jcarousel({
		scroll: 1,
		auto: 1,
		animation: 500,
		buttonNextHTML: null,
		buttonPrevHTML: null,
		wrap: 'both',
		initCallback: function(carousel, state){
			if ( state == 'init' )
			{
				jQuery('#astroloji-carousel').bind('mouseover', function() {
					carousel.stopAuto();
				});
				jQuery('#astroloji-carousel').bind('mouseout', function() {
					carousel.startAuto();
				});
			}
		}
	});
</script> 
