<script type="text/javascript">
	jQuery('#havadurumu-carousel').jcarousel({
		scroll: 1,
		visible: 1,
		auto: 6,
		animation: 1000,
		buttonNextHTML: null,
		buttonPrevHTML: null,
		wrap: 'both',
		initCallback: function(carousel, state){
			if ( state == 'init' )
			{
				jQuery('.havadurumu-carousel-next').bind('click', function() {
					carousel.next();
					return false;
				});
				jQuery('.havadurumu-carousel-prev').bind('click', function() {
					carousel.prev();
					return false;
				});
				jQuery('#havadurumu-carousel').bind('mouseover', function() {
					carousel.stopAuto();
				});
				jQuery('#havadurumu-carousel').bind('mouseout', function() {
					carousel.startAuto();
				});
			}
		},
		itemFirstInCallback:{
			onBeforeAnimation: function(object, item, offset){
				if(offset %2 == 0 && false){
					object.startAuto(3);
				}
				else{
					object.startAuto(6)
				}
			}
		}
	});
</script>
