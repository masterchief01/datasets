<script type="text/javascript"> 
	$(function(){
		$("ul.openMenuBox li").eq(0).trigger("click");
	});

	function tepkilerFirstLoad(){
		$('.tepkidivler').hide();
		$('.tepki_bugun').show();
		$('.sub_header [rel-list="bugun"]').addClass('selected');
	}
	
	tepkilerFirstLoad();

	$('.sub_header li').click(function(){
		var anahtar = $(this).attr('rel-list');
		$('.sub_header li').removeClass('selected');
		$(this).addClass('selected');
		$('.tepkidivler').hide();
		$('.tepki_' + anahtar).show();
		
	});
	
	$("ul.openMenuBox li").click(function(){
		var kat = $(this).find("a").text();
		kat = kat.replace(/ /g,"_"); 
		$("div#tepkiKats").each(function(){
			$(this).hide();
		});
		$("div."+kat).show();
	});
		
	$(".acilirMenu,img#acilir").click(function() {
		var openMenu = $("ul.openMenuBox");
		if ( openMenu.is(':hidden') ){	
			openMenu.slideDown('fast');			
		}
		else{openMenu.slideUp('fast');}
		$("ul.openMenuBox").mouseleave(function(){
			$("ul.openMenuBox").slideUp('fast');
		});
		return false;
	});


	function tepkiHaberleriGetir(secenek_id){
		jQuery.ajax({
			type: 'GET',
			url: 'dynamics/tepkiHaberleri/' + secenek_id,
			success: function(ajaxCevap) {
				$('#divWidgetTepkiHaberleri').html(ajaxCevap);
				tepkilerFirstLoad();
			}
		});
	}
	
	
</script><!--expire:3600--></div><div id="divEncokCont"><div class="EnCokOkunanHaberler">
