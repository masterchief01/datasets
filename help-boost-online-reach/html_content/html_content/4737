<script type="text/javascript">
  (function() {
    var imagesEkonomi = [];
            imagesEkonomi.push("http:\/\/im.haberturk.com\/2012\/11\/16\/794782_manset.jpg?1353083616");
            imagesEkonomi.push("http:\/\/im.haberturk.com\/2012\/11\/16\/794772_manset.jpg?1353082242");
            imagesEkonomi.push("http:\/\/im.haberturk.com\/2012\/11\/16\/794769_manset.jpg?1353079525");
            imagesEkonomi.push("http:\/\/im.haberturk.com\/2012\/11\/16\/794743_manset.jpg?1353081459");
            imagesEkonomi.push("http:\/\/im.haberturk.com\/2012\/11\/16\/794756_manset.jpg?1353082047");
            imagesEkonomi.push("http:\/\/im.haberturk.com\/2012\/11\/16\/794751_manset.jpg?1353081707");
            imagesEkonomi.push("http:\/\/im.haberturk.com\/2012\/11\/16\/794758_manset.jpg?1353080648");
            imagesEkonomi.push("http:\/\/im.haberturk.com\/2012\/11\/16\/794745_manset.jpg?1353081295");
            imagesEkonomi.push("http:\/\/im.haberturk.com\/2012\/11\/16\/794734_manset.jpg?1353075378");
            imagesEkonomi.push("http:\/\/im.haberturk.com\/2012\/11\/16\/794741_manset.jpg?1353077661");
        })();

	var switchesEkonomi = $('#ekonomi-control .controlEkonomi a');
	var slidesEkonomi = $('#ekonomi-container .header');
	var cycleItemEkonomi = 0;

	 switchesEkonomi.each(function(idx) {
		 $(this).bind('mouseover',
			 function() {
			    nextSlideEkonomi(idx);
          $('.ekonomi-wrap').show();
		 });
	 });

   $('.ekonomi-close').bind('click',
     function(){
      $(this).parent().slideUp();
    });

   $('.item-next a').bind('click',
     function() {
		   cycleItemEkonomi++;
		   if ( cycleItemEkonomi > 9 )
		   {
			   cycleItemEkonomi = 0;

		   }
       nextSlideEkonomi(cycleItemEkonomi);
       $('.ekonomi-wrap').show();
   });

   $('.item-prev a').bind('click',
     function() {
		   cycleItemEkonomi--;
		   if ( cycleItemEkonomi < 0 )
		   {
			   cycleItemEkonomi = 9;

		   }
       nextSlideEkonomi(cycleItemEkonomi);
       $('.ekonomi-wrap').show();
    });

	 function nextSlideEkonomi(idx, cycle)
	 {
		if ( cycle )
		{
		   cycleItemEkonomi++;
		   if ( cycleItemEkonomi > 9 )
		   {
			   cycleItemEkonomi = 0;

		   }
		   idx = cycleItemEkonomi;
		}

		var obj = $('#ekonomi-control a:eq(' + idx + ')');
		$(obj).data('slide', slidesEkonomi.eq(idx));
		$(obj).data('idx', idx);

		switchesEkonomi.removeClass('over');
		slidesEkonomi.removeClass('active');
		$(obj).addClass('over');
		$(obj).data('slide').addClass('active');
	 }
  /* var intervalId;

	 intervalId = setInterval("nextSlideEkonomi(0, true)", 12000);
	 $('#ekonomi-container').bind('mouseout', function() {
		intervalId = setInterval("nextSlideEkonomi(0, true)", 12000);
	 });

	 $('#ekonomi-container').bind('mouseover', function() {
		clearInterval(intervalId);
	 });
*/
</script>
