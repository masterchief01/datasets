        var googleAd = new FD.GoogleAd({
                adBlockTitle: "Ads by Google",
                maximumAdsInBlock: 3,
                checkExternalReferrer: true,
                split: 1,
            googleAds: google_ads,
            googleAdsId: "googleAds",
            moreGoogleAdsId: "moreGoogleAds"
        });
        googleAd.insertGoogleAds();
    }
