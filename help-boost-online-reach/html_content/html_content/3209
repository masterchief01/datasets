<script type="text/javascript">
    function er_showAd() {
        var regex = new RegExp("externalReferrer=(.*?)(; |&|$)", "gi");
        var value = regex.exec(document.cookie);
        if (value && value.length == 3) {
           var externalReferrer = value[1];
           return (!FD.isInternalReferrer() || ((externalReferrer) && (externalReferrer > 0)));
        }
        return false;
    }
    function cA () {
        var cs = "817-grey.gif".split(",");
        for (var i=0;i< document.dcdAdsAA.length; i++) {
            var cAd = document.getElementById(document.dcdAdsAA[i]);
            for (var j=0; j<cs.length; j++) {
                var c = cs[j].trim();
                if (c.length > 0 && cAd.innerHTML.indexOf(c)>0) {
                    document.dcdAdsAI.push(cAd.hash);
                }
            }
        }
    }
    function initAds() {
        for (var i=0;i< document.dcdAdsI.length; i++) {
            var shadowHolder = document.getElementById("adShD" + i);
            if (shadowHolder) {
                shadowHolder.removeChild(document.getElementById(document.dcdAdsI[i]));
                while (shadowHolder.childNodes.length > 0) {
                    var child = shadowHolder.childNodes[0];
                    shadowHolder.removeChild(child);
                    document.getElementById(document.dcdAdsH[i]).appendChild(child);
                }
            }
        }
        if (er_showAd()) {
            for (var i=0;i< document.dcdAdsE.length; i++) {
                var erAd = document.getElementById(document.dcdAdsEH[i]);
                erAd.className = document.dcdAdsEC[i];
                erAd.appendChild(document.dcdAdsE[i]);
            }
        }
        cA();
        if (document.dcdAdsAI.length > 0) {
            var pingServerUrl = "/action/pingServerAction?i="
            var sep = "";
            for (var i=0;i< document.dcdAdsAI.length; i++) {
                pingServerUrl += sep + document.dcdAdsAI[i];
                sep = ",";
            }
            var xmlHttp = null;
            try
            {
                xmlHttp = new XMLHttpRequest();
            }   catch(e)             {
                try
                {
                    xmlHttp = new ActiveXObject("Microsoft.XMLHttp");
                } catch(e) {
                    xmlHttp = null;
                }
            }
            if (xmlHttp != null) {
                xmlHttp.open( "GET", pingServerUrl, true);
                xmlHttp.send( null );
            }
        }

    }
    document.refreshAds = function rAds(adID) {
        if (document.dcdAdsR) {
            for (var i=0;i< document.dcdAdsR.length; i++) {
                var f = document.getElementById(document.dcdAdsR[i]);
                if (f) {
                    if (!adID || adID =="*" || f.parentNode.id==adID ) {
                        f.parentNode.innerHTML = f.parentNode.innerHTML;
                    }
                }
            }
        }
    }
    window.setTimeout(initAds, 1);
</script>


