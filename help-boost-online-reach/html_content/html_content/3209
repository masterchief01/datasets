        function getAdCookie(name) {
            var regex = new RegExp(name + "=(.*?)(; |&|$)", "gi");
            var value = regex.exec(document.cookie);
            if (value && value.length == 3) {
               value = value[1];
            } else {
                value = "";
            }
            return value;
        }
        document.dcAdsCParams = "";

