	function clearSizeSelection(){
		var currentSize = document.getElementById('size').value;


	if(! currentSize == '') {
	  //	document.getElementById('size_' + currentSize).style.background = '#CECDB2';
		document.getElementById('aTag_' + currentSize).style.background = '#CECDB2';

	}
	document.getElementById('stockmsg').innerHTML = '';
 	document.getElementById('size').value = '';

}



 function setSize(){
 	var product_id = document.getElementById('product_id').value;
	var currentSize = document.getElementById('size').value;

 	
 		if(productsList[product_id].sizes['38']){
			  var tempsize = productsList[product_id].sizes['38'];

			  if (tempsize > 0) {
					document.getElementById('size_38').className = '';
 					document.getElementById('aTag_' + '38').style.background = '#FFF';
				    if(currentSize == '38') validateSize(currentSize)

			    } else {

					document.getElementById('size_38').className = 'notav';
	 				document.getElementById('aTag_' + '38').style.background = '#CECDB2';
					if(currentSize == '38') clearSizeSelection();
			   }

 		} else {

 			document.getElementById('size_38').className = 'notav';
 			document.getElementById('aTag_' + '38').style.background = '#CECDB2';
 			if(currentSize == '38')  clearSizeSelection();

 		  }
 	
