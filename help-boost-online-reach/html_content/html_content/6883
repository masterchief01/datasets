 function validateSize(thisSize){

 	var product_id = document.getElementById('product_id').value;

 		if(productsList[product_id].sizes[thisSize]){

 		  	var tempsize = productsList[product_id].sizes[thisSize];
 		  		if (tempsize > 0){
					var currentSize = document.getElementById('size').value;
					document.getElementById('stockmsg').innerHTML = '';
					if(tempsize <=10) document.getElementById('stockmsg').innerHTML = 'Only '+ tempsize +' Left!';


					if(! currentSize == ''){
					  //	document.getElementById('size_' + currentSize).style.background = '#fff';
						document.getElementById('aTag_' + currentSize).style.background = '#fff';
 		  			}
 		  		 //	document.getElementById('size_' + thisSize).style.background = '#A7A78F';
                    document.getElementById('aTag_' + thisSize).style.background = '#A7A78F';
 		  			document.getElementById('size').value = thisSize;
 		  		}


 		  }

 }

function checkOrder(){

	var product_id = document.getElementById('product_id').value;
	var thesize = document.getElementById('size').value;

	if(product_id == '' ||
	   thesize == '' || ! productsList[product_id].sizes[thesize] ){

		alert('Please select a Size');
		return false;
	   }

    var tempsize = productsList[product_id].sizes[thesize];

	if(document.getElementById('quantity').value == ''){
		alert('Please select a Quantity');
		return false;
 	  }


    if(tempsize < document.getElementById('quantity').value){
    alert('There are only '+ tempsize + ' left in stock.');
    return false;
    }

 }

 function setProduct(newproduct_id){

 oldID = document.getElementById('product_id').value;

 document.getElementById('thumb_'+oldID).innerHTML='<BR>';
 document.getElementById('thumb_'+newproduct_id).innerHTML='';

 document.getElementById('product_id').value = newproduct_id;
 if(productsList[newproduct_id].lgimage != '')
 	{
 	document.getElementById('largeproduct').style.backgroundImage = 'url('+productsList[newproduct_id].lgimage+')';
 	}
 }

