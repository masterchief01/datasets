<script type="text/javascript">
        var accordionCont = $('accordion');
        var toggles = accordionCont.getElements('.toggler');
        var lastToggler = toggles.getLast('h3');
	var myAccordion = new Fx.Accordion(accordionCont.getElements('.toggler'), accordionCont.getElements('.slider'),  {
		opacity: 0,
		alwaysHide: false,
		show: 0,
		onActive: function(toggler, slider){
                        if(toggler == lastToggler){
                            toggler.removeClass('hed-3');
                            toggler.addClass('hed-1');
                        }
		},
		onBackground: function(toggler, slider){
                        if(toggler == lastToggler){
                            toggler.removeClass('hed-1');
                            toggler.addClass('hed-3');
                        }
		}
	});
</script>
