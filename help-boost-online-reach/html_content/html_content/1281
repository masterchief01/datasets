  <div class="entry-header">
  
  
	
  <div class="category"><!--<a href="/chicago/fashion/fashion-tips" data-track="entry:header:Fashion Tips">-->Fashion Tips<!--</a>--></div>
  
  <div class="deliminator"></div>
  
  <div class="date">
          Mar 23, 2012 6:00 AM
      <!-- <span itemprop="datePublished">2012-03-23 06:00:00</span> -->
    
  </div>
  <div class="asset-info">
      <!-- Facebook Like -->
      <div class="facebook" itemprop="interactionCount">
        <fb:like layout="button_count" href="http://www.refinery29.com/fashion-fixes-at-work" width="90"></fb:like>
      </div>
