	  $(document).ready(function() {
	    // dumb, but I can't figure out a way to set a class name on the body tag, so do it with Javascript
	    if (window.location.pathname.split('/').length > 0 && window.location.pathname.split('/')[1] != '') {
  	    $('BODY').addClass('body-' + window.location.pathname.split('/')[1]);
	    }    
      $.expr[':'].external = function(obj){
          return !obj.href.match(/^mailto\:/)
                  && (obj.hostname != location.hostname);
      };
      $('a:external').click(function(){
        _gaq.push(['_trackEvent','Outbound Clicks',this.hostname.replace('www.',''),this.href]);
        setTimeout('document.location = "' + $(this).attr('href') + '"', 300);
        return false;
      });
    });
	</script>
	


