        var promoDisplayNumber;

        // Set up interaction on the newsletter unit. Only gets called if the
        // newsletter unit is shown.
        var setUpNewsletter = function () {
            var $newsletterSignup = $('#article-promo-newsletter');
            var $form = $newsletterSignup.find('form');

            var successMsg = function (msg) {
                $form.hide();
                $newsletterSignup.find('.message').removeClass('error').addClass('success').html(msg);
            };

            var errorMsg = function (msg) {
                $newsletterSignup.find('.message').removeClass('success').addClass('error').html('<strong>Error:</strong> ' + msg);
            };

            $('#mc-embedded-subscribe-form').submit(function(event) {
                event.preventDefault();
                $(this).ajaxSubmit({
                    url: '/newsletters/ajax-sign-up/',
                    dataType: 'json',
                    success: function(data, statusText, xhr, $form) {
                        if (data && data.success) {
                            if (data.existing_user === true) {
                               successMsg("Your newsletter subscription preference have been updated");
                            } else {
                               successMsg("<h3>You're almost finished.</h3>"
                                + " We just need to confirm your email address."
                                + "<br/><br />"
                                + "To complete the subscription process, please click "
                                + "the link in the email we just sent you.");
                            }
                            window.Atlantic.Omniture.trackLink('nwsltrsubmit`dynamic');
                            Atlantic.Utils.createCookie('newsletter-enrollment', topStoriesNewsletterId, 30);
                        } else if (data && data.error) {
                            errorMsg(data.error);
                        } else {
                            errorMsg("An unknown error occurred");
                        }
                    },
                    error: function() {
                        errorMsg("An unknown error occurred");
                    }
                });
                return false;
            });
        }

        if ((newsletterEnrollment === null
            || newsletterEnrollment.split(',').indexOf(topStoriesNewsletterId) === -1)
            && document.location.pathname.search(/^\/magazine/) === -1) {

            // Generate a random integer between 1 and 3
            promoDisplayNumber = Math.ceil(Math.random() * 3);
        } else {
            promoDisplayNumber = Math.ceil(Math.random() * 2);
        }

        switch (promoDisplayNumber) {
            case 1:
                $('#article-promo-newsletter').hide();
                $('#article-promo-digital').hide();
                break;

            case 2:
                $('#article-promo-newsletter').hide();
                $('#article-promo-magazine').hide();
                break;

            case 3:
                $('#article-promo-magazine').hide();
                $('#article-promo-digital').hide();

                setUpNewsletter();
                break;
        };

    })();
    </script>
</div>

