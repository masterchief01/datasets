            <a class="email-btn"
                onclick="$.event.trigger('social:share', [{type:'email', action:'share', el:this}])"
                href="mailto:?subject=How%20a%20German%20Elementary%20School%20Taught%20Sex%20Ed%20%5BGraphic%5D&amp;body=Children%27s%20book%20images%2C%20NSFW%3F%20NSFGFG%20%28German%20First-Graders%29%3F%0A%0ARead%20More%3A%0Ahttp%3A%2F%2Fwww.theatlantic.com%2Fhealth%2Farchive%2F2013%2F04%2Fhow-a-german-elementary-school-taught-sex-ed-graphic%2F275344%2F">Email</a>
