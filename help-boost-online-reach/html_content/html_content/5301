		<script type="text/javascript"> 
                                $(document).ready(function() {
                                    $('#text').val('Keywords');
                                    $('#postal_code').val('Zip Code');
                                    
                                    $('#text').focus(function() {
                                    if($('#text').val()=='Keywords')
                                    {
                                    $('#text').val('');
                                    }
                                    });
                                    
                                    $('#postal_code').focus(function() {
                                    if($('#postal_code').val()=='Zip Code')
                                    {
                                    $('#postal_code').val('');
                                    }
                                    });
                                    
                                    $('#text').blur(function() {
                                    if($('#text').val()=='')
                                    {
                                    $('#text').val('Keywords');
                                    }
                                    });
                                    
                                    $('#postal_code').blur(function() {
                                    if($('#postal_code').val()=='')
                                    {
                                    $('#postal_code').val('Zip Code');
                                    }
                                    });
                                });
                                </script>

