    var lastIdx = 0;
    var currIdx = 0;
    var adCount = 0;
    var AdList = new Array();

    $(document).ready(function() {
      adCount = AdList.length;
      rotateAd();
    });
    function rotateAd() {
        var currIdx = Math.floor(Math.random()*adCount);

        $('#adImage' + AdList[lastIdx]).hide();
        $('#adImage' + AdList[currIdx]).show();
	lastIdx = currIdx;
    }

    window.setInterval('rotateAd()', 8000);
</script>
