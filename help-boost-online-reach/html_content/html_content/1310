<div id="countdown-callout">
      <span id="cd-d1" class="cd-0"></span>
      <span id="cd-d2" class="cd-0"></span>
      <span id="cd-h1" class="cd-0"></span>
      <span id="cd-h2" class="cd-0"></span>
      <span id="cd-m1" class="cd-0"></span>
      <span id="cd-m2" class="cd-0"></span>
      <span id="cd-s1" class="cd-0"></span>
      <span id="cd-s2" class="cd-0"></span>
