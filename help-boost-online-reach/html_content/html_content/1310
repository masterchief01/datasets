<script type="text/javascript">
    $("#countdown-callout").click(function() {
        var track = '?utm_campaign=osn_countdown';
        var url = 'http://www.onionsportsnetwork.com/channels/sportsdome/';
        if(/theonion.com/.test(window.location.href)) {
            window.top.location.href = url+track;
        }
        else {
            window.top.location.href = url;
        }
        return false;
    });

    function countdown_update() {
        var target = new Date("01/11/2011 22:30 EST");
        var now = new Date();
        var diff = Math.floor(target - now) / 1000;
        var tmp = diff;
        var s = tmp % 60;
        tmp = Math.floor(tmp/60);
        var m = tmp % 60;
        tmp = Math.floor(tmp/60);
        var h = tmp % 24;
        var d = Math.floor(tmp/24);
        if (diff >= 0) {
            $("#cd-d1").attr("class", "cd-" + Math.floor(d/10));
            $("#cd-d2").attr("class", "cd-" + (d % 10) );
            $("#cd-h1").attr("class", "cd-" + Math.floor(h/10));
            $("#cd-h2").attr("class", "cd-" + (h % 10) );
            $("#cd-m1").attr("class", "cd-" + Math.floor(m/10));
            $("#cd-m2").attr("class", "cd-" + (m % 10) );
            $("#cd-s1").attr("class", "cd-" + Math.floor(s/10));
            $("#cd-s2").attr("class", "cd-" + Math.floor((s % 10)) );
            setTimeout(countdown_update, 500);
        }
        else {
            $("#countdown-callout").hide();
        }
    }

    var now = new Date();
    if (now.getDate() == 11 && now.getMonth() == 0) {
        $("#countdown-callout").addClass("tonight");
    }
    countdown_update();


</script>


	    
