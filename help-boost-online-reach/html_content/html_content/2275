                        FD.trigger("FDVideoPlayerAdFinished");
                    },
                    "onAdLoaded": function(event) {
                        var videoPlayer = $f("video-player-latest-player");
                        if ((event) && (event.ad) && (event.ad.type == "flash") && (videoPlayer)) {
                            if ((event.request) && (event.request.adTagUrl) &&
                                ((event.request.adTagUrl.indexOf("show=localexperts") > -1) || (event.request.adTagUrl.indexOf("show=economicupdate") > -1) ||
                                (event.request.adTagUrl.indexOf("show=homebuyinghappiness") > -1) || (event.request.adTagUrl.indexOf("show=domain") > -1))) {
                            } else {
                                FD.trigger("FDVideoPlayerFlashAdLoaded");
                            }
                        }
                    },
                    "onCustomAdEvent": function(event) {
                        var videoPlayer = $f("video-player-latest-player");
                        if ((event) && (event.name == "killAd") && (videoPlayer)) {
                            FD.trigger("FDVideoPlayerFlashAdCompleted");
                        } else if ((event) && (event.name == "pauseContent") && (videoPlayer)) {
                            FD.trigger("FDVideoPlayerFlashAdPause");
                        } else if ((event) && (event.name == "playContent") && (videoPlayer)) {
                            FD.trigger("FDVideoPlayerFlashAdPlay");
                        } else if ((event) && (event.name == "videoComplete") && (videoPlayer)) {
                            FD.trigger("FDVideoPlayerFlashAdCompleted");
                            FD.trigger("FDVideoPlayerFlashAdPlay");
                        }
                    },
                    "onAdStart": function(event) {
                        FD.trigger("FDVideoPlayerAdStarted");
                    },
                    "url": "http://resources.smh.com.au/common/media-common-1.0/swf/flowplayer/dfp/flowplayer.dfp-smh.com.au-1.6.swf"
                },
                "rtmp": {
                    "url": "http://resources.smh.com.au/common/media-common-1.0/swf/flowplayer/flowplayer.rtmp-3.2.1.swf"
                }
            }
        }));
     })();


