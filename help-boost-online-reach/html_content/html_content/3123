<script language="javascript" type="text/javascript">

    function SetHeadingOverlay(title) {
        if (title == "Newsletter") {
            document.getElementById("ctl00_ctl00_ctl05_OverlayLoginHeading").innerHTML = "Please log in to access your newsletters";
        }
        else if (title == "Grocery List") {
            document.getElementById("ctl00_ctl00_ctl05_OverlayLoginHeading").innerHTML = "Please log in to access your grocery list";
        }
        else if (title == "Recipe Box") {
            document.getElementById("ctl00_ctl00_ctl05_OverlayLoginHeading").innerHTML = "Please log in to access your recipe box";
        }
        else if (title == "Saved Recipe") {
            document.getElementById("ctl00_ctl00_ctl05_OverlayLoginHeading").innerHTML = "Please log in to view your saved recipes";
        }
        else if (title == "Personal Recipe") {
            document.getElementById("ctl00_ctl00_ctl05_OverlayLoginHeading").innerHTML = "Please log in to add your personal recipe";
        }
    }

    // 20 APR
    function ShowLoginOverlayHeader(domainName, friendlyUrl) {

        $("#loginOverlayHeader .loginFrameB").html('<iframe frameborder="0" src="http://' + domainName + '/Access/LoginOverlayFrame.aspx?FriendlyURL=' + friendlyUrl + ' "class="loginIframe" id="ctl00_ctl00_MainContent_MainContent_RecipeUtility1_iframeForLogin"></iframe>');
        $("#loginOverlayHeader").css('z-index', '99999999');
        $("#backgroundPopupHeader").css('z-index', '99999999');
        $("#loginOverlayHeader").show();
        $("#backgroundPopupHeader").show();

    }
    //
    $(document).ready(function() {
        $("#loginOverlayHeader .closeBut").click(function() {
            $("#loginOverlayHeader,#backgroundPopupHeader").hide();


        });

        $(".sub-menu-recipe-box").hover(
			  		function() {
			  		    $(".menu-recipe-box").addClass('active');
			  		},
			  		function() {
			  		    $(".menu-recipe-box").removeClass('active');
			  		}
							);



        if ($('.RecipeBoxDisabled').attr('class')) {
            $("#Div1 .HeaderMenu .AspNet-Menu-Horizontal ul.AspNet-Menu ul li.first, #Div1 .HeaderMenu .AspNet-Menu-Horizontal ul.AspNet-Menu ul li.last, #Div1 .HeaderMenu .AspNet-Menu-Horizontal ul.AspNet-Menu ul li").css({ 'border-color': '#999' });

        }

    });

		
		
</script>




