<script language="javascript" type="text/javascript" >
//$(document).ready(function(){
//$("#custom-doc #ft span .column ul li:last-child span").hide();
    //});
    function setDynamicOmniture(linkTitle, linkTrack) 
    {
       
            var linkVal = '';

            linkVal = linkTitle;
           
            if(linkTrack =="Exit Link")
            {
                trackExitLink(null, 'Footer', linkVal, location.pathname.toLowerCase(), true);
            }
            else if(linkTrack == "Custom Link")
            {
                trackLink('Footer', linkVal, location.pathname.toLowerCase(), true);
            }
        
        
    }
   
</script>

<script language="javascript" type="text/javascript" >
//$(document).ready(function(){
//$("#custom-doc #ft span .column ul li:last-child span").hide();
    //});

    function footerTrackLink(linkTitle, linkTrack) 
    {
        if (linkTrack === "Custom Link") 
        {

            trackLink('Footer', linkTitle, location.pathname.toLowerCase(), true);
        }
        else if (linkTrack == "Exit Link") 
        {
            trackExitLink(null, 'Footer', linkTitle, location.pathname.toLowerCase(), true);
        }
    }
   
</script>
<script language="javascript" type="text/javascript" >
//$(document).ready(function(){
//$("#custom-doc #ft span .column ul li:last-child span").hide();
    //});
    
    
</script>




