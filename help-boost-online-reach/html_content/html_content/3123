<script language="javascript" type="text/javascript">


$(document).ready(function(){

	$("#folder-overlay .closeBut").click(function(){
	$("#folder-overlay .overlay-rgt .ErrorMessage").hide();
	});

});

function addTofolderSubmit() {
    if (!CheckSearchKeyword()) return false;

    SetProgressBar();

    return true;    
}

function SetProgressBar() {
    document.getElementById("ctl00_ctl00_MainContent_MainContent_RecipeUtility1_addToBookmarks_divAddToFolder").style.display = "none";
    document.getElementById("ctl00_ctl00_MainContent_MainContent_RecipeUtility1_addToBookmarks_divProgressBar").style.display = "block";
    $(".closeBut").css('visibility', 'hidden');
}

function AddToRecipeBoxTrackLink(propList, boolCreate) {
    var s = s_gi("rditasteofhomeglobal");
    var linkLocation = 'Add To My Recipe Box Nav';
    var linkName = "";
    s.linkTrackVars = 'prop24,events';

    if (boolCreate) {
        linkName = 'Create Folder and Save';
        s.linkTrackEvents = 'event29,event35';
        s.events = 'event29,event35';
    }
    else {
        linkName = 'Save';
        s.linkTrackEvents = 'event29';
        s.events = 'event29';
    }

    s.prop24 = propList;

    s.tl(true, 'o', linkName);
}


function CheckSearchKeyword() {
  
    var searchPhrase = document.getElementById("ctl00_ctl00_MainContent_MainContent_RecipeUtility1_addToBookmarks_txtNewFolder").value;
    var lblMessage = document.getElementById("ctl00_ctl00_MainContent_MainContent_RecipeUtility1_addToBookmarks_lblMessage");
    lblMessage.innerHTML = "";
    var errorMessage = checkSpecialChar(searchPhrase);
    if (errorMessage && errorMessage.length) {
        lblMessage.innerHTML = errorMessage;
        lblMessage.style.display = "inline";
        return false;
    }
    if ($.trim(searchPhrase.toLowerCase()) == 'search') {
        lblMessage.innerHTML = "Folder name is not allowed";
        lblMessage.style.display = "inline";
        return false;
    }
    else if (Page_ClientValidate("Bookmark")) {
        if (lblMessage.innerHTML == "Folder name is not allowed") {
            lblMessage.innerHTML = " ";
            lblMessage.style.display = "none";
        }
        return true;
    }
    else {
        if (lblMessage.innerHTML == "Folder name is not allowed") {
            lblMessage.innerHTML = " ";
            lblMessage.style.display = "none";
        }
        return false;
    }
} 

</script>
  


                                     

            
