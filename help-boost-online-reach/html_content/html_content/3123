<script type="text/javascript" language="javascript">
 
    function SetFocusheading(chk) {
        var date = new Date();
        date.setTime(date.getTime() + (-1 * 24 * 60 * 60 * 1000));
        var expires = "; expires=" + date.toGMTString();   
      
        if (chk == "Rate") {
            document.getElementById("ctl00_ctl00_MainContent_MainContent_RecipeUtility1_LoginHeading").innerHTML = "Please log in to rate this recipe";
            document.getElementById("ctl00_ctl00_MainContent_MainContent_RecipeUtility1_hidPoupSource").value = "LoginForRating";
            document.cookie = "showAddToRecipeBoxPopup=1" + expires + ";";
            document.cookie = "LoginForRecipeBox=1" + expires + ";";
            document.cookie = "LoginForGroceryList=1" + expires + ";";
        }
        else if (chk == "RecipeBox") {
        document.getElementById("ctl00_ctl00_MainContent_MainContent_RecipeUtility1_LoginHeading").innerHTML = "Please log in to add this to your recipe box";
        document.getElementById("ctl00_ctl00_MainContent_MainContent_RecipeUtility1_hidPoupSource").value = "LoginForRecipeBox";
        document.getElementById("ctl00_ctl00_MainContent_MainContent_RecipeUtility1_hidPopupRegistrationSource").value = "Add to My Recipe Box";                
        document.cookie = "showAddToRecipeBoxPopup=1;";        
        document.cookie = "LoginForRating=1" + expires + ";";
        document.cookie = "LoginForGroceryList=1" + expires + ";";
        }
        else if (chk == "GroceryList") {
            document.getElementById("ctl00_ctl00_MainContent_MainContent_RecipeUtility1_LoginHeading").innerHTML = "Please log in to add this to your grocery list";
            document.getElementById("ctl00_ctl00_MainContent_MainContent_RecipeUtility1_hidPoupSource").value = "LoginForGroceryList";
            document.getElementById("ctl00_ctl00_MainContent_MainContent_RecipeUtility1_hidPopupRegistrationSource").value = "Add to My Grocery List";
            document.cookie = "LoginForGroceryList=1;";
            document.cookie = "LoginForRating=1" + expires + ";";
            document.cookie = "LoginForRecipeBox=1" + expires + ";";
            document.cookie = "showAddToRecipeBoxPopup=1" + expires + ";";
        }
        
    }

    function fnSetValue(text) {
        document.getElementById("ctl00_ctl00_MainContent_MainContent_RecipeUtility1_hidFromPage").value = text;
    }
    
    function ShowLoginOverlay(domainName, recipeID, recipeTypeID, pageIndex, sort, friendlyUrl, regSource, source, loginParams)
        {       
        $("#popupContact .loginFrameB").html('<iframe frameborder="0" src="http://' + domainName + '/Access/LoginOverlayFrame.aspx?RecipeID=' + recipeID + '&RecipetypeID=' + recipeTypeID + '&CurrentPageIndex=' + pageIndex + '&Sort=' + sort + '&FriendlyURL=' + friendlyUrl + '&registrationsource=' + regSource + '&Source=' + source + '&LoginParams=' + loginParams + ' "class="loginIframe" id="ctl00_ctl00_MainContent_MainContent_RecipeUtility1_iframeForLogin"></iframe>');
            $("#popupContact").show();  
			 $("#backgroundPopup").show(); 


}

function SetRecipeBoxLink() {

    document.getElementById("ctl00_ctl00_MainContent_MainContent_RecipeUtility1_lnkAddToBox").innerHTML = "In Recipe Box";
    document.getElementById("ctl00_ctl00_MainContent_MainContent_RecipeUtility1_lnkAddToBox").setAttribute("title", "In Recipe Box");
    document.getElementById("ctl00_ctl00_MainContent_MainContent_RecipeUtility1_lnkAddToBox").setAttribute("href", "/Recipe-Box");
    document.getElementById("ctl00_ctl00_MainContent_MainContent_RecipeUtility1_lnkAddToBox").removeAttribute("onclick");
    $(".closeBut").css('visibility', 'visible');
}

</script>

