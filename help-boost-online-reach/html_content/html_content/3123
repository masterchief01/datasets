<script language="javascript" type="text/javascript">
function destroycatfish()
{
    var rollupPanel = document.getElementById("rollup");
    if (rollupPanel != null) {
        rollupPanel.innerHTML = "";
    }
}

var cookieString = document.cookie;
if (cookieString.indexOf("Rollup=1") > -1)
{
    var rollupPanel = document.getElementById("rollup");
    if (rollupPanel != null) {
        rollupPanel.innerHTML = "";
    }
} 
else {
    var timeNow = new Date();
    timeNow.setTime(timeNow.getTime() + 7 * 86400000);

    document.cookie = "Rollup=1;expires=" + timeNow + ";path=/";
}
function submitorder(url) {
    var cdsname = document.getElementsByName("cds_name")[0].value;
    var cdszip = document.getElementsByName("cds_zip")[0].value;
    var cdsemail = document.getElementsByName("cds_email")[0].value;
    var cdsaddress1 = document.getElementsByName("cds_address_1")[0].value;
    var cdsaddress2 = document.getElementsByName("cds_address_2")[0].value;
    var cdscity = document.getElementsByName("cds_city")[0].value;
    var cdsstate = document.getElementsByName("cds_state")[0].value;

    var querystring = '&cds_name=' + cdsname + '&cds_zip=' + cdszip + '&cds_email=' + cdsemail + '&cds_address_1=' + cdsaddress1 + '&cds_address_2=' + cdsaddress2 + '&cds_city=' + cdscity + '&cds_state=' + cdsstate;

    if (url != undefined)
    {
    window.open(url + querystring, '_blank');
    return false;
    }
}
</script>


