<script>
$(function(){
    var $context = $('#looks'),
        $categoryFilter = $('#header-bottom .look-category-filter'),
        $looksContainer = $('.looks', $context),
        $retriever = $looksContainer.retriever({
            more: true
        }),
        retriever = $retriever.data('retriever')
            .onPostRender(function($html){
                $html.find('.look-image-wrapper').look()
            });

    $categoryFilter
        .on('change.ft-api', 'input[name="category"]', function(){
            retriever
                .setAll($('form', $categoryFilter).serializeJSON())
                .refresh();
        });

    $('.look-image-wrapper', $context).look();
});
</script>


