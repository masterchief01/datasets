<script>
$(function(){
    // Constants
    var VERTICAL_PADDING = 20,
        STORE_TYPEAHEAD_SIZE = 10;

    var $modal = $('#item-searcher-modal'),
        $resultContainer = $('.result-container', $modal),
        $noResultContainer = $('.no-result', $resultContainer),
        $itemsContainer = $('.items', $resultContainer),
        $selectedContainer = $('.selected-item-container', $modal),
        $saveBtn = $('.save.btn', $modal),
        $preview = $('.preview', $modal),
        currentCaller = null,
        saved = false,

        $aligner = $resultContainer.aligner({
            containerSelector: '.items',
            selector: '.item'
        }),
        aligner = $aligner.data('aligner'),

        $retriever = $itemsContainer.retriever({
            url: FT.urls.queryItem,
            autoScrollTarget: $resultContainer
        }),
        retriever = $retriever.data('retriever')
            .set('image-type', 'variable')
            .onPreLoad(function(){
                $noResultContainer.hide();
            })
            .onEmpty(function(){
                $noResultContainer.show();
            })
            .onPostRender(function($html){
                aligner.render($html.filter('.item'));
            }),

        buildParams = function(){
            var $form = $('.filter-container .tab-pane.active form', $modal);
            return $form.serializeJSON();
        },
        refresh = function(){
            retriever
                .setAll(buildParams())
                .refresh();
        },

        updatePreview = function(image){
            var $image = $('img', $preview),
                previewSize = parseInt($preview.width()),
                paddingSize = previewSize / 2;

            for(var i = 0; i < 2; i++){
                if(image.pos[i] < paddingSize) image.pos[i] = paddingSize;
                if(image.pos[i] > image.size[i] - paddingSize) image.pos[i] = image.size[i] - paddingSize;
            }

            targetWidth = image.size[0],
            targetHeight = image.size[1],
            offsetX = image.pos[0],
            offsetY = image.pos[1],
            imageLeft = 0,
            imageTop = 0;

            $image
                .attr('src', image.url)
                .css({
                    width: image.size[0],
                    height: image.size[1],
                    left: previewSize / 2 - image.pos[0],
                    top: previewSize / 2 - image.pos[1]
                });

            $preview.show();
        },
        selectItem = function(itemId){
            var $active = $('.thumb.active', $selectedContainer),
                $thumb = $('.thumb:not(.active):first', $selectedContainer),
                exist = false;

            if(!$thumb.length) return;

            $active.each(function(){
                if($(this).attr('data-item-id') == itemId)
                    exist = true;
            });

            if(exist) return;

            $thumb
                .find('img.item').attr('src', FT.getItemImageUrl(itemId, 'thumb-small')).end()
                .attr('data-item-id', itemId)
                .addClass('active');

            $thumb.transition({
                perspective: '200px',
                rotateY: '+=360deg'
            });

            refreshSaveBtn();

            /*
            $thumb
                .transition({ : '-=5' }, 50)
                .transition({ x: '+=10' }, 50)
                .transition({ x: '-=10' }, 50)
                .transition({ x: '+=5' }, 50);
            */
        },
        refreshSaveBtn = function(){
            if($('.thumb.active', $selectedContainer).length)
                $saveBtn.removeClass('disabled');
            else
                $saveBtn.addClass('disabled');
        };

    $modal
        .on('resize.modal.ft-api', function(){
            // The modal should be visible to calculate the dimensions
            var windowHeight = $(window).height(),
                headerHeight = $('.modal-header', $modal).outerHeight(),
                filterHeight = $('.filter-container', $modal).outerHeight(),
                externalHeight = $('.external-add-container', $modal).outerHeight(),
                footerHeight = $('.modal-footer', $modal).outerHeight();

            if(!$modal.is(':visible')) return;

            $resultContainer.css('height',
                windowHeight
                - headerHeight
                - filterHeight
                - externalHeight
                - footerHeight
                - VERTICAL_PADDING * 2
            );
        })
        .on('show.modal.ft-api', function(e){
            if(!$(e.target).hasClass('modal')) return;

            $modal.partialShow();
            $modal.triggerHandler('resize');
            $modal.partialHide();
        })
        .on('shown.modal.ft-api', function(e){
            if(!$(e.target).hasClass('modal')) return;

            refresh();
        })
        .on('hide.modal.ft-api', function(){
        })
        .on('hidden.modal.ft-api', function(){
            if(currentCaller){
                if(saved){
                    var items = [];
                    $(".selected-item-container .thumb.active").each(function(){
                        items.push($(this).attr('data-item-id'));
                    });
                    currentCaller.resolve(items);
                }else{
                    currentCaller.reject();
                }
                currentCaller = null;
            }

            saved = false;
            aligner.reset();
            $preview.hide();
            $saveBtn.addClass('disabled');
            $('form', $modal).reset();
            $('.thumb', $selectedContainer)
                .removeClass('active')
                .find('img').attr('src', '');
        })
        .find('.filter-container')
            .on('change.ft-api', function(){
                $resultContainer.scrollTop(0);
                aligner.reset();
                refresh();
            })
            .find('.nav-tabs')
                .on('shown.ft-api', '[data-toggle="tab"]', function(){
                    $('.filter-container', $modal).triggerHandler('change');
                })
                .end()
            .find('.color-filter')
                .on('click.ft-api', '.color[data-color]', function(e){
                    var $this = $(this),
                        $filter = $this.parents('.color-filter:first'),
                        $all = $filter.find('.color[data-color]'),
                        $input = $('input[name="color"]', $filter);

                    $all.removeClass('active');

                    if($(e.target).hasClass('close'))
                        $input.val('');
                    else
                        $input.val($this.addClass('active').attr('data-color'));
                    $('.filter-container', $modal).triggerHandler('change');
                })
                .end()
            .find('.store-filter')
                .find('input[name="store-name"]')
                    .typeahead({
                        items: STORE_TYPEAHEAD_SIZE,
                        source: function(q, callback){
                            var params = {
                                q: q,
                                autocomplete: 1,
                                size: STORE_TYPEAHEAD_SIZE
                            };
                            FT.ajax('/query/store', params)
                                .done(function(data){
                                    callback($.map(data.json, function(store){
                                        return store.name;
                                    }));
                                });
                        }
                    })
                    .resettable()
                    .on('change.ft-api', function(){
                    })
                    .end()
                .end()
            .end()
        .find('.items')
            .on('click.ft-api', '.item > a', function(){
                selectItem($(this).parent().attr('data-item-id'));
            })
            .end()
        .find('.selected-item-container')
            .on('click.ft-api', '.thumb.active .image', function(){
                $(this)
                    .find('img').attr('src', '').end()
                    .parents('.thumb:first').removeClass('active');

                refreshSaveBtn();
            })
            .end()
        .find('.external-add-container')
            .on('click.ft-api', '.external-add.btn:not(.disabled)', function(){
                var $url = $('input[name="url"]', $modal),
                    popupUrl = FT.urls.sayit + '?url=' + $url.val();

                window.open(popupUrl, 'sayit', 'width=400,height=500');

                $url.val('');

                FT.callback.on('sayit', function(itemId){
                    selectItem(itemId);
                });
            })
            .end()
        .find('.modal-footer')
            .on('click.ft-api', '.save.btn:not(.disabled)', function(){
                saved = true;
                $modal.modal('hide');
            })
    ;

    // Listen the window resize event and forward it to the modal
    $(window).resize(function(){
        $modal.triggerHandler('resize');
    });

    // Register interface to the global FT namespace
    (function(FT){
        var defaults = {
                preview: {
                    url: null,
                    size: [0, 0],
                    pos: [0, 0]
                }
            },
            openItemSearcher = function(options){
                var opts = $.extend(true, {}, defaults, options),
                    defer = $.Deferred();

                if(currentCaller){ // TEMP
                    defer.reject('ItemSearcher is already opened.');
                    return defer.promise();
                }
                currentCaller = defer;

                if(opts.preview.url)
                    updatePreview(opts.preview);

                $modal.modal('show');

                return defer.promise();
            };

        FT.openItemSearcher = openItemSearcher;
    })(window.FT);
});
</script>
    <div id="inactivate-dot-modal" class="modal fade hide">
    <div class="modal-header">
        <a class="close" data-dismiss="modal">&times;</a>
        <h3>
            <i class="icon icon-hanger-pink-big"></i>
            <span class="text">Why do you want to delete?</span>
        </h3>
    </div>
    <div class="modal-body">
        
        <label class="radio">
            <input type="radio" name="reason" value="nm" />
            Not matched to this look
            
        </label>
        
        <label class="radio">
            <input type="radio" name="reason" value="ug" />
            Looks ugly
            
        </label>
        
        <label class="radio">
            <input type="radio" name="reason" value="ht" />
            I hate them
            
        </label>
        
        <label class="radio">
            <input type="radio" name="reason" value="ot" />
            Other
            
            <div class="note-container">
                <textarea name="reason-note" rows="2" placeholder="Please explain the reason of deleting."></textarea>
            </div>
            
        </label>
        
    </div>
    <div class="modal-footer">
        <button class="delete btn btn-primary disabled" data-dismiss="modal" data-loading-text="Loading...">Delete</button>
    </div>
</div>

<script>
$(function(){
    var $modal = $('#inactivate-dot-modal'),
        $deleteBtn = $('.delete.btn', $modal),
        $noteContainer = $('.note-container', $modal);

    $modal
        .on('change.ft-api', 'input[type="radio"]', function(){
            $deleteBtn.removeClass('disabled');
            if($(this).val() == 'ot')
                $noteContainer.show();
            else
                $noteContainer.hide();
        })
        .on('hidden.ft-api', function(){
            $('input[type="radio"]', $modal).removeAttr('checked');
            $('textarea', $modal).val('');
            $noteContainer.hide();
            $deleteBtn.addClass('disabled');
        });

    // Register interface to the global FT namespace
    (function(FT){
        var openInactivateDot = function(){
                var defer = $.Deferred();

                $modal
                    .off('.inactivate')
                    .one('click.inactivate.ft-api', '.delete.btn:not(.disabled)', function(){
                        defer.resolve(
                            $('input[name="reason"]:checked', $modal).val(),
                            $('textarea[name="reason-note"]', $modal).val()
                        );
                    })
                    .modal('show');

                return defer.promise();
            };

        FT.openInactivateDot = openInactivateDot;
    })(window.FT);
});
</script>
    <div id="thank-modal" class="modal fade hide">
    <div class="modal-header">
        <a class="close" data-dismiss="modal">&times;</a>
        <h3>
            <i class="icon icon-hanger-pink-big"></i>
            <span class="text">
                You have <strong><span class="available-count"></span> THANKS</strong> left today.
            </span>
        </h3>
    </div>
    <div class="modal-body">
        <p class="available">
            Show your appreciation by giving a <i class="icon icon-hanger-pink-small"></i> THANKS to <img class="craver thumb-small" />
        </p>
        <p class="unavailable">
            You've used all of THANKS today. <br /> You'll get new ones tomorrow!
        </p>
        <p class="sent">
            <i class="icon icon-hanger-pink-small"></i>
            <span class="text">Sent a THANKS to <img class="craver thumb-small" /></span>
        </p>
    </div>
    <div class="modal-footer">
        <button class="btn" data-dismiss="modal">Close</button>
        <button class="thank btn btn-primary" data-loading-text="Sending...">THANKS</button>
    </div>
</div>

<script>
$(function(){
    var $modal = $('#thank-modal'),
        states = ['available', 'unavailable', 'sent'];

    $modal
        .on('show.ft-api', function(){
            var count = $(this).data('available-count'),
                $thankBtn = $('.thank.btn', this);

            $modal
                .find('.available-count').text(count).end()
                .find('img.craver').attr('src', FT.getCraverImageUrl($(this).data('receiver-id'), 'thumb-small')).end();

            if(!count){
                $thankBtn.addClass('disabled');
                $modal.addClass('unavailable');
            }else{
                $thankBtn.removeClass('disabled');
                $modal.addClass('available');
            }
        })
        .on('hidden.ft-api', function(){
            $modal
                .removeClass(states.join(' '))
                .find('.modal-footer .btn').removeClass('disabled').end();

            $.removeData($modal);
        })
        .on('click.ft-api', '.thank.btn:not(.disabled)', function(){
            var $this = $(this),
                data = {
                    'type': 'thank',
                    'receiver-id': $modal.data('receiver-id'),
                    'look-id': $modal.data('look-id'),
                    'dot-id': $modal.data('dot-id')
                };

            $this.button('loading');
            FT.ajax(FT.urls.apiThank, data, 'POST')
                .done(function(){
                    var currentCount = parseInt($modal.find('.available-count').text());
                    $('.thank-btn[data-receiver-id="{0}"][data-look-id="{1}"]'.format(data['receiver-id'], data['look-id'])).addClass('thanked');

                    $this.button('reset');
                    $.wait().then(function(){
                        $this.addClass('disabled');
                    });

                    $modal
                        .removeClass(states.join(' '))
                        .addClass('sent')
                        .find('.available-count').text(currentCount - 1).end()
                        .find('p.sent').pulsate({ count: 2 }, function(){
                            $.wait(500).then(function(){
                                $modal.modal('hide');
                            });
                        });
                })
                .fail(function(){
                    $this.button('reset');
                });
        });

    // Register thank interface to the global FT namespace
    (function(FT){
        $('body').on('click.ft-api', '.thank-btn[data-receiver-id]:not(.thanked)', function(){
            var $this = $(this);

            if(!FT.craver.isLogin){
                FT.goLogin();
                return;
            }

            FT.ajax(FT.urls.apiThank, { type: 'query_available' })
                .done(function(data){
                    var count = parseInt(data.count);

                    $modal
                        .data('receiver-id', $this.attr('data-receiver-id'))
                        .data('look-id', $this.attr('data-look-id'))
                        .data('dot-id', $this.attr('data-dot-id'))
                        .data('available-count', parseInt(data.count))
                        .modal('show');
                });
        });
    })(window.FT);
});
</script>
    <div id="confirm-modal" class="modal fade hide">
    
    <div class="modal-body">
        <p class="message"></p>
        <div>
            <button class="btn" data-action="yes" data-dismiss="modal">Yes</button>
            <button class="btn" data-action="no" data-dismiss="modal">No</button>
        </div>
    </div>
</div>

