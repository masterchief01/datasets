	<script type="text/javascript"> 
                var flyoutTimeout=undefined; 
                $(document).ready(function(){ 
                        $("#nav-reviews,#flyoutWrapper").bind("mouseenter",function(){ 
                                if(undefined!=flyoutTimeout) 
                                        clearTimeout(flyoutTimeout); 
                                flyoutTimeout=setTimeout("showReviewsFlyout()",30); 
                        }); 
                
                
                        $("#flyoutWrapper,#nav-reviews").bind("mouseleave",function(){ 
                                if(undefined!=flyoutTimeout) 
                                        clearTimeout(flyoutTimeout); 
                                flyoutTimeout=setTimeout("hideReviewsFlyout()",30); 
                        }); 
                }); 
                
                function hideReviewsFlyout(){ 
                        $("#nav-reviews").removeClass("selected"); 
                        $('#flyoutWrapper').slideUp("fast"); 
                } 
                
                function showReviewsFlyout(){ 
                              $("#nav-reviews").addClass("selected"); 
                                $('#flyoutWrapper').slideDown(30);                 
                } 
                
	</script> 
    
	
