	<script>
    
    var ti = 0;
    function twitter_login($this){
        if( ti == 0){ 
            ti = ti+1;
            var auth_url = $this.attr('auth-url');
            twitter=window.open(auth_url,'Twitter Auth Page','height=500,width=750,screenX=200,screenY=50');
            self.location.reload();
            if (window.focus) {twitter.focus()}
        }else{
            alert('That twitter link has expired...\nWe will refresh the page for you, so you can try again! :)');
            location.reload();
        }
    }
    
</script>
