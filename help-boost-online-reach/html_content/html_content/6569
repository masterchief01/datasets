  $('ul.site-headlines a').hover(
    function() {
      var xd = $(this).width() - ($(this).closest('ul').width() - 30);
      if (xd > 0) $(this).parent().stop().animate({'margin-left': -xd+'px'}, 1000);
    },
    function() {
      $(this).parent().stop().animate({'margin-left': '0px'}, 1000);
