<div id="fb-root"></div>

<script type="text/javascript">
//<![CDATA[

  window.fbAsyncInit = function() {
    FB.init({
      appId: '138711277798', 
      status: true, 
      cookie: true, 
      xfbml: true,
      channelUrl: document.location.protocol + '//www.funnyordie.com/fb_channel.html',
      oauth: true
    });
    $('#fb-root').trigger('fb_connect:loaded');
  };
  jQuery("body").bind('lastAd.loaded', function() {
    var fb = document.createElement('script'); 
    fb.async = true;
    fb.src = document.location.protocol +
      '//connect.facebook.net/en_US/all.js#xfbml=1';
    document.getElementById('fb-root').appendChild(fb);
  });
  
  $(document).bind('fb_connect:loaded', function(event) {
    if ($('#fb_connect_href').length) {
      $('#fb_connect_href').click( function(event) {
        $("#fb_connect").hide();
        $("#fb_spinner").show();
        FB.login(handleFBSessionResponse, {scope:'email'});
        GA.event("fb_connect", "login", "60ece2663b");
        return false;
      });
    }
    FB.Event.subscribe('edge.create', function(href, widget) {
      GA.event("sharing", "facebook_like", href);
    });
    
    FB.Event.subscribe('comment.create', function() {
      GA.event('sharing', 'facebook_comment', '60ece2663b');
    });
  });
  
  function handleFBSessionResponse(response) {
    if ((response.authResponse) && (response.authResponse.accessToken)) {
      $.post('/users/fb_connect_login', {fb_at: response.authResponse.accessToken});
    }
  }
  
  function add_fb_at() {
    FB.getLoginStatus(function(response) {
      if ((response.authResponse) && (response.authResponse.accessToken)) {
        $('#fb_at').val(response.authResponse.accessToken);
      }
    });  
  }
  
  $(function() {
    $('#facebook_login_form').submit(function(event) {
      $('#fb_login_form').hide();
      $('#fb_login_spinner').show();
      add_fb_at();
      if ($('#fb_at').val() == '') event.preventDefault();
    });
  });

//]]>
</script>

