      }).prependTo('.anythingControls');

      $('#slideshow h4').show();
  	  $('#slides img, #slides hr, #slides .title').hide();
  	  $('#slides .caption').hide().eq(startPanel-1).addClass('active').show();
  	  $('#slideshow h4 span').text($('#title_'+startPanel).text());
