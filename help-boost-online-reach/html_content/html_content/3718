        startPanel: startPanel,
        hashTags: false,
        resizeContents: false,
        startStopped: true,
        buildNavigation: false,
        buildStartStop: false,
    
        onSlideBegin: function(slider) {
          $('#slides .active').removeClass('active').hide();
        },
        onSlideComplete: function(slider) {
          if(slider.currentPage == 12) {
            $("#slideshow").addClass("slideshow-done");
            slider.startStop(false);
          } else {
            $("#slideshow").removeClass("slideshow-done");
          }
          $('#slideshow h4 span').text($('#title_'+slider.currentPage).text());
          $('#caption_'+slider.currentPage).addClass('active').fadeIn();
