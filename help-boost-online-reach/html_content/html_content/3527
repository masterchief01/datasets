<script type="text/javascript">
OAS_AD.addHandler('takeover:loaded', function() {
  var takeover = $('ocom_takeover_topper');
  takeover.style.paddingTop = "9px";
  document.write("<style>#ocom_content_wrapper{background-image:none}</style>");
});
// Must use plain old javascript (no dependencies)
(function() {
  var input = document.getElementById('header_q'),
      query = window.location.search.substring(1),
      vars = query.split("&");

  for (var i=0;i<vars.length;i++) {
    var pair = vars[i].split("=");
    if (pair[0] == 'q') {
      var qstr = pair[1];
      input.value = decodeURIComponent(qstr).replace(/\+/, ' ');
      input._has_term = true;
      break;
    }
  }

  if (input) {
    input.onfocus = function() {
      if (!input._has_term) {
        input.value = '';
        input._has_term = true;
      }
    }

    input.onkeydown = function(event) {
      if ((event || window.event).keyCode == 13) {
        _submitSearch();
      }
    };

    document.getElementById('header_q_btn').onclick = function() {
      _submitSearch();
    };
  }

  var moreLink = document.getElementById('ocom_more_link');
  var moreNav = document.getElementById('ocom_more_nav');
  var showTimer, hideTimer;

  function _submitSearch() {
    window.location = "http://myown.oprah.com/search/index.html?q=" + encodeURIComponent(input.value);
  }

  function getOffset(el, context) {
    var rt = {x:0, y:0},
      docElem = document.documentElement;

    while (el && el != context) {
      rt.y += el.offsetTop;
      rt.x += el.offsetLeft;
      el = el.offsetParent;
    }

    return rt;
  }

  function clearAll() {
    if (showTimer) {
      clearTimeout(showTimer);
      showTimer = null;
    }
    if (hideTimer) {
      clearTimeout(hideTimer);
      hideTimer = null;
    }
  }

  function showNav() {
    clearAll();
    if (moreNav.style.display != 'none') return;
    showTimer = setTimeout(function() {
      showTimer = null;
      moreNav.parentNode.removeChild(moreNav);
      document.body.appendChild(moreNav);
      var subnav = document.getElementById('ocom_subnav');
      if (subnav) {
        offs = getOffset(subnav);
        moreNav.style.zIndex = 10;
        moreNav.style.display = 'block';
        moreNav.style.top = '169px';
        moreNav.style.left = (offs.x + 988 - 145) + 'px';
      }
    }, 100);
  }

  function hideNav() {
    clearAll();
    if (moreNav.style.display == 'none') return;
    hideTimer = setTimeout(function() {
      hideTimer = null;
      moreNav.style.display = 'none';
      //if (advert) advert.style.visibility = 'visible';
    }, 300);
  }

  moreLink.onmouseover = showNav;
  moreLink.onmouseout = hideNav;
  moreNav.onmouseover = showNav;
  moreNav.onmouseout = hideNav;

})();
//var tmp_str = $('dart_ad_leaderboard').innerHTML;
//alert(tmp_str);
//var match = tmp_str.replace(/<script language="JavaScript" st.php(.*?)\&gt;/, '');
</script>

