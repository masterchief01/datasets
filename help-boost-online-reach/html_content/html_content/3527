<script type="text/javascript">
function logIn() {
  if (!loginCheck({
    login_req: 1,
    onLogin: setTopHeader
  })) return;
}
function setReferrer(obj) {
  if (Global.redirect_url_ref) { 
    obj.href += ('?redirect_url_ref=' +  Global.redirect_url_ref);
    Global.redirect_url_ref = null;
  }
}

setTopHeader();
</script>
