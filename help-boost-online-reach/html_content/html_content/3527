<script type="text/javascript">
function postForm(form,url,page)
{
  form.action = url;
  form.page.value = page;
  form.submit();
  return true;
}

function rateMedia(rating) {
  var myAjax = new Ajax.Request( Global.SERVER_NAME + '/cgi-bin/ajax_processor.cgi', {
    method: 'post',
    parameters: {
      op:         'rate_media',
      entity_id:  '100000002',
      media_id:   '24824',
      rating:     rating
    },
    onComplete: function(request) {
      alert('You successfully rated this article');
      $('history_count').innerHTML = parseInt($('history_count').innerHTML) + 1;
      switchInfoTab('ratings');

      var url = "http://myown.oprah.com/common/media_rate.inc";
      var pars = "entity_id=100000002&media_id=24824&rating_click=rateMedia&quote_align=bottom";
      var myAjax = new Ajax.Updater('media_rate_container', url, {
        method: 'get',
        parameters: pars,
        evalScripts: true
      });
    }
  });
}
</script>
<script>
(function() {

var _actualSlide = 5,
    _size = 6,
    _pageWidth = 559,
    thumbs;

/* */

  function _initAjaxLinks() {
    /* */
  }
  _initAjaxLinks();
var thumbs;

    function _doNext() {
      if (thumbs.data.currentPage == Math.ceil((_size / 5) - 1 )) {
        $('next_slide').addClassName('inactive');
      }

      if (thumbs.data.currentPage != 0) {
        $('previous_slide').removeClassName('inactive');
      }

      if (thumbs.data.currentPage == 0) {
        $('previous_slide').addClassName('inactive');
      }

      if (thumbs.data.currentPage != Math.ceil((_size / 5) - 1)) {
        $('next_slide').removeClassName('inactive');
      }

      _initAjaxLinks();
      PACKAGE_AD.initializeLinks('#slide_navigation a')
    }

    function _initCarousel() {
      thumbs = new Maui.Carousel("slide_navigation" , {
        prev: 'previous_slide',
        next: 'next_slide',
        pageSize: 5,
        pageWidth: _pageWidth,
        currentPage: parseInt(_actualSlide/5),
        loop: false,
        tween: false
      });

      $('slide_navigation').observe('carousel:afterPage', _doNext );
    }

    _initCarousel();
    _doNext();
})();

</script>
</div>

