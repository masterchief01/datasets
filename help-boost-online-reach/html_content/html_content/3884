						<script type="text/javascript">

   $jq(document).ready(function(){
    // Check height of div
    if ($jq("#outer").length > 0) {
        var outer=document.getElementById('outer');
        var inner=document.getElementById('inner');
        var outersize = outer.offsetHeight;
        var innersize = inner.offsetHeight;

        if (innersize <= outersize) {
            $jq("#morebio").hide();
        }
    }

    });

    function toggleHeight(button) {
        var outer=document.getElementById('outer');
        var inner=document.getElementById('inner');
        var inOut = outer.offsetHeight <= 49 ? false : true;
        var finalHeight = inOut ? 49 : inner.offsetHeight + 20 ;
        outer.style.height=finalHeight + 'px';
        button.innerHTML = inOut ? 'More >>' : '<< Less';
    }

</script>





