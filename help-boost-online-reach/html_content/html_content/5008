		var randIndex = Math.ceil(Math.random() * numSlugs) - 1;

		function getCookie(NameOfCookie) {
			if (document.cookie.length > 0) {
				begin = document.cookie.indexOf(NameOfCookie+"=");
				if (begin != -1) {
					begin += NameOfCookie.length+1;
					end = document.cookie.indexOf(";", begin);
					if (end == -1) end = document.cookie.length;
					return unescape(document.cookie.substring(begin, end)); 
				}
			}
			return null;
		}
		
		function setCookie(NameOfCookie, value, expiredays) {
			var ExpireDate = new Date ();
			ExpireDate.setTime(ExpireDate.getTime() + (expiredays * 24 * 3600 * 1000));
			document.cookie = NameOfCookie + "=" + escape(value) + ((expiredays == null) ? "" : "; expires=" + ExpireDate.toGMTString()) + "; path=/";
		}
		

		function initSeqMode() {
			var seqMode = getCookie("fpdSeqMode");
			if(seqMode == "one") seqMode = 1;
			else seqMode = 0;
			setSeqMode(seqMode);
		}

		initSeqMode();

		function setSeqMode(mode) {
			if(mode == 1) {
				scSeqElem.src = "http://foodporndaily.com/wp-content/themes/fpd-010/img/sequential-off.gif";
				scRandElem.src = "http://foodporndaily.com/wp-content/themes/fpd-010/img/random-on.gif";
				mainLinkElem.href = "http://foodporndaily.com/pictures/" + randomSlugs[randIndex];
				setCookie("fpdSeqMode", "one");
			} else {
				scSeqElem.src = "http://foodporndaily.com/wp-content/themes/fpd-010/img/sequential-on.gif";
				scRandElem.src = "http://foodporndaily.com/wp-content/themes/fpd-010/img/random-off.jpg";
				mainLinkElem.href = mainLinkOrigValue;
				setCookie("fpdSeqMode", "zero");
			}
