	function log() { 
	var loca=document.location+"";
	if (window.status.indexOf('go to') == 0) 
	{ 
	bug = new Image(); 
	if(loca.indexOf('?') > -1)
	{
		bug.src = loca + '&site=' + loca + '&target=' + window.status.substring(6) + '';
	}
	else
	{
		bug.src = loca + '?site=' + loca + '&target=' + window.status.substring(6) + '';
