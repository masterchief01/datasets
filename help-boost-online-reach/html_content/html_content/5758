<script type="text/javascript">
    jQuery(document).ready(function() {
    jQuery("#TopNavSearchText").autocomplete('/Services/PredictiveSearch/SearchSuggestionService.svc/json/suggestions',
            { width: 320,
                max: 10,
                selectFirst: false,
                scroll: false,
                scrollHeight: 250,
                minChars: 3,
                dataType: "json",
                cacheLength: 0,
                extraParams: { terms: function() { return jQuery("#TopNavSearchText").val() + "*"; } },
                parse: function(data) {
                    var items = data[1];
                    var parsed = [];
                    for (var i = 0; i < items.length; i++)
                        parsed[i] = {
                            data: [items[i]],
                            value: items[i],
                            result: [items[i]]
                        };
                    return parsed;
                }
            });
    });

</script>


 
<script language="JavaScript" type="text/javascript">
    //<![CDATA[

    jQuery(function() {
        jQuery('#TopNavSearchText').keypress(function(e) {
            if (e.keyCode == '13') {
                e.preventDefault();
                TopNavSearch();
            }
        });

        jQuery('#TopNavSearch').click(function(e) {
            e.preventDefault();
            TopNavSearch();
        });
    });
    
    function TopNavSearch() {
        if (TopNavSearchHasErrors()) {
            alert('Enter Search Term');
        } else {
            dcsMultiTrack('DCS.dcsuri', '/banner', 'WT.ti', 'Banner', 'DCSext.SearchSource', 'BannerKeyword', 'WT.ad', '');
            dcsMultiTrack('DCS.dcsuri', '/site_search', 'WT.ti', 'User Submitted Site Search');
            ntptEventTag('ev=search_BT&sitekeyword=' + jQuery('#TopNavSearchText').val().toLowerCase());
            MarinTagging('UTM:I||searc369||||');
            PerformSearch(jQuery('#TopNavSearchText').val());            
        }
    }
    
    function TopNavSearchHasErrors() {
        var result = false;
        var searchTerm = jQuery('#TopNavSearchText').val();
        result = (searchTerm == '' || searchTerm == 'Enter recipe name, ingredient or keyword');
        return result;
    }
    //]]>
</script>


