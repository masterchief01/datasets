<script language="javascript" type="text/javascript">
    function similarRecipe(url, id, title) {
        var recipeId = id;
        var recipeTitle = title;
        window.location = url;

        ntptEventTag('ev=relatedRecipes_click&RecipeId=' + recipeId + '&RecipeTitle=' + encodeURIComponent(recipeTitle));

        dcsMultiTrack('DCS.dcsuri', '/recipes/similar_recipe_click', 'WT.ti', 'Recipe Page Find Similar');

    }

</script>



