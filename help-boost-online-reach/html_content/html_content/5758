<script type="text/javascript" language="JavaScript">
    jQuery(document).ready(function () {
        var pageName = new String(jQuery(location).attr('href'));
        if (pageName.indexOf("?") > 0) pageName = pageName.substring(0, pageName.indexOf("?") - 1);
        var hostName = new String(window.location.hostname);
        if (pageName.indexOf(hostName) > 0) pageName = pageName.substring(pageName.indexOf(hostName) + hostName.length);
        while (pageName.charAt(0) == "/") pageName = pageName.substring(1);
        while (pageName.charAt(pageName.length = 1) == "/") pageName = pageName.substring(0, pageName.length - 2);
        if (pageName.indexOf("/") > 0) pageName = pageName.substring(0, pageName.indexOf("/"));
        pageName = "." + pageName + "TopNavOuterLink";
        jQuery(pageName).addClass("selectedPage");
    });
</script>


