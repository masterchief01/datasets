    function checkFacebookShareCounts() {
    var getByClass = function(c) {
        var nodes = [];
        var elements = document.getElementsByTagName('*');

        for (var i = 0, len = elements.length; i < len; i++) {
            if (elements[i].className.indexOf(c) >=0 ) {
                nodes.push(elements[i]);
            }
        }
        return nodes;
    };
    var fbShares = getByClass('fb_share_count_inner');
    if(fbShares.length > 0) {
      for (var index = 0, len = fbShares.length; index < len; ++index) {
        var item = fbShares[index];
        if (item.innerHTML == '&nbsp;') {
          item.innerHTML = '0';
        }
      }
    }
}

