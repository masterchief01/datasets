			localStorage.setItem('jList', '9149928,9150087,9150056,9149748,9150115,9149899,9149973,9150064,9150004,9150099');
		}
	});

	var g_ext = null;

	function pinifyTeaser(){
 		if (!window.external.msIsSiteMode()) {
 			//$('body').pinable({discStyle: 'topHat'});
 			$('body').prepend('<div class="pinifyNotification"><div id="pinifyTeaser"></div></div>');
 			$('#pinifyTeaser').pinify('pinTeaser', {
 				icon: '/template/ver1-0/i/jumplist/Telegraph_T20x20.png',
 				addStartLink: false,
 	            linkText: 'Click here to add this site to the start menu',
 	            pinText: 'Drag this icon to your Taskbar to get faster access to the latest news.',
 	            backgroundColor: '#8dac5a',
 	            style: {
 				        backgroundImage: "/template/ver1-0/i/jumplist/toolbar_bg.png",
 				        closeButtonImage: "/template/ver1-0/i/jumplist/close.png"
 				    }
 			});
 		}
 	}

 	function pimpMyList(){
		try {
			if (window.external.msIsSiteMode()) {
				g_ext = window.external;
				g_ext.msSiteModeClearJumpList();
				g_ext.msSiteModeCreateJumpList("Latest on News - 17 Mar 2012");
				g_ext.msSiteModeAddJumpListItem("Doctor 'distorted diagnoses for local authorities'", "/health/9149928/Doctor-distorted-diagnoses-for-local-authorities.html", "/template/ver1-0/i/icons/favicon.ico");
			
				g_ext.msSiteModeAddJumpListItem("Hosepipe ban firms losing millions of gallons though leaks", "/earth/earthnews/9150087/Hosepipe-ban-firms-losing-millions-of-gallons-though-leaks.html", "/template/ver1-0/i/icons/favicon.ico");
			
				g_ext.msSiteModeAddJumpListItem("Maude: brother's Aids death changed my gay marriage views", "/family/9150056/Francis-Maude-My-brothers-Aids-death-transformed-my-views-on-gay-marriage.html", "/template/ver1-0/i/icons/favicon.ico");
			
				g_ext.msSiteModeAddJumpListItem("Millions to lose rights to national pay rates", "/finance/budget/9149748/National-pay-rates-will-be-scrapped-in-budget.html", "/template/ver1-0/i/icons/favicon.ico");
			
