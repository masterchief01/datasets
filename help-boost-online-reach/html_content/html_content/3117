            $(window).load(function(){
                $("#news-content").fitVids();

                if(!window.related_news.length) {
                    $('#similar-news').hide();
                }
                    
                /*
                $.sharedCount(location.href, function(data){
                    $('#sharedcount').html((data.StumbleUpon || 0) + (data.Reddit || 0) + (data.Facebook.total_count || 0) + (data.Delicious || 0) + (data.GooglePlusOne || 0) + (data.Buzz || 0) + (data.Twitter || 0) + (data.Diggs || 0) + (data.Pinterest || 0) + (data.LinkedIn || 0));
                });
                
                */

                $('#comment-jumper').click(function() {
                    $(document).scrollTop($('#comment-anchor').offset().top - $('#masthead').height() - 40);
                });
                // window.resizeFeature();

                $("#masthead-wrapper").sticky({ topSpacing: 0, center: true, className: "sticky" });

                $(".news-cover .heading").dotdotdot({});

                $('.news-covers').masonry({
                    itemSelector: '.news-cover, .ad, .ad-leaderboard',
                    gutterWidth: 28
                });

                $('#similar-news .content').masonry({
                    itemSelector: '.news-cover',
                    gutterWidth: 28
                });

                window.resizePage();

                $(window).bind('resize', $.throttle(200, false, function() {
                    // window.resizeFeature();
                    window.resizePage();

                    $('#masthead-wrapper-sticky-wrapper').width(
                        $(window).width()
                    );
                }));

                $(window).bind('scroll', $.throttle(200, false,  function() {
                    if(($(document).height() - $(document).scrollTop() - $(window).height()) < window.lazyLoadThreshold) {
                        window.renderNewArticles();
                    }

                    if($("#comment-jumper:above-the-top").length) {
                        $("#masthead-wrapper").addClass('beyond-share');
                    } else {
                        $("#masthead-wrapper").removeClass('beyond-share');
                    }
                }));

                window.mouseOnAddThis = false;
                
                $(document).on('mouseenter', '.news-cover', function() {
                    $(this).find('.share').show();
                });

                $(document).on('mouseleave', '.news-cover', function() {
                    var self=this;
                    setTimeout(function() {
                        if (!window.mouseOnAddThis) {
                            $(self).find('.share').hide();
                        }
                    }, 200);
                });

                $(document).on('mouseenter', '.atm', function() {
                    window.mouseOnAddThis = true;
                });

                $(document).on('mouseleave', '.atm', function() {
                    window.mouseOnAddThis = false;
                    $('.share').hide();
                });

                $(document).on('click', '.news-cover .category', function() {
                    window.location.href = "/category/" + $(this).html();
                });
            });
        </script>
