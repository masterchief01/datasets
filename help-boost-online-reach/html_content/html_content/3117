            window.renderNewArticles = function() {
                if (window.loadingNewArticles) {
                    return;
                }
                window.loadingNewArticles = true;
                $('#loading-animation').show();
                $.getJSON('news-archive.php', {lastnewsid: window.lastNewsID}, function(data) {
                    var newRowID = 'news-covers'+window.lastNewsID;
                    // var newRow = $.el.div({id: newRowID, class: 'news-covers row dynamic-width'});
                    var newsCoversDiv =$('#newsroll .news-covers');
                    
                    //var imgPattern = new RegExp('(<img src=["\'])http://editorial.designtaxi.com/(.*)(png|jpg|jpeg|gif["\'])');

                    // $(newRow).hide();

                    $.each(data, function(k, v) {
                        /*
                        var matches = imgPattern.exec(v.body);
                        if(matches) {
                            v.img_path = "thumbnail.php?image=" + matches[2] + matches[3];
                        } else {
                            v.img_path = "img/img-placeholder.png";
                        }
                        */
                        v.news_id = parseInt(v.news_id);
                        newsCoversDiv.append(newsCoverTemplate.render(v));
                        window.lastNewsID = v.news_id < window.lastNewsID ? v.news_id : window.lastNewsID;
                    });

                    newsCoversDiv.append(
                        $.el.div({class: 'ad newItem'},
                            $.el.div({class: 'information'},
                                $.el.a({href: 'advertise.html'}, 'Advertise with us')
                            ),
                            // $.el.iframe({scrolling: 'no', class: 'ad-300x250', seamless: 'seamless', scrolling: 'no', src: 'ad-300x250.html'})
                            $.el.iframe({scrolling: 'no', class: 'ad-300x250', seamless: 'seamless', scrolling: 'no', src: 'ad-300x250.html'})
                        )
                    );

                    window.rearrangeLeaderboards();

                    // var newRowSelector = $('#'+newRowID);
                    newsCoversDiv.find(".news-cover.newItem .heading").dotdotdot({});
                    // newRowSelector.masonry({
                    //     itemSelector: '.news-cover, .ad',
                    //     gutterWidth: 28
                    //     // isFitWidth: true
                    // });
                    newsCoversDiv.masonry('reload');
                    newsCoversDiv.find('.newItem').fadeIn(function() {
                        $(this).removeClass('newItem');
                    });

                    window.loadingNewArticles = false;
                    $('#loading-animation').fadeOut();
                    stButtons.locateElements();
                    ReinitializeAddThis();
                });
            }
