				function(el){
					var test = el.up().nextSiblings()[0];
					if (test) {
						test = test.childElements()[0];
						if (test) {
							if (test.style.cursor == "pointer") {
								el.style.width="250px"; 
								el.style.height="";
								test.style.width="250px";
								test.style.height = "";
							}
						}
					}
				}
			);
		</script>
		
