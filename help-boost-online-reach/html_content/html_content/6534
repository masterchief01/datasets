<script type="text/javascript">

  window.fbAsyncInit = function() {
  
    FB.init({ appId: '155023867867753', status: true, cookie: true, xfbml: false});
    
    /* Check if the user is NOT logged into R29, but IS logged into Facebook.  Fix it */
    if (R29.url.param.getVar('fb') && !R29.user.token() && FB.getSession()) { //We have some sort of session, we *may* be logged in.
      FB.getLoginStatus(function(response) {
        if(response.status == 'connected') {
          return document.location = 'https://welcome.refinery29.com/register?from=http%3A%2F%2Fwww.refinery29.com%2Favengers-eye-makeup&amp;sig=e5ea3&amp;app=blog&amp;fb=1';
        }    
      });      
    }
        
    //FACEBOOK LOGIN
    $('a.fb_button, a.r29_fb_button').live('click', function() {
      R29.FBLoginPopup();
      return false;
    });
    
    if (typeof window.fbAsyncOnInit == "function") {
      window.fbAsyncOnInit();
    }
    
    /* GOOGLE ANALYTICS */

    FB.Event.subscribe('edge.create', function(targetUrl, fb_ref) {
    	if (fb_ref._attr.ref == "homepage"){
    		_gaq.push(['_trackEvent', 'FacebookIndex', 'like']);
    	}
    	else if (fb_ref._attr.ref == "entrypage"){
    		_gaq.push(['_trackEvent', 'FacebookSolo', 'like']);
    	}
    	else if (fb_ref._attr.ref == "sidebar"){
    		_gaq.push(['_trackEvent', 'FacebookSidebar', 'like']);
    	}
      _gaq.push(['_trackSocial', 'facebook', 'like', targetUrl]);
    });
    
    FB.Event.subscribe('edge.remove', function(targetUrl) {
      _gaq.push(['_trackSocial', 'facebook', 'unlike', targetUrl]);
    });
    
    FB.Event.subscribe('message.send', function(targetUrl) {
      _gaq.push(['_trackSocial', 'facebook', 'send', targetUrl]);
    });
    
       
  };
  
  R29.FBLoginPopup = function () {

    FB.login(function(response) {
    
      if (response.session) {
        //They *may* have given us perms, but who knows.
        return document.location = 'https://welcome.refinery29.com/connect/fb?from=http%3A%2F%2Fwww.refinery29.com%2Favengers-eye-makeup&amp;sig=e5ea3&amp;app=blog&amp;fb=1';
      } else {
        //Oh well, something is wrong, let's try the normal login
        return document.location = 'https://welcome.refinery29.com/register?from=http%3A%2F%2Fwww.refinery29.com%2Favengers-eye-makeup&amp;sig=e5ea3&amp;app=blog&amp;fb=1';
      }
      
     },{ scope: 'email,offline_access,publish_stream,publish_actions' } );
  }
  
  
</script>

